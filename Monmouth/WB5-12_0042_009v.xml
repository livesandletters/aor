        <marginalia hand="Italian" anchor_text="Totonesiu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        J[ohn] d[ee]. Tautenesus omnibus in clama[n]tib[us], occasione[m] dedere, vt loco, Tautenesus nomen permanerut [deleted] in hodieru[m] vsq[ue] diem.
                    </marginalia_text>
                    <person name="John Dee"/>
                    <location name="Totnes"/>
                </position>
            </language>
            <translation>John Dee. Totness. Everyone crying out, they gave (the name) Tautenesus for the occasion, so that they preserve the name Tautenensis in that place even to this day.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Albion ; giga[n]tibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        habitata gyga[n]tes olim.
                    </marginalia_text>
                </position>
            </language>
            <translation>Then inhabited by giants.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Bryto[n]es">
            <language ident="LA">
