    	<!-- Underlines -->
    	<underline method="pen" type="straight" language="EN" text="to warde the campe in the"/>
    	<underline method="pen" type="straight" language="EN" text="kepte watche without the Trenche, distaunte"/>
    	<underline method="pen" type="straight" language="EN" text="call Scoutes"/>
    	<underline method="pen" type="straight" language="EN" text="did; thinking that the armie might"/>
    	<underline method="pen" type="straight" language="EN" text="bee deceiued"/>
    	<underline method="pen" type="straight" language="EN" text="the difficultie, [that] is"/>
    	<underline method="pen" type="straight" language="EN" text="either corrupted, or"/>
    	<underline method="pen" type="straight" language="EN" text="oppressed of the"/>
    	<underline method="pen" type="straight" language="EN" text="in parte"/>
    	<underline method="pen" type="straight" language="EN" text="altogether on"/>
    	<underline method="pen" type="straight" language="EN" text="iudged it perillous"/>
    	<underline method="pen" type="straight" language="EN" text="of the watche, was with in [the] trenche"/>
    	<underline method="pen" type="straight" language="EN" text="did withall diligence keepe"/>
    	<underline method="pen" type="straight" language="EN" text="greate order punished with death"/>
    	<underline method="pen" type="straight" language="EN" text="obserued not the same"/>
    	<underline method="pen" type="straight" language="EN" text="ordinarely euery night, the thirde parte of the armie armed, &amp;"/>
    	<underline method="pen" type="straight" language="EN" text="same, the fowerth parte alwaies on foote"/>
    	<underline method="pen" type="straight" language="EN" text="bee destributed, throughout"/>
    	<underline method="pen" type="straight" language="EN" text="all the banckes, &amp; throughout all the places of the"/>
    	<underline method="pen" type="straight" language="EN" text="with double warde, placed in euery quadrant of"/>
    	<underline method="pen" type="straight" language="EN" text="stande still"/>
    	<underline method="pen" type="straight" language="EN" text="continually should goo from the one"/>
    	<underline method="pen" type="straight" language="EN" text="the other"/>
    	<underline method="pen" type="straight" language="EN" text="also in the daie"/>
    	<underline method="pen" type="straight" language="EN" text="haue the enemie nere"/>
    	<underline method="pen" type="straight" language="EN" text="watche worde"/>
    	<underline method="pen" type="straight" language="EN" text="and renuyng the same euery"/>
    	<underline method="pen" type="straight" language="EN" text="of greate importaunce, and"/>
    	<underline method="pen" type="straight" language="EN" text="great saufegarde obseruing"/>
    	<underline method="pen" type="straight" language="EN" text="greate diligence, to knowe at"/>
    	<underline method="pen" type="straight" language="EN" text="who lodgeth not in the Campe"/>
    	<underline method="pen" type="straight" language="EN" text="and whoo commeth a newe"/>
    	
    	<underline method="pen" type="straight" language="EN" text="vsed no Scoutes"/>
