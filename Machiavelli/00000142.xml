    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="the[m] vnto [the] enemie"/>
    	<underline method="pen" type="straight" language="EN" text="those that"/>
    	<underline method="pen" type="straight" language="EN" text="may beleeue thee"/>
    	<underline method="pen" type="straight" language="EN" text="with art"/>
    	<underline method="pen" type="straight" language="EN" text="by chaunce"/>
    	<underline method="pen" type="straight" language="EN" text="The army"/>
    	<underline method="pen" type="straight" language="EN" text="desired to fighte with Anibals"/>
    	<underline method="pen" type="straight" language="EN" text="same desire"/>
    	<underline method="pen" type="straight" language="EN" text="master of his"/>
    	<underline method="pen" type="straight" language="EN" text="good, to atte[m]pt"/>
    	<underline method="pen" type="straight" language="EN" text="contrarie opinions"/>
    	<underline method="pen" type="straight" language="EN" text="deuide the armye"/>
    	<underline method="pen" type="straight" language="EN" text="other fought, and comming"/>
    	<underline method="pen" type="straight" language="EN" text="ouerthrowen, if Fabius"/>
    	<underline method="pen" type="straight" language="EN" text="had not rescued him"/>
    	<underline method="pen" type="straight" language="EN" text="maister of"/>
    	<underline method="pen" type="straight" language="EN" text="howe it was a wise waye"/>
    	<underline method="pen" type="straight" language="EN" text="Fabius"/>
    	<underline method="pen" type="straight" language="EN" text="to incourage the[m] to fight"/>
    	<underline method="pen" type="straight" language="EN" text="to disdaine the"/>
    	<underline method="pen" type="straight" language="EN" text="speake slanderous woordes"/>
    	<underline method="pen" type="straight" language="EN" text="haue intelligence with them, and to"/>
    	<underline method="pen" type="straight" language="EN" text="corrupted part of them"/>
    	<underline method="pen" type="straight" language="EN" text="inca[m]pe in place, where"/>
    	<underline method="pen" type="straight" language="EN" text="maye see the"/>
    	<underline method="pen" type="straight" language="EN" text="make some light skirmishe"/>
    	<underline method="pen" type="straight" language="EN" text="is dayly seene"/>
    	<underline method="pen" type="straight" language="EN" text="facilitie is despised"/>
    	<underline method="pen" type="straight" language="EN" text="them to be vnworthie"/>
    	<underline method="pen" type="straight" language="EN" text="an oration for the"/>
    	<underline method="pen" type="straight" language="EN" text="to"/>
    	<underline method="pen" type="straight" language="EN" text="them of their cowardnesse, and"/>
    	<underline method="pen" type="straight" language="EN" text="them ashamed"/>
    	<underline method="pen" type="straight" language="EN" text="ashamed, to tell them that you will fight alone"/>
    	<underline method="pen" type="straight" language="EN" text="not beare you company"/>
    	<underline method="pen" type="straight" language="EN" text="this advertismente"/>
    	<underline method="pen" type="straight" language="EN" text="Souldiour obstinate to fighte"/>
    	<underline method="pen" type="straight" language="EN" text="sende home any of their substaunce"/>
    	<underline method="pen" type="straight" language="EN" text="any place, till the warre"/>
    	<underline method="pen" type="straight" language="EN" text="flying saue their life"/>
    	<underline method="pen" type="straight" language="EN" text="saueth not them their goodes"/>
    	<underline method="pen" type="straight" language="EN" text="the loue whereof"/>
    	<underline method="pen" type="straight" language="EN" text="make men obstinate in"/>
    	<underline method="pen" type="straight" language="EN" text="the souldiours"/>
    	<underline method="pen" type="straight" language="EN" text=", with speaking"/>
    	<underline method="pen" type="straight" language="EN" text="all the army must be"/>
    	<underline method="pen" type="straight" language="EN" text="heades thereof"/>
    	
    	<underline method="pen" type="straight" language="EN" text="courage soldiours"/>
    	<underline method="pen" type="straight" language="EN" text="make the soldiour most obstinately"/>
    	
