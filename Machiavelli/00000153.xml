	<underline method="pen" type="straight" language="EN" text="should come grosse"/>
    	<underline method="pen" type="straight" language="EN" text="assault thee on twoo sides, those"/>
    	<underline method="pen" type="straight" language="EN" text="to bee made"/>
    	<underline method="pen" type="straight" language="EN" text="strong with the other twoo sides, that are not"/>
    	<underline method="pen" type="straight" language="EN" text="doubling the orders in eche"/>
    	<underline method="pen" type="straight" language="EN" text="deuiding"/>
    	<underline method="pen" type="straight" language="EN" text="artillerie"/>
    	<underline method="pen" type="straight" language="EN" text="the Veliti, and the horse. If"/>
    	<underline method="pen" type="straight" language="EN" text="on three or on foure sides"/>
    	<underline method="pen" type="straight" language="EN" text="that either"/>
    	<underline method="pen" type="straight" language="EN" text="thou"/>
    	<underline method="pen" type="straight" language="EN" text="lacke prudence"/>
    	<underline method="pen" type="straight" language="EN" text="thou shalt be wise"/>
    	<underline method="pen" type="straight" language="EN" text="minding, safelye to"/>
    	<underline method="pen" type="straight" language="EN" text="so great, that on"/>
    	<underline method="pen" type="straight" language="EN" text="many men, as"/>
    	<underline method="pen" type="straight" language="EN" text="almost in all thy army"/>
    	<underline method="pen" type="straight" language="EN" text="so vnwise"/>
    	<underline method="pen" type="straight" language="EN" text="selfe in the daunger &amp; force of an enemie"/>
    	<underline method="pen" type="straight" language="EN" text="hath three times more men ordayned then"/>
    	<underline method="pen" type="straight" language="EN" text="catche hurte, thou canst blame no man but"/>
    	<underline method="pen" type="straight" language="EN" text="through thy fault"/>
    	<underline method="pen" type="straight" language="EN" text="some mischaunce"/>
    	<underline method="pen" type="straight" language="EN" text="shall bee without the shame"/>
    	<underline method="pen" type="straight" language="EN" text="thee, as vnto the Scipions"/>
    	<underline method="pen" type="straight" language="EN" text="to Asdrubal in Italie"/>
    	<underline method="pen" type="straight" language="EN" text="haue not manye more"/>
    	<underline method="pen" type="straight" language="EN" text="disorder thee, to assaulte thee"/>
    	<underline method="pen" type="straight" language="EN" text="foolishnesse, and thy good"/>
    	<underline method="pen" type="straight" language="EN" text="become so thinne"/>
    	<underline method="pen" type="straight" language="EN" text="then easyly thou"/>
    	<underline method="pen" type="straight" language="EN" text="ouerthrow"/>
    	<underline method="pen" type="straight" language="EN" text="withstande an other"/>
    	<underline method="pen" type="straight" language="EN" text="ruinate him"/>
    	<underline method="pen" type="straight" language="EN" text="not seene"/>
    	<underline method="pen" type="straight" language="EN" text="necessarie and a profitable thing, to accustome"/>
    	<underline method="pen" type="straight" language="EN" text="to"/>
    	<underline method="pen" type="straight" language="EN" text="them selues"/>
    	<underline method="pen" type="straight" language="EN" text="and in"/>
    	<underline method="pen" type="straight" language="EN" text="marching"/>
    	
    	<underline method="pen" type="straight" language="EN" text="the armie is assaulted on tvvoo sides"/>
    	
