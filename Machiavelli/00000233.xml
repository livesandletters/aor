    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="an ordinary battaile or band of fowre hundred and fiftie men"/>
    	<underline method="pen" type="straight" language="EN" text="is redoubled by"/>
    	<underline method="pen" type="straight" language="EN" text="parte the fiue ranckes of Pike"/>
    	<underline method="pen" type="straight" language="EN" text="the foremoste"/>
    	<underline method="pen" type="straight" language="EN" text="placed beehinde"/>
    	<underline method="pen" type="straight" language="EN" text="come to assaulte them at theyr backes"/>
    	<underline method="pen" type="straight" language="EN" text="to fighte on the flancke"/>
    	<underline method="pen" type="straight" language="EN" text="with twoo hornes"/>
    	<underline method="pen" type="straight" language="EN" text="a voyde place in the middeste"/>
    	<underline method="pen" type="straight" language="EN" text="an armye appoincted to fighte the battaile with the enemies"/>
    	<underline method="pen" type="straight" language="EN" text="the verye same is"/>
    	<underline method="pen" type="straight" language="EN" text="plainelier set foorthe in"/>
    	<underline method="pen" type="straight" language="EN" text="other twoo figures nexte"/>
    	<underline method="pen" type="straight" language="EN" text="forme of a fowre square armie: as in"/>
    	<underline method="pen" type="straight" language="EN" text="a fowresquare fashion"/>
    	<underline method="pen" type="straight" language="EN" text="to the ordinarye forme, to fighte a"/>
    	<underline method="pen" type="straight" language="EN" text="manner of incamping"/>

