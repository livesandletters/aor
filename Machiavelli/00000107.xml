    <annotation>
    	<!-- Marginalia -->
    	<marginalia hand="Italian" anchor_text="The description of a battaile that is a fighting.">
    		<language ident="EN">
    			<position place="head" book_orientation="0">
    				<marginalia_text>
    					A visible, &amp; liuely Description of A most valiant, &amp;
    					victorious Battail.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	<marginalia hand="Italian">
    		<language ident="EN">
    			<position place="tail" book_orientation="0">
    				<marginalia_text>
    					The seueral causes, &amp; reasons of euery Particular,
    					ar expressly declarid in [the] militar Analysis following.
    				</marginalia_text>
    			</position>
    		</language>
    	</marginalia>
    	
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="to kepe the enemie at a baye, and to"/>
    	<underline method="pen" type="straight" language="EN" text="people sayde of him"/>
    	<underline method="pen" type="straight" language="EN" text="not heare the artillery"/>
    	<underline method="pen" type="straight" language="EN" text="Durs haue already"/>
    	<underline method="pen" type="straight" language="EN" text="extraordinarie Veliti, issuing out of"/>
    	<underline method="pen" type="straight" language="EN" text="the lighte horsemen"/>
    	<underline method="pen" type="straight" language="EN" text="moste speedelye"/>
    	<underline method="pen" type="straight" language="EN" text="moste meruailous furie, and greatest crye"/>
    	<underline method="pen" type="straight" language="EN" text="assault the enemie"/>
    	<underline method="pen" type="straight" language="EN" text="whose artillerie"/>
    	<underline method="pen" type="straight" language="EN" text="ouer the heades"/>
    	<underline method="pen" type="straight" language="EN" text="shoote the seconde time, the Veliti, &amp; our horsemen"/>
    	<underline method="pen" type="straight" language="EN" text="nowe gotten it"/>
    	<underline method="pen" type="straight" language="EN" text="for to defende"/>
    	<underline method="pen" type="straight" language="EN" text="neither oure ordinaunce"/>
    	<underline method="pen" type="straight" language="EN" text="the enemies, can anye more do their"/>
    	<underline method="pen" type="straight" language="EN" text="much vertue, strength and agilitie oure"/>
    	<underline method="pen" type="straight" language="EN" text="men fighteth"/>
    	<underline method="pen" type="straight" language="EN" text="exercise, which"/>
    	<underline method="pen" type="straight" language="EN" text="confidence, that they"/>
    	<underline method="pen" type="straight" language="EN" text="marcheth in good order"/>
    	<underline method="pen" type="straight" language="EN" text="to giue the charge"/>
    	<underline method="pen" type="straight" language="EN" text="our artillerie, whiche to"/>
    	<underline method="pen" type="straight" language="EN" text="Capitayn incourageth them"/>
    	<underline method="pen" type="straight" language="EN" text="sheweth them the"/>
    	<underline method="pen" type="straight" language="EN" text="the Veliti and light horsemen"/>
    	<underline method="pen" type="straight" language="EN" text="beholde how the armies be affronted"/>
    	<underline method="pen" type="straight" language="EN" text="with howe muche valiauntnesse"/>
    	<underline method="pen" type="straight" language="EN" text="much silence"/>
    	<underline method="pen" type="straight" language="EN" text="the Captayne co[m]maundeth the men"/>
    	<underline method="pen" type="straight" language="EN" text="[that] they sustayn, &amp; not charge"/>
    	<underline method="pen" type="straight" language="EN" text="howe our lighte"/>
    	<underline method="pen" type="straight" language="EN" text="horsemen be"/>
    	<underline method="pen" type="straight" language="EN" text="giue the charge on"/>
    	<underline method="pen" type="straight" language="EN" text="enemies, Harkebutters"/>
    	
    	<underline method="pen" type="straight" language="EN" text="a battaile that is a fighting"/>
    		
