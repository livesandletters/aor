    <annotation>
    	<!-- Underlines -->
	<underline method="pen" type="straight" language="EN" text="those thinges, which can not be withstode, there must bee giuen way, as"/>
    	<underline method="pen" type="straight" language="EN" text="made to the Eliphants"/>
    	<underline method="pen" type="straight" language="EN" text="to the carres full of hookes"/>
    	<underline method="pen" type="straight" language="EN" text="ordered"/>
    	<underline method="pen" type="straight" language="EN" text="wonne a battaile after"/>
    	<underline method="pen" type="straight" language="EN" text="impossible, that an armie thus ordered"/>
    	<underline method="pen" type="straight" language="EN" text="armed, should not ouercome at"/>
    	<underline method="pen" type="straight" language="EN" text="should bee ordayned, as they"/>
    	<underline method="pen" type="straight" language="EN" text="adayes"/>
    	<underline method="pen" type="straight" language="EN" text="but one"/>
    	<underline method="pen" type="straight" language="EN" text="no Targaettes"/>
    	<underline method="pen" type="straight" language="EN" text="vnarmed, that they cannot"/>
    	<underline method="pen" type="straight" language="EN" text="their battailes by flancke"/>
    	<underline method="pen" type="straight" language="EN" text="the armie thinne"/>
    	<underline method="pen" type="straight" language="EN" text="behinde the other, hauing"/>
    	<underline method="pen" type="straight" language="EN" text="they do it confusedly, and"/>
    	<underline method="pen" type="straight" language="EN" text="three names"/>
    	<underline method="pen" type="straight" language="EN" text="into three companies"/>
    	<underline method="pen" type="straight" language="EN" text="vawarde"/>
    	<underline method="pen" type="straight" language="EN" text="battaile"/>
    	<underline method="pen" type="straight" language="EN" text="rerewarde"/>
    	<underline method="pen" type="straight" language="EN" text="to marche"/>
    	<underline method="pen" type="straight" language="EN" text="to distinguish the"/>
    	<underline method="pen" type="straight" language="EN" text="battaile, they binde"/>
    	<underline method="pen" type="straight" language="EN" text="all to the"/>
    	<underline method="pen" type="straight" language="EN" text="brunte, and to the first"/>
    	<underline method="pen" type="straight" language="EN" text="your horsemen were repulced"/>
    	<underline method="pen" type="straight" language="EN" text="they retired to the"/>
    	<underline method="pen" type="straight" language="EN" text="ride of them"/>
    	<underline method="pen" type="straight" language="EN" text="they withstoode, and draue"/>
    	<underline method="pen" type="straight" language="EN" text="maye withstande"/>
    	<underline method="pen" type="straight" language="EN" text="but in a grosse"/>
    	<underline method="pen" type="straight" language="EN" text="the heade"/>
    	<underline method="pen" type="straight" language="EN" text="fiue ranckes of Pikes"/>
    	<underline method="pen" type="straight" language="EN" text="seuen, so that"/>
    	
    	<underline method="pen" type="straight" language="EN" text="rule against"/>
    	<underline method="pen" type="straight" language="EN" text="as cannot be"/>
    	
