    <page filename="196.tif" reader="John Dee" pagination="190"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Nec saepius narrare dixerim nefas, quod Cicero pro Cluentio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>S[a]epi[us] aliq[ua]n[do] narra[nd]dum est Pro Cluentio.</marginalia_text>
                    <book title="Pro Cluentio"/>
                </position>
            </language>
            <translation>Sometimes more than one narrative is given in Pro Cluentio.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="superstione praeceptorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pra[e]ceptorum Superstitio.</marginalia_text>
                </position>
            </language>
            <translation>Superstitious precepts.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cur igitur si singula probanda aut refellenda erunt, non singula etiam narrentu?">
            <language ident="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>A fine.</marginalia_text> <!-- M.B. Not sure if this is correct? -->
                </position>
            </language>
            <translation>A purpose.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="turpiter conuertuntur, ut si peperisse narres">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ordo narra[ndi] turpis</marginalia_text> <!-- FSF: I am not sure how to expand this here; maybe narra[nd]i? as in the order of narrating? I don't know -->
                </position>
            </language>
            <translation>Disgraceful order of narrating.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="falsae expositiones">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Falsa[e] expositio[n]es</marginalia_text>
                </position>
            </language>
            <translation>False exposition.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="color">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Color.</marginalia_text>
                </position>
            </language>
            <translation>Colour.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="pro re, pro tempore intuenda quae"/>
        <underline method="pen" type="straight" language="LA" text="Nec saepius narrare dixerum nefas, quod Cicero pro Cluentio fecit"/>
        <underline method="pen" type="straight" language="LA" text="superstitione praeceptorum contra rationem causa trahi. Narrationem"/>
        <underline method="pen" type="straight" language="LA" text="Quaedam uer&ograve; etiam turpiter conuertuntur, ut"/>
        <underline method="pen" type="straight" language="LA" text="peperisse narres, deinde concepisse: apertum testamentum, deinde refignatum"/>
        <underline method="pen" type="straight" language="LA" text="utrumcunque"/>
        
