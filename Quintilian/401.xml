        <marginalia hand="Italian" anchor_text="Obstat enim, quicquid no[n] adiuuat">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Obstat enim quicquid no[n] adiuvat</marginalia_text>
                </position>
            </language>
            <translation>For whatever is no help is a hindrance.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="genus diuiditur in species, nihil habet proprium, ut arboris species sunt pinus, &amp; olea, &amp; 
