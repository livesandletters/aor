        <!-- M.B. Some Greek marginalia remaining. -->
        
        <marginalia hand="Italian" anchor_text="Celsus tamen, &amp; non negligens autor Visellius, in hac eam parte posuerunt, Rutilius quide[m] Lupus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Celsus Visellius Rutilius</marginalia_text>
                    <person name="Aulus Cornelius Celsus"/>
                    <person name="Visellius Rufus"/>
                    <person name="Publius Rutilius Lupus"/>
                </position>
            </language>
            <translation>Celsus, Visellius, Rutilius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="singula singulis opponuntur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Singula singulis in comparatione opponuntur</marginalia_text>
                </position>
            </language>
            <translation>Are one by one compared against each other.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ἀντ[ί]θετ[ο]υ">  <!-- M.B. For the second ligature τ[ο] see (W. H. Ingram) http://www.grailheart.com/media/languages/GreekLigatures.pdf p.388.. Note the first ligature looks a like π but I think it is actually 'τ[ί].'  -->
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>ἀντίθετομ</marginalia_text>
                </position>
            </language>
            <translation>Antithet.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="διαλλαγήH">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>διαλλαγ[ή]v</marginalia_text> <!-- M.B> Not sure of ligature here. -->
                </position>
            </language>
            <translation>Diallage</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ἐπακ[ο]λούθHσιυ">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>ἐπακ[ο]λο[ύ]θHσις</marginalia_text> <!-- κ[ο] ligature see Ingram p. 385 and ο[ύ] ligature.-->
                </position>
            </language>
            <translation>Nonessential consequence.</translation>
        </marginalia>

