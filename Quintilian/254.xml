    <annotation>
        
        <marginalia hand="Italian" anchor_text="Homerica dispositione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Homerica dispositio qualis</marginalia_text> 
                </position>
            </language>
            <translation>
                Such a Homerican arrangement.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ratio causae cuiusq[ue] postulabit, ordinabuntur: uno, ut ego censeo, excepto, ne &agrave; potenti&sharp-S;imis ad leui&sharp-S;ima decrescat 
            oratio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vnu[m] locando in vitiu[m] evita[n]du[m]</marginalia_text> 
                </position>
            </language>
            <translation>
              One fault in placing must be avoided.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quidam executi sunt uerbosius, quibus placuit, proposita locorym communium materia, quo quaeq[ue] res modo dici posset, oste[n]dere,">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Non debent nec possu[n]t o[mn]iu[m] argume[n]toru[m] sedes ostendi.</marginalia_text>
                </position>
            </language>
            <translation>The places of all arguments neither has to nor can be revealed.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="declamationes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Declamatio[n]es</marginalia_text> 
                </position>
            </language>
            <translation>Declamations.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nam ut illi robur lacertos, barbamq[ue] ante omnia, &amp; alia, quae natura propria maribus dedit, parum existimant decora, quaeq[ue] fortia, si liceret, forent, ut dura molliunt: 
            ita nos habitum orationis ipsum uirilem, &amp; illam uim stricte, robusteq[ue] dicendi tenera quadam elocutionis cute operimus, &amp; dum leuia sint, ac nitida, quantum ualeant, nihil interesse arbitramur.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Similitudo.</marginalia_text> 
                </position>
            </language>
            <translation>Similarity.</translation>
        </marginalia>
        
