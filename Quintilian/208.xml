    <annotation>
        
        <marginalia hand="Italian" anchor_text="propter quae partitione non semper est utendum.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quapropter no[n] semp[er] partitione utendu[m] est</marginalia_text>
                </position>
            </language>
            <translation>Why a partition is not always used.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mouendum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Movere.</marginalia_text>
                </position>
            </language>
            <translation>To move.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iucunda partitio est: ut quid quaq[ue] de re dicturi sumus, ordine appareat. At si unum crimen uarie defenderimus, superuacua">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Si vnu[m] cr[i]mi[n]e defe[n]derimus sup[er]vacua partitio est.</marginalia_text> 
                </position>
            </language>
            <translation>If we defend one crime, a parition is superfluous.</translation> <!-- M.B. ?? -->
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Non enim solum oratoris est docere, sed plus eloquentia circa mouendum ualet"/>
