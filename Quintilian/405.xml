<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="405.tif" reader="John Dee" pagination="399"/> 
    <annotation>
        
        <!-- M.B. Greek marginalia to be captured later. -->
        
        <marginalia hand="Italian" anchor_text="Nec mutator uocabulis uis rerum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Non mutatur vocabulis vis reru[m]</marginalia_text>
                </position>
            </language>
            <translation>The force of things does not change names.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="figura">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>figura</marginalia_text>
                </position>
            </language>
            <translation>Figure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ironia alia sit tropi, alia schematis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ironia tropus schema</marginalia_text>
                </position>
            </language>
            <translation>Irony as a trope and as a schema.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="homines, si aliud acceperu[n]t quam quod habuerant nome[n], ijdem sunt tame[n]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>simile</marginalia_text>
                </position>
            </language>
            <translation>Similiar.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nominibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>no[m]i[n]a</marginalia_text>
                </position>
            </language>
            <translation>Names.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="frequentur in easdem sente[n]tias &amp; tropon, &amp; figuram">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Tropus &amp; figura co&euml;mit sa[e]pe</marginalia_text> <!-- M.B. Not sure of word following 'figura'. --> <!-- FSF: I think it is coemit -->
                </position>
            </language>
            <translation>Trope and figure are often ?</translation> <!-- M.B. Tropes and figures ae ofetn bought? Not sure how coemit would translate, if its comeit?  -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="debeamus figuram. Nam duobus modis dicitur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>figura duob[us] d[icitu]r modis</marginalia_text>
                </position>
            </language>
            <translation>Figure is used in two manners.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Transfert tamen uerbum, aut partem eius &agrave; suo loco in"/>
        <underline method="pen" type="straight" language="LA" text="Horum nihil in figuras cadit. Nam &amp; proprijs uerbis, &amp; ordine collocatis, fieri figura potest. Quomodo"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m multiplicem habeant, quam'q[ue] scrupulosam disputationem"/>
        <underline method="pen" type="straight" language="LA" text="appareat. Nec mutatur uocabulis uis rerum"/>
        <underline method="pen" type="straight" language="LA" text="No[n] enim nominibus prosunt, sed effectibus. ut statu[m] co[n]iecturalem, an inficialem, an facti, an substantiae, an de 
            substa[n]tia nominemus, nihil interest, dum ide[m] quaeri sciamus"/>
        <underline method="pen" type="straight" language="LA" text="Tam enim translatis uerbis, qu&agrave;m proprijs figuratur oratio"/>
        
        <mark name="tripunctus" method="pen" place="right_margin"/> <!-- M.B. Both tripuncti have tails.  -->
        <mark name="tripunctus" method="pen" place="right_margin"/>
        
        <drawing type="manicule" book_orientation="0" place="right_margin" method="pen"/>
        
    </annotation>
