    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="249.tif" reader="John Dee" pagination="243" signature="q 2"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Itaq[ue[ in hoc gnere maxim&egrave; quaeritur, an simile sit, quod infertur.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Notandum</marginalia_text>
                </position>
            </language>
            <translation>It should be noted.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Socraticu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Socraticus mos</marginalia_text>
                    <person name="Socrates"/>
                </position>
            </language>
            <translation>The Socratic manner.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aspasia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Aspasia</marginalia_text>
                    <person name="Aspasia"/>
                </position>
            </language>
            <translation>Aspasia.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="malle enim responderat, se malle alienum aurum qu&agrave;m suum.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ratio occurendi Socratica[e] argumentatio[n]i</marginalia_text>
                    <person name="Socrates"/>
                </position>
            </language>
            <translation>Reason for a Socratic argument to occur.</translation> <!-- M.B. Not certain of how to translate 'occurendi'. --> <!-- FSF: Reason for a socratic argument to occur? not sure either -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="minuti&sharp-S;imas partes ista secuisse, &amp; aliquid simile minus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Similis, Diuisio</marginalia_text>
                </position>
            </language>
            <translation>Similar, division.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Contrariorum quoq[ue] aliter accipi opposita">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Contrarioru[m] Diuisio</marginalia_text>
                </position>
            </language>
            <translation>The division of contrary things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iure simili">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ius simile</marginalia_text>
                </position>
            </language>
            <translation>Similar law.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cicero">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cicero</marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Cicero.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="matriomoniu[m] sit mente co&euml;untium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Matrimoniu[m] mente co&euml;u[n]tiu[m]</marginalia_text>
                </position>
            </language>
            <translation>Marriage through the will of the partners.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Itaq[ue] in hoc genere"/>
        <underline method="pen" type="straight" language="LA" text="ut apud Aeschinem Socraticu[m] male respondit Aspasia Xenophontis uxor, quod Cicero his uerbis transfert: Dic mihi quaeso Xenophontis uxor, si uicina"/>
        <underline method="pen" type="straight" language="LA" text="male enim responderat, se malle alienum aurum"/>
        <underline method="pen" type="straight" language="LA" text="esse simile minus"/>
        <underline method="pen" type="straight" language="LA" text="aliquid plus"/>
        <underline method="pen" type="straight" language="LA" text="Et di&sharp-S;imilibus inesse simile"/>
        <underline method="pen" type="straight" language="LA" text="similibus di&sharp-S;imile"/>
        <underline method="pen" type="straight" language="LA" text="argume[n]ta duci ex iure simili, ut"/>
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen"/>
        
    </annotation>
