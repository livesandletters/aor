        <marginalia hand="Italian" anchor_text="species firman probationem habet generis, infirmam refutationem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Spe[cie]s firma[m] probatione[m], infirma[m] refutationem habet</marginalia_text>
                </position>
            </language>
            <translation>Species offer stong proof but a weak refutation.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Praeterea finimus aut ui, sicut superiora, aut ἐτυμoλoγίᾳ,"> <!-- M.B. Translate Greek marginalia later. -->
