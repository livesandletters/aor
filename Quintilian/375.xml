        <underline method="pen" type="straight" language="LA" text="deprehenditur carens arte oratoria"/>
        <underline method="pen" type="straight" language="LA" text="non animis sol&ugrave;m, sed etiam auribus est ingrati&sharp-S;ima. Vitanda"/>
        <underline method="pen" type="straight" language="LA" text="Sed huic uicina Periphrasis uirtus habetur"/>
        <underline method="pen" type="straight" language="LA" text="Ego meis oculis uidi"/>
        <underline method="pen" type="straight" language="LA" text="affirmationis gratia adhibetur"/>
        <underline method="pen" type="straight" language="LA" text="At uitium erit, quoties otiosum fuerit, &amp; supererit, non cum adijcietur"/>
        <underline method="pen" type="straight" language="LA" text="ut sic dixerim, operositas"/>
        <underline method="pen" type="straight" language="LA" text="Nam &amp; tumida, &amp; exilia, &amp; praedulcia, &amp; abundantia, &amp; arce&sharp-S;ita, &amp; exultantia 
            sub idem nomen cadunt"/>
        <underline method="pen" type="straight" language="LA" text="&amp; specie boni fallitur"/>
        <underline method="pen" type="straight" language="LA" text="Nam caetera c&ugrave;m uitentur, hoc petitur"/>
        <underline method="pen" type="straight" language="LA" text="stultum, co[m]mune, co[n]trarium, superuacuu[m]"/>
        <underline method="pen" type="straight" language="LA" text="maxime improrijs, redundantibus, comprehensione obscura, compositione fracta, uocum similium, aut 
            ambiguarum puerili"/>
        
