        <marginalia hand="Italian" anchor_text="Iustum omne continetur natura, uel co[n]stitutione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iustu[m] o[mn]e Natura co[n]stitutione</marginalia_text>
                </position>
            </language>
            <translation>All justice [depends on] nature [or] convention.</translation>
        </marginalia>
        
