            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Primu[m] genus caus[a]e</marginalia_text>
                </position>
            </language>
            <translation>First type of causes.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="secundum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>secundu[m]</marginalia_text>
