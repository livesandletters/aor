        <marginalia hand="Italian" anchor_text="Lis est omnis aut de praemio, aut de poena, aut de qua[n]titate">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Lis omnis aut de pra[e]mio aut et poena aut de quantiate</marginalia_text>
                </position>
            </language>
            <translation>Every dispute involves a reward, or a penalty or the amount of these.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Est autem qualitas alia de summo genere, atq[ue] ea quide[m] non simplex.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qualitatis Diuisio</marginalia_text>
                </position>
            </language>
            <translation>The division of quality.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="suasorijs">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Suasoria[e]</marginalia_text>
                </position>
            </language>
            <translation>Persuasive.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ac non facie[n]da, appetenda, uitanda">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Facie[n]da &amp; no[n] appete[n]da, vita[n]da</marginalia_text>
                </position>
            </language>
            <translation>Should be sought or avoided.</translation>
        </marginalia>
        
