    <page filename="310.tif" reader="John Dee" pagination="304"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Nam culpam, praesertim deprehe[n]sam, pertinaciter tueri, culpa altera est.">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Culpa[m], presertim deprehensam, p[er]tinaciter tueri, culpa altera est</marginalia_text>
                </position>
            </language>
            <translation>To defend a crime, obstinately, especially after being caught, amounts to a second crime.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Illud uix saltem praecipiendum uidetur, ne turdibus &amp; clamosus tantu[m] sit altercator, &amp; quales facti sunt, wui literas nesciu[n]t.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Infantis</marginalia_text>
                </position>
            </language>
            <translation>Inarticulate.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="improbatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Improbitas</marginalia_text>
                </position>
            </language>
            <translation>Dishonesty.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iudex">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iudex respici[en]dus</marginalia_text>
                </position>
            </language>
            <translation>The judge is to be respected.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Faciunt hoc medici quoq[ue] ut remedia perinde perseuerent adhibere, uel definant, ut illa recipi uel respui uident.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>A simili</marginalia_text>
                </position>
            </language>
            <translation>A resemblance.</translation>
        </marginalia>
        
