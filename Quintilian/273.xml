    <annotation>
       
        <marginalia hand="Italian" anchor_text="Ego uel hoc uno malo sic era[m] afflictus, ut me iam nulla fortuna posset efficere felicem.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quintiliani vxor.</marginalia_text>
                    <person name="Quintilian"/>
                </position>
            </language>
            <translation>Quintilian's wife.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sed di&sharp-S;imulare qu&icirc; possum, quid illi gratiae in uultu, quid incunditatis in sermone, quos ingenij igniculos, quam praestantiam placidae, &amp; 
            (quod scio uix posse credi) tantu[m] altae mentis ostenderit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pueri laus.</marginalia_text>
                </position>
            </language>
            <translation>In praise of the boy.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Iuro per mala mea, per infelice[m] conscientiam, per illos manes">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iuramenta.</marginalia_text>
                </position>
            </language>
            <translation>Oaths.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="celerius occidere festinata[m] maturitatem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Celerius occ&icirc;dit festinata maturitas</marginalia_text> <!-- FSF: a circumflex is usually the other way round, so I am not sure what this sign on the i is -->
                </position>
            </language>
            <translation>Early ripeness means a quicker fall.</translation>
        </marginalia>
       
