    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="biothanatos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Biothanati.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those died of violence.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="aurifices">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Aurifices aut tales
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Goldsmiths or such.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="δυοδεκατημόριον">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>δυοδεκατημόριον</marginalia_text>
                </position>
            </language>
            <translation>
                Twelfth part.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quod ego meo damno didici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What I learnt through my pain.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="&Mars; in horosc. co[n]stituto, in exiliu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Exiliu[m] horoscopa[n]te &Mars;
                    </marginalia_text>
                    <symbol_in_text name="Mars"/>
                </position>
            </language>
            <translation>
                An exile after drawing an horoscope of Mars.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        [At qui p[otes]t esse in matutino ortu in secundo du[m] dies est, [nisi] &Sun; in 7 domo sit vel circa - deleted]
                    </marginalia_text>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>
                [But what can be in the rise of the early morning in the second [house], while the day is [unless] the Sun is in the seventh house or close] 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Maximu[m] q[uod]da[m] facimus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The greatest we create.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="horosc. loco fuerit inuentus biothanatos enim sine dubitatione perficiet"/>
        <underline method="pen" type="straight" language="LA" text="&Jupiter;, quodam subtili artificio saepe pollentes, aut aurifices"/>
        <underline method="pen" type="straight" language="LA" text="autem si fuerit partiliter in horosc. constitutus, sine radiatione &Jupiter;, faciet largiores uiros, plus uolentes"/>
        <underline method="pen" type="straight" language="LA" text="graues"/>
        <underline method="pen" type="straight" language="LA" text="qui quicquid ausi fuerint nulla potuerunt ratione co[m]plere"/>
        <underline method="pen" type="straight" language="LA" text="uxores etiam"/>
        <underline method="pen" type="straight" language="LA" text="sed haec prima quide[m] aetate depereunt"/>
        <underline method="pen" type="straight" language="LA" text="Sed &amp; ego scio, quod in plurimis genituris inueni, multos &Mars; in horosc. co[n]stituto, in exiliu[m] datos, &amp; bene &amp;strenue 
            res pub. administrasse, d&eacute;q[ue] eis benemeritos"/>
        <underline method="pen" type="straight" language="LA" text="praesertim si in matutino ortu fuerit"/>
        <underline method="pen" type="straight" language="LA" text="aute[m] &agrave; frigore, aut"/>
        <underline method="pen" type="straight" language="LA" text="Erunt autem semper tales, in aestu animi. &amp; in maximis necessitatibus constituti"/>
        <underline method="pen" type="straight" language="LA" text="periculorum continuatione decipere"/>
        <underline method="pen" type="straight" language="LA" text="Alienas aut[em], q[ui] sic &Mars; habuerit, desiderabit, &amp; omnibus in uidebit"/>       
        
