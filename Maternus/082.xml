    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="biothanatos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Biothanati
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those who died of violence. 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Sol">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quomodo possu[n]t ta[n]tu[m] a sole distare?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In what way can they distance themselves only from the sun?
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Si uero Sol per noctem, in horoscopi signo fuerit inuentus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Sol per nocte[m] in horosc.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The sun in horoscopes throughout the night. 
            </translation>
        </marginalia>
        
