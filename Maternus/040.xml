    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="040.tif" reader="John Dee" pagination="17" signature="b 3"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Babylonij exaltatio[n]es pro domicilijs p[osuere] at revera planeta[e] melius in exaltationibus dececernunt. <!-- FSF: not sure --> <!--JG: Perhaps it's posuere?-->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The Babylonians placed the exaltations for the dwellings and in fact the planets resolved better in the exaltations.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Babylonij.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The Babylonians.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cum in ipso decano stella fuerit, licet sit in alieno domicilio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        stella in decano suo a[e]qua[m] exeret potestatem ac si esset in suo domicilio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A star in its decan exerts the same power as it does in its domicile. 
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="maximae felicitatis insignia decernunt, quoties in hominum genituris, maxima stellarum pars partiliter altitudinum suarum signa possederit"/>
        <underline method="pen" type="straight" language="LA" text="domicilia earum ese uoluerunt"/>
        <underline method="pen" type="straight" language="LA" text="decernere in altitudinibus suis, q[uam] in domiciliis"/>
        <underline method="pen" type="straight" language="LA" text="cum per haec signa exaltentur singuli planetae, ita etiam domos"/>
        <underline method="pen" type="straight" language="LA" text="&amp; cum in ipso decano stella fuerit, licet sit in alieno"/>
        
