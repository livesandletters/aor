    <annotation>
        
        <marginalia hand="English_secretary"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>percipere</marginalia_text>  
                </position>
            </language>
            <translation>To perceive.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Quanta'q[ue] qu&agrave;m parui facerent discrimina motus. Postqu&agrave;m omnis coeli species redeuntibus astris
             Percepta in proprias sedes, &amp; reddita certis Factorum ordinibus sua cui'q[ue] potentia formae Per uarios usus, artem experientia fecit"/>
        <underline method="pen" type="straight" language="LA" text="Tunc'q[ue] in desertis habitabat montibus aurum. Ignotus'q[ue] nouos pontus subduxerat orbis."/>
        <underline method="pen" type="straight" language="LA" text="Sed cum longa dies acuit mortalia corda, Et labor ingenium miseris dedit, &amp; sua"/>
        <underline method="pen" type="straight" language="LA" text="Semper enim ex aliis alios proseminat"/>
        <underline method="pen" type="straight" language="LA" text="praedicare"/>
        <underline method="pen" type="straight" language="LA" text="Sollicitare umbras"/>
        <underline method="pen" type="straight" language="LA" text="Omnia conando docilis solertia uicit"/>
        
    </annotation>
