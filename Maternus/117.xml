    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="117.tif" reader="John Dee" pagination="94"/>
    <annotation>
        
        <marginalia hand="English_secretary"> 
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Signoru[m] varietas se[m]p[er] perpende[n]da tu[m] ip[s]a dom[us] tu[m] domini dom[us] [co[n]ditu] diurna [cum] nocturna genesis [etc]. 
                    </marginalia_text> 
                </position>
            </language>
            <translation>
                The variety of signs must always be perpetuated both its house as the house of the lord, preserved both by day and night of the horoscope.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="caligarios">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Caligarij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bootmakers.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tabernarios">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Tabernarij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Inn-keepers.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Si uero in deiectis geniturae locis, &agrave; &Mercury; defluens &Moon; in"/>
        <underline method="pen" type="straight" language="LA" text="Si uero minuta lumine in"/>
        <underline method="pen" type="double" language="LA" text="diurna genitura, &agrave; &Mercury; defluens"/>
        <underline method="pen" type="straight" language="LA" text="feratur ad &Mars;"/>
        <underline method="pen" type="straight" language="LA" text="in nocturna genitura"/>
        
    </annotation>
