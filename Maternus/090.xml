    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="090.tif" reader="John Dee" pagination="67" signature="f 4"/>
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Varia[e] &Venus; decernendi potestates difficile inveniri possunt. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Various abilities of determining can be invented easily. <!-- M.B. Shouldn't 'Venus' be in the translation too? -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Comparatio pictura[e] et influenti[a]e coelestis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The comparison of pictures and celestial influences.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="semper &Venus;, cum &Moon;">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &Venus; et &Moon;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Venus and the moon
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mulieris genitura
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The constellation of a woman.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Signi natura i[r]e auget <!-- FSF: I am not sure about the contraction --> <!-- M.B. I'm not sure either? -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &Venus; gaudet sole[m] sequi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Venus rejoices in following the sun.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &Venus; datrix vitae.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Venus the donor of life.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="habet enim haec stella uarias, sed quae difficile inueniri possint decernendi"/>
        <underline method="pen" type="straight" language="LA" text="&amp; erunt ab omni uitiorum labe sepositi"/>
        <underline method="pen" type="straight" language="LA" text="nouercis, aut filiabus, uel fratribus coire cognuntur"/>
        <underline method="pen" type="straight" language="LA" text="&Venus;, cum &Moon; simul posita, aut de quadrato coniuncta, amoris illecebras, aut infamiae"/>
        <underline method="pen" type="straight" language="LA" text="cupiditate praeposteros"/>
        <underline method="pen" type="straight" language="LA" text="Alias quoq[ue] mulieres, uiriles facit actus appetere. Sed haec tunc fortius facit, si simul cum &Moon;, ista radiatione coniuncta"/>
        <underline method="pen" type="straight" language="LA" text="Gaudet e[ni]m per nocte[m], si se &Moon;, &Mars; Meruri&oacute;q[ue] co[n]iu[n]xerit"/>
        <underline method="pen" type="straight" language="LA" text="co[n]stituit[ur] &Venus;, si cu[m] &Sun; fuerit collocata"/>
        
