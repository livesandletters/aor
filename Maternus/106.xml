    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &Moon; cursus post natiuitatis diei diem ..tu[m] ita est notandus vt eadem ... simili r[ati]o[n]e tunc pendere ab sa[e]pe loco putandu[m] sit vt in ip[s]a natiuitate
                    </marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>
                The course of the moon after the day of the birthday <!-- FSF: not sure --> <!-- M.B. I can't figure this one out either.. -->
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Occasio seque[n]tis libri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The opportunity for the following book. <!-- FSF: not sure -->
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Huius studij fructus est dignitas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The dignity is the fruit of this study.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="conueniet, nec ta[n]tum eodem die, quo natus homo"/>
        <underline method="pen" type="straight" language="LA" text="prima uestigia lucis ingreditur, sed diligenti etiam ratione, tertio die perquirere debemus, cui se stellae &Moon;"/>
        <underline method="pen" type="straight" language="LA" text="immulgentur alimenta nutricia"/>
        <underline method="pen" type="straight" language="LA" text="Nam &amp; tertio die, sicuti primo, omnia simili ratione"/>
        <underline method="pen" type="straight" language="LA" text="caninae" colour="red"/>
        <underline method="pen" type="straight" language="LA" text="contentionis iurgiosa certamina" colour="red"/>
        <underline method="pen" type="straight" language="LA" text="Deserui itaque hoc studium, ne imperitotum"/>
        <underline method="pen" type="straight" language="LA" text="conualescente consensu"/>
        <underline method="pen" type="straight" language="LA" text="alienis utilitatibus excuba[n]s, maximis me insidiis, &amp; maximorum periculorum discriminibus implicarem"/>
        <underline method="pen" type="straight" language="LA" text="ratione restitutionis, qu&agrave;m liberali animo, co[n]temptis forensibus"/>
        <underline method="pen" type="straight" language="LA" text="itaque constitutus, &amp; forensium certaminum"/>
        <underline method="pen" type="straight" language="LA" text="quodammodo inuidiae laqueis"/>
        <underline method="pen" type="straight" language="LA" text="scripsi, ut &agrave; terrena quoda[m]modo conuersatione sepositus, ex qualicu[m]q[ue] parte, 
