    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="173.tif" reader="John Dee" pagination="150" signature="[n 3[v]]"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Varia eiusde[m] stell[a]e decreta pro vario eius situ
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Various ordinances of the same stars for its various positions.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        .1. Locus in mundo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                1. Place in the world.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Orandu[m] vt stella[e] bene locentur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be prayed for so that the stars will be well placed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="bonitatis suae perdit salutare praesidium, aut si malignu[m] fuerit, fortius ad noncendum exasperat malitiae potestatem"> 
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Malus locus boni planeta[e] bonitate[m] debilitat et mali malitia[m] exasperat.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A bad place weakens the benevolence of a good planet et roughens the wickedness of a bad [planet].
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="aliud in diurnis, aliud in nocturnis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        .2. Dies an nox
                    </marginalia_text>
                </position>
            </language>
            <translation>
                2. Day or night.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        .3. Aspectus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                3. Aspect.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Ex ista ergo locoru[m] uarietate, aut erigit[ur] stellaru[m] potestas, aut decidit"/>
        <underline method="pen" type="straight" language="LA" text="Sed tu quibuscu[n]q[ue] precibus implora, ne quando sextum, aut duodecimu[m] ab"/>
        <underline method="pen" type="double" language="LA" text="horosc. locu[m]"/>
        <underline method="pen" type="straight" language="LA" text="secundu[m] etiam et octauum in decretis hominu[m], aut beneuolarum, aut maleuolarum cursus obtineat. Haec"/>
        <underline method="pen" type="straight" language="LA" text="stellarum sydus insederit, aut bonitatis suae perdit salutare praesidium, aut si malignu[m] fuerit, fortius ad nocendum exasperat malitiae potestatem. 
            Fimantur etiam in his locis maleuloae stellae, cum &Moon; positae"/>
        <underline method="pen" type="straight" language="LA" text="sine aliquo remidio, infortunioru[m] exitia decernunt"/>
        <underline method="pen" type="straight" language="LA" text="augusta maiestatis radiatione"/>
        <underline method="pen" type="straight" language="LA" text="Quinta pars &Leo;"/>
        
