    <page filename="102.tif" reader="John Dee" pagination="79" signature="g 4"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="hi uxorem difficile consequentur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Qui difficile uxorem co[n]sequentur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He who with difficulty seeks for a wife.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Qua[n]tum refert 8m vel matutinu[m] vel vespertinu[m] in hoc loco?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                How much does either the eighth morning or eighth evening matter in this place?
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="naucleri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Naucleri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Skippers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="metallorum praepositos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Metallor[um] prepositi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those in command of metals.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="apocos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Apocopi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Apocopes.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="spathici, aut colobici"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Spathici. Colobici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mutilated. <!-- FSF: not sure about spathici; seems to be related to spatula -->
                <!-- M.B. See: https://books.google.co.uk/books?id=IRJuwgs5dbAC&pg=PA814&dq=%22spathici%22&hl=en&sa=X&ved=0ahUKEwjZ4ru09dbVAhXGIcAKHfs8BwUQ6AEIZjAJ#v=onepage&q=%22spathici%22&f=false -->
                <!-- FSF: it remains unclear to me -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mortem repentinam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mors repe[n]tina
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A sudden death.
            </translation>
        </marginalia>
        
