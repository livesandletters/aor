    <page filename="359.tif" reader="John Dee" pagination="92" signature="[H 4[v]]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Luna in trino aspectu">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &Trine; &Moon; ad alios planetas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The moon ... to the other planets. <!-- FSF: no idea how to translate the triangle or in trino aspectu as in the text -->
                <!-- M.B The triangle refers to the trine or 120 degrees. See https://en.wikipedia.org/wiki/Astrological_aspect#Trine-->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Saturnus in ascende[n]te">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &Saturn; in asc.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Rising Saturn.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in quarta &agrave; Sole">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quarta a sole.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A quarter from the sun.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="uenatione"/>
