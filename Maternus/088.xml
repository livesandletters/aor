        <marginalia hand="English_secretary" anchor_text="mulier in sexto geniturae suae loco">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        In mulieris genitura.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the constellation of the woman.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="expositos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Expositi. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Exposed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Lucianus id libidinis genius describit
                    </marginalia_text>
                    <person name="Lucian"/>
                </position>
            </language>
            <translation>
                Lucian, this genius of pleasure writes.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="per gonorream">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        p[er] Gonorream
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Through Gonorrhea.
                <!-- M.B. See https://books.google.co.uk/books?id=X8BaAAAAcAAJ&pg=RA1-PA228&dq=%22gonorream%22&hl=en&sa=X&ved=0ahUKEwjDrs7Q_8fVAhXkK8AKHa__Bcg4HhDoAQhdMAg#v=onepage&q=%22gonorream%22&f=false 
