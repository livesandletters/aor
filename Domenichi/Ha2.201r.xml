	<page filename="Ha2.201r.tif" signature="P[r]" pagination="225" reader="Harvey"/>	<!-- ABO 205right -->
	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Temperantia, nutrix Prudentia[e]: Justitia,
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						custos fortitudinis.
					</marginalia_text>
				</position>
			</language>
			<translation>Temperance, the nurse of prudence: justice, the guard of fortitude.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Inuicta munitio,
						virtus indomita.
					</marginalia_text>
				</position>
			</language>
			<translation>An invincible defence, an unrestrained virtue. </translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Symposia Platonis, et Xenophontis. Mensa Philosophica.
					</marginalia_text>
					<person name="Plato"/>
					<person name="Xenophon"/>
					<book title="Mensa philosophica"/>
				</position>
			</language>
			<translation>The Symposia of Plato and Xenophontis. The Mensa philosophica.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Puerilis est animus, qui non est serius,
						et firmus in omni Casu.
					</marginalia_text>
				</position>
			</language>
			<translation>The mind which is not serious and firm in every case, is childish.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						filosofo non filosofo.
					</marginalia_text>
				</position>
			</language>
			<translation>The philosopher, not a philosopher.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Non sunt philosophi, qui non sunt mundi exemplaria.
					</marginalia_text>
				</position>
			</language>
			<translation>Philosophers, who are not examples of the world, are not philosophers.</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Maior animus, in maiori periculo.
					</marginalia_text>
				</position>
			</language>
			<translation>In greater danger, the mind is greater.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Rudis est, qui non est concinnus artifex suorum facinorum.
					</marginalia_text>
				</position>
			</language>
			<translation>He who is not the elegant master of his deeds, is uncultivated.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Ad Artis, et Virtutis normam; examina[m] di Excellentissimi. Acuteq[ue]
						Videndum, an corrigi possint Auctorum, Actorumq[ue] eminentissimi:
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="270">
					<marginalia_text>
						et q[u]omodo possint dexterrim&egrave; pra[e]stari.
					</marginalia_text>
				</position>
			</language>
			<translation>Towards the norm of art and virtue; and towards the examen of the most excellent. And it must be observed acutely <!-- CG: I don't understand the use of 'di' in the Latin. Did Harvey make a mistake? -->
			whether the most eminent of authors and actors can be corrected: and in what way they can be exceeded most skillfully.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Maximus mundi Methodicus, et Practicus: Zeteticus, et Empiricus.
					</marginalia_text>
				</position>
			</language>
			<translation>The greatest methodical and practical man of the world: inquisitive and empirical.</translation>
		</marginalia>
		
