		<marginalia hand="Italian">
			<language ident="LA">
				<position place="head" book_orientation="0">
					<marginalia_text>
						Non tamen parui libri, sed magnus animus facit virum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>s[upr]a. /</marginalia_text>
				</position>
			</language>
			<translation>[See] above.</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Spiritus heroum, et philosophoru[m]; prophetaru[m], et apostoloru[m];
					</marginalia_text>
				</position>
				<position place="left_margin" book_orientation="0">
					<marginalia_text>
						Apollonij, et Maximi;
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						instar omnium Librorum: imm&ograve; plus qu&agrave;m omnes Libri.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						risoluto, e discreto.	
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						In corde Pyropus: in Lingua magnes: in manu adamas. Tria ad
					</marginalia_text>
				</position>
				<position place="right_margin" book_orientation="0">
					<marginalia_text>
						Omnia, potentiss[im]a Instrumenta.
					</marginalia_text>
				</position>
			</language>
		</marginalia>	
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						A seipso pendet: et per seipsum valet Eutrapelus.
					</marginalia_text>
					<person name="Eutrapelus"/>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Simplex est, qui non est Orlando Furioso furiosior. Nec Achillem
						metuerim;
					</marginalia_text>
				</position>
				<position place="tail" book_orientation="90">
					<marginalia_text>
						nec Alexandru[m]; nec ipsu[m] Ca[e]sarem; nec vllu[m] Herou[m], aut Gigantum.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Ca[e]tera fragilia: vir sibi constat.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="270">
					<marginalia_text>
						Nec Hic: nec Ille: sed Tu.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
