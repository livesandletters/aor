	<annotation>
		<!-- Marginalia -->
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="head" book_orientation="0">
					<marginalia_text>
						( The currentest &amp; pregnantest Entelechie in =
					</marginalia_text>
				</position>
				<position place="intext" book_orientation="0">
					<marginalia_text>
						= in art, philosophie, physique, Lawe, diuinitie, pollicie,
						warr, intercourse, nauigation, &amp; Whatsoeuer facultie, or emploiment
						of importance. No practis comparable.
					</marginalia_text>
				</position>
			</language>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Il diuino Aretino,	
					</marginalia_text>
					<person name="Pietro Aretino"/>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Flagellum Principum:
					</marginalia_text>
				</position>
			</language>
			<language ident="IT">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						et esso Apollonio Tianeo,
					</marginalia_text>
					<person name="Apollonius of Tyana"/>
				</position>
			</language>
			<language ident="LA">
				<position place="intext" book_orientation="0">
					<marginalia_text>
						Malleus Tyrannorum. Etiam hodi&egrave; Junius Brutus, Tyrannomastix.
					</marginalia_text>
					<person name="Lucius Junius Brutus"/>
				</position>
			</language>
			<translation>
				The divine Aretino was the scourge of princes and Apollonius of Tyana was the hammer of tyrants.
				Even today, Junius Brutus is the Tyrant-Whip.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Hermes, Microcosmus.
					</marginalia_text>
					<person name="Hermes"/>
				</position>
			</language>
			<translation>
				Hermes, a Microcosm.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Eudromus, Neocosmus.
					</marginalia_text>
					<person name="Eudromus"/>
				</position>
			</language>
			<translation>
				Eudromus, a new world.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Meus mihi Efficiens: mea Materia: mea Forma: meus
						Finis. Mea vnius Effecta, et Adiuncta.
					</marginalia_text>
				</position>
			</language>
			<translation>
				My own efficient cause, material cause, formal cause, and final cause.
				My only effect and supplement.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Quod non potest Deucalion, potest Vulcanus: et contra.
					</marginalia_text>
					<person name="Deucalion"/>
					<person name="Vulcan"/>
				</position>
			</language>
			<translation>
				That which Deucaleon cannot do, Vulcan can; and vice-versa.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="EN">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						The sacking of Gadis. Shall I tell in a word, What conquer'd Cales? A strong Imagination,
						&amp; braue tales.
					</marginalia_text>
					<location name="Cadiz"/>
					<location name="Cales"/>
				</position>
			</language>
		</marginalia>
		
