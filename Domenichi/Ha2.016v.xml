        		<marginalia hand="Italian">
        			<language ident="LA">
            			<position place="head" book_orientation="0">
                    				<marginalia_text>
                    					Eutrapelus viuum speculum omnis excellentia[e].
                      			</marginalia_text>
            			           <person name="Eutrapelus"/>
            			            <emphasis method="chalk" type="straight"/>
            			</position>
        			</language>
        			<translation>
        				Eutrapelus is a living mirror of every excellence.
        			</translation>
        		</marginalia>
