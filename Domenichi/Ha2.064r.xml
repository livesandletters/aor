		<underline method="pen" type="straight" language="IT" text="uerso la tauola"/>
		<underline method="pen" type="straight" language="IT" text="linea diritta dal filo della"/>
		<underline method="pen" type="straight" language="IT" text="elegantemente quella parte"/>
		<underline method="pen" type="straight" language="IT" text="occult&ograve; la deformit&agrave; dell'occhio nell'ombra della tauola"/>
		<underline method="pen" type="straight" language="IT" text="tutti tre"/>
		<underline method="pen" type="straight" language="IT" text="&amp; l'uno dietro"/>
		<underline method="pen" type="straight" language="IT" text="present&ograve; il suo quadro"/>
		<underline method="pen" type="straight" language="IT" text="di Polignoto"/>
		<underline method="pen" type="straight" language="IT" text="si conturb&ograve;"/>
		<underline method="pen" type="straight" language="IT" text="esser si brutto, &amp;"/>
		<underline method="pen" type="straight" language="IT" text="collericamente di corte"/>
		<underline method="pen" type="straight" language="IT" text="maligna libert&agrave; haueua"/>
		<underline method="pen" type="straight" language="IT" text="all'arte, c'hauer"/>
		<underline method="pen" type="straight" language="IT" text="alla dignit&agrave; reale"/>
		<underline method="pen" type="straight" language="IT" text="di Scoppa piacque"/>
		<underline method="pen" type="straight" language="IT" text="grandemente"/>
		<underline method="pen" type="straight" language="IT" text="piu fresca et&agrave;, nella"/>
		<underline method="pen" type="straight" language="IT" text="d'esser uccellato"/>
		<underline method="pen" type="straight" language="IT" text="troppo imprudente adulatione"/>
		<underline method="pen" type="straight" language="IT" text="L'adulatione &egrave;"/>
		<underline method="pen" type="straight" language="IT" text="costare a chi"/>
		<underline method="pen" type="straight" language="IT" text="come adulatore"/>
		<underline method="pen" type="straight" language="IT" text="Diocle"/>
		<underline method="pen" type="straight" language="IT" text="suo ritratto"/>
		<underline method="pen" type="straight" language="IT" text="sodisfece egregiamente a tutti i cortigiani"/>
		<underline method="pen" type="straight" language="IT" text="especialmente ad Antigono"/>
		<underline method="pen" type="straight" language="IT" text="modestia di Diocle nell'hauer"/>
		<underline method="pen" type="straight" language="IT" text="della riputation dell'arte"/>
		<underline method="pen" type="straight" language="IT" text="mancar di moderato giudicio, per"/>
		<underline method="pen" type="straight" language="IT" text="non parer discortese"/>
		<underline method="pen" type="straight" language="IT" text="difetto dell'effigie"/>
		<underline method="pen" type="straight" language="IT" text="sempre piu scarso"/>
		<underline method="pen" type="straight" language="IT" text="Antigono gli don&ograve; quattro talenti"/>
		<underline method="pen" type="straight" language="IT" text="ritenne honoratamente"/>
		<underline method="pen" type="straight" language="IT" text="tutte le cose"/>
		<underline method="pen" type="straight" language="IT" text="solo la uia del mezo &egrave; di salute"/>
		
		<underline method="chalk" type="straight" language="IT" text="L'adulatione &egrave; dolce"/>
		<underline method="chalk" type="straight" language="IT" text="cose sola la uia del mezo &egrave; di salute"/>
		
