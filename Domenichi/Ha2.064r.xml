		<marginalia hand="Italian">
			<language ident="IT">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Il diuino Apollonio Tianeo.
					</marginalia_text>
					<person name="Apollonius of Tyana"/>
				</position>
			</language>
			<translation>
				The divine Apollonius of Tyana.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Distinctio discretionis, optima.
					</marginalia_text>
				</position>
			</language>
			<translation>
				An excellent distinction in discretion.
			</translation>
		</marginalia>
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="tail" book_orientation="0">
					<marginalia_text>
						Contr&agrave; Machiauelli politismum.
					</marginalia_text>
					<person name="Machiavelli"/>
				</position>
			</language>
			<translation>
				Against the politics of Machiavelli.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="left_margin" book_orientation="90">
					<marginalia_text>
						Malo discretionis speculu[m] chalybeum, qu&agrave;m adulationis cristallinu[m].
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				I prefer the iron mirror of discretion than the crystalline mirror of flattery.
			</translation>
		</marginalia>
		
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="90">
					<marginalia_text>
						Libri, vt opes, non sunt in tua potestate: sed oportet
						peritum esse suam Bibliothecam. 
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				Books, as with riches, are not in your power: but it is necessary
				to be experienced in one's Library.
			</translation>
		</marginalia>
				
		<marginalia hand="Italian">
			<language ident="LA">
				<position place="right_margin" book_orientation="270">
					<marginalia_text>
						Orationis ad Reginam temperatus stilus. s[upr]a, 134.
					</marginalia_text>
					<emphasis method="chalk" type="straight"/>
				</position>
			</language>
			<translation>
				The mild style of the oration to the Queen. [See] above, pg. 134.
			</translation>
		</marginalia>
		
