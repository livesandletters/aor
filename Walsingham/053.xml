        <underline method="pen" type="straight" language="LA" text="sicut potest de iure facere tanquam dominus de feodo suo, quia nos ei homagium nostru[m]"/>
        <underline method="pen" type="straight" language="LA" text="plena potestate"/>
        <underline method="pen" type="double" language="LA" text="voluntate reddimus ei terram Scotiae"/>
        <underline method="pen" type="straight" language="LA" text="gentem totam cum homagijs"/>
        <underline method="pen" type="straight" language="LA" text="factoq[ue]; sigillo communi"/>
        <underline method="pen" type="straight" language="LA" text="Scotiae, praecedente eum semper per vnam dietam Episcopo Dunelmensi"/>
        <underline method="pen" type="straight" language="LA" text="Scotorum tempore coronationis"/>
        <underline method="pen" type="straight" language="LA" text="vti pro throno, vsque VVestmonasterium transtulit illum"/>
        <underline method="pen" type="straight" language="LA" text="caelebrantium cathedram sacerdotum"/>
        <underline method="pen" type="straight" language="LA" text="magnatum Scotiae fidelitates"/>
        <underline method="pen" type="straight" language="LA" text="homagia"/>
        <underline method="pen" type="straight" language="LA" text="hoc literas patentes eorum sigillis"/>
        <underline method="pen" type="straight" language="LA" text="promittimus pro nobis, &amp; haeredibus nostris, sub poena corporum nostrorum &amp; castellorum, ac omnium
             quae habere possumus, quod nos seruiemus ei bene &amp; fideliter contra omnes gentes, quae viuere &amp; mori possunt, omnibus vicibus quibus"/>
        <underline method="pen" type="straight" language="LA" text="nostris, &amp; eos"/>
        <underline method="pen" type="straight" language="LA" text="Insuper &amp; iuramus tactis sacrosanctis Euangelijs, &amp; poste&agrave; nos omnes, &amp; quilibet nostrum,"/>
        <underline method="pen" type="straight" language="LA" text="se secit homagium dicto domino nostro regi Angliae in haec verba."/>
        <underline method="pen" type="straight" language="LA" text="legalitatem seruabo Edvvardo regi Angliae &amp; haeredibus suis, de"/> 
        <underline method="pen" type="straight" language="LA" text="terreno honore, co[n]tra omnes qui possunt viuere &amp; mori:"/>
        <underline method="pen" type="straight" language="LA" text="arma"/>
        <underline method="pen" type="straight" language="LA" text="nec ero in consilio vel auxilio contra eum, vel haeredes suos"/>
        <underline method="pen" type="straight" language="LA" text="sed fideliter recognoscam, &amp; fideliter faciam seruitia, quae pertinent ad tenementum, quod de eo tenere clamito, sic"/>
        <underline method="pen" type="straight" language="LA" text="regni regis Angliae Edvvardi"/>
        <underline method="pen" type="double" language="LA" text="domini nostri"/>
        <underline method="pen" type="straight" language="LA" text="vicesimo quinto"/>
        <underline method="pen" type="straight" language="LA" text="cui mandauit rex, vt omnes tenentes de rege terras aliquas vocaret, &amp; eorum homagia fidelitat&eacute;sque reciperet."/>
