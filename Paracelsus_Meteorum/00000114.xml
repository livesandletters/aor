    <page filename="00000114.tif" reader="John Dee" pagination="[50[v]]"  signature="[Nij[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Nement ein jrdischen Sulphur, ein jrdisch Salnitrum, ein jrdischen Mercurium, vnd die drey bringent durch kunst in ein Corpus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Sal nitru[m] vulgare &Mercury; &Sulfur; } in vnu[m] componere</marginalia_text>
                    <marginalia_symbol name="Mercury"/>
                    <marginalia_symbol name="Salt"/>
                </position>
            </language>
            <translation>Vulgar salnitrum, mercury, and salt composed into one.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Superis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Superi</marginalia_text>
                </position>
            </language>
            <translation>Gods.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Superis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nota. praxim supernam</marginalia_text>
                </position>
            </language>
            <translation>Note. Celestial practice.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="jrdischen Sulphur"/>
        <underline method="pen" type="straight" language="DE" text="jrdisch Salnitrum"/>
        <underline method="pen" type="straight" language="DE" text="jrdischen Mercurium"/>
        <underline method="pen" type="double" language="DE" text="kunst in ein Corpus"/>
        <underline method="pen" type="straight" language="DE" text="nach jrdischer arth ein wund"/>
        <underline method="pen" type="straight" language="DE" text="erbarlichen impetum finden"/>
        <underline method="pen" type="straight" language="DE" text="genommen wird von den"/>
        <underline method="pen" type="double" language="DE" text="Superis"/>
        <underline method="pen" type="double" language="DE" text="Magistris"/>
        <underline method="pen" type="straight" language="DE" text="vn[d] zusam[m]en in Aludel das ist in seinen Sternen"/>
        <underline method="pen" type="double" language="DE" text="zerlassen den Sulphur"/>
        <underline method="pen" type="double" language="DE" text="der Salniter"/>
        <underline method="pen" type="straight" language="DE" text="vereinigt er sich zusammen"/>
        <underline method="pen" type="straight" language="DE" text="doch ohne bewegung vnd impetu"/>
        <underline method="pen" type="straight" language="DE" text="vbrige gradus der Hitze wie vor gemelt"/>
        <underline method="pen" type="straight" language="DE" text="so geu&sharp-S;t es der Stern au&sharp-S; in die eusser regiones, das ist in"/>
        <underline method="pen" type="double" language="DE" text="Lufft"/>
        <underline method="pen" type="double" language="DE" text="Gew&uuml;lcke"/>
        
        <drawing type="manicule" book_orientation="0" place="left_margin" method="pen"/>
        
    </annotation>
