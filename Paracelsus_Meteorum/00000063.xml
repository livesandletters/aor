        <marginalia hand="Italian" anchor_text="Dieser Vulcanus ist kein Geist ist auch nicht ein Person: In dem theilet er sich von[n] den Saganis. Sondern es ist ein Arbeiter &amp; gar nichts mehr thut dann der Natur warten">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vulcanus no[n] est ex Saganis vnus.</marginalia_text>
                    <person name="Vulcan"/>
                </position>
            </language>
            <translation>Vulcan is not one of the Sagani.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sondern ist ein Arbeiter">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vulcani officium</marginalia_text>
                    <person name="Vulcan"/>
                </position>
            </language>
            <translation>The duty of Vulcan.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="seim sahmen in die Vltimam materiam"/>
        <underline method="pen" type="double" language="DE" text="der Geist geistet"/>
        <underline method="pen" type="straight" language="DE" text="Prima materia"/>
        <underline method="pen" type="straight" language="DE" text="alles das so im Gott befolen zumachen"/>
        <underline method="pen" type="straight" language="DE" text="Componiert"/>
        <underline method="pen" type="straight" language="DE" text="dispensiert"/>
        <underline method="pen" type="straight" language="DE" text="vnd ordiniert"/>
        <underline method="pen" type="straight" language="DE" text="Regen"/>
        <underline method="pen" type="straight" language="DE" text="Schnee"/>
        <underline method="pen" type="straight" language="DE" text="Reiff"/>
        <underline method="pen" type="straight" language="DE" text="Hagel"/>
        <underline method="pen" type="straight" language="DE" text="Stral"/>
        <underline method="pen" type="straight" language="DE" text="ist kein Geist"/>
        <underline method="pen" type="straight" language="DE" text="ein Person"/>
        <underline method="pen" type="straight" language="DE" text="ein Arbeiter"/>
        <underline method="pen" type="straight" language="DE" text="thut dann der Natur warten"/>
        <underline method="pen" type="straight" language="DE" text="vnd das au&sharp-S; ir zubringen das in jr ist vnd"/>
