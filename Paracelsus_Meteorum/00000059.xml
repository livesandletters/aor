    <page filename="00000059.tif" reader="John Dee" pagination="23" signature="Fiij"/> 
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Sagani 4 Elementoru[m] no[n] habent a[n]i[m]am humana[m]. Licet intellectum, prudentiamq[ue] habeant.</marginalia_text>
                </position>
            </language>
            <translation>Of the four elements the Sagani do not have the human soul. Although they have intellect and prudence.</translation> <!--JG: Not sure-->
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Homo - Adami[anes] - Sagani[enes].</marginalia_text> <!--JG: Not sure-->
                </position>
            </language>
            <translation>Man - those of Adam - Saganians.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Menschviech">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Manbeasts.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Gnomi; Nymphen">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Gnomi. Nympha.</marginalia_text>
                </position>
            </language>
            <translation>Gnomes. Nymphes.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Bergen Th&auml;lern Erden Wasser">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In hilles, dales Mynes, and Waters.</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="das er keinies Elementes ist">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Homo est nullus vnius Elementi.</marginalia_text>
                </position>
            </language>
            <translation>Man is not of one element.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="das er keinies Elementes ist">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>[Nota.]</marginalia_text> <!--JG: Not sure-->
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="nicht bey"/>        
        <underline method="pen" type="straight" language="DE" text="wir Menschen seindt durch Gott geschaffen"/>
        <underline method="pen" type="straight" language="DE" text="der Seele ohn todt"/>
        <underline method="pen" type="straight" language="DE" text="Seele die andern nicht haben"/>
        <underline method="pen" type="straight" language="DE" text="menschliche vernunfft vnd Kunst"/>
        <underline method="pen" type="straight" language="DE" text="Seel nicht"/>
        <underline method="pen" type="double" language="DE" text="Menschviech seindt sie"/>
        <underline method="pen" type="straight" language="DE" text="Gnomi haben alle K&uuml;nst der Menschen"/>
        <underline method="pen" type="double" language="DE" text="die Nymphen"/>
        <underline method="pen" type="straight" language="DE" text="Da ist aber keine Seele f&uuml;r die Christus gestorben sey"/>
        <underline method="pen" type="straight" language="DE" text="mit deren bludt vnd fleisch er"/>
        <underline method="pen" type="straight" language="DE" text="Dan[n] viel wirt der Natur zugelegt das au&sharp-S; der Natur nicht also ist sondern durch solche Saganas geschehen"/>
        <underline method="pen" type="straight" language="DE" text="Bergen"/>
        <underline method="pen" type="straight" language="DE" text="Th&auml;lern"/>
        <underline method="pen" type="straight" language="DE" text="Erden"/>
        <underline method="pen" type="straight" language="DE" text="Wasser"/>
        <underline method="pen" type="straight" language="DE" text="er keines"/>
        <underline method="pen" type="double" language="DE" text="alleine ist"/>
        
