    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>&Sun; est sal ex tribus p[ri]m[is].</marginalia_text>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>The sun is salt from the three principles.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="den dreien ersten">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Tria prima</marginalia_text>
                </position>
            </language>
            <translation>The tree principles.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Die Sonne ist Sal perspicuum diaphanum, von den dreien ersten clarificiert extrahiert vnd mundiert von aller dunckele">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Sun; est sal diaphanum omni macula et tenebra, vacuum</marginalia_text>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>The sun is transparent salt, devoid of all blemishes and darkness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="den Weisse schein in die Son[n]e vn[d] der Rotte schien in den Mon vnd Sternen" id="NYSLWin188:00000051:m0001">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lume[n] &Sun; - album &Moon; - rubeu[m]</marginalia_text>
                </position>
            </language>
            <translation>Light. The sun is white [and] the moon is reddish.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Solis Creatio qualis</marginalia_text>
                </position>
            </language>
            <translation>What kind of creation of the sun.</translation>
        </marginalia>
        
