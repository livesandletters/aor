    <page filename="00000080.tif" reader="John Dee" pagination="[33[v]]" signature="[J[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="auch gehet durch die Poras der Erde[n] durch die Mannern">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Venti Terra[e] poros et Muros penetrat</marginalia_text>
                </position>
            </language>
            <translation>The wind penetrates the limestone and the walls of the earth.</translation> <!--JG: Unsure, venti = plural. Also, text is about water, not about wind. Could be that it should be 'penetra[n]t' though-->
            <!-- FSF: the texts tells of the impact of wind on water, so I think wind is what Dee had in mind; since neither the text nor the script suggest a plural, I would assume that this is a mistake by Dee? -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="das da vo[n] Erdbidmen werden">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Terra[e]motus Vnde.</marginalia_text>
                </position>
            </language>
            <translation>Hence earthquakes.</translation> 
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="dergleichen gangen"/>
        <underline method="pen" type="double" language="DE" text="sondern auff de[m] Wasser"/>
        <underline method="pen" type="straight" language="DE" text="die Poras der Erde[n]"/>
        <underline method="pen" type="straight" language="DE" text="Manuern"/>
        <underline method="pen" type="straight" language="DE" text="Berg"/>
        <underline method="pen" type="straight" language="DE" text="felsen"/>
        <underline method="pen" type="straight" language="DE" text="vnd kompt nicht durch das Wasser herau&sharp-S; so kompt es in die Erden"/>
        <underline method="pen" type="straight" language="DE" text="gehet durch die Erden so lange"/>
        <underline method="pen" type="straight" language="DE" text="bi&sharp-S; es sein Exitum findt oder im selbigen consumiert wirdt"/>
        <underline method="pen" type="straight" language="DE" text="das da vo[n] Erbdimen"/>
        
        
