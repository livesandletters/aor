    <page filename="00000018.tif" reader="John Dee"  signature="[Aij[v]]" pagination="[2[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ex fructibus eoru[m] cognoscetis [etc]</marginalia_text>
                </position>
            </language>
            <translation>You will recognize from their fruits, [et cetera].</translation> 
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="erste[n] Matery na&sharp-S; sein">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Materia p[r]ima Aqueoru[m], humida</marginalia_text> 
                </position>
            </language>
            <translation>The first matter of watery things is humid.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Erden">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Materia prima Terrenorum, sicca</marginalia_text> 
                </position>
            </language>
            <translation>The first matter of earthy things is dry.</translation>  
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hat nun sein mutter au&sharp-S; dem Element wasser: Vnd im Element wasser wachsen die Vltimae materiae, der anfang Liquidum ist vnd Humidum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mineraliu[m] prima Materia, Liquido[rum]</marginalia_text> 
                </position>
            </language>
            <translation>Of minerals, the first matter is liquid.</translation>  
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="Dan[n] wie das ist das vo[n] jm wachst also ist auch das dasselbige gemacht vn[d] geboren hat"/>
        <underline method="pen" type="straight" language="DE" text="saltz"/>
        <underline method="pen" type="straight" language="DE" text="stein"/>
        <underline method="pen" type="straight" language="DE" text="seiner erste[n]"/>                                                          
        <underline method="pen" type="double" language="DE" text="Matery na&sharp-S;"/>
        <underline method="pen" type="straight" language="DE" text="sein"/>
        <underline method="pen" type="straight" language="DE" text="in ein Coagulation"/>
        <underline method="pen" type="straight" language="DE" text="d&uuml;rrer Materia"/>
        <underline method="pen" type="straight" language="DE" text="Compactirt sey"/>
        <underline method="pen" type="double" language="DE" text="ein Liquor"/>
        <underline method="pen" type="double" language="DE" text="Liquidum ist vnd Humidum"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="DE" text="hat nun sein mutter au&sharp-S; dem Element wasser: Vnd im Element wasser wachsen die Vltimae materiae, der anfang Liquidum ist vnd Humidum"/>
     
