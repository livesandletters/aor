        <marginalia hand="Italian" anchor_text="Eclipsi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Luna scilic[et] inter posita.</marginalia_text>
                </position>
            </language>
            <translation>Of course the moon posited inbetween.</translation>
        </marginalia>
        
        <marginalia hand="Italian" id="NYSLWin188:00000049:m0001">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Stella[e] continuo excitant natura[e] [ac] suas functiones obt&uuml;ndas</marginalia_text> <!--JG: Unsure about ac--> <!-- FSF: I think it is natura[m] and ad -->
                </position>
            </language>
            <translation>The stars continuously stir up nature and strike their functions.</translation> 
        </marginalia>
        
