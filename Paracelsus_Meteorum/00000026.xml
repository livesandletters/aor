<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="00000026.tif" reader="John Dee" pagination="[6[v]]" signature="[Bij[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Coelu[m] p[ri]m&ograve; Creatu[m] tanqu[am] vas pro reliquis Elementis</marginalia_text>
                </position>
            </language>
            <translation>The heaven was created first as if a vessel for the other elements.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="das alle vier Corpora der vier Elementen gemacht seindt au&sharp-S; nichts">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ex nihilo omnia.</marginalia_text>
                </position>
            </language>
            <translation>Everything [created] from nothing.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fiat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fiat</marginalia_text>
                </position>
            </language>
            <translation>Let it be done.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nichts">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nihil.</marginalia_text>
                </position>
            </language>
            <translation>Nothing.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="etwas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aliquid.</marginalia_text>
                </position>
            </language>
            <translation>Something.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Also da&sharp-S; das wort Fiat ist worden ein dreyfach Corpus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fiat, produxit corpus triforme siue triplex: vel potius Vnitrinum.</marginalia_text>
                </position>
            </language>
            <translation>Let it be done, he produced a threefold or triple corpus; or rather a triple in one.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="Reseruauel"/>
        <underline method="pen" type="double" language="DE" text="au&sharp-S; nichts"/>
        <underline method="pen" type="straight" language="DE" text="das wort Gottes"/>
        <underline method="pen" type="straight" language="LA" text="Fiat"/>
        <underline method="pen" type="double" language="DE" text="Nichts"/>
        <underline method="pen" type="double" language="DE" text="dem etwas"/>
        <underline method="pen" type="straight" language="DE" text="Dasselbige Corpus aller vier"/>
        <underline method="pen" type="double" language="DE" text="drey Species"/>
        <underline method="pen" type="double" language="DE" text="dreyfach Corpus"/>
        
    </annotation>
</transcription>
