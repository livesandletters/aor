    <annotation>
        
        <marginalia hand="Italian" anchor_text="Dan[n] auch Christus in Parabolis sein Wort verk&uuml;ndet hat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Parabolaris Doctrina</marginalia_text>
                </position>
            </language>
            <translation>An allegorical doctrine.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="die zeit vnd stunde solcher Winder aufferstehung">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Tempus et hora quando ventus suborietur</marginalia_text>
                </position>
            </language>
            <translation>The time and hour when the wind will come into being.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Astronomisch">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Astronomia tempora ventoru[m] docere debet</marginalia_text>
                </position>
            </language>
            <translation>Astronomy ought to teach the times of the winds.</translation>
        </marginalia>
        
