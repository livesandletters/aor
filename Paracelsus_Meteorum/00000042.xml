    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Stellae ex Coelo ortae sunt, et in eo manent.</marginalia_text>
                </position>
            </language>
            <translation>The stars arise from the heaven and remain in it.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="nach dem vnd"/>
        <underline method="pen" type="straight" language="DE" text="Sternen vnd irem vrsprung"/>
        <underline method="pen" type="double" language="DE" text="in ir nichts"/>
        <underline method="pen" type="double" language="DE" text="das treibt sie auff die Erden vnd behalt nichts in ir gibt"/>
        <underline method="pen" type="straight" language="DE" text="von der Erden"/>
        <underline method="pen" type="straight" language="DE" text="vnd auff"/>
        <underline method="pen" type="straight" language="DE" text="in der Erden ist nichts"/>
        <underline method="pen" type="straight" language="DE" text="au&sharp-S; dem Himmel"/>
        <underline method="pen" type="double" language="DE" text="alle Gestirn"/>
        <underline method="pen" type="double" language="DE" text="vnd au&sharp-S; im genommen"/>
        <underline method="pen" type="straight" language="DE" text="aber m&uuml;ssen nicht still stehen sonder sie m&uuml;ssen f&uuml;rgehen iren Circkel"/>
        <underline method="pen" type="double" language="DE" text="sie kugeln"/>
        <underline method="pen" type="straight" language="DE" text="f&uuml;r waltzen vnd wallendt"/>
        <underline method="pen" type="straight" language="DE" text="Vnd seindt also von Himmel gescheiden vn[d] doch im Himmel"/>
        <underline method="pen" type="straight" language="DE" text="eingeleibt"/>
        
        <drawing type="manicule" book_orientation="0" place="tail" method="pen" anchor_text="Vnd seindt also von Himmel gescheiden vn[d] doch im Himmel"/>
        
    </annotation>
