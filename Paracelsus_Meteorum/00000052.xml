    <page filename="00000052.tif" reader="John Dee" pagination="[19[v]]" signature="[Eiij[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="als ein Geist au&sharp-S;gezogen">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nota.</marginalia_text>
                </position>
            </language>
            <language ident="DE">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Als ein Geist auszgezogen.</marginalia_text>
                </position>
            </language>
            <translation>Note. Shown like a mind.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Also ist ein Corpus Solis, Sulphur ein Corpus Lunae, vnd der Liquor ein Corpus der finstere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sal - &Sun; Sulph - &Moon; Mer[curiu]m - Teneb.</marginalia_text>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>Salt - the sun; Sulfur - the moon; Mercury - darkness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Darumb hat Gott sechs tag gew&uuml;rcket bi&sharp-S; er extrahiert separiert geformiert has">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Separatio diuina, sex dieru[m] labor in Genesi.</marginalia_text>
                    <book title="Book of Genesis"/>
                </position>
            </language>
            <translation>Divine separation, six days of labour in Genesis.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in sechs tagen herau&sharp-S; gescheiden">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Creatio Dei in sex diebus</marginalia_text>
                </position>
            </language>
            <translation>The creation of God in six days.</translation>
        </marginalia>
        
        <underline method="pen" type="triple" language="DE" text="als ein Geist au&sharp-S;gezogen"/>
        <underline method="pen" type="double" language="DE" text="die Sonne[n]"/>
