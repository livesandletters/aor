        <underline method="pen" type="double" language="LA" text="Corpus Sulphuris"/>
        <underline method="pen" type="straight" language="DE" text="R&ouml;tte"/>
        <underline method="pen" type="straight" language="DE" text="Diaphinitet vnd Perspicuitet"/>
        <underline method="pen" type="double" language="DE" text="Sulphuris Rubei"/>
        <underline method="pen" type="double" language="DE" text="Liquor ein Corpus der finstere"/>
        <underline method="pen" type="double" language="DE" text="hat zu sein"/>
        <underline method="pen" type="double" language="DE" text="selbst gescheiden wider"/>
        <underline method="pen" type="straight" language="DE" text="sechs tag gew&uuml;rcket bi&sharp-S; er extrahiert"/>
        <underline method="pen" type="straight" language="DE" text="separiert"/>
        <underline method="pen" type="straight" language="DE" text="geformiert"/>
        <underline method="pen" type="straight" language="LA" text="Prima materia, Vltimas materias"/>
        <underline method="pen" type="straight" language="DE" text="auff die zergehung der Welt"/>
        <underline method="pen" type="double" language="DE" text="eins geschaffen"/>
        <underline method="pen" type="straight" language="DE" text="sechs tagen herau&sharp-S; gescheiden"/>
        
