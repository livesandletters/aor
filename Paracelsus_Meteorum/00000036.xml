    <annotation>
        
        <marginalia hand="Italian" marginalia_id="00000036_0001" internal_ref="00000036_u001">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Coralloru[m] Tinctura</marginalia_text>
                </position>
            </language>
            <translation>The tint of corals.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dann die R&ouml;tte ist ein reines Corpus in dem all krafft ligt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Virtutes reru[m] in spiritibus suis solum.</marginalia_text>
                </position>
            </language>
            <translation>The virtues of the things [are] only in their spirits.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Stral">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fulmen.</marginalia_text>
                </position>
            </language>
            <translation>Lightning.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ein au&sharp-S;gezogenes wesen von allen Corporibus gescheiden">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Essentia separata a corpore.</marginalia_text>
                </position>
            </language>
            <translation>The essence seperated from the body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sein Spiritus so gar vber ander veheme[n]tia vber andere alle">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Spiritus Coeli est maxima veheme[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>The spirit of heaven is the utmost vigorous.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="von seinem Element"/>
        <underline method="pen" type="double" language="DE" text="ire R&ouml;tte ist darzu gut" id="00000036_u001"/>
        <underline method="pen" type="triple" language="DE" text="ein reines Corpus"/>
        <underline method="pen" type="triple" language="DE" text="Krafft ligt"/>
        <underline method="pen" type="double" language="DE" text="Stral"/>
        <underline method="pen" type="straight" language="DE" text="au&sharp-S;gezogenes wesen von allen Corporibus"/>
        <underline method="pen" type="straight" language="DE" text="gescheiden allein in sein eigen leib"/>
        <underline method="pen" type="straight" language="DE" text="reiniste vnd prugireste"/>
        
        <mark name="comma" method="pen" place="intext"/> <!-- wesen von-->
        <mark name="comma" method="pen" place="intext"/> <!-- gescheiden allein-->
        
