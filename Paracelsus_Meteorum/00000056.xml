    <annotation>
        
        <marginalia hand="Italian" anchor_text="Die Superi seindt die Pennates">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Superi Pennates</marginalia_text>
                </position>
            </language>
            <translation>Penates are gods.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Die Superi seindt die Pennates">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Verissim&egrave;, su[n]t Nymphy e[t] Gnomi.</marginalia_text>
                </position>
            </language>
            <translation>Most truthfully, there are nymphs and gnomes.</translation>
        </marginalia>
        
        <underline method="pen" type="triple" language="DE" text="durch verstandt"/>
        <underline method="pen" type="double" language="DE" text="obern Superorum"/>
        <underline method="pen" type="double" language="DE" text="wissenheit"/>
        <underline method="pen" type="double" language="DE" text="ein Geist"/>
        <underline method="pen" type="straight" language="DE" text="Was in der Welt ist das auch im Firmament wissenhafft bein in sein. Dann"/>
        <underline method="pen" type="straight" language="DE" text="vber des Mensche[n] verstandt wie bi&sharp-S;her so ist"/>
        <underline method="pen" type="straight" language="DE" text="den obern"/>
        <underline method="pen" type="double" language="DE" text="als sch&uuml;tzen zu einem zil"/>
        <underline method="pen" type="double" language="DE" text="geschickligkeit"/>
        <underline method="pen" type="double" language="DE" text="was gewaldt sie haben"/>
        <underline method="pen" type="straight" language="DE" text="der Philosophey"/>
        
    </annotation>
