    <annotation>
        
        <marginalia hand="Italian" anchor_text="ad apparentiam imaginamenta porrigere, quorum nec mox appareat vestigium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Appare[n]tia no[n] existe[n]ti[um]</marginalia_text>   
                </position>
            </language>
            <translation>The appearance of things which do not exist.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="numinum aduocatione, vti solitos Magos, per huiuscemodi consensum reru[m] tradit: Ex quo liquet Magiae alter[a]e partis artis infamia, quod numinis appellatione praetenta" id="RCP9978:013:m0001">
            <!-- FSF: I think this is English secretary -->
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Numinum appellatio. aduocatio p[er] aptas Ilices.</marginalia_text>    
                </position>
            </language>
            <translation>The name of the divine will. Counsel through suitable holm oaks.</translation>  
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Quae fascinati imaginamur pr[a]eter imaginamenta, nullam habent actionis &amp; essentiae veritatem"/>
