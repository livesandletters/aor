        <underline method="pen" type="straight" language="LA" text="Epoden"/>
        <underline method="pen" type="straight" language="LA" text="multo grauius est"/>
        <underline method="pen" type="straight" language="LA" text="etiam numinum aduovatione, vti solitos Magos, per huiuscemodi consensum reru[m]" id="RCP9978:013:u0001"/>
