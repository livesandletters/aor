    <page filename="022.tif" reader="John Dee" signature="[B iij[v]]"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="spiritum mundi">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Spiritus Mundi.</marginalia_text>
                </position>
            </language>
            <translation>The spirit of the world.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Deos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dij</marginalia_text>
                </position>
            </language>
            <translation>Gods.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="anima ipsa, ut ipsi atruunt, primum sit mobile">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pri[mum] Mo[bile] a[n]i[m]a</marginalia_text>
                </position>
            </language>
            <translation>The first movable thing, the soul.</translation>
        </marginalia>
                
        <marginalia hand="English_secretary" anchor_text="Quintam essentiam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Q[uinta] Ess[enti]a</marginalia_text>
                </position>
            </language>
            <translation>The fifth essence.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Medium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>M[edium]</marginalia_text>
                </position>
            </language>
            <translation>The middle.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="animae coelestes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>A[n]i[m]a[e] Ca[e]lestes</marginalia_text>  
                </position>
            </language>
            <translation>Celestial souls.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="hauritur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>haurire spiritu[m] mundi</marginalia_text>
                </position>
            </language>
            <translation>To absorb the spirit of the world.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Anima 1 - Primu[m] Mobile Spiritus 2 - Q[uinta] Ess[enti]a Corpus. 3.</marginalia_text>
                </position>
            </language>
            <translation>The soul, 1, the first movable thing; the spirit, 2, the fifth essence; the body, 3.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="etiam animas"/>
        <underline method="pen" type="straight" language="LA" text="asserebant vnius Entis animam egredi, &amp; ingredi in aliud"/>
        <underline method="pen" type="straight" language="LA" text="ut Adamas est impedime[n]to Magneti lapidi, ne ferru[m] attrahat"/>
        <underline method="pen" type="straight" language="LA" text="primum sit mobile"/>
        <underline method="pen" type="straight" language="LA" text="sponte, inquam, &amp; per se mobile"/>
        <underline method="pen" type="straight" language="LA" text="Medium reru[m] esse aiunt spiritum mundi, quem pleriq[ue]"/>
        <underline method="pen" type="straight" language="LA" text="sed quoddam quintu[m], praeter ea substans"/>
        <underline method="pen" type="double" language="LA" text="animae"/>
        <underline method="pen" type="straight" language="LA" text="coelestes adsint corpori crassiori"/>
        <underline method="pen" type="double" language="LA" text="ferme"/>
        <underline method="pen" type="straight" language="LA" text="noster ipse spiritus"/>
        <underline method="pen" type="straight" language="LA" text="virtus anim[a]e"/>
        <underline method="pen" type="straight" language="LA" text="Quintam essentiam"/>
        <underline method="pen" type="straight" language="LA" text="hauritur e[ni]m per radios stellarum, quatenus res se ijs reddiderint conformes"/>
        <underline method="pen" type="straight" language="LA" text="hunc igitur spiritum omnis occulta proprietas p[ro]pagatur, se&apos;q[ue] miscet tum herbis, metallis"/>
        <underline method="pen" type="straight" language="LA" text="quidem ab elementali natura"/>
        
