    <annotation>
        
        <marginalia hand="Italian" anchor_text="au&sharp-S; den fr&uuml;chten" id="NYSLWin189:00000041:m0001"> 
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Ex fructibus eorum cognoscetis eos</marginalia_text> <!--JG: 'cognoscetis'-->
                    <book title="Gospel of Matthew"/>
                </position>
            </language>
            <translation> You shall know them by their fruits.</translation> <!-- Corrected the first part of this translation. Most likely a reference to Matthew 7:20 or adapted from it-->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="geist de&sharp-S; quecksilbers"> <!-- FSF: connected with a stroke -->
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Nota. Quid si hunc Mercurium vocemus succu[m] ex tribus primis ut vbiq[ue] supra no[m]inauit?</marginalia_text>
                </position>
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>At &Mercury;ij spiritus co[n]tra[...] declaratus et &Salt; spiritus</marginalia_text> <!-- FSF: to be checked and completed --> <!-- NBW: That remaining unknown word might be "contraram? -->
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Salt"/>
                </position>
            </language>
            <translation>Note. What, if we call this Mercury, a spirit from the tree primal parts, as he called [it] everywhere above? While the spirit of Mercury declares [...].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mercurio Solis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mercurius &Sun;15&Venus;15&Mercury;ij</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Venus"/>
                </position>
            </language>
            <translation>Mercury Sun 15 Venus 15 Mercury 2.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="spiritibus salis"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>spiritus salia[e]</marginalia_text>
                </position>
            </language>
            <translation>The spirits of salt.</translation>  
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="eysen z&u_ring; kupffer"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Thermaru[m] Nocumenta</marginalia_text> 
                </position>
            </language>
            <translation>Nuisance of the baths.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="concordantz"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Concordantia.</marginalia_text>
