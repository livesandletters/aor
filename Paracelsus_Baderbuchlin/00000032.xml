    <annotation>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Istam primam materia[m] triu[m], Ego, Q, Ess[enti]am eorundem haud male iudicari censeo</marginalia_text> <!--JG: Could Essam be 'Essentiam', since 'am' appears in superscript?-->
                </position>
            </language>
            <translation>I, Q, recommend that this prime matter of the three, their essence, is by no means to be judged badly.</translation> <!-- JG: Not sure, no clue about 'Q'-->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="die sieben mineralia">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>&Saturn;&Jupiter;&Mars;&Sun;&Venus;&Mercury;</marginalia_text>
                    <marginalia_symbol name="Saturn"/>
                    <marginalia_symbol name="Jupiter"/>
                    <marginalia_symbol name="Mars"/>
                    <marginalia_symbol name="Sun"/>
                    <marginalia_symbol name="Venus"/>
                    <marginalia_symbol name="Mercury"/>
                    <marginalia_symbol name="Moon"/>
                </position>
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>7 Mineralia</marginalia_text>
                </position>
            </language>
            <translation>Saturn, Jupiter, Mars, Sun, Venus, Mercury, Moon. 7 Minerals.</translation>
        </marginalia>
               
        <marginalia hand="Italian" anchor_text="die tugend krafft vnnd eygenschafft">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Thermarum virtus unde</marginalia_text>
                </position>
            </language>
            <translation>Hence the properties of the baths.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Confortatio</marginalia_text>
                </position>
            </language>
            <translation>Invigoration.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="DE" text="So die sieben mineralia ligen in primis tribus"/>
        <underline method="pen" type="straight" language="DE" text="verb&ouml;seret"/>
        <underline method="pen" type="straight" language="DE" text="so mischt sich die krafft"/>
        <underline method="pen" type="straight" language="DE" text="in ein vereinigung"/>
        <underline method="pen" type="straight" language="DE" text="au&sharp-S; disem"/>
        <underline method="pen" type="straight" language="DE" text="primarum trium"/>
        <underline method="pen" type="straight" language="DE" text="inn eim wasser"/>
        <underline method="pen" type="straight" language="DE" text="dem selbigen"/>
        <underline method="pen" type="straight" language="DE" text="durch es laufft"/>
        <underline method="pen" type="straight" language="DE" text="das in jnen ist die prima materia trium"/>
        <underline method="pen" type="straight" language="DE" text="der contractur"/>
        <underline method="pen" type="straight" language="LA" text="hyposarcha"/>
        <underline method="pen" type="straight" language="LA" text="materice"/>
        <underline method="pen" type="straight" language="DE" text="so au&sharp-S; der sterckung kommen"/>
        <underline method="pen" type="straight" language="DE" text="schedlich aber den kranckheyten so die glider"/>
        <underline method="pen" type="straight" language="LA" text="alopiciam"/>
        <underline method="pen" type="straight" language="LA" text="morpheam"/>
        <underline method="pen" type="straight" language="LA" text="pruritum"/>
