        <underline method="pen" type="straight" language="IT" text="Ammiraglio vn fratello del"/>
        <underline method="pen" type="straight" language="IT" text="sapeuano esprimere alcune parole in"/>
        <underline method="pen" type="straight" language="IT" text="per nome tutti i"/>
        <underline method="pen" type="double" language="IT" text="Christiani"/>
        <underline method="pen" type="double" language="IT" text="luogo rimasi"/>
        <underline method="pen" type="straight" language="IT" text="i Christiani subito incominciarono a venire in discordia tra loro, &amp; a pigliar ciascuno le donne"/>
        <underline method="pen" type="double" language="IT" text="l'oro"/>
        <underline method="pen" type="straight" language="IT" text="che egli poteua: &amp;"/>
        <underline method="pen" type="straight" language="IT" text="vccisero vn Giacopo"/>
        <underline method="pen" type="straight" language="IT" text="poscia insieme con altri noue"/>
        <underline method="pen" type="straight" language="IT" text="andati con le loro donne ad vn Cacique, chiamato Caunabo"/>
        <underline method="pen" type="straight" language="IT" text="delle minere"/>
        <underline method="pen" type="straight" language="IT" text="questi gli amazz&ograve;, &amp;"/>
        <underline method="pen" type="straight" language="IT" text="gente al Natale"/>
        <underline method="pen" type="straight" language="IT" text="oue non era altri, che Diego di Arana con dieci persone"/>
        <underline method="pen" type="straight" language="IT" text="s'erano sparsi per diuersi luoghi"/>
        <underline method="pen" type="straight" language="IT" text="il Caunabo"/>
        <underline method="pen" type="straight" language="IT" text="quali i Christiani habitauano con le loro donne"/>
        <underline method="pen" type="straight" language="IT" text="si annegarono otto"/>
        <underline method="pen" type="straight" language="IT" text="tre perirono"/>
        <underline method="pen" type="straight" language="IT" text="che non mostrauano"/>
        <underline method="pen" type="straight" language="IT" text="per difendere i Christiani, col Caunabo, fu ferito, &amp; se ne fugg&igrave;"/>
        
