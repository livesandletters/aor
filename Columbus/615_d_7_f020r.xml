    <annotation>
        
        <underline method="pen" type="straight" language="IT" text="l'Isola di Porto"/>
        <underline method="pen" type="double" language="IT" text="conteneuano nove caraffe di vino."/>
        <underline method="pen" type="straight" language="IT" text="Indie sono di queste canne"/>
        <underline method="pen" type="straight" language="IT" text="soffiando molto tempo i Ponenti"/>
        <underline method="pen" type="straight" language="IT" text="gittaua alcuni Pini in quelle Isolle"/>
        <underline method="pen" type="straight" language="IT" text="de Fiori"/>
        <underline method="pen" type="straight" language="IT" text="di faccia molto larga"/>
        <underline method="pen" type="straight" language="IT" text="aspetto diverso da' Christiani"/>
