    <annotation>
        
        <marginalia hand="Italian" method="pen" anchor_text="l'acque seguono quiui il corso de' venti">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nota de ventis et aqua cursu.</marginalia_text>
                </position>
            </language>
            <translation>Note the course of the wind and the water.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" method="pen" anchor_text="Tiburoni tol">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Tiburoni fish</marginalia_text>
                </position>
            </language>
            <translation>Bonnethead shark (Sphyrna tiburo)</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="IT" text="tira l'acqua in su fino alle nubi"/>
        <underline method="pen" type="straight" language="IT" text="pi&ugrave; gro&sharp-S;a, che vna botte, torcendola a guisa di turbine"/>
        <underline method="pen" type="straight" language="IT" text="che le correnti di quella Costa si conformauano co' temporali, &amp; che allhora andauano col"/>
        <underline method="pen" type="straight" language="IT" text="verso Leuante"/>
        <underline method="pen" type="straight" language="IT" text="regnauano i Leuanti"/>
        <underline method="pen" type="straight" language="IT" text="correuano all'Occidente"/>
        <underline method="pen" type="straight" language="IT" text="l'acque seguono quiui il corso de' venti, che pi&ugrave; soffiano."/>
        <underline method="pen" type="straight" language="IT" text="Tiburoni a'"/>
        <underline method="pen" type="straight" language="IT" text="augurij"/>
        <underline method="pen" type="straight" language="IT" text="e lo tagliano,"/>
        
    </annotation>
