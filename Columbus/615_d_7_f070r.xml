        <underline method="pen" type="double" language="IT" text="popolatione, &amp; dominio"/>
        <underline method="pen" type="straight" language="IT" text="gli si offeriuano"/>
        <underline method="pen" type="straight" language="IT" text="vi sariano restati"/>
