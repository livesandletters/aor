    <annotation>
        <underline method="pen" type="straight" language="IT" text="discono alcune cose, o le diminuiscono"/>
        <underline method="pen" type="straight" language="IT" text="taciono quello, che giustamente"/>
        <underline method="pen" type="straight" language="IT" text="scriuere con molta particolarit&agrave;"/>
        <underline method="pen" type="straight" language="IT" text="mia scrittura si"/>
            <underline method="pen" type="straight" language="IT" text="disetto, n&oacute; ci sar&agrave; quello"/>
        <underline method="pen" type="straight" language="IT" text="ch'&egrave; la poca, &amp; incerta verit&agrave; di quello, che scriuono."/>
            <underline method="pen" type="straight" language="IT" text="gli scritti, &amp; lettere"
    </annotation>
