    <page filename="268.tif" reader="John Dee" pagination="[124v]"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Hi igitur sunt termini naturaliu[m] morborum &amp; mortis. uiolenti autem sunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pro morte naturali et violenta.</marginalia_text>
                </position>
            </language>
            <translation>For a natural and violent death.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Si tame[n] ego &agrave; generalibus periculis abstinuero, clade, peste, naufragio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Generalia pericula.</marginalia_text>
                </position>
            </language>
            <translation>Common dangers.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>&Saturn; ipse, na[m] retrogradus est.</marginalia_text>
                    <symbol_in_text name="Saturn"/>
                </position>
            </language>
            <translation>Saturn itself, for it is retrograde.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Postmodu[m] direxi quadratu[m] Solis dextrum ad ascende[n]s"/>
        <underline method="pen" type="straight" language="LA" text="&amp; perueniet ad ipsum in 58 anno"/>
        <underline method="pen" type="straight" language="LA" text="Si tame[n] ego &agrave; generalibus periculis abstinuero, clade, peste, naufragio"/>
        <underline method="pen" type="straight" language="LA" text="solum in his, sed etiam in filijs, &amp; uita uxoris"/>
        
    </annotation>
