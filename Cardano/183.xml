        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>.5. Hora</marginalia_text>
                </position>
            </language>
            <translation>5. Hour.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ut artes natorum"/>
