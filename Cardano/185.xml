    <page filename="185.tif" reader="John Dee" pagination="83" signature="X iij"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0"> 
                    <marginalia_text>Alia est hominis vltima a[e]tas, alia senilis.</marginalia_text> 
                </position>
            </language>
            <translation>One is the greatest age of man, the other of the elderly.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="per reuolutione[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>De revolutione et eius virtute.</marginalia_text> 
                </position>
            </language>
            <translation>On a revolution and its character.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Respondemus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Responsum.</marginalia_text> 
                </position>
            </language>
            <translation>Reply.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cum per aeternos motus sit connexio, merito potuit quis dicere, in genitura quaecunq[ue] uentura sint, perfecte contineri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Absolutu[m] respo[n]sum secunda[e] qu[a]estio[n]i.</marginalia_text> 
                </position>
            </language>
            <translation>The perfect response to the second question.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Respondemus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Natiuitatis non debent absoluta dari Judicia, revolutionibus aut directionibus non consideratis.</marginalia_text> 
                </position>
            </language>
            <translation>Perfect judgements of a nativity ought not to be given without the consideration of the revolutions and directions.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="praeuenitur">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>moritur</marginalia_text>
                </position>
            </language>
            <translation>It dies.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>vero</marginalia_text>
                </position>
            </language>
            <translation>Certainly.</translation>
        </marginalia>
        
