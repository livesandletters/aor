        <marginalia hand="English_secretary"> <!-- FSF: possibly anchor Si Luna to & descehndat -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Moon; in Septimo a dignitate desc[endat].</marginalia_text> <!--JG: Not sure-->
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>The moon may descend in the seventh [house] by dignity.</translation> <!-- FSF: not sure if it is house or place, given that septimo is masculine -->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Cum Luna fuerit in eadem parte cum Sole, &amp; ambo in quadrato vel opposito Saturni">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Conjunction; &Sun; et &Moon; in &Square; &Opposition; &Saturn;.</marginalia_text>
                    <symbol_in_text name="Conjunction"/>
                    <symbol_in_text name="Sun"/>
                    <symbol_in_text name="Moon"/>
                    <symbol_in_text name="Square"/>
                    <symbol_in_text name="Opposite_planets"/>
                    <symbol_in_text name="Saturn"/>
                </position>
            </language>
            <translation>The conjunction of the sun and the moon, [and] both in square [or] opposition to Saturn.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nutrietur">
