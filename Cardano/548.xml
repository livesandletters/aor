            <translation>The most elegant aphorism.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Pestilentiae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pestilentia[e].</marginalia_text>
                </position>
