    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Figura[m] illa[m] sculpta[m] esse mihi asserebat, neglige[n]tia vero no[n] esse edita[m].</marginalia_text>
                </position>
            </language>
            <translation>He alleged to me that this figure was engraved, in truth it was not edited out of negligence.</translation>
        </marginalia>
        
