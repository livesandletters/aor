    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Quamobrem patet, deliquia mortem in ascendente minime significare. idem autem dico de magnis coniunctionibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Eclipses nec magnas &Conjunction; in asc morte[m] significare</marginalia_text>
                </position>
            </language>
            <translation>[It is well known] that eclipses nor great conjunctions signify death in ascending.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text></marginalia_text> <!--JG: CG to transcribe and translate-->
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ERgo eclipses &amp; coniunctiones magnae nihil operantur, nisi ut causae generals"> 
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Eclipsiu[m] effectus</marginalia_text> 
                </position>
            </language>
            <translation>The effect of eclipses.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="magnis coniunctionibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>De magnis co[n]iunctionbius</marginalia_text> 
                </position>
            </language>
            <translation>On great conjunctions.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Quamobrem patet"/>
        <underline method="pen" type="straight" language="LA" text="significare. idem autem"/>
        <underline method="pen" type="straight" language="LA" text="nisi cu[m[ regione calamitate[m] effecerint"/>
        <underline method="pen" type="straight" language="LA" text="fortuito"/>
        <underline method="pen" type="straight" language="LA" text="obiter"/>
        <underline method="pen" type="straight" language="LA" text="Multas alias eclipses hoc modo obseruaui nihil"/>
        <underline method="pen" type="straight" language="LA" text="Ergo eclipses &amp; coniunctiones magnae nihil operantur, nisi ut causae generales: unde plus nocent his qui, cum effectus initio, cum urbe &amp; prouincia concordant, quam his quibus in Horoscopo auot loco uitae ceciderint"/>
        <underline method="pen" type="straight" language="LA" text="Si tamen hora ipsa reuolutionis annuae, dominum ascendentis infestum habuerint, esse potest, ut mali aliquid minentur"/>
        <underline method="pen" type="straight" language="LA" text="tantu[m] est propositum docere, Ptolemaei regulas minie ueras esse,"/>
        <underline method="pen" type="straight" language="LA" text="Lunae, minime semper malum"/>
        
