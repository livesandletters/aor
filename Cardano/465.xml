    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="465.tif" reader="John Dee" pagination="223" signature="KK iij"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>De morte</marginalia_text>
                </position>
            </language>
            <translation>On death.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Et oportet ut in hoc uerseris experimento longo, propter regionum &amp; morum uarietatem, no[n] enim secus continget, qu&acirc;m ei qui herbas uult cognoscere, potest enim per longum experimentum inter simillima exiguam perspicere differentiam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Regionis co[n]suetudo</marginalia_text>
                </position>
            </language>
            <translation>The custom of the region</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="corpus mortui insepultum iacebit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quid cu[m] cadavere fiet post viole[n]tam mortem.</marginalia_text>
                </position>
            </language>
            <translation>What happens with the corpse after a violent death.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" >
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>De fama &amp; honore post mortem.</marginalia_text>
                </position>
            </language>
            <translation>On the fame and honour after death.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary"  anchor_text="ad eum enim deferetur haereditas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ad que[m] deferetur ha[e]reditas.</marginalia_text>
                </position>
            </language>
            <translation>To whom the inheritance will be offered.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary"  anchor_text="Electio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Electio.</marginalia_text>
                </position>
            </language>
            <translation>Election.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Gemini &amp; Sagitarius, &amp; Pisces, multiplex genus mortis significabunt, quod &amp; Saturnus &amp; Mars cu[m] simul domoninant[ur]"/>
        <underline method="pen" type="straight" language="LA" text="si in signis auium, aut quadrupedum, aut piscium fuerint"/>
        <underline method="pen" type="straight" language="LA" text="Electio non proficit"/>
        
        <mark name="bracket" method="pen" place="right_margin"/>
        
    </annotation>
