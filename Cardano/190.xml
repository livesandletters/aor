    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="190.tif" reader="John Dee" pagination="[85v]" signature="[Y[v]]"/>
    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Significat &Mercury;ij [Mercurij] natura[m] no[n] solu[m] ex alijs pendere sed etia[m] aliqua[m] propria[m] 
                    et peculiare[m] significatione[m] habere.</marginalia_text>
                    <symbol_in_text name="Mercury"/>
                </position>
            </language>
            <translation>He signifies that the nature of Mercury not only is dependent on others, but even has any particular and peculiar signification.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Luminaria</marginalia_text> 
                </position>
            </language>
            <translation>Lights.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>&id_est; peculiaris qu[a]edam natura // &id_est; in numero</marginalia_text> 
                </position>
            </language>
            <translation>That is, a certain specific nature. That is, in a number.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="undecim">
            <language ident="LA">
                <position place="intext" book_orientation="0"> 
                    <marginalia_text>septem planeta[e] / et [quattuor] cardines.</marginalia_text>
                </position>
            </language>
            <translation>Seven planets and four poles.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="decemnouem">
            <language ident="LA">
                <position place="intext" book_orientation="0"> 
                    <marginalia_text>12 dom[us]. 7 planet[ae]</marginalia_text>
                </position>
            </language>
            <translation>Twelve houses, seven planets.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="radios mittunt">
            <language ident="LA">
                <position place="intext" book_orientation="0"> 
                    <marginalia_text>&id_est; planetis ip[s]is.</marginalia_text> 
                </position>
            </language>
            <translation>That is, of the planets themselves.</translation> 
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="q"> <!-- FSF: I'd anchor as q[uod] -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0"> <!-- intext -->
                    <marginalia_text>qui.</marginalia_text>
                </position>
            </language>
            <translation>Which.</translation> 
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="domini secundum substantiam faciunt impressione[m] in loco permanentem, non domini secundu[m]"/>
        <underline method="pen" type="straight" language="LA" text="&amp; Mercurio inest proprietas, ut fiat in natura dominatorum secundum substantiam"/>
        <underline method="pen" type="straight" language="LA" text="proportio taliu[m] locoru[m] est consimilis loco mitte[n]tis cu[m] quada[m] proportione contrarietatis. In"/>
        <underline method="pen" type="straight" language="LA" text="q[uod]"/>
        <underline method="pen" type="double" language="LA" text="semper"/>
        <underline method="pen" type="straight" language="LA" text="dominat&ograve;r omnium uirtutum in toto"/>
        <underline method="pen" type="straight" language="LA" text="&amp; Luna delatrix"/>
        <underline method="pen" type="straight" language="LA" text="etiam optime dispositis alijs planetis"/>
        
        <mark name="comma" method="pen" place="intext"/> <!-- fiat in-->
        
        <numeral place="intext" language="LA" text="quada[m]">1</numeral>
        <numeral place="intext" language="LA" text="semper">2</numeral>
        
    </annotation>
