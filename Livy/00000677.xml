        <underline method="pen" type="straight" language="LA" text="suis legatis se, qui cum"/>
        <underline method="pen" type="straight" language="LA" text="certiores factos"/>
        <underline method="pen" type="straight" language="LA" text="Rumoris huic quia neminem"/>
        <underline method="pen" type="straight" language="LA" text="neq[ue] affirmata"/>
        <underline method="pen" type="straight" language="LA" text="nec pro uana praetermissa"/>
        <underline method="pen" type="straight" language="LA" text="&AElig;toli legati in"/>
        <underline method="pen" type="straight" language="LA" text="confitendo"/>
        <underline method="pen" type="straight" language="LA" text="seu suae culpae, seu errori ueniam peterent supplices"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; beneficijs in populum"/>
        <underline method="pen" type="straight" language="LA" text="exprobrantes uirtutem suam in Philippi"/>
        <underline method="pen" type="straight" language="LA" text="offenderunt aures insolentia"/>
        <underline method="pen" type="straight" language="LA" text="uetera &amp; obliterata repetendo"/>
        <underline method="pen" type="straight" language="LA" text="plurium maleficiorum gentis, qu&agrave;m beneficiorum"/>
        <underline method="pen" type="straight" language="LA" text="misericordia opus"/>
        <underline method="pen" type="straight" language="LA" text="iram &amp; odium irritarent"/>
        <underline method="pen" type="straight" language="LA" text="uno senatore"/>
        <underline method="pen" type="straight" language="LA" text="permitterent ne arbitrium de se populo"/>
        <underline method="pen" type="straight" language="LA" text="habituri ne eosdem, quos populus Romanus, socios &amp;"/>
        <underline method="pen" type="straight" language="LA" text="nihil ad ea respondentes"/>
        <underline method="pen" type="straight" language="LA" text="totos adhuc Antiochi &AElig;tolos esse, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="unica ea spe pend&ecirc;re animos"/>
        <underline method="pen" type="straight" language="LA" text="bellum cum haud dubijs hostibus gerendum, perdomandos que feroces animons esse"/>
