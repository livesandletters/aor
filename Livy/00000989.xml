        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text> 
                        LL. Consulum, consensu populi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The laws of the consuls, by general assent of the people.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text> 
                        polemic&egrave;, et politic&egrave;.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Militarily and politically. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text> 
                        Vnus princeps, maior toto populo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One ruler is mightier than the entire people.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text> 
                        Lex popularis. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The popular law.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Affectatorem regni">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text> 
                        Democratica Lex contra Regni affectatorem.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The democratic law against the one who strives for a kingdom.
            </translation>
        </marginalia>
        
