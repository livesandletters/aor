    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0126.tif" reader="John Dee" pagination="98" signature="[gg.i.[v]]"/>
    <annotation> 
        
        <marginalia hand="Italian" anchor_text="vertit stilum in tabulis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vertit stilu[m] in tabulis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'He applied the blunt end of his style to [his] records' [Loeb 221, 401].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="causam omnem euertit suam. nihil enim sibi reliqui fecit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        caussa[m] evertere: nihil reliq[ui] facere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make an end to all causes; to make something disappear.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="desperatissimo perfugio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        desperatiss[imu]m p[ro]fugiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most desperate escape.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="perditissima defensio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        perditiss[im]a defe[n]sio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most hopeless defence.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cor&agrave;m">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        coram
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the presence of.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="laqueos se induerit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        induere se in laqueos
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put on noses.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fecisset, aliorum exe[m]plo, institut&oacute;que">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        facere alior[um] exe[m]plo instituto
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To do something after the example and custom of others.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in re tam clara, tam testata, tam abs te ipso peruulgata">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        res [ clara testata peruulgata ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A clear, proved, [and] widely known matter.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mendum ista litura correxit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        me[n]du[m] corrigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To correct the error.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tabulae sunt in medio, quae se corruptas atque interlitas esse clamant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        tabule [ corrupt[a]e interlit[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Corrupted and smeared records.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="deme[n]tiam [...] induat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        inducere deme[n]tiam
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To cover up the madness.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="co[n]trauenit, si contr&agrave; omni ratione pugnauit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]trauenire co[n]tr&agrave; pugnare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To oppose, to fight against.
            </translation>
        </marginalia>
        
