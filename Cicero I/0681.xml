        <underline method="pen" type="straight" language="LA" text="animos &agrave; minore cura ad summum timorem traduceret. Sed tamen in eiusmodi perturbatione rerum quanquam omnia sunt metuenda, nihil magis qu&agrave;m perfidiam timemus: &amp;"/>
        <underline method="pen" type="straight" language="LA" text="tantum habeo polliceri"/>
        <underline method="pen" type="straight" language="LA" text="praesentibus cumulat&egrave; satisfacturum"/>
        <underline method="pen" type="straight" language="LA" text="quorum vtrum minus velim, non facile possum existimare"/>
        <underline method="pen" type="straight" language="LA" text="est quiddam tertiu[m]"/> <!-- Note at head -->
        <underline method="pen" type="straight" language="LA" text=",&amp; dignitatem tuam in virtute, atque in rebus gestis tuis, atque in tua grauitate positam existimare. Siquid ex iis rebus, quas tibi fortuna largita est, nonnullorum hominum perfidia detraxerit, id maiori illis fraudi, qu&agrave;m tibi futurum"/>
        <underline method="pen" type="straight" language="LA" text="vtor ad omnia"/>
        <underline method="pen" type="straight" language="LA" text="posita sunt in coniectura"/>
        <underline method="pen" type="straight" language="LA" text="clamore, conuiti&oacute;que iactatus est"/>
        <underline method="pen" type="straight" language="LA" text="asperre &amp; acerbe"/>
        <underline method="pen" type="straight" language="LA" text="magno silentio est"/>
        <underline method="pen" type="straight" language="LA" text="desertum se, atque abiectum fore"/>
        <underline method="pen" type="straight" language="LA" text="sine vlla dubitatione"/>
        <underline method="pen" type="straight" language="LA" text="Sed nostri hominis tarditatem &amp; taciturnitatem"/>
        <underline method="pen" type="straight" language="LA" text="Caeteris iniuriis, quae praepositae"/>
        <underline method="pen" type="straight" language="LA" text="ex consularibus"/>
        <underline method="pen" type="straight" language="LA" text="sunt partim obscurius iniqui, partim non dissumlanter irati."/>
        <underline method="pen" type="straight" language="LA" text="vt infringatur hominum improbitas &amp; co[n]siliis tuorum amicorum"/>
        <underline method="pen" type="straight" language="LA" text="debilitat cogitationes &amp; inimicorum &amp; proditorum."/>
        <underline method="pen" type="straight" language="LA" text="mihi &agrave; teneris (vt Graeci dicunt) vnguiculis"/>
        <underline method="pen" type="straight" language="LA" text="Illustrabit (mihi crede) tuam amplitudinem hominum iniuria."/>
        <underline method="pen" type="straight" language="LA" text="non fallam opinionem tuam."/>
        <underline method="pen" type="straight" language="LA" text="quonia[m] interuallo locorum, &amp; temporu[m] disiuncti sumus, per"/>
        <underline method="pen" type="straight" language="LA" text="id erit causae"/>
        <underline method="pen" type="straight" language="LA" text="mihi certoru[m] hominu[m] potestas"/>
        
