        <marginalia hand="English_secretary" anchor_text="quoties iam tibi extorta est sica ista de manibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quoties extorta est sica ex manib[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                How often was the dagger extorted from his hands?
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="infestis oculis omnium conspici">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        infestis oculis co[n]spici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be observed with unsteady eyes.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
