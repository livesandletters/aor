                    <marginalia_text>
                        &agrave; maiore
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="patria">
            <language ident="LA">
