        <underline method="pen" type="straight" language="LA" text="solent homine nobiles"/>
        <underline method="pen" type="straight" language="LA" text="seu recte seu perperam facere coeperunt, ita in vtroque excellunt"/>
        <underline method="pen" type="straight" language="LA" text="vt nemo nostro loco natus assequi posset"/>
        <underline method="pen" type="straight" language="LA" text="aut sponsionem iubet facere"/>
        <underline method="pen" type="straight" language="LA" text="Conturbatus san&egrave; discedit"/>
        <underline method="pen" type="straight" language="LA" text="optio ta[m] misera t&aacute;mque iniqua daretur"/>
        <underline method="pen" type="straight" language="LA" text="se capitis damnaret"/>
        <underline method="pen" type="straight" language="LA" text="si sponsione[m] fecisset"/>
        <underline method="pen" type="straight" language="LA" text="spes esset"/>
        <underline method="pen" type="straight" language="LA" text="in hoc summa iudicii caus&aacute;que tota consistit"/>
        <underline method="pen" type="straight" language="LA" text="Cum maiores ita co[n]stituerint, vt qui pro capite diceret, is posteriore loco diceret"/>
        <underline method="pen" type="straight" language="LA" text="ea ingenia co[n]uerti ad perniciem, quae antea versabantur in salute atque uxilio ferendo"/>
        <underline method="pen" type="straight" language="LA" text="hesterno die fecerunt"/>
        <underline method="pen" type="straight" language="LA" text="in ius adducerent"/>
        <underline method="pen" type="straight" language="LA" text="nisi tu quod esset tuum ius"/>
        <underline method="pen" type="straight" language="LA" text="nostru[m] ius contra illos obtineremus"/>
        <underline method="pen" type="straight" language="LA" text="ita sine iniuria potentia[m] leuem atque inopem esse arbitrantur"/>
        <underline method="pen" type="straight" language="LA" text="ne dicendo tempus absuma[m]"/>
        <underline method="pen" type="straight" language="LA" text="nec illud mihi arrogabo, me posse causam commodius demonstrare, qu&agrave;m antea demonstrata sit: neque tame[n] tam multa"/>
        <underline method="pen" type="straight" language="LA" text="faciam, propterea qu&ograve;d &amp; ab illo qui ant&egrave; dixit, informata iam causa est: &amp; &agrave; me qui neque excogitare, neque pronu[n]ciare multa possum, 
            breuitas postulatur, quae mihimetipsi amicissima est"/>
        <underline method="pen" type="straight" language="LA" text="totam causae meae dictione[m] certas in partes diuidam"/>
        <underline method="pen" type="straight" language="LA" text="quod tibi natura dat vt semper possis, id mihi causa dat"/>
        <underline method="pen" type="straight" language="LA" text="hodie possim"/>
        
