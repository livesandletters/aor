    <page filename="0241.tif" reader="John Dee" pagination="213" signature="oo.iii."/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="mala consuetudine dissolutu[m] est">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        mala co[n]suetudine dissolutu[m] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Abrogated by this evil practice.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text=" nam vt quaeque res est turpissima, sic maxime &amp; maturissime">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        maturisse tardissim&egrave;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very speedily, very slowly.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        lex spo[n]siorius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The law of the one giving the promise.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="facto quidem turpe">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        facto turpe
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Done by infamous conduct.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="actione co[n]fligere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        actio[n]e co[n]fligere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring one's one action.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="summo loco natam &amp; probatissimam foeminam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        femina summo loco nata probatiss[ima]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A woman, born into the highest family and highly esteemed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="adiuncta mercatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        adiuncta mercari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To buy adjacent land.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="se ipse inferebat &amp; intrudebat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        se inferre intrudere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To obtrude themselves, to push themselves into.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="mala consuetudine"/>
        <underline method="pen" type="straight" language="LA" text="na[m] vt quaeque res est turpissima, sic maxime"/>
        <underline method="pen" type="straight" language="LA" text="si quis, quod spopondit, qua in re verbo se vno obligauit, id non facit, maturo iudicio, sine vlla religione iudicis condemnatur"/>
        <underline method="pen" type="straight" language="LA" text="actione co[n]fligere"/>
        <underline method="pen" type="straight" language="LA" text="si aut mihi praescribat quemadmodum meum ius persequar: aut"/>
        <underline method="pen" type="straight" language="LA" text="Veruntamen nimiae vestrae venignitati pareremus, si alia"/>
        <underline method="pen" type="straight" language="LA" text="alterius ego vobis hodierno die causam profecto auferam, ne diutius de controuersia nostra ac de communi iure dubitetis. 
            &amp; si forte videbor altius initium rei demonstrandae petisse, qu&agrave;m me ratio iuris, &amp; ius, de quo iudicium est, &amp; natura causae coegerit: quaeso vt ignoscatis. non enim"/>
        <underline method="pen" type="straight" language="LA" text="ha[n]c persona[m]"/>
        
