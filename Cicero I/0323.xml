    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="C. Marium, quem vere patrem patriae, parentem inquam vestrae libertatis, atque huiusce Reipublicae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Caius Marius p[ate]r patria[e] parens libertatis parens Reip.
                    </marginalia_text>
                    <person name="Gaius Marius"/>
                </position>
            </language>
            <translation>
                Gaius Marius, father of the fatherland, parent of freedom, parent of the state.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in fide non stetit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in fide stare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be trusted.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="innumerabileis hostium copias in Italia fudisset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fundere innumerabiles copias
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To rout countless troops.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="religionem &amp; sanctimonia[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        religio sanctimonia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Holy and sacred estates.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="gloriae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Gloria
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Glory.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="fistulas quibus aqua suppeditabatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fistulis aqua[m] suppeditari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To supply water with pipes.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="salutem"/>
        <underline method="pen" type="straight" language="LA" text="ade&oacute;ne ignarus es disciplinae, consuetudin&iacute;sque nostrae, vt haec nescias"/>
        <underline method="pen" type="straight" language="LA" text="quoniam sensu &amp; vita caret"/>
        <underline method="pen" type="straight" language="LA" text="omnia sua secum vn&agrave;"/>
        <underline method="pen" type="straight" language="LA" text="neque quisquam nostrum in Reipublicae periculis cum laude ac virtute versatur, quin spe posteritatis fruct&uacute;que ducatur"/>
        <underline method="pen" type="straight" language="LA" text="virorum bonorum mentes diuinae mihi, atque aeternae videntur esse, tum maxime qu&ograve;d optimi &amp; sapientissimi cuiusque animus ita praesentit in posterum, 
            vt nihil nisi sempiternum spectare videatur"/>
        <underline method="pen" type="straight" language="LA" text="deorum religionem &amp; sanctimonia[m] demigrasse"/>
        <underline method="pen" type="straight" language="LA" text="Etenim Quirites exiguum nobis vitae curriculum natura circunscripsit, immensum gloriae. quare si eos qui iam de vita decesserunt ornabimus,
            iustiorem nobis mortis conditione[m] relinquemus. Sed"/>
