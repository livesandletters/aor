        <underline method="pen" type="straight" language="LA" text="ne illa quidem curo mihi scribas, quae maximis in rebus reip[sis]. geruntur quotidie, nisi quid ad meipsum pertinebit."/>
        <underline method="pen" type="straight" language="LA" text="nuntiabunt, perferet multa etiam ipse rumor."/>
        <underline method="pen" type="straight" language="LA" text="longe in posterum prospiciente"/>
        <underline method="pen" type="straight" language="LA" text="dies nullis in aliis, nisi de rep. sermonibus versatus sum: quae nec possunt scribi, nec scribe[n]da sunt. tantu[m]"/> <!-- NBW: Check Abbreviation -->
        <underline method="pen" type="straight" language="LA" text="iam iidem illi &amp; boni &amp; mali ciues videntur, qui nobis viceri sole[n]t"/>
        <underline method="pen" type="straight" language="LA" text="mea co[m]mendatissima"/>
        <underline method="pen" type="straight" language="LA" text="in eo mihi sunt omnia"/>
        <underline method="pen" type="straight" language="LA" text="eg&iacute;que omneis illos adolescenteis, quos ille iactitat. difficile"/>
        <underline method="pen" type="straight" language="LA" text="ver&ograve; incessi omnibus laetitiis"/>
        <underline method="pen" type="straight" language="LA" text="Ego voluptatem animi nimiam."/>
        <underline method="pen" type="straight" language="LA" text="dum illum rideo, pen&egrave; sum factus ille."/>
        <underline method="pen" type="straight" language="LA" text="enim possum adduci vt"/>
        <underline method="pen" type="straight" language="LA" text="me laetitia extulerunt: ex"/>
        <underline method="pen" type="straight" language="LA" text="domum misi vnam epistolam, quin esset ad te altera: nec"/>
        <underline method="pen" type="straight" language="LA" text="tantum modo"/>
        <underline method="pen" type="straight" language="LA" text="quod esset ad laureolam"/>
        <underline method="pen" type="straight" language="LA" text="exercitum adduxi"/>
        <underline method="pen" type="straight" language="LA" text="probe ornatum auxiliis"/>
        <underline method="pen" type="straight" language="LA" text="enim in his locis, Hiccine est ille, qui vrbem? quem"/>
