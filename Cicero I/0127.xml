        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        [ scriptu[m] incisu[m] ]
                    </marginalia_text>
                </position>
            </language>
