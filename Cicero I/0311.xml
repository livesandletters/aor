    <page filename="0311.tif" reader="John Dee" pagination="283"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="horreba[n]t, vestra[m] vim metuebant, mentionem legis Agrariae pertimescebant">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        horrere metuere p[er]timescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To shudder, to dread, to tremble.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="carme[n] hic Tribunus ple. non vobis, sed sibi intus canit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Carme[n] sibi intus canere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To sing a song for oneself.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="inuidiam deponere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        inuidia[m] deponere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put down hate.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="propter vastitatem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vastitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Devastation.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ingenti pecunia vobis inducetur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        inge[n]ti pecunia induci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be charged a huge sum.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="adducentur pecuniae magnitudine">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        adduci pecuni[a]e magnitudi[n]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be adduced a great amount of money.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="diligentiam maiorum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Diligentia maiorum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The diligence of the ancestors.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="coerceas? opprimas? redigas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        coercere opprimere redigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To coerce, to crush, to rule it.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="rogabuntur atque orabu[n]tur, vt agros partim"/>
        <underline method="pen" type="straight" language="LA" text="atque hoc carme[n] hic Tribunus ple."/>
        <underline method="pen" type="straight" language="LA" text="maiorum vestrorum sanguine &amp; sudore"/>
        <underline method="pen" type="straight" language="LA" text="quoquo precio coemptus erit, tamen ingenti pecunia"/>
        <underline method="pen" type="straight" language="LA" text="sterilitatem incultum"/>
        <underline method="pen" type="straight" language="LA" text="quasi de aliqua sentina, ac non de optimorum ciuiu[m] genere loqueretur. Vos"/>
        <underline method="pen" type="straight" language="LA" text="retinete istam possessione[m] gratiae, libertatis, suffragioru[m], dignitatis, vrbis, fori, ludorum, festorum dierum, caeterorum omnium co[m]modorum"/>
        <underline method="pen" type="straight" language="LA" text="vectigalibus abalienatis, sociis vexatis, regibus atque omnibus gentibus exinanitis, illi pecunias habebunt, vos agros no[n] habebitis. Facile"/>
        <underline method="pen" type="straight" language="LA" text="Reipub. partibus, est opraeprecium diligentiam"/>
        <underline method="pen" type="straight" language="LA" text="recordari, qui colonias sic idoneis in locis contra"/>
        <underline method="pen" type="straight" language="LA" text="suum praesidium"/>
        <underline method="pen" type="straight" language="LA" text="vestris possint collocare"/>
        
