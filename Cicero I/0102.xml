        <underline method="pen" type="straight" language="LA" text="nostra diligentia, indulgenti&aacute;que consumatur"/>
        <underline method="pen" type="straight" language="LA" text="cur hunc dolorem cineri eius, atque ossibus inussisti, vt liberis eius bona patria, voluntate patris, iure, legibus tradita eriperes, &amp; cui tibi"/>
        <underline method="pen" type="straight" language="LA" text="commodum, condonares"/>
        <underline method="pen" type="straight" language="LA" text="Nec petitionem, inquit, nec possessionem dabo"/>
        <underline method="pen" type="straight" language="LA" text="detrahes ornamenta non solum fortunae, sed etiam ingenuitatis"/>
        <underline method="pen" type="straight" language="LA" text="tamen te interponis"/>
        <underline method="pen" type="straight" language="LA" text="quid habes quod reprehendas? nihil opinor"/>
        <underline method="pen" type="straight" language="LA" text="audiens esse quisquam"/>
        <underline method="pen" type="straight" language="LA" text="facile est docere. sed in re tam vsitata, satis"/>
        <underline method="pen" type="straight" language="LA" text="ei in disciplinam tradite"/>
        <underline method="pen" type="straight" language="LA" text="mirum est hominis ingenium, mira prudentia"/>
        <underline method="pen" type="straight" language="LA" text="componit edictum his verbis, vt quiuis intelligere"/>
        <underline method="pen" type="straight" language="LA" text="vnius hominis causa conscriptum esse"/>
        <underline method="pen" type="straight" language="LA" text="exegerat enim iam mercedem"/>
        
