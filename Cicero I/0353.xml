    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="duo corpora esse Reipublicae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Duo corpora republi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two bodies of the state.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Congemuit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Congemescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To groan.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="satis seuere pro rei indignitate">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        satis seuere p[ro] rei [dig - deleted] indignitate
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Severe enough for the indignity of the matter.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="perpurgatus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        perpurgatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Cleansed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="desultorius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        desultorius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very superficially.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="vnum debile infirmo capite, alterum firmum sine capite. huic"/>
        <underline method="pen" type="straight" language="LA" text="re pro rei indignitate decreuit"/>
        <underline method="pen" type="straight" language="LA" text="etenim sciebam Catilinam non latus aut ventrem, sed caput &amp; collum solere petere"/>
        <underline method="pen" type="straight" language="LA" text="Magna est autem comitiis Consularibus"/>
        <underline method="pen" type="straight" language="LA" text="repentina volunt inclinatio, praesertim cum incubuit ad virum bonu[m] &amp; multis aliis adiume[n]tis petitionis ornatu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Nunc mihi tertius ille locus est orationis de ambitus criminibus, perpurgatus ab iis, qui ante me dixerunt: &agrave; me quoniam ita Murena voluit, retractandus. quo"/>
        <underline method="pen" type="straight" language="LA" text="ingenioso &amp; bono, Seruio"/>
        <underline method="pen" type="straight" language="LA" text="homini in omni"/>
        <underline method="pen" type="straight" language="LA" text="sed pauca quae meum animum repente mouerunt, prius de L. Murenae fortuna conquerar"/>
        <underline method="pen" type="straight" language="LA" text="vt non queam satis, neque communem omnium nostrum conditionem, neque huius euentum fortun&aacute;mque miserari. qui prim&ugrave;m"/>
        <underline method="pen" type="straight" language="LA" text="continuis familiae maior&uacute;mque suoru[m] vnum asce[n]dere gradum dignitatis conatus"/>
        <underline method="pen" type="straight" language="LA" text="venit in periculum, ne &amp; ea quae relicta, &amp; haec quae ab ipso parta sunt, amittat"/>
        <underline method="pen" type="straight" language="LA" text="propter studiu[m] nouae laudis etiam in veteris"/>
        <underline method="pen" type="straight" language="LA" text="Quae cum sunt grauia Iudices, tum illud acerbissimum est, quod habet eos accusatores, non qui odio inimicitiarum ad accusandum, 
            sed qui studio accusandi ad inimicitias descenderent. nam"/>
        <underline method="pen" type="straight" language="LA" text="nobis erat in hac ciuitate natus, vt eius opes &amp; ingenium"/>
        <underline method="pen" type="straight" language="LA" text="vix cuiquam inimicus esse deberet"/>
        <underline method="pen" type="straight" language="LA" text="quasi desultorius in quadrigarum curriculum incurrere"/>
        
