        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="qui timeam ne mihi in carcere habitandu sit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Cicero carcere[m] no[n] timet
                    </marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>
                Cicero does not fear the imprisonment.
            </translation>
        </marginalia>
        
