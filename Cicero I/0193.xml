    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0193.tif" reader="John Dee" pagination="165" signature="ll.iii."/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Erat hyems summa: te[m]pestas, vt ipsum Sopatrum dicere audistis, perfrigida: imber maximus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        tempestas p[er]frigida hiems summa imber maxim[us] ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very cold weather, midwinter, very strong rain.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="diuaricari ac deligari">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        diuaricari deligari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be spread out; to be disclosed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in aere, in imbri, in frigore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in [ aere imbre frigore ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the wind, rain, [and] cold.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="obriguisset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        obrigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To stiffen.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vix viuus aufertur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vix viu[us] aufertur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He was removed half dead [Loeb 293, 391].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="disposite istu[m] accusare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        disposit&egrave; accusare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To accuse in an ordered manner.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="distinguere ac separare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        distinguere separare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To distinguish, to separate.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="non queo dicere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        no[n] queo dicere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I am not able to say.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="singularis significatur insolentia, superbia, contumacia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        singularis insole[n]tia superbia co[n]tumacia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unique insolence, pride, disobedience.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in eorum locum substitutus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in eoru[m] locu[m] substitui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To take up their place.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="transducere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        transducere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring across.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="stultitia, nequitia, inertia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        stultitia nequitia inertia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foolishness, wickedness, ignorance.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="patibulo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        patibulum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Whipping block.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qui honorem illi habuerant">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        habere honore[m] alicui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To honour somebody.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vinctum, astrict&uacute;mque">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vinctus astrictus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bound, tied.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="viuum te non relinquam, moriere virgis, nisi"/>
        <underline method="pen" type="straight" language="LA" text="hyems summa: te[m]pestas, vt ipsum Sopatrum dicere audistis, perfrigida"/>
        <underline method="pen" type="straight" language="LA" text="imber maximus, cum iste imperat lictoribus vt"/>
        <underline method="pen" type="straight" language="LA" text="necesse est omnibus, cum esset vinctus"/>
        <underline method="pen" type="straight" language="LA" text="iam pen&egrave; obriguisset"/>
        <underline method="pen" type="straight" language="LA" text="No[n] possum disposite istu[m]"/>
        <underline method="pen" type="straight" language="LA" text="ingenio, veru[m]etiam artificio quoda[m] singulari"/>
        <underline method="pen" type="straight" language="LA" text="sed ea quo pacto distinguere ac separare possim"/>
        <underline method="pen" type="straight" language="LA" text="ia[m] non queo dicere"/>
        <underline method="pen" type="straight" language="LA" text="quis hoc no[n] intelligeret"/>
        <underline method="pen" type="straight" language="LA" text="bonorum absentium patrocinio?"/>
        <underline method="pen" type="straight" language="LA" text="in eorum locum substitutus"/>
        <underline method="pen" type="straight" language="LA" text="tu ista"/>
        <underline method="pen" type="straight" language="LA" text="patibulo"/>
        
