    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="pauloane interrupta est oratio mea">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        paulo ante interupta est or[ati]o mea
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A little before my speech was interrupted.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="exemplo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Exempla
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Examples.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Quis plenior inimicorum fuit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        quis plenior inimicor[um]?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Who has more enemies?
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="si idem extrema persequitur, qui inchoauit, iam omnia perfecta videamus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        si ide[m] extrema p[er]sequit[ur] q[ui] inchoauit, ia[m] o[mn]ia p[er]fecta videam[us].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                If he who began [the operations] follows it to the end, then we will see them all accomplished.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="instauratas [...] renouatas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        instaurare renovare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To renew, to restore.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="inimicitias depono">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        deponere inimicitias.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lay aside enmities.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="exempla [...] petenda">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        petere exemplu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To seek a precedent.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="celebrantur aures">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        aures celebrari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To frequent the ears.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tela vndique esse intenta">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        tela esse intenta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Spears are pointed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="amicus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Amicitia 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Friendship.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="bellum indixi atque intuli">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        bellu[m] indicere inferre.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To declare and wage war.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]ficere 479
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To construct. Page 479. <!--JG: Not sure what Dee means here, p. 479 does not exist in this copy, jumps to 479-->
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="qua pauloante interrupta"/>
        <underline method="pen" type="straight" language="LA" text="affectum videmus, &amp; vere vt dicam pen&egrave; confrectu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vt si idem extrema persequitur, qui inchoauit, iam omnia perfecta videamus. si succeditur"/>
        <underline method="pen" type="straight" language="LA" text="maximi"/>
        <underline method="pen" type="straight" language="LA" text="non solum memoria &amp; testimonio, sed etiam annalium literis, &amp; summi poetae voce laudatus est, qu&ograve;d"/>
        <underline method="pen" type="straight" language="LA" text="Atque vt vetera, quae sunt innumerabilia, mitta[m], tuus pater"/>
        <underline method="pen" type="straight" language="LA" text="Multa praetereo, qu&ograve;d intueor cora[m] haec lumina atque ornamenta Reip. P. Seruilium, &amp; M. Lucullum"/>
        <underline method="pen" type="straight" language="LA" text="cuius literis, fama, nunciis, celebrantur aures quotidie meae nouis nominibus gentium, nationum, locorum"/>
        <underline method="pen" type="straight" language="LA" text="Caesare reducit, reconciliat, restituit in gratiam"/>
        <underline method="pen" type="straight" language="LA" text="nemini ego possum esse benemerenti de Rep. no[n] amicus"/>
        <underline method="pen" type="straight" language="LA" text="me in amicos inflammare potuit, inimicis placare non possit? quod mihi"/>
        
