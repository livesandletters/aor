    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="perlongo interuallo prop&egrave; memoriae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        p[er]longo interuallo memori[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                After a very long period of memory.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sine repulsa Consules facti sunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        sine repulsa co[n]sule[m] fieri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be made consul without rejection.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cicero primus consul fuit
                    </marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>
                Cicero was first consul.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tam insigne, tam singulare vestrum beneficium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        beneficiu[m] insigne singulare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A favour, remarkable and extraordinary.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cogitationes, quae mihi nullam partem neque diurnae neque nocturnae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cogitatio[n]es nocturn[a]e diurn[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Thoughts at day and night.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        propositio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Proposal.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="esse locus huic voci">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        locu[m] esse huic voci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be a place for this voice.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="de tantis vestris beneficiis nullo modo possum"/>
        <underline method="pen" type="straight" language="LA" text="&amp; eum locum, quem nobilitas praesidiis firmatum, atque omni ratione obuallatum tenebat, me duce rescidistis, virtut&iacute;que"/>
        <underline method="pen" type="straight" language="LA" text="vt vester honos ad mei temporis diem petitus, non ad alienae petitionis occasionem intercaeptus, nec"/>
        <underline method="pen" type="straight" language="LA" text="Sed tame[n] magnificentius, atque ornatius esse illo nihil potest, qu&ograve;d meis comitiis non tabellam indicem tacitae libertatis, sed vocem viuam prae vobis, 
            indicem vestrarum erga me voluntatum"/>
        <underline method="pen" type="straight" language="LA" text="Hoc ego tam insigne, tam singulare vestrum beneficium Quirites, cum ad animi mei fructum, atque laetitiam duco esse permagnum, tum ad curam, 
            sollicitudin&eacute;mque multo maius"/>
        <underline method="pen" type="straight" language="LA" text="Versantur enim Quirites in animo meo multae, &amp; graues cogitationes, quae mihi nullam partem neque diurnae neque nocturnae quietis impartiunt"/>
        <underline method="pen" type="straight" language="LA" text="cui errato nulla venia, recte facto exigua laus, &amp; ab inuitis expressa proponitur"/>
        <underline method="pen" type="straight" language="LA" text="omnia potius perpetienda esse duco: qu&agrave;m non ita gerendum Consulatum, vt in omnibus meis factis, atque co[n]siliis, vestru[m] dem e factu[m], 
            consili&uacute;mque laudetur"/>
        <underline method="pen" type="straight" language="LA" text="vt nobilissimis hominibus lo[n]ge praeponerer: no[n] &amp; in hoc magistratum"/>
        <underline method="pen" type="straight" language="LA" text="sed mihi ad huiusce verbi vim, &amp; interpretationem vehementer opus est vestra sapientia"/>
        <underline method="pen" type="straight" language="LA" text="qui cum populi non solum commoda, verumetiam salutem"/>
        <underline method="pen" type="straight" language="LA" text="plena[m] sollicitudinis"/>
        <underline method="pen" type="straight" language="LA" text="timoris: in qua nihil erat mali, nihil aduersi, quod non boni metuerent, improbi expectarent"/>
        <underline method="pen" type="straight" language="LA" text="Omnia turbulenta consilia contra hunc Reipublicae statum, &amp; contra vestrum"/>
        
