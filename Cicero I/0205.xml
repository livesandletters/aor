    <annotation>
        
        <marginalia hand="Italian" anchor_text="praesidia ad illorum aduentu[m] opposita">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        p[rae]sidia opposita
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Garnison stationed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="contagione[m] imitandi eius belli periculosam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]tagio periculosa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dangerous contagion.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aditus omnis hominibus sine vlla facultate nauium non modo disiunctus, sed etiam clausus fuit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        aditus disiunctus clausus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The approach, disjoint and closed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="praedicatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        predicari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be proclaimed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="edicta Praetorum fuerunt eiusmodi, vt ne quis cum telo seruus esset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Edictum ne quis Seruus cu[m] telo esset
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Edicted that no slave should be armed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="seueritatem exe[m]pli">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        seueritas exempli
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The severity of the example.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aper ingens">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Aper ingens
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A huge boar.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Domitium crudelem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Domitius crudelis
                    </marginalia_text>
                    <person name="Lucius Domitius"/>
                </position>
            </language>
            <translation>
                Cruel Domitius.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="coniunctum sit, qu&agrave;m negociatores nostri cu[m] Siculis, vsu, re, ratione, concordia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]iu[n]ctu[m] vsu re ratio[n]e concordia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Linked by custom, material, rule, like-mindedness.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="consensiones">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        consensio[n]es
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Agreements.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="seruitiu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        seruitium
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Servitude.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in suspitionem vocata est coniurationis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in suspitio[n]em vocari co[n]iuratio[n]is
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be called into suspicion of conspiracy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt par fuit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vt par fuit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                At once.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vsquequaque">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vsqueq[ua]q[ue]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every time.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="splendidissima ciuitas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ciuitas splendidiss[im]a
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most splendid city.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="honestissimus maxim&uacute;sque conuentus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Conue[n]tus honestiss[im][us] maxim[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The greatest and most honest assembly.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="vt ne quis cum"/>
        <underline method="pen" type="straight" language="LA" text="Vetus est quod dicam &amp;"/>
        <underline method="pen" type="straight" language="LA" text="propter seueritatem exe[m]pli nemini fortasse vestrum"/>
        <underline method="pen" type="straight" language="LA" text="Durum hoc fortasse videatur, neque ego vllam in partem disputo: tantum intelligo, maluisse Domitium crudelem in animaduertendo, qu&agrave;m in praetermittendo dissolutu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vsu, re, ratione, concordia"/>
        <underline method="pen" type="straight" language="LA" text="nullum est malum domesticu[m], quod"/>
        <underline method="pen" type="straight" language="LA" text="videte qu&agrave;m non"/>
        <underline method="pen" type="straight" language="LA" text="nolite vsquequaque"/>
        <underline method="pen" type="straight" language="LA" text="in metu belli, fura[n]di"/>
        <underline method="pen" type="straight" language="LA" text="&amp; ad palum alliga[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="expectate facinus qu&agrave;m multis improbum, vincam tame[n] expectatione[m] omnium"/>
        <underline method="pen" type="straight" language="LA" text="nisi id, quod ego non quaero, quod denique in re"/>
        <underline method="pen" type="straight" language="LA" text="remitto tibi hoc totu[m] atque ista te cura libero"/>
        
