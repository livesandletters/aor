    <annotation>
        
        <marginalia hand="Italian" anchor_text="qua facie fuerit, qua statura">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>qua facie? qua statura?</marginalia_text>
                </position>
            </language>
            <translation>What face? what height?</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sensim dicebat; cursim dicis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>dicere sensim cursim ]</marginalia_text>
                </position>
            </language>
            <translation>To speak cautiously; to speak contradictorily.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="emendatore &amp; correctore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>corrector eme[n]dator</marginalia_text>
                </position>
            </language>
            <translation>Corrector. Improver.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="vini exhalandi, no[n] acuendi ingenii causa declamitas"/>
        <underline method="pen" type="straight" language="LA" text="dicis"/>
        <underline method="pen" type="straight" language="LA" text="pro tanta mercede nihil sapere disceres"/>
        <underline method="pen" type="straight" language="LA" text="omnia effundam, vt si saepius decertandu[m] sic, vt erit, semper nouus venia[m]: quam facultate[m] mihi multitudo istius vitiorum, peccatorumque largitur. Visne igitur te inspiciamus &agrave; puero? sic opinor &agrave; principio ordiamur"/>
        <underline method="pen" type="straight" language="LA" text="nemo vnqua[m] puer emptus libidinis causa ta[m] fuit in domini potestate, qu&agrave;m tu in Curionis"/>
        <underline method="pen" type="straight" language="LA" text="tamen tu nocte socia, horta[n]te libidine, cogente mercede, per tegulas dimitterere"/>
        <underline method="pen" type="straight" language="LA" text="Sed reliquum cursum"/>
        <underline method="pen" type="straight" language="LA" text="que[m] quide[m] celeriter perstringa[m]"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m mihi sunt, tame[n], vt facitis, atte[n]te audiatis"/>
        <underline method="pen" type="straight" language="LA" text="talibus in rebus excitare animos no[n] cognitio solu[m] reru[m], sed etia[m] recordatio: tametsi incidamus oportet media, ne nimis sero ad extrema veniamus"/>
        
