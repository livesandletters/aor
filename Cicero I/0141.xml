    <annotation> 
        
        <marginalia hand="English_secretary" anchor_text="pars virilis postulat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        pars virilis postulat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The share of a man demands.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Odistis [...] despictis [...] contemnitis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        odistis despicitis co[n]temnitis ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                You hated, despised, contempted.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="omnia cum summo dedecore ac turpiditudine, tum singulari stultitia, atque inhumanitate oblita">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        o[mn]ia oblita [ su[m]mo dedecore turpitudine singulari stultitia inhumanitate.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Everything is defiled by extreme indecency, vileness, exceptional stupidity, coarseness.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cubicularii">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cubicularij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bed-chamber servants.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="viuere in pecunia tenui">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        viuere in pecunia tenui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live with little means.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vestro marte">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vestro marte
                    </marginalia_text>
                    <person name="Mars"/>
                </position>
            </language>
            <translation>
                By your campaign.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="caeteris antecellit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        c[a]eteris antecellit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It surpasses the others.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Nemine[m] vestru[m] praeterit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        nemine[m] v[est]r[u]m preterit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                You must be aware [Loeb 293, 15].
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="alimur, ac sustinemur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ali. sustineri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be fed, to be sustained.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="stipe[n]diariu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        stipe[n]diariu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Stipend.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="tua verbera, cum secures, cum cruces ad ciuium Ro. supplicia fixas recordetur?"/>
        <underline method="pen" type="straight" language="LA" text="contr&aacute;que sunt omnia cum summo dedecore"/>
        <underline method="pen" type="straight" language="LA" text="nos ita viuere in pecunia tenui, vt prorsus nihil acquirere velimus"/>
        <underline method="pen" type="straight" language="LA" text="plures hic sociorum vrbes, qu&agrave;m ille hostium spoliasse videatur"/>
        <underline method="pen" type="straight" language="LA" text="veru[m] haec dicentur alio loco, &amp; dicta sunt"/>
        <underline method="pen" type="straight" language="LA" text="Nunc proficiscemur ad reliqua, si pauca ant&egrave; erimus &agrave; vobis Iudices deprecati"/>
        <underline method="pen" type="straight" language="LA" text="id fuit nobis gratum"/>
        <underline method="pen" type="straight" language="LA" text="quae magnitudine iniuriae fer&egrave;"/>
        <underline method="pen" type="straight" language="LA" text="iucunditatis in agendo &amp; varietatis minus habebit"/>
        <underline method="pen" type="straight" language="LA" text="autem authoritate, &amp; prudentia dignissimum est Iudices, in audiendo diligentiam no[n] minus"/>
        <underline method="pen" type="straight" language="LA" text="quae si magna, atque adeo maxima vobis vide[n]tur, qu&agrave;m varie, &amp; qu&agrave;m copiose dica[n]tur expectare"/>
        <underline method="pen" type="straight" language="LA" text="&amp; plerisque Poenoru[m], quasi victoriae praemiu[m], ac poena belli"/>
        
