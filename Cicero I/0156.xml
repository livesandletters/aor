    <annotation> 
        
        <marginalia hand="Italian" anchor_text="agris, atque vrbibus expleti, atque saturati cum hoc cumolo quaestus decedere[n]t">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        explet[us] saturat[us] vrbib[us] agris ] cu[m] hoc cumulo quest[us] decedere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To retire finished and stuffed, from the cities and lands, with that pile of profit.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vexauit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vexare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To shake.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nobilissimam ciuitate[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Ciuitas nobiliss[ima]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most famous city.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="agros Iudices fer&egrave; optimos ac noblissimos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ager optimus nobiliss[imus]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The richest and most famous land.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="annum tertium: qu&ograve;d &amp; recentissimus est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        rece[n]tissim[us] annus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The nearest year.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="memoria tenetis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        memoria tenetis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An official statement.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="symphonia caneret">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Symphonia canere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To sing with accompaniment.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Sentio Iudices modera[n]dum mihi esse iam orationi meae, fugiend&aacute;mque vestra[m] satietatem: non versabor in vno genere diutius, &amp; ita caetera"/>
        <underline method="pen" type="straight" language="LA" text="oratione mea tollam, vt in causa relinquam"/>
        <underline method="pen" type="straight" language="LA" text="Duarum mihi ciuitatum reliquos"/>
        <underline method="pen" type="straight" language="LA" text="Iudices fer&egrave; optimos ac nobilissimos, AEtnensium &amp; Leontinorum"/>
        <underline method="pen" type="straight" language="LA" text="vnum annum eligam, quo"/>
        <underline method="pen" type="straight" language="LA" text="Attendite Iudices"/>
        <underline method="pen" type="straight" language="LA" text="Audistis haec Iudices: quae nunc ego omnia praetereo ac relinquo."/>
        <underline method="pen" type="straight" language="LA" text="Nihil de luxuria Apronii loquor, nihil de insolentia, nihil de singulari nequitia ac turpitudine"/>
        <underline method="pen" type="straight" language="LA" text="tantum de quaestu ac lucro dicam vnius agri"/>
        <underline method="pen" type="straight" language="LA" text="breuis est oratio"/>
        <underline method="pen" type="straight" language="LA" text="dic, dic quaeso clarius"/>
        
