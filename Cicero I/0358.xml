    <page filename="0358.tif" reader="John Dee" pagination="330"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="in exitu iam est meus co[n]sulatus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]sulatu[m] esse in exitu
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The consulship is at its end.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="intonuit vox perniciosa">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        intonuit vox perniciosa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The sinister voice thunders.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vigilantem Consulem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vigilans co[n]sul
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A vigilant consul.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ne sufficiatur Consul">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ne suffriciatur consul.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That a consul is elected.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="non tibi, sed patriae natus es">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        No[n] nobis sed patri[a]e nati
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be born not for our, but the country's sake.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in discrimen extremum venimus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        in discrime[n] extremu[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come to the end of a road.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="in capite, atque in ceruicibus nostris restiterunt"/>
        <underline method="pen" type="straight" language="LA" text="quorum ego ferrum &amp; audacia[m] reieci in campo, debilitaui in foro, compressi etiam domi meae saepe Iudices: his vos"/>
        <underline method="pen" type="straight" language="LA" text="initia sunt in hac ciuitate consilia Iudices, vrbis delendae, ciuium trucidandorum, nominis Romani extinguendi. atque haec ciues, 
            ciues inquam (si eos nomine apppellari fas est) de patria sua &amp; cogita[n]t, &amp; cogitauerunt. horum ego quotidie consiliis occurro, audaciam debilito, sceleri resisto"/>
        <underline method="pen" type="straight" language="LA" text="in exitu iam est meus co[n]sulatus"/>
        <underline method="pen" type="straight" language="LA" text="Atque ad haec mala Iudices, quid accedat"/>
        <underline method="pen" type="straight" language="LA" text="proscpicis tempestatem anni tui"/>
        <underline method="pen" type="straight" language="LA" text="vox perniciosa designati"/>
        <underline method="pen" type="straight" language="LA" text="multum omnes boni"/>
        <underline method="pen" type="straight" language="LA" text="ne sufficiatur Consul"/>
        <underline method="pen" type="straight" language="LA" text="qui non mihi, non tibi, sed patriae natus es, videre quid agatur: retinere"/>
        <underline method="pen" type="straight" language="LA" text="fortuna constitutum ad amplexandum ocium: scientia, ad bellum gerendum: animo &amp; vsu, ad quod velis negocium"/>
        <underline method="pen" type="straight" language="LA" text="illius, vt orbetur auxilio Respublica, vt minuatur contra suum furorem imperatoru[m] copia, vt maior facultas"/>
        <underline method="pen" type="straight" language="LA" text="verum etiam de vestra salute sententiam"/>
        <underline method="pen" type="straight" language="LA" text="sine gemitu hoc dici non potest"/>
        <underline method="pen" type="straight" language="LA" text="latrocinium armatus opprimat: ego togatus vobis, bon&iacute;sque omnibus adiutoribus, hoc quod concaeptum"/>
        <underline method="pen" type="straight" language="LA" text="periculum parturit"/>
        <underline method="pen" type="straight" language="LA" text="&amp; comprimam"/>
        
