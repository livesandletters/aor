        <underline method="pen" type="straight" language="LA" text="perditoru[m] de me consilia frangere"/>
        <underline method="pen" type="straight" language="LA" text="hanc etiam potentium beneuolentiam"/>
        <underline method="pen" type="straight" language="LA" text="vt opinio mea fert,"/>
        <underline method="pen" type="straight" language="LA" text="nulla in caeteros maleuolentia suffusum: contr&aacute;que"/>
        <underline method="pen" type="straight" language="LA" text="magnum &amp; excelsum, tum etiam apertum &amp; simplicem. Vidi"/>
        <underline method="pen" type="straight" language="LA" text="mihi potestas praesentis tui fuerit"/>
        <underline method="pen" type="straight" language="LA" text="tibi erit eidem, cui salus mea fuit, etiam dignitas curae"/>
        <underline method="pen" type="straight" language="LA" text="neque ita multae"/>
        <underline method="pen" type="straight" language="LA" text="diiungo me fere"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; prima adolescentia delectarunt"/>
        <underline method="pen" type="straight" language="LA" text="sed eos quos erat infinitum bene de me"/>
        <underline method="pen" type="straight" language="LA" text="veteribus nostris delectationibus"/>
        <underline method="pen" type="straight" language="LA" text="non transferis"/>
        <underline method="pen" type="straight" language="LA" text="velim qu&agrave;m familiarissime certiore[m], &amp; qu&agrave;m saepissime"/>
        <underline method="pen" type="straight" language="LA" text="&iacute;dque me, non modo vt tu sentias, sed omnes gentes, etiam vt posteritas omnis intelligat, esse"/>
        <underline method="pen" type="straight" language="LA" text="tam de iure certum est"/>
        <underline method="pen" type="straight" language="LA" text="sine vlla mora prouinciam successori concedere:"/>
        <underline method="pen" type="straight" language="LA" text="suspicione tuae cupiditatis non possis illius cupiditatem refutare. Ego vtru[m]que meum puto esse, &amp; quid sentiam oste[n]dere, &amp; quod feceris defendere."/>
        <underline method="pen" type="straight" language="LA" text="foelicitate quidem vellem, consequi"/>
  
  
