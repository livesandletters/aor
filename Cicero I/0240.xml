    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="considerati hominis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Confiderati ho[min]is officiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The duty of a thoughtful person.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="&egrave; co[n]suetudine recedatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &egrave; co[n]suetudine recedi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To withdraw from the practice.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="longe alia ratione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        longe alia r[ati]o[n]e, atq[ue]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Far from the other reasoning, and.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="probi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Probi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Honest.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="impude[n]ter fecisse, astute &amp; callide">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        facere { impude[n]t[er] astute callide }
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To do outragiously, shrewdly and cleverly.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="excellens ingeniu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        excellens ingeniu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An exceptional ability.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in consuetudinem venit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        In co[n]suetudine[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To become customary.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="qua de re iure decertari oporteret, armis non contendere"/>
        <underline method="pen" type="straight" language="LA" text="&egrave; co[n]suetudine"/>
        <underline method="pen" type="straight" language="LA" text="se, quo impudentius egerit, hoc superiore[m] discessurum"/>
        <underline method="pen" type="straight" language="LA" text="tum enim"/>
        <underline method="pen" type="straight" language="LA" text="causae spes erat posita in defensione mea: nu[n]c in confessione aduersarii"/>
        <underline method="pen" type="straight" language="LA" text="de quibus ego antea laboraba[m], ne si improbi essent, falsi"/>
        <underline method="pen" type="straight" language="LA" text="si probi existimare[n]tur, quod dixissent probarent. nunc"/>
        <underline method="pen" type="straight" language="LA" text="Veru[m]tamen cum illoru[m] causae actione[m] considero"/>
        <underline method="pen" type="straight" language="LA" text="video quid impude[n]tius dici possit"/>
        <underline method="pen" type="straight" language="LA" text="se iniecturos vobis causam delibera[n]di, &amp; iudicandi iustam mora[m] ac religione[m]"/>
        <underline method="pen" type="straight" language="LA" text="excellens ingeniu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Sed cu[m] de eo iure mihi dicendu[m] sit quod"/>
        <underline method="pen" type="straight" language="LA" text="quod constitutu[m] sit &agrave; maioribus, conseruatu[m] vsque ad hoc te[m]pus: quo sublato"/>
        <underline method="pen" type="straight" language="LA" text="solum pars aliqua iuris diminuta, sed etia[m] vis ea quae iuri maxime est aduersaria, iudicio confirmata esse videatur"/>
        
