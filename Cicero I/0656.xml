        </marginalia>
        
        <marginalia hand="Italian" anchor_text="reticentia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>retice[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>To keep silent.</translation>
