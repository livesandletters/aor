    <annotation>
        
        <marginalia hand="Italian" anchor_text="Siciliam adiit, Africam explorauit: inde Sardiniam [...] haec tria frumentaria subsidia Reip.">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sicilia, Affrica, et Sardinia fueru[n]t reip. subsidia frume[n]taria
                    </marginalia_text>
                    <location name="Sicily"/>
                    <location name="Africa"/>
                    <location name="Sardinia"/>
                </position>
            </language>
            <translation>
                Sicily, Africa and Sardinia were the corn suplliers for the state.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="se cum in Italiam recaepisset">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        recipere se
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To hold back oneself.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="extrema hyeme">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        extrema su[m]ma ] hieme
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Extreme and highest winter.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        A maiore 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sermones ac literaae perferuntur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        sermones p[ro]ferri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To deliver a speech.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="haec tria frumentaria subsidia"/>
        <underline method="pen" type="straight" language="LA" text="firmissimis praesidiis, classib&uacute;sque muniuit"/>
        <underline method="pen" type="straight" language="LA" text="in Italiam recaepisset"/>
        <underline method="pen" type="straight" language="LA" text="firmissim&iacute;sque praesidiis adornauit"/>
        <underline method="pen" type="straight" language="LA" text="Brundisio profectus est"/>
        <underline method="pen" type="straight" language="LA" text="ad imperium Populi Romani Ciliciam adiunxit"/>
        <underline method="pen" type="straight" language="LA" text="huius imperio ac potestati se dedideru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="quo bello omnes gentes ac nationes premeba[n]tur, Cn. Pompeius extrema hyeme apparauit, ineunte vere suscaepit, media aestate confecit"/>
        <underline method="pen" type="straight" language="LA" text="Ac prim&ugrave;m, quanta innocentia debe[n]t esse Imperatores? qua[n]ta deinde omnibus in rebus te[m]perantia? qua[n]ta facilitate? qua[n]to ingenio? 
            qua[n]ta humanitate? quae"/>
        <underline method="pen" type="straight" language="LA" text="Quirites, sed ea magis ex aliorum contentione, qu&agrave;m ipsa per sese cognosci atque intelligi possunt"/>
        <underline method="pen" type="straight" language="LA" text="ad bellu[m] administrandu[m]"/>
        <underline method="pen" type="straight" language="LA" text="agnoscere videamini"/>
        <underline method="pen" type="straight" language="LA" text="ego autem neminem nomino. quare irasci mihi nemo poterit"/>
        <underline method="pen" type="straight" language="LA" text="Neque enim potest exercitum is continere Imperator qui seipsum non co[n]tinet, neque seuerus esse in iudicando qui alios in se seueros esse iudices non vult"/>
        <underline method="pen" type="straight" language="LA" text="tantum excellere caeteris"/>
        <underline method="pen" type="straight" language="LA" text="modo manus tanti exercitus, sed ne vestigium quidem cuiquam pacato nocuisse dicatur? Iam ver&ograve; quemadmodum milites hyberne[n]t"/>
        <underline method="pen" type="straight" language="LA" text="hyemis enim, no[n] auaritiae perfugium maiores nostri in sociorum atque amicorum tectis esse voluerunt"/>
        <underline method="pen" type="straight" language="LA" text="non enim"/>
        <underline method="pen" type="straight" language="LA" text="eximia vis remigum, aut ars inaudita quaedam gubernandi, aut venti aliqui noui, tam celeriter in vltimas terras pertulerunt: sed hae res quae caeteros remorari solent, 
            non retardarunt. non auaritia ab instituto cursu ad praedam aliquam reuocauit"/>
        <underline method="pen" type="straight" language="LA" text="Itaque omnes quidem nunc in his locis Cn. Pompeium sicut aliquem non ex hac vrbe missum, sed de coelo delapssum intuentur. nunc denique incipiunt credere, 
            fuisse homines Romanos hac quondam abstinentia, quod iam nationibus exteris incredibile, ac fals&ograve; memoriae proditum videbatur. nunc imperii nostri splendor illis gentibus lucet"/>
        <underline method="pen" type="straight" language="LA" text="seruire Pop. Rp. qu&agrave;m imperare aliis maluisse"/>
        
