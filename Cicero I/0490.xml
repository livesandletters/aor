    <page filename="0490.tif" reader="John Dee" pagination="462"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="misera &amp; prostrata Respublica">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Resp. misera prostrata
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A state, miserable and overthrown.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="non solum facto tuo, sed etiam exemplo Remp. vulnerasti: neque tantu[m] ipse improbus es">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Improbi Magistratus no[n] facto solu[m], sed etiam ex exemplo remp. vulnerauit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The magistrate of the wicked damaged the state not only by their act, but also by their example.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="di&iacute;sque hominibus inuitis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        dijsq[ue] ho[m]i[n]ib[us] invictis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By invincible gods and people.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="honore indignissimum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        honor&egrave; indigniss[im][us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unworthy of honour.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quo etiam maiore es malo mactandus, qu&ograve;d non solum facto suo, sed etiam exemplo Remp. vulnerasti: neque tantu[m] ipse improbus es, 
            sed etia[m] alios docere voluisti"/>
        <underline method="pen" type="straight" language="LA" text="Ac si haec omnia conte[m]nis ac despicis, qu&ograve;d ita tibi persuaseris, vt pal&agrave;m dictitas, te di&iacute;sque hominibus inuitis, 
            amore in te incredibili quodam C. Caesaris, omnia quae velis co[n]secuturum"/>
        <underline method="pen" type="straight" language="LA" text="qui omnia in pecunia posuisset, honore animo aequo carere debere"/>
        <underline method="pen" type="straight" language="LA" text="vt repulsam tuam triumphum suum duxerint"/>
        <underline method="pen" type="straight" language="LA" text="respicit, quin ingemiscat: nemo mentionem facit, quin execretur: si vitant, fugiunt, audire de te nolunt, cum viderunt, tanquam auspicium malum detestantur: si cognati"/>
        <underline method="pen" type="straight" language="LA" text="tribules execrantur, vicini metuunt, affines erubescunt, strumae denique ab ore improbo demigrarunt, &amp; aliis iam se locis collocarunt: si es odium publicum populi, 
            Senatus, vniuersorum hominum"/>
        <underline method="pen" type="straight" language="LA" text="Sed vt aliquando audiamus qu&agrave;m copiose ad rogata respondeas, conculdam iam interrogatione[m] meam, t&eacute;que in extremo pauca de ipsa"/>
        
