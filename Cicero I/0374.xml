        <underline method="pen" type="straight" language="LA" text="tene[n]s iuraret, crederet nemo"/>
        <underline method="pen" type="straight" language="LA" text="quasi bona comesse Romae"/>
        <underline method="pen" type="straight" language="LA" text="firmame[n]tum in illo teste"/>
        <underline method="pen" type="straight" language="LA" text="magnitudine poculoru[m], bibend&oacute;que superauit"/>
        <underline method="pen" type="straight" language="LA" text="fictam voce[m], &amp; simulatum dolorem"/>
        <underline method="pen" type="straight" language="LA" text="Sed quid ego"/>
        <underline method="pen" type="straight" language="LA" text="tandium disputo"/>
        <underline method="pen" type="straight" language="LA" text="postremo de salute omnium nostrum, de fortunis ciuitatis, de summa Reip. taceo? quam"/>
        <underline method="pen" type="straight" language="LA" text="qua[n]ta in co[n]uersione reru[m] ac perturbatione versemur"/>
        <underline method="pen" type="straight" language="LA" text="vestrae sententiae optimo cuique infestissimae atque inimicissimae reperiantur"/>
        <underline method="pen" type="straight" language="LA" text="sepulchrum L. Catilinae"/>
        <underline method="pen" type="straight" language="LA" text="ornatum, hominum audacissimorum ac domesticorum hostium co[n]uentu, epul&iacute;sque celebratum est"/>
        <underline method="pen" type="straight" language="LA" text="Iusta Catilinae facta sunt"/>
        <underline method="pen" type="straight" language="LA" text="mactare victima[m] gratiorem"/>
        <underline method="pen" type="straight" language="LA" text="sanguine illius nefarium in vos omnes odium saturaueritis"/>
        <underline method="pen" type="straight" language="LA" text="si ita diis placet, sufferamus"/>
        <underline method="pen" type="straight" language="LA" text="illud vero ferri no[n] potest, qu&ograve;d per Senatores &amp; per Equites Ro. qui haec omnia pro salute omnium communi consilio, vna mente atque virtute gesserunt, harum rerum authores duces, principes spoliati omnibus fortunis, ac de ciuitate expelli posse arbitra[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="nulla varietas est inter homines opinionis, nulla voluntatis, nulla sermonis"/>
        <underline method="pen" type="straight" language="LA" text="Quare siquis illuc me vocat, venio"/>
        <underline method="pen" type="straight" language="LA" text="discaeptatore[m] no[n] modo no[n] recuso, sed etiam deposco"/>
        <underline method="pen" type="straight" language="LA" text="Vis absit, ferrum ac lapides remoueantur"/>
        <underline method="pen" type="straight" language="LA" text="facessant, seruitia sileant: nemo erit tam iniustus"/>
        
