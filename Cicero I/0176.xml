        <underline method="pen" type="straight" language="LA" text="neque id est nouu[m], neque reprehe[n]dentum"/>
        <underline method="pen" type="straight" language="LA" text="Etenim hoc dico"/>
        <underline method="pen" type="straight" language="LA" text="&amp; magna voce dico"/>
        <underline method="pen" type="straight" language="LA" text="factu[m] est, improbe factu[m]"/>
        <underline method="pen" type="straight" language="LA" text="sed do"/>
        <underline method="pen" type="straight" language="LA" text="&amp; co[n]cedo"/>
        <underline method="pen" type="straight" language="LA" text="negligite praeterita, si vultis: sed ne reliquas spes turbetis, atque omnes prouincias euertatis, id prouidete"/>
        <underline method="pen" type="straight" language="LA" text="solebat, authoritate vestra via[m] patefaciatis illustrem atque lata[m]"/>
        <underline method="pen" type="straight" language="LA" text="certe hoc quod adhuc"/>
        <underline method="pen" type="straight" language="LA" text="Improbi sunt qui precunias contra leges cogu[n]t: stulti, qui quod livere iudicatu[m] est, praetermittunt"/>
        <underline method="pen" type="straight" language="LA" text="sitis hominibus licentia[m] pecuniaru[m] eripiendarum"/>
        <underline method="pen" type="straight" language="LA" text="in quo prim&ugrave;m aestimationis iniquitas, atque improbitas"/>
        <underline method="pen" type="straight" language="LA" text="Videtis ia[m] profexto Iudices, hac aestimatione &agrave; vobis co[m]probata, neque modu[m] posthac auaritiae cuiusqua[m], neque poena[m] improbitatis futura[m]"/>
        <underline method="pen" type="straight" language="LA" text="co[m]missurum, vt de pecuniis repetundis co[n]demnari possit. quanta[m] enim quisque"/>
        <underline method="pen" type="straight" language="LA" text="tanta[m] licebit"/>
        <underline method="pen" type="straight" language="LA" text="Praeclaros viros existimas iudices nos habere, quos alienis peccatis concessuros putes, quo"/>
     
