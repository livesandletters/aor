        </marginalia>
        
        <marginalia hand="Italian" anchor_text="supplicio dignus est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        supplicio dignus est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It merits punishment.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="do hoc vobis, &amp; co[n]cedo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        do condedo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I give, I concede.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="reliquas spes turbetis">
            <language ident="LA">
