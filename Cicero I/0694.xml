        <underline method="pen" type="straight" language="LA" text="superatum: abduxi exercitum"/>
        <underline method="pen" type="straight" language="LA" text="Ibi quintum &amp; vigesimum iam diem aggeribus, vinies, turribus"/>
        <underline method="pen" type="straight" language="LA" text="oppugnabam oppidum munitissimum Pindenissum, tantis opibus, ta[n]t&oacute;que negotio"/>
        <underline method="pen" type="straight" language="LA" text="ad summam"/>
        <underline method="pen" type="straight" language="LA" text="te in praesenti scripsi"/>
        <underline method="pen" type="straight" language="LA" text="haec aestas habuit hunc exitum satis foelicem"/>
        <underline method="pen" type="straight" language="LA" text="spissus,"/>
        <underline method="pen" type="straight" language="LA" text="vestra oratoria, sed haec etiam leuia nostratia? desunt"/>
        <underline method="pen" type="straight" language="LA" text="Mirum me desyderium tenet vrbis"/>
        <underline method="pen" type="straight" language="LA" text="incredibile meorum, atqui in primis tui, satietas autem provinciae: vel"/>
        <underline method="pen" type="straight" language="LA" text="tam accessio"/>
        <underline method="pen" type="straight" language="LA" text="vel quia belli magni timor impendet"/>
        <underline method="pen" type="straight" language="LA" text="eos qui venari"/>
        <underline method="pen" type="straight" language="LA" text="sed mira paucitas est"/>
        <underline method="pen" type="straight" language="LA" text="siquae ridenda essent"/>
        <underline method="pen" type="straight" language="LA" text="iam annum munus co[n]fecero"/>
        <underline method="pen" type="straight" language="LA" text="me erudiant de omni rep."/> 
        <underline method="pen" type="straight" language="LA" text="discessit, iter habebant"/>
        <underline method="pen" type="straight" language="LA" text="Omnis peregrinatio (quod ego ab adoclescentia iudicaui) obscura &amp; sordida est iis, quorum industria Romae potest illustris"/>
        <underline method="pen" type="straight" language="LA" text="hercule ambulatiuncula"/>
        <underline method="pen" type="straight" language="LA" text="prudentibus, fidelit&eacute;rque suadentibus idem videri. Ego"/>
        
