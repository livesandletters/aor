    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="prouidentem ant&egrave; &amp; praecauentem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        p[ro]uidens pr[a]ecaue[n]s
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Forewarned and forearmed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="coitio facta sit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        coitio facta est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Collusion happened.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in angustum venirent">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        in angustu[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come close.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="restrictos &amp; tenaces">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        restrictus. tenax.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Restricted, restrained.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in idem crimen vocandum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vocare in crime[n].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To call to judgement.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text=" caluit re recenti, nu[n]c in causa refrixit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Caluit in re rece[n]ti nu[n]c in c[aus]a refrixit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It deceived the recent issue, now during the case it has cooled down.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetitio.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nimium retinens Equistris iuris &amp; libertatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        nimiu[m] retinens iuris &amp; liberta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Excessibely retainment of right and freedom.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="neque necesse est me id persequivoce, quod vos me[n]te videatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        no[n] necesse est me id p[er]seq[ui] voce quod vos mente videatis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is not necessary that with my voice I set forth what you see with your mind.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ne disseminato dispers&oacute;que sermoni, fortunas nnocentium subiiciendas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        subicere fortunas disseminato dispers&oacute;q[ue] sermone.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To subject oneself to the fate spread and dispersed by rumour.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Maledictum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Maledictum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Insult.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="An te illa argumenta duxerunt"/>
        <underline method="pen" type="straight" language="LA" text="Quibuscum me Iudices, pugna[n]tem meo more pristino non videbitis: non"/>
        <underline method="pen" type="straight" language="LA" text="quia neuqe necesse est me id persequi voce, quod vos me[n]te videatis"/>
        <underline method="pen" type="straight" language="LA" text="vt eorum reprehensionem vos vestrae prude[n]tiae assumere, meae modestiae remittere debeatis"/>
        <underline method="pen" type="straight" language="LA" text="fictis auditionibus"/>
        <underline method="pen" type="straight" language="LA" text="ne disseminato"/>
        <underline method="pen" type="straight" language="LA" text="nihil est autem tam volucre, qu&agrave;m maledictu: nihil facilius emittitur, nihil citius excipitur, nihil latius dissipatur"/>
        <underline method="pen" type="double" language="LA" text="fontem maledicti"/>
        <underline method="pen" type="double" language="LA" text="sine capite"/>
