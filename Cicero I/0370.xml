        <underline method="pen" type="straight" language="LA" text="amplius vnis moribus, &amp; nunquam mutatis legibus viuunt"/>
        <underline method="pen" type="straight" language="LA" text="ciuitatis disciplinam, atque grauitatem non solum Graeciae, sed haud scio an cunctis gentibus anteponendam iure dicam. quae tam procul &agrave; Graecorum omnium regionibus, disciplinis, ling&aacute;que diuisa, cum in vltimis terris cincta Gallorum gentibus, barbariae fluctibus allauatur, sic optimatum consilio gubernatur, vt omnes"/>
        <underline method="pen" type="straight" language="LA" text="possint, qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="quae fama, quae gloria, quae doctrina, quae plurimis artibus, quae etiam imperio &amp; bellica laude floruit, paruum que[m]dam locu[m], vt scitis, Europae tenet, semp&eacute;rque tenuit"/>
        <underline method="pen" type="straight" language="LA" text="augeret gente[m], sed vt obsessam"/>
        <underline method="pen" type="straight" language="LA" text="quid alienigenae de vobis loqui solea[n]t, sed quid vosmetipsi de genere"/>
        <underline method="pen" type="straight" language="LA" text="Phrygem plagis fieri solere meliorem"/>
        <underline method="pen" type="straight" language="LA" text="experiri velis, in Caria id possimum esse faciu[n]dum"/>
        <underline method="pen" type="straight" language="LA" text="despicatui ducitur"/>
        <underline method="pen" type="straight" language="LA" text="dicatur"/>
        <underline method="pen" type="straight" language="LA" text="seruus primarum partium no[n] Lydus esset"/>
        <underline method="pen" type="straight" language="LA" text="Equidem mihi iam satis"/>
        <underline method="pen" type="straight" language="LA" text="videor de Asiatico"/>
        <underline method="pen" type="straight" language="LA" text="Sed tamen vestru[m] est Iudices, omnia quae dici possunt in hominu[m] leuitatem, inconstantiam, cupiditatem, etia[m] si &agrave; me minus dicuntur, vestris animis &amp; cogitatione comprehendere"/>
        <underline method="pen" type="straight" language="LA" text="summissa voce agam, tantu[m] vt Iudices"/>
        <underline method="pen" type="straight" language="LA" text="At Cn. Pompeius captis Ierosolymis"/>
        <underline method="pen" type="straight" language="LA" text="in primis hoc, vt multa alia sapienter: qui in tam suspitiosa ac maledica ciuitate locu[m] sermoni obtrectatoru[m] non reliquit. non"/>
        <underline method="pen" type="straight" language="LA" text="atque integerrimu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vox in corona[m] turb&aacute;mque effunditur"/>
        <underline method="pen" type="straight" language="LA" text="Sua cuique ciuitati religio Laeli est"/>
        <underline method="pen" type="straight" language="LA" text="tamen istorum religio sacrorum &agrave; splendore huius imperii, grauitate nominis nostri, maiorum institutis abhorrebat"/>
        
