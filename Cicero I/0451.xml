        <underline method="pen" type="straight" language="LA" text="Vigilandum est semper, multae insidiae sunt bonis"/>
        <underline method="pen" type="straight" language="LA" text="Etenim honorum gradus summis hominibus &amp; infimis sunt pares, gloriae dispares"/>
        <underline method="pen" type="straight" language="LA" text="honorum populi finis est"/>
        <underline method="pen" type="straight" language="LA" text="iam octingenti"/>
        <underline method="pen" type="straight" language="LA" text="qui ciuitatem dominatu regio liberauit"/>
        <underline method="pen" type="straight" language="LA" text="quibus re bene gesta"/>
        <underline method="pen" type="straight" language="LA" text="omnes enim istiusmodi artes in iis reprehenduntur, qui cum professi fuerint, satisfacere non possunt, non in his qui abfuisse ab his studiis confitentur"/>
        <underline method="pen" type="straight" language="LA" text="non linguae volubilitas"/>
        <underline method="pen" type="straight" language="LA" text="nihil in eo nisi frugalitatem, laborem, vigilantiam esse curamus"/>
        <underline method="pen" type="straight" language="LA" text="facile patior id te agere multis verbis, quod ad iudicium non pertineat: &amp; id te accusantem tandiu dicere, quod ego defensor sube periculo possim confiteri"/>
        <underline method="pen" type="straight" language="LA" text="homines vnquam aliud Romae, nisi de"/>
        
