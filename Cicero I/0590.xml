    <page filename="0590.tif" reader="John Dee" pagination="562" signature="[nnn.ii[v]]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="tam impius, tam demens, tam diis hominibusque hostis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>impius domens dijs ho[min]ib[u]sq[ue] hostis</marginalia_text>
                </position>
            </language>
            <translation>Impious. Out of one's mind. The enemy to gods and men.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="inuasit in eius viri fortunas; copias se subito ingurgitauisset">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>invadere in fortunas ingurgitare in copias</marginalia_text>
                </position>
            </language>
            <translation>To lay hands on the posession. To plunge into the wealth [Loeb 189, 119].</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="catamitum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Male parta, male dilabu[n]tur</marginalia_text>
                </position>
            </language>
            <translation>'Ill gotten is ill squandered' [Loeb 189, 119].</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Quae Charybdis tam vorax?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Que Charibdis ta[m] vorax?</marginalia_text>
                </position>
            </language>
            <translation>'Was ever a Charybdis so ravenous?' [Loeb 189, 119].</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="oceanus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Oceanus.</marginalia_text>
                </position>
            </language>
            <translation>The ocean.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="atque id">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>atq[ue] id.</marginalia_text>
                </position>
            </language>
            <translation>And that.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="fecit diuortiu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>facer diuortium.</marginalia_text>
                </position>
            </language>
            <translation>To divorce.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="hoc est dicere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>hoc est dicere.</marginalia_text>
                </position>
            </language>
            <translation>That is to say.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="antesignanus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>antesignanus</marginalia_text>
                </position>
            </language>
            <translation>'A commando' [Loeb 189, 123].</translation>
        </marginalia> 
        
        <underline method="pen" type="straight" language="LA" text="miseru[m] me, consumptis enim lachrymis, tamen infixus animo haeret dolor"/>
        <underline method="pen" type="straight" language="LA" text="no[n] te execrandum Pop. Rom. non detestabilem, non omnes tibi deos, omnes homines &amp; esse inimicos, &amp; futors"/>
        <underline method="pen" type="straight" language="LA" text="que[m], Male parta, male dilabu[n]tur: incredibile, ac simile portenti est"/>
        <underline method="pen" type="straight" language="LA" text="oceanus mediussidius, vix videtur"/>
        <underline method="pen" type="straight" language="LA" text="dissipatas, tam distantibus in locis positas, tam cito absorbere potuisse"/>
        <underline method="pen" type="straight" language="LA" text="mimae: domus erat aleatoribus referta, plena ebriorum"/>
        <underline method="pen" type="straight" language="LA" text="potabatur, atque id locis pluribus: sufferebantur"/>
        <underline method="pen" type="straight" language="LA" text="Quamobre[m] desinite mirari haec tam celeriter esse consumpta. non"/>
        <underline method="pen" type="straight" language="LA" text="quanuis enim sine mente, sine sensu sis, vt es: tamen"/>
        <underline method="pen" type="straight" language="LA" text="ver&ograve; te vnqua[m] neque vigilantem, neque in somnis credo me[n]te posse consistere. necesse est, quanuis sis, vt est, violentus &amp; furens"/>
        <underline method="pen" type="straight" language="LA" text="Consul &amp; impudicissimus, &amp; Consul"/>
        <underline method="pen" type="straight" language="LA" text="gustaras ciuilem sanguinem"/>
        
