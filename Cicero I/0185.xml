            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        auulso emblemate
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Wrenched off the embossed work.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="intelligentia[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
