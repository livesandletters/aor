    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="summam mihi superbiae, crudelitat&iacute;sque famam inussisset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        inurere su[m]ma fama[m] superbi[a]e crudelitatis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To brand with the greatest fame of pride and cruelty.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="astuti &amp; ingrati">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Astut[us] &amp; Ingrat[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Clever and ungrateful.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="quem contra">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        que[m] co[n]tra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Against him.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Saltatorem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        saltator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dancer.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="conuiciatoris">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]viciator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Abuser.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="sed ne concedi quidem potest vt amicis nostris accusantibus, non etiam alienissimos defendamus"/>
        <underline method="pen" type="straight" language="LA" text="Nam cum praemia mihi tanta pro hac industra sint data, qua[n]ta antea nemini: labores per quos ea caeperis, cu[m] adeptus sis, deponere, 
            esset hominis &amp; astuti &amp; ingrati"/>
        <underline method="pen" type="straight" language="LA" text="quem contra"/>
        <underline method="pen" type="straight" language="LA" text="cum tui fontes vel inimicis tuis pateant, nostros reuulos etiam amicis putes clausos esse oportere"/>
        <underline method="pen" type="straight" language="LA" text="existimarem nefarium si amico, crudelem si misero, superbum si Consuli defuissem"/>
        <underline method="pen" type="straight" language="LA" text="quod da[n]dum est amicitiae, large dabitur &agrave; me, vt tecum agam Serui non secus"/>
        <underline method="pen" type="straight" language="LA" text="quod tribuendu[m] est officio, fidei, religioni, id ita moderabor, vt meminerim me co[n]tra amici studiu[m] pro amici periculo dicere"/>
        <underline method="pen" type="straight" language="LA" text="illa, quae grauissima esse debebat, ita fuit infirma &amp; leuis, vt illos lex magis quaedam accusatoria, qu&agrave;m vera maledicendi facultas de vita L. Murenae dicere"/>
        <underline method="pen" type="straight" language="LA" text="ex qua laus familiae, memoria generi, honoes &amp; gloria nomini constituta est"/>
        <underline method="pen" type="straight" language="LA" text="in hisce rebus loci est, qu&ograve;d omnia"/>
        
