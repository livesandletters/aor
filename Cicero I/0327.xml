        <underline method="pen" type="straight" language="LA" text="De te autem Catilina cum quiescunt, probant: cum patiuntur, decernunt: cum tacent, clamant. neque hi solum, quorum"/>
        <underline method="pen" type="straight" language="LA" text="quoru[m] ego vix abs te iandiu manus ac tela contineo"/>
        <underline method="pen" type="straight" language="LA" text="vtinam tibi istam mentem dii immortales donarent"/>
        <underline method="pen" type="straight" language="LA" text="Catilina is es, vt te aut pudor &agrave; turpitudine, aut metus &agrave; periculo, aut ratio &agrave; furore reuocarit"/>
        <underline method="pen" type="straight" language="LA" text="secerne te &agrave; bonis: infer patriae bellum: exulta impio latrocinio, vt &agrave; me non eiectus ad alienos, sed inuitatus"/>
        <underline method="pen" type="straight" language="LA" text="tuae sacrarium sceleru[m] tuorum constitutu[m]"/>
        <underline method="pen" type="straight" language="LA" text="tua ista cupiditas affrenata, ac furiosa rapiebat"/>
        <underline method="pen" type="straight" language="LA" text="natura peperit, voluntas exercuit, fortuna seruauit. nunquam"/>
        <underline method="pen" type="straight" language="LA" text="habes vbi ostentes tuam illam praeclaram patientiam famis"/>
        
