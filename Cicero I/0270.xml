        <underline method="pen" type="straight" language="LA" text="quoad pareret, habitaret: diligenti&aacute;mque adhiberet, vt id quod concaeperat seruaret, vt saluum parere posset"/>
        <underline method="pen" type="straight" language="LA" text="Quid de Oppianico suspicatus sit, videtis. quid iudicarit, obscurum non"/>
        <underline method="pen" type="straight" language="LA" text="Quid Oppianicus fecerit cognoscite: vt illum"/>
        <underline method="pen" type="straight" language="LA" text="merces abortionis"/>
        <underline method="pen" type="straight" language="LA" text="illam, quam in aluo co[m]mendatam &agrave; viro continebat, victa auaritia sceleri Oppianici vendidit"/>
        <underline method="pen" type="straight" language="LA" text="Attendite"/>
        <underline method="pen" type="straight" language="LA" text="erant enim non matrimonii dignitate, sed sceleris societate co[n]iunctae"/>
        <underline method="pen" type="straight" language="LA" text="Fuit Aiulius quidam Larinas perdita nequitia &amp; summa egestate, arte quada[m] praeditus ad libidines adolescentuloru[m] excitandas acco[m]modata. qui"/>
        <underline method="pen" type="straight" language="LA" text="&amp; fortunas eius patrias expugnare posse"/>
        <underline method="pen" type="straight" language="LA" text="facilius in solitudine, perfici rem eiusmodi commodius in tubra posse arbitrati sunt"/>
        <underline method="pen" type="straight" language="LA" text="Romae vixerint, quibus conuiuiis, quibus flagitiis, quantis &amp; qu&agrave;m profusis sumptibus, non modo conscio, sed etiam co[n]uiua &amp; aditore Oppianico, longum est mihi dicere, presertim ad alia properanti"/>
        <underline method="pen" type="straight" language="LA" text="conualescit"/>
        <underline method="pen" type="straight" language="LA" text="atque ille continuo nullo indice, rece[n]tis maleficii co[n]scientia perterritus, omnia"/>
        
