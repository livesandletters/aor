        <underline method="pen" type="straight" language="LA" text="Lo[n]gi subsellii"/>
        <underline method="pen" type="straight" language="LA" text="iudicatio &amp; mora,"/>
        <underline method="pen" type="straight" language="LA" text="reliquum quod est"/>
        <underline method="pen" type="straight" language="LA" text="incredibiliter"/>
        <underline method="pen" type="straight" language="LA" text="remunerer"/>
        <underline method="pen" type="straight" language="LA" text="nam protecto non est meu[m], qui in scribendo (vt soles admirari) tantum industriae ponam, committere ut negligens scribendo fuisse videar: praesertim cum id non modo negligentis, sed etiam ingrati animi crimen futurum sit."/> <!--JG: I think it's profecto instead of protecto-->
        <underline method="pen" type="straight" language="LA" text="non instituta, sed iam inverterata amicitia, cures"/> <!--JG: Should be inueterata-->
        <underline method="pen" type="straight" language="LA" text="qua[m]honorificentissime, quamprim&uacute;mque decernatur"/>
        <underline method="pen" type="straight" language="LA" text="difficultas nauigandi fuit odiosa"/>
        <underline method="pen" type="straight" language="LA" text="aestiuisque confectis"/> <!-- Might be aestiui[n]sque? -->        
        <underline method="pen" type="straight" language="LA" text="meque totum, &amp; mea, &amp; meos"/>
        <underline method="pen" type="straight" language="LA" text="est ad nos allatum"/>
        <underline method="pen" type="straight" language="LA" text="negotiu[m] facesserent"/>
        <underline method="pen" type="straight" language="LA" text="primo nuntio co[m]motus"/>
        <underline method="pen" type="straight" language="LA" text="quod nihil tam praeter opinionem meam accidere"/>
        <underline method="pen" type="straight" language="LA" text="vt me collegi"/>
        <underline method="pen" type="straight" language="LA" text="caetera mi facillima"/>
        <underline method="pen" type="straight" language="LA" text="certissimum &amp; iustissimum"/>
        <underline method="pen" type="straight" language="LA" text="&amp; ages victor ex inimicorum dolore triumphum iustissimum. Ego"/>
        <underline method="pen" type="straight" language="LA" text="sic, contestans omneis deos, promitto atque confirmo"/>
        <underline method="pen" type="straight" language="LA" text="rogando, deprecatoris: laborando, propinqui: authoritate, chari hominis (vt spero) apud ciuitates: grauitate, Imperatoris suscepturu[m] officia atque parteis."/>
        <underline method="pen" type="straight" language="LA" text="vinca[m] meis officiis cogitationes tuas"/>
        <underline method="pen" type="straight" language="LA" text="mihi a te"/>
        <underline method="pen" type="straight" language="LA" text="co[m]missum facinus, &amp; admissum dedecus co[n]fitebor"/>
        <underline method="pen" type="straight" language="LA" text="a te tractatus est praestanti ac singulari fide"/>
        <underline method="pen" type="straight" language="LA" text="praestat tibi memoriam, beneuolentiamque qua[m] debet"/>
        <underline method="pen" type="straight" language="LA" text="a me inuitissimo decessisset"/>
        <underline method="pen" type="straight" language="LA" text="conscendens iam nauem"/>
        <underline method="pen" type="straight" language="LA" text="quin tibi amplitudo"/>
        <underline method="pen" type="straight" language="LA" text="solicitudo futura sit"/>
        <underline method="pen" type="straight" language="LA" text="Censura[m] vt &amp; debes &amp; potes"/>
        <underline method="pen" type="straight" language="LA" text="omnibus in perpetuu[m] video summo"/>
        <underline method="pen" type="straight" language="LA" text="Illud pugna &amp; enitere"/>
        <underline method="pen" type="straight" language="LA" text="te beneuolentiam nauare possimus"/>
        <underline method="pen" type="straight" language="LA" text="tributa merces est laborum, &amp; vigiliarum mearum"/>

              
