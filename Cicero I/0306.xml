        <underline method="pen" type="straight" language="LA" text="longum putauit, &amp; timuit nequid praeteriret. adscripsit"/>
        <underline method="pen" type="straight" language="LA" text="Pergamum"/>
        <underline method="pen" type="straight" language="LA" text="vtrum oratio ad eius rei discaeptationem deerit: an cum idem &amp; disseret"/>
        <underline method="pen" type="straight" language="LA" text="falsum"/>
        <underline method="pen" type="straight" language="LA" text="quid, qu&ograve;d disputari contr&agrave; nullo pacto potest? quoniam statutum"/>
