        <marginalia hand="Italian" anchor_text="vir omnibus rebus praecellentissimus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vir o[mn]ib[us] reb[us] p[rae]celletiss[im][us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A man, most distinguished in all things.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="temperationem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        temperatio
                    </marginalia_text>
                </position>
            </language>
