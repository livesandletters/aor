                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fraus scelerata nefaria 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Desire for atrocity and crime.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="adamasti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        adamare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To covet.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="auditione expetere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        auditio[n]e expetere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To fall in lust with.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Melita">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Melita
                    </marginalia_text>
                    <location name="Mljet"/>
                </position>
            </language>
            <translation>
                Mljet.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="satis lato ab Sicilia mari, periculos&oacute;que">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        mare latu[m] periculosu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A wide and dangerous sea.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vestem conficiendam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        veste[m] co[n]ficere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To weave clothes.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="non modo breuiter de vnoquoque dicendum est, sed etiam praetereunda videntur esse permulta"/>
        <underline method="pen" type="straight" language="LA" text="vt ad maiora istius &amp; illustriora in"/>
        <underline method="pen" type="straight" language="LA" text="temperantia, sed etiam intelligentia te atque istos, qui se elegantes dici volunt, vicerit. nam"/>
        <underline method="pen" type="straight" language="LA" text="eo sacrario"/>
        <underline method="pen" type="straight" language="LA" text="sacra per mulieres ac virgines confici solent. hoc"/>
        <underline method="pen" type="straight" language="LA" text="omnibuc acerbum, indignu[m], luctuosum denique"/>
        <underline method="pen" type="straight" language="LA" text="Res non procrastinatur"/>
        <underline method="pen" type="straight" language="LA" text="e&oacute;ne tu seruos ad spoliandum fanum immittere ausus"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve; liberos adire ne orandi quidem causa fas erat?"/>
        <underline method="pen" type="straight" language="LA" text="h&iacute;sne rebus manus affere non dubitasti, &agrave; quibus etiam oculos cohibere te religionu[m] iura cogebant?"/>
        <underline method="pen" type="straight" language="LA" text="auribus tu tanta[m] cupiditatem"/>
        <underline method="pen" type="straight" language="LA" text="expetere"/>
        <underline method="pen" type="straight" language="LA" text="im&ograve; vero alia co[m]plura. ex quibus eligam spoliatione[m] nobilissimi atque antiquissimi fani, de qua"/>
