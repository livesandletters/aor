    <annotation>
        
        <marginalia hand="Italian" anchor_text="ars certare videretur cum copia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        arte[m] certare cu[m] copia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The skill vies with the richness [Loeb 293, 361].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ad amplissimi templi ornamentum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        templu[m] ampliss[imu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most lavish temple.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in templo clarissimo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        te[m]plu[m] clariss[imu]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most splendid temple.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dare, donare, dicare, consecrare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        dare donare dicare co[n]secrare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To give, to present, to dedicate, to consecrate.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="habe[n] profect&ograve; in animo Capitoliu[m] sic ornare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        habere in a[n]i[m]o capito. ornare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have in mind to adorn the Capitol.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="interuerso regali hoc dono">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        interu[er]so dono
                    </marginalia_text>
                </position>
            </language>
            <translation>
                By a reverse present.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in Rege tam nobili">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        rex nobilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A notable king.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="clarissimo, pulcherrim&oacute;que monimento">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        monime[n]tu[m] clariss[imu]m pulcherimu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A most famous and beautiful building.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ] magnifice[n]tius copiosius preclarius ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                More magnificently, more amply, more splendidly.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="vt ars certare videretur"/>
        <underline method="pen" type="straight" language="LA" text="velle illud etiam atque etiam considerare"/>
        <underline method="pen" type="straight" language="LA" text="dies vnus, alter, plures: non"/>
        <underline method="pen" type="straight" language="LA" text="os hominis"/>
        <underline method="pen" type="straight" language="LA" text="Rex maximo conuentu Syracusis in foro, ne quis forte me in crimine obscuro versari atque affingere aliquid suspitione"/>
        <underline method="pen" type="straight" language="LA" text="templo clarissimo"/>
        <underline method="pen" type="straight" language="LA" text="hoc sibi eripi miserum esse"/>
        <underline method="pen" type="straight" language="LA" text="Quae vox, quae latera, quae vires huius vnius criminis querimoniam possint sustinere?"/>
        <underline method="pen" type="straight" language="LA" text="accaepturas nationes exteras, quemadmodum huius tui facti famam in regna aliorum, atque"/>
        <underline method="pen" type="straight" language="LA" text="vltimas terras peruentura[m] putasti, cum"/>
        <underline method="pen" type="straight" language="LA" text="solius hoc esse facinus"/>
        <underline method="pen" type="straight" language="LA" text="vt te[m]pli dignitas, imperi&iacute;que nostri nome[n] desiderat"/>
        <underline method="pen" type="straight" language="LA" text="in iniuria tam acerba"/>
        <underline method="pen" type="straight" language="LA" text="Hoc loco Q. Catule te appello. loquor"/>
        <underline method="pen" type="straight" language="LA" text="nominis aeterna memoria simul cum"/>
        <underline method="pen" type="straight" language="LA" text="Tibi haec cura suscipienda, tibi haec opera sume[n]da est, vt"/>
        <underline method="pen" type="straight" language="LA" text="Audisti, &amp; audies omnes &egrave; conuentu Syracusano, qui ita dicant, sese audientibus"/>
        
