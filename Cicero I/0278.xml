    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="causas ille multas &amp; graueis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        habere caussas multas graues
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have many [or] severe causes.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="causa in ista meditatione versatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        caussa[m] in hac meditatio[n]e v[er]sari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To think over a case.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="pecunia vestigi&iacute;sne vobis odoranda">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        pecunia vestigijs odora[n]da
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Money to be sniffed for along the paths.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad conciliatione[m] gratiae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Conciliatio gr[ati]a[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Reconciliation.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Sapientissimu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sapie[n]tiss[im][us] quis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Someone most wise.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="stultus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Stultus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fool.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="in ista meditatione"/>
        <underline method="pen" type="straight" language="LA" text="Albiana pecunia vestigi&iacute;sne vobis"/>
        <underline method="pen" type="straight" language="LA" text="denique omnis fraus &amp; audacia"/>
        <underline method="pen" type="straight" language="LA" text="hinc pudor, honesta vita, &amp; nulla suspitio"/>
        <underline method="pen" type="straight" language="LA" text="patiamini veritate patefacta, atque omni errore sublato, e&ograve; transire illius turpitudinis infamiam,
            vbi caetera maleficia consistant, ab eo inuidiam discedere aliquando, ad quam nunquam accessisse culpam videtis"/>
        <underline method="pen" type="straight" language="LA" text="tali prudentia, etiam vsu"/>
        <underline method="pen" type="straight" language="LA" text="Sapientissimu[m] esse dicu[n]t eum, cui quod opus sit ipsi veniat in mentem. proxime accedere illum, qui
            alterius bene inuentis obtemperet. in stultitia contr&agrave; est: minus enim stultus est is, cui nihil in me[n]tem venit, qu&agrave;m ille, qui quod stulte
            alteri venit in mentem, comprobat"/>
        <underline method="pen" type="straight" language="LA" text="cum faucibus premeretur"/>
        <underline method="pen" type="straight" language="LA" text="nihil autem erat periculosius nec turpius, qu&agrave;m ad quam rem accepisset"/>
        <underline method="pen" type="straight" language="LA" text="te vero illud idem quod tum explosum &amp; eictu[m] est"/>
        <underline method="pen" type="straight" language="LA" text="nec sine ignominia calumniae relinquere accusationem"/>
        <underline method="pen" type="straight" language="LA" text="ab vtroque alienissimu[m], sordissimu[m], turpissimum, qu&agrave;m per"/>
        <underline method="pen" type="straight" language="LA" text="Sed quid ego haec pluribus, quasi de re obscura"/>
        <underline method="pen" type="straight" language="LA" text="ista ipsa res suo"/>
        
