        <underline method="pen" type="straight" language="LA" text="nulla vox alia istius miseri inter dolore[m] crepit&uacute;mque plagaru[m] audebatur"/>
        <underline method="pen" type="straight" language="LA" text="ciuitatis omnia verbera depulsurum, cruciat&uacute;mque &agrave; corpore deiecturum"/>
        <underline method="pen" type="straight" language="LA" text="O nomen dulce libertatis! O ius eximium nostrae ciuitatis! O lex Portia leg&eacute;sque Semproniae! 
            O grauiter desiderata &amp; aliquando reddita plebi Rom. tribunitia potestas! huccine"/>
        <underline method="pen" type="straight" language="LA" text="si te illius acerba imploratio &amp; vox miserabilis non inhibebat"/>
        <underline method="pen" type="straight" language="LA" text="fletu &amp; gemitu maximo commouebare? in crucem tu agere ausus es quenquam, qui se ciuem Rom. esse diceret"/>
        <underline method="pen" type="straight" language="LA" text="enim vt animi multitudinis in istum dolore &amp; odio &amp; communis periculi metu co[n]citarentur"/>
        <underline method="pen" type="straight" language="LA" text="Statui egomet mihi tum modu[m] orationi meae"/>
        <underline method="pen" type="straight" language="LA" text="id quod sapentissim&egrave; fecit, facere laetatus sum"/>
        <underline method="pen" type="straight" language="LA" text="iam exploratum est"/>
        <underline method="pen" type="straight" language="LA" text="futurum"/>
        <underline method="pen" type="straight" language="LA" text="sed secu[n]dum arbitrium tuum"/>
        <underline method="pen" type="straight" language="LA" text="Cu[m] haec omnia quae polliceor, cumulat&egrave; tuis proximis plana fecero, tum istuc ipsum tenebo quod abs te mihi datur: eo co[n]tentum me esse dicam"/>
        <underline method="pen" type="straight" language="LA" text="Hoc teneo: h&icirc;c haereo Iudices: hoc sum co[n]tentus"/>
        <underline method="pen" type="straight" language="LA" text="omitto ac negligo caetera: sua confessione induatur, ac iuguletur necesse est"/>
        <underline method="pen" type="straight" language="LA" text="tua te accuso oratione"/>
        
