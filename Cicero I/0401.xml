    <page filename="0401.tif" reader="John Dee" pagination="373" signature="aaa.iii."/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="praeclarum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Caput p[rae]claru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An astounding clause.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="pedibus iret">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        pedibus ire.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To walk.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Contraria circa ide[m] versantur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                They turned against these.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="non saepe vnum Consulem improbum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Numq[uam] duo co[n]sules imp[ro]bi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Never two wicked consuls.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="perenne ac proprium manere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        perenne ac p[ro]p[ri]u[m] manere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To remain continual and proper.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="me[n]tes angustae, humiles, prauae, oppletae tenebris ac sordibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mentes angust[a]e humiles praua[e] opplet[a]e tenebris ac sordibus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Minds, narrow, cringing and depraved, filled with darkness and filth.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="rem non minus strenue, qu&agrave;m postea publicam confecisset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]ficere rem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To complete a matter.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="detrime[n]to">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        detrimentum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Defeat.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cincinnatus ganeo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cincinnatus ganneo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A curled debauchee.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Consul imperiosus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Powerful consul.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="cum praeclarum caput recitaretur"/>
        <underline method="pen" type="straight" language="LA" text="P. Lentulus Consul fuit! quo quanto maius dedissetis, si superiore anno fuisset! nec enim eguissem medicina"/>
        <underline method="pen" type="straight" language="LA" text="causam semper fore firmissima[m] dicere solebat"/>
        <underline method="pen" type="straight" language="LA" text="perenne ac proprium manere potuisset"/>
        <underline method="pen" type="straight" language="LA" text="me[n]tes angustae, humiles, prauae, oppletae tenebris ac sordibus, nomen"/>
        <underline method="pen" type="straight" language="LA" text="qui me duo sceleratissimi"/>
        <underline method="pen" type="straight" language="LA" text="non Co[n]sules, sed latrones"/>
        <underline method="pen" type="straight" language="LA" text="enim vllam vllius boni spem haberet in eo, cuius primum tempus aetatis pal&agrave;m fuisset ad omnes libidines diuulgatu[m]? 
            qui ne &agrave; sanctissima quidem parte corporis potuisset hominu[m] impura[m] intemperantiam propulsare"/>
        <underline method="pen" type="straight" language="LA" text="egestate[m] &amp; luxuriam domestico lenocinio suste[n]tauit"/>
        <underline method="pen" type="straight" language="LA" text="profecto egestate &amp; improbitate"/>
        <underline method="pen" type="straight" language="LA" text="se ad lenonis impurissimi pedes abiecissent, quo vultu cincinnatus"/>
        <underline method="pen" type="straight" language="LA" text="ille vnguentis oblitus, cum toga praetexta, quam omnes"/>
        <underline method="pen" type="straight" language="LA" text="irrisit squallore[m] vestrum &amp; luctu[m] grauissimae ciuitatis"/>
        <underline method="pen" type="straight" language="LA" text="vestrum malum gemeretis, nihil diceret"/>
        <underline method="pen" type="straight" language="LA" text="ne aperte incommoda patriae lugeretis, ediceret"/>
        <underline method="pen" type="straight" language="LA" text="vini, somni, stupri plenus, madenti coma, co[m]posito ca[pillo]"/>
 
