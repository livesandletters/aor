        <marginalia hand="English_secretary" anchor_text="vigilaret, &amp; cum me ha[n]c causam doceret, in morbum grauem periculos&uacute;mque incidit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vigilia morbu[m] intulit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To tend to the illness.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        In scelere ac crudelitate ca[e]citas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Blindness in crime and cruelty.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cuius ea stultitia est, vt eam nemo hominem: ea vis, vt nemo foeminam: ea crudelitas, vt nemo matrem appellare possit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        mulier homo appelat[ur]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A woman is called a man.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
