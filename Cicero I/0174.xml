    <annotation> 
        
        <marginalia hand="Italian" anchor_text="pecunia est Verres: vna, qua frumentu[m] tibi emeres in cellam: altera, qua frume[n]tu[m] emeres &agrave; ciuitatibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        pecunia [ vna altera ].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One [and] another sum of money.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="refertius erit aerarium">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &AElig;rariu[m] referti[us].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A fuller treasury.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="inter se genere iniuriae, no[n] magnitudine pecuniae differat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        inter se genere iniuri[a]e no[n] magnitudine differunt
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The injustices do not differ in size, but in kind amongst themselves.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sophocles">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sophocles
                    </marginalia_text>
                    <person name="Sophocles"/>
                </position>
            </language>
            <translation>
                Sophocles.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="magno co[n]uentu acta res est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        magno co[n]uentu acta res est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The matter was presented in a large gathering.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="large, liberalit&eacute;rque">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        large liberaliter.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generously, liberally.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="locus intra oceanu[m] iam nullus est, neque tam longinquus, neque tam reconditus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        locus longinquus reconditus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A place, remote, hidden.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        histori[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Stories.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="authoritas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Authoritas 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Authority.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ex his quidem hominibus qui nuper fuerunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ex hijs ho[m]i[n]ib[us] q[ui] nuper fueru[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>
                From these men who were of our time.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="tum refertius"/>
        <underline method="pen" type="straight" language="LA" text="erit aerarium Populi Ro. qu&agrave;m vnquam fuit. At"/>
        <underline method="pen" type="straight" language="LA" text="inter se genere iniuriae, no[n] magnitudine pecuniae differat"/>
        <underline method="pen" type="straight" language="LA" text="vtrum crimini defensio, an comitatus exilio quaeritur?"/>
        <underline method="pen" type="straight" language="LA" text="quaeda[m] omnino in te singularia, quae in nullum aliu[m] hominem"/>
        <underline method="pen" type="straight" language="LA" text="quaeda[m] tibi cu[m] multis co[m]munia"/>
        <underline method="pen" type="straight" language="LA" text="vt ego assentiar orationi, defensionem"/>
        <underline method="pen" type="straight" language="LA" text="tamen non probabo"/>
        <underline method="pen" type="straight" language="LA" text="Potius enim te da[m]nato, caeteris angustior locus improbitatis defende[n]dae relinquetur, qu&agrave;m te absoluto"/>
        <underline method="pen" type="straight" language="LA" text="alii quod audacissime feceru[n]t, recte fecisse existime[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="bellu[m], sed luctu[m]"/>
        <underline method="pen" type="straight" language="LA" text="In eiusmodi re ac moribus, si is qui erit adductus in iudiciu[m], cum manifestis in flagitiis tenebitur, alios eadem fecisse dicet, illi exe[m]pla no[n] deerunt, 
            reip. salus deerit, si improborum exemplis improbi, iudicio ac periculo liberabuntur"/>
        <underline method="pen" type="straight" language="LA" text="co[m]sumitur? quid sedetis? cur no[n] in media oratione mea co[n]surgitis atque disceditis?"/>
        <underline method="pen" type="straight" language="LA" text="autem istoru[m] audacias, ac libidines aliqua ex parte resecare? desinite dubitare vtru[m] sit"/>
        <underline method="pen" type="straight" language="LA" text="propter multos improbos vni parcere, an vnius improbi supplicio multorum improbitatem cohercere?"/>
        <underline method="pen" type="straight" language="LA" text="plurimum sole[n]t &amp; authoritatis habere ad proba[n]dum, &amp; iucu[n]ditatis ad audiendu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Magna est hominu[m] authoritas, &amp; tanta, vt etia[m] delicti suspitionem tegere possit"/>
        
