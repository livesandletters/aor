    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="nihil apud hunc lautum, nihil elegans, nihil exquisitum (laudabo inimicum) ne magnopere quidem quicquam praeter libidines sumptuosum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        lautu[m] elegans exquisitu[m] sumpuosu[m] ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Tasteful, refined, elegant, exquisite.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ludos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ludi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Games.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nihil scitote esse luxuriosius, nihil libidinosius, nihil posterius, nihil nequius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        nihil luxuriosi[us] p[er]fidiosius posteri[us] nequius ] 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nothing more exuberant, treacherous, later, depraved.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="furunculus, nunc vero etiam rapax, qu&ograve;d sordidus, qu&ograve;d contumax, qu&ograve;d superbus, 
            qu&ograve;d fallax, qu&ograve;d perfidiosus, qu&ograve;d impudens, qu&ograve; auax">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        furunculus rapax sordidus co[n]tumax sup[er]bus fallax p[er]fidiosus impudens audax
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pilferer, rapacious, disgraceful, defiant, arrogant, deceitful, treacherous, shameless, audacious.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="subrancida">
