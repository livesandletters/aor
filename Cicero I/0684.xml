    <annotation> 
        
        <marginalia hand="Italian" anchor_text="rumor erat">
            <language ident="LA"> 
                <position place="head" book_orientation="0">
                    <marginalia_text>rumor erat</marginalia_text> 
                </position>
            </language>
            <translation> It was rumored.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="me sibi ille affixum habebit">
            <language ident="LA"> 
                <position place="head" book_orientation="0">
                    <marginalia_text>me illi affixum habebit</marginalia_text> 
                    <person name="Publius Clodius Pulcher"/>
                </position>
            </language>
            <translation>He had impressed that thing upon me.</translation> <!-- NBW: illi doesn't really agree here since its a dative form? -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cum pares esse non possent, pugnare desisterent">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cu[m] par esse no[n] possis pugnare dessistas</marginalia_text> 
                </position>
            </language>
            <translation>When you are not able to be evenly matched, avoid battle.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="otium nobis exoptandum est">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Otium.</marginalia_text>
                </position>
            </language>
            <translation>Tranquility.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sanctissimum nomen pietas">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pietas.</marginalia_text>
                </position>
            </language>
            <translation>Piety.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quidam abundantia quadam amoris">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Amor.</marginalia_text>
                </position>
            </language>
            <translation>Love.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ille perennis inimicus">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Clodius.</marginalia_text>
                    <person name="Publius Clodius Pulcher"/>
                </position>
            </language>
            <translation>Publius Clodius Pulcher.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Caesare &amp; cum Appio">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>C[a]esar. Appius.</marginalia_text>
                    <person name="Julius Caesar"/>
                    <person name="Appius Claudius Pulcher"/>    
                </position>
            </language>
            <translation>Julius Caesar. Appius Claudius Pulcher.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Vatinium autem scire">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vatinium cur defe[n]debat.</marginalia_text>
                    <person name="Publius Vatinius"/>                
                </position>
            </language>
            <translation>Why he defended Publius Vatinius.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="scribis esse factum">
            <language ident="LA"> 
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>scribis &amp; adscribis</marginalia_text>
                </position>
            </language>
            <translation>Being written and rewritten.</translation> 
        </marginalia>
        
