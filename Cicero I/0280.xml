        <marginalia hand="English_secretary" anchor_text="his rebus iudicatis standum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        hijs standu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those to be remained.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad emendas sententias dedisse pecuniam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ad eme[n]das se[n]te[n]tias dare pecunia[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To give money for the statements to be changed.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="se non iactatione populari, sed dignitate atque innocentia"/>
        <underline method="pen" type="straight" language="LA" text="clarissimi atque"/>
        <underline method="pen" type="straight" language="LA" text="&amp; quiduis potius, qu&agrave;m iudiciu[m], aut disceptatio, aut quaestio"/>
        <underline method="pen" type="straight" language="LA" text="vide ne mea co[n]iectura multo sit verior. ego"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d in eodem genere, in quo ipsi offendissent, alios reprehe[n]dissent, se"/>
        <underline method="pen" type="straight" language="LA" text="Quapropter nemine[m] dubitare existimo"/>
        <underline method="pen" type="straight" language="LA" text="id quod nescio an dici"/>
        <underline method="pen" type="straight" language="LA" text="omitto &amp; haec alia permulta, quae illo iudicio in Stalenum dicta sunt"/>
        <underline method="pen" type="straight" language="LA" text="honestissimis hominibus &amp; disertis co[n]trouersiam"/>
        <underline method="pen" type="straight" language="LA" text="haec illius reco[n]ciliatio &amp; persona viri boni suscepta, sicut in statuis in auratis, quas posuit ad Iuturnae, quibus subscrisit, reges ab se in gratiam esse reductos: exagitabantur"/>
        <underline method="pen" type="straight" language="LA" text="ad emendas sententias dedisse pecuniam"/>
        <underline method="pen" type="straight" language="LA" text="no[n] modo non sit contra Cluentium, sed maxime nostram causam defension&eacute;mque confirmet?"/>
        
