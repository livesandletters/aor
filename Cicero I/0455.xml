    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="in locos causam coniicere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in locos caussa[m] co[n]ijcere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To throw together the reason and the places.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="miserabilores epilogos possem dicere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        miserabiliores epilogos dicere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To give a very miserable peroration.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="declamatore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Declamator.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Declaimer.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="subaccusare.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        subaccusare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put under a charge.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="furialis illa vox">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        furialis vox
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A frenzied voice.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="no[n] discaepta[n]do decertandu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        no[n] discepta[n]do decerta[n]du[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be decided not by debating.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="improbos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Improbi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Wicked.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Sed hoc nescio qhomodo frequenter in me congessisti, san&eacute;que in eo creber fuisti, te iccirco in locos causam coniicee voluisse, ne ego mea consuetudine aliquid de thensis misericordiae causa dicere[m], quod"/>
        <underline method="pen" type="straight" language="LA" text="nam mihi eripuisti ornamentu[m] orationis meae"/>
        <underline method="pen" type="straight" language="LA" text="non vobis videor cu[m] aliquo declamatore, no[n] causa laboris &amp; fori"/>
        <underline method="pen" type="straight" language="LA" text="Nam qu&ograve;d in eo me reprehendisti, qu&ograve;d nimium multos defenderim: vtinam &amp; tu, qui potes, &amp; caeteri qui defugiunt, vellent me labore hoc leuare. sed sit vestra"/>
        <underline method="pen" type="straight" language="LA" text="Sed sunt haec leuiora, illa vero grauiora atque magna"/>
        <underline method="pen" type="straight" language="LA" text="cum quid sentirent, nesciretur: furialis illa vox nefariis stupris, religiosis altaribus effoeminata, secu[m]"/>
        <underline method="pen" type="straight" language="LA" text="cui quide[m] belli intestini &amp; vastitatis metus inferebatur"/>
        <underline method="pen" type="straight" language="LA" text="vinci autem improbos &agrave; bonis, fateor fuisse praeclarum, si"/>
        <underline method="pen" type="straight" language="LA" text="Consulibus. Nihil amplius dico, nisi illud Victoriae nostrae graues aduersarios paratos, interitus nullos vltores esse videcam. Hisce ego auxiliis"/>
        <underline method="pen" type="straight" language="LA" text="tu id in me reprehendis, qu&ograve;d Q. Metello"/>
        
