        <marginalia hand="English_secretary" anchor_text="ad salutem modo extinguendam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        extinguere salute[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To destroy prosperity.
            </translation>
        </marginalia>

        <marginalia hand="English_secretary" anchor_text="gloriam per tales viros infringenda[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
