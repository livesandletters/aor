    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="sectam sequi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        sectam sequi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To follow a party.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="amicitiam patriae praeponeret">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Amicitia patri[a]e p[rae]ponit[ur]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Friendship is put before country.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="mortis erat instar turpissimae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        erat mortis instar turpissim[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It is like the ugliest death.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="mortem eius honestaret">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        morte[m] honestare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To honour the death.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="rationes ad eos scopulos appulisse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        R[ati]o[n]es ad scopulos appelere 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Reasons to drive upon the rocks.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="imprudentia laberis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Imprude[n]tia laberis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Because of imprudence you will fall.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="causam enim suscaepisti antiquioruem memoria tua">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        caussa antiq[ui]or memoria tua
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A cause older than your memory.
            </translation>
        </marginalia>
        
