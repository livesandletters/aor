    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0555.tif" reader="John Dee" pagination="527" signature="[lzlzlz.vii.]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="nihil autem erat illo bla[n]dius, nihil hoc benignius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>nihil benignius blandius</marginalia_text>
                </position>
            </language>
            <translation>Nothing more kind and pleasant.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fabricatus gladium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>fabricari gladium</marginalia_text>
                </position>
            </language>
            <translation>To forge a sword.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vsus magister est optimus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vsus magister optimus</marginalia_text>
                </position>
            </language>
            <translation>'Practice is the best of teachers' [Loeb 252, 375].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Non igitur reus ex ea causa quae iudicata est, redundat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>no[n] igitur re[us] redundat.</marginalia_text>
                </position>
            </language>
            <translation>Therefore the plaintiff does not overflow.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="conscie[n]tia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Conscie[n]tia</marginalia_text>
                </position>
            </language>
            <translation>Conscience.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Id est difficile, quod cum spe magna sis ingressus, id non exequi vsque ad extremu[m]"/>
        <underline method="pen" type="straight" language="LA" text="si vsus magister est"/>
        <underline method="pen" type="straight" language="LA" text="cuius conscie[n]tia nihil est quod quisquam timeat, si non pertimescat suam"/>
        <underline method="pen" type="straight" language="LA" text="meminerit solum, sed etiam quatenus commissum"/>
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen" anchor_text="Conscie[n]tia"/>
        
    </annotation>
