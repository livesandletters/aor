        <marginalia hand="English_secretary" anchor_text="qui autem in Republica sit hostis, eum ciuem esse nullo modo posse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Nullus hostis in rep. est Ciuis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No enemy of the state is its citizen.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="sed hanc tantam, tam exitiosam haberi coniurationem"/>
        <underline method="pen" type="straight" language="LA" text="videtis. huic si paucos putatis affines esse, vehementer erratis"/>
        <underline method="pen" type="straight" language="LA" text="seminatu[m] est hoc malu[m]: manauit non solu[m] per Italia[m], verumetiam tra[n]scendit Alpes"/>
        <underline method="pen" type="straight" language="LA" text="obscure serpens multas iam prouincias"/>
        <underline method="pen" type="straight" language="LA" text="qui haec delere conati sunt, morte esse multa[n]dos. altera[m]"/>
        <underline method="pen" type="straight" language="LA" text="spiritu"/>
        <underline method="pen" type="straight" language="LA" text="immortalibus non esse supplicii causa constituta[m], sed aut necessitate[m] naturae, aut laboru[m] miseriaru[m] quietem esse"/>
        <underline method="pen" type="straight" language="LA" text="sapientes nunquam inuiti, fortes etiam saepe libenter oppetiuerunt. vincula ver&ograve;, &amp; ea sempiterna, certe ad singulare[m] poenam nefarii sceleris inuenta sunt"/>
        <underline method="pen" type="straight" language="LA" text="videtur ista res iniquitatem, si imperare velis: difficultatem, si rogare"/>
        <underline method="pen" type="straight" language="LA" text="horribiles custodias circundat, &amp; digna scelere"/>
        <underline method="pen" type="straight" language="LA" text="eripit etiam spem, quae sola homines in miseriis co[n]solari solet"/>
        <underline method="pen" type="straight" language="LA" text="aliqua in vita formido improbis esset posita, apud inferos eiusmodi quaeda[m] illi antiqui supplicia impiis constituta esse voluerunt, 
            qu&ograve;d videlicet intelligebant his remotis non esse mortem ipsam pertimescendam"/>
        <underline method="pen" type="straight" language="LA" text="meorum periculorum rationes vtilitas Reipublicae"/>
        <underline method="pen" type="straight" language="LA" text="qui autem in Republica sit hostis, eum ciuem esse nullo"/>
        
