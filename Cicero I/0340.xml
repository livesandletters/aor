    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="latius opinione disseminatu[m] est hoc malu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        latius opinio[n]e disseminatu[m] est hoc malu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'This evil has been sown more widely than you think' [Loeb 324, 141].
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sustentando ac prolatando">
            <language ident="LA">
