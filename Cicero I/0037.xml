    <annotation> 
        
        <marginalia hand="Italian" anchor_text="non enim, que[m]admodum putatis, omnia sunt in eloquentia">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        No[n] tu[n]c quemadmodu[m] putatis o[mn]ia su[n]t in eloquentia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'For eloquence does not, as you imagine, decide everything' [Loeb 240, 89].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Correctio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Correction.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        εἰρωνεῖα
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Irony.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Coarguit Neuiu[m] a loco &amp; te[m]pore
                    </marginalia_text>
                    <person name="Nevius"/>
                </position>
            </language>
            <translation>
                It refutes Nevius in place and time.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Prouerbialiter Dictam
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Expressed in a proverb.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="DCC millia passuu[m] conficiuntur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        DCC millia passuu[m] co[n]fici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To accomplish a journey of 700 miles.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
