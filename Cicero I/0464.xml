    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="voce ac sente[n]tia sua co[m]probauerunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        voce &amp; sententia co[m]probare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To approve by voice and vote.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="obnunciaret">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        obnu[n]ciare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To announce.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="relegarentur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        relegari
                    </marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        to be banished
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vrbem nondum excisam &amp; euersam, sed iam captam atque oppressam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vrbs excisa. euersa. capta. oppressa.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The city, annihilated, overthrown, captured, subdued.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="despicatissimi hominis furori, co[n]temptissimorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        despicatiss[imus] co[n]temptiss[imus].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most despiciple and despised.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nullius animi, nullius consilii fuisse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        esse nulli[us] a[nim]i nulli[us] co[n]silij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be of no courage, of no judgement.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="iurare">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        iurare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To swear.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in causa populari si non moderate, at certe populariter, abstinent&eacute;rque versato">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        In caussa populari versari. { moderate populariter abstine[n]ter.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To maneuver in the popular case moderately, in the people's interest, and with self restraint.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="causam Senatus pal&agrave;m, Equester ordo acerrime, cunta Italia publice, omnes boni proprie enix&eacute;que suscaeperant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        suscipere caussa[m]. ] pal&agrave;m acerrime publice proprie enixe
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To assume the case, openly, most keenly, publicly, properly, earnestly. 
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="victore exercitu">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        victor exerritus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A wandering victor.
            </translation>
        </marginalia>
        
