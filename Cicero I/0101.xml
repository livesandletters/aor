        <marginalia hand="Italian" anchor_text="testamentum P. Annius fecerat non improbum, non inofficiosum, no[n] inhumanum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Testame[n]tu[m] no[n] improbu[m] no[n] inofficiosu[m] no[n] inhumanu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A will, neither wrong, nor neglectfull, nor unfeeling.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Voconia lex">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
