        <marginalia hand="Italian" anchor_text="par ac similis scelestoru[m] ciuiu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>par. similis } scelestor[um] ciuiu[m]</marginalia_text>
                </position>
            </language>
            <translation>"The equal and counterpart of the criminals" [source: Loeb Classical Library 462: 378-379].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="culpas; increpuisti">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Culpare. inrepare</marginalia_text>
                </position>
            </language>
            <translation>To blame. To rebuke.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="rudimenta &amp; incunabula virtutis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>rudimenta cunabula } virtutis</marginalia_text>
                </position>
            </language>
            <translation>The rudiments and cradles of virtue.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="suu[m] annu[m] in Rep[ublica] perbacchatus est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>suu[m] annu[m] p[er]bacchari</marginalia_text>
                </position>
            </language>
            <translation>To waste his year.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ancillatus sum; addixi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>ancillari addicere</marginalia_text>
                </position>
            </language>
            <translation>To serve. To enslave.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="par ac similis scelestoru[m]"/>
        <underline method="pen" type="straight" language="LA" text="Nam qu&ograve;d mea[m] aetate[m] increpuisti, tantu[m] me abesse puto ab impudicitia, quantu[m] tu &agrave; pudicitia. Sed"/>
        <underline method="pen" type="straight" language="LA" text="petula[n]ter in vxore[m] &amp; in filia[m] mea[m] inuasisti"/>
        <underline method="pen" type="straight" language="LA" text="vnus enim satis es materiae habens, nequi quicquam"/>
        <underline method="pen" type="straight" language="LA" text="testame[n]tis eoru[m] locupletior essem"/>
        <underline method="pen" type="straight" language="LA" text="queae si tus mihi vt vicia obiicis, temeritas tua reprehe[n]dentur, non mea vicia culpabuntur"/>
        <underline method="pen" type="straight" language="LA" text="facere non potuit, qu&agrave;m qu&ograve;d te tale[m] filium genuit"/> 
        <underline method="pen" type="straight" language="LA" text="immensae gulae impudicissimi corporis quaestus sufficere non potuit, &amp; aetas tua iam"/>
        <underline method="pen" type="straight" language="LA" text="vtru[m] inhonestioribus corporis partibus re[m] quaesierit, an amiserit. Domu[m]"/>
        <underline method="pen" type="straight" language="LA" text="patre turpissime venalem habuit, ve[n]didit"/>
        
