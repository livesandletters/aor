    <annotation>
        
        <marginalia hand="Italian" anchor_text="conseruator">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>conseruator</marginalia_text>
                </position>
            </language>
            <translation>Keeper.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Dum genus hominum, dum Populi Ro. nomen extabit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>du[m] gen[us] ho[m]i[nu]m extabit, du[m] populi Ro[mani] no[min]em [licet]</marginalia_text>
                </position>
            </language>
            <translation>So long as the human race exists and the name of the Roman people is allowed. [see: Loeb 189, 105].</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="delere &amp; euertere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>delere euertere.</marginalia_text>
                </position>
            </language>
            <translation>To destroy [and] to overthrow.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="literis mandari, hoc memoriae prodi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>litteris mandari memori[a]e prodi</marginalia_text>
                </position>
            </language>
            <translation>To be entrusted to records. To be handed over to memory.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="posteritatem quide[m] omniu[m] seculorum vnqua[m] immemore[m] fore">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>posteritatem o[mn]i[u]m seculor[um] hui[us] rei vnq[uam] immemorem fore.</marginalia_text>
                </position>
            </language>
            <translation>'That all ages to come will never cease to recall this fact' [Loeb 189, 107].</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="decus, ac lumen fuit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>decus lumen. ]</marginalia_text>
                </position>
            </language>
            <translation>Glory. Light.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sobole[m] iuue[n]tutis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>soboles juue[n]tut[is]</marginalia_text>
                </position>
            </language>
            <translation>'Rising youth' [Loeb 189, 107].</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Similitudo</marginalia_text>
