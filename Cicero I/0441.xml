    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="deus aliquis elapsus de coelo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Deus no[n] descendit de co[e]lo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God did not descend from heaven.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="terraemotus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Terr[a]emotus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An earthquake.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="faciles sunt preces">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Preces ad deu[m] sunt faciles
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Prayers to god are easy.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="reficit &amp; recreat mentem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        reficere recreare ] me[n]tem
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To restore and revive the mind.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="intueor &amp; contemplor">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        [ intueor co[n]templor ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I consider and contemplate.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="extimesco">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        extimesco
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I dread.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="magna ratione fecisse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        magna r[ati]o[n]e facere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have a cogent reason [Loeb 158, 409].
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="vnus est inferior gradus aut interitus, aut seruitutis"/>
