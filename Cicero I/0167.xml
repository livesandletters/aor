        <marginalia hand="Italian" anchor_text="ex paternis probris ac vitiis emergere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ex paternis probris ac vitijs emergere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To escape the father's shames and vices. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pater tam negligens, ac dissolutus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        pater negligens dissolutus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A careless and negligent father.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Emundi duo genera">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Emundi duo genera
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two kinds of purchase.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="duodetricies">
