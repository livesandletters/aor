            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pecunia publica ex aerario erogata">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        pecunia publica ex [a]erario erogata
                    </marginalia_text>
                </position>
            </language>
