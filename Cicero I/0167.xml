            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pensitar&iacute;tne">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        pensitare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To ponder.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dicet extempore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        dicere extempore
                    </marginalia_text>
                </position>
            </language>
            <translation>
