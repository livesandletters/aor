                    <marginalia_text>
                        habere in matrimonio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To marry.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quoniam pater tam negligens, ac dissolutus est"/>
        <underline method="pen" type="straight" language="LA" text="Verum huc longius, qu&agrave;m voluntas fuit, ab epistola Timarchidis digressa est oratio mea"/>
        <underline method="pen" type="straight" language="LA" text="vt possim illud probare, si velim"/>
        <underline method="pen" type="straight" language="LA" text="sic hanc rem totam administrasti"/>
        <underline method="pen" type="straight" language="LA" text="Sed ego habebo rationem authoritatis meae: meminero quo animo, quo consilio ad causam publicam accesserim: no[n] agam"/>
        <underline method="pen" type="straight" language="LA" text="nihil probari velim me dicente, quod non ant&egrave; mihimetipsi probatum sit"/>
        <underline method="pen" type="straight" language="LA" text="binis centesimis"/>
        <underline method="pen" type="straight" language="LA" text="mihi autem hoc perarduum est demo[n]strare"/>
        <underline method="pen" type="straight" language="LA" text="ita vir bonus est, vt etiam si inimicissimus isti esset, tamen eius testimonio credi"/>
        <underline method="pen" type="straight" language="LA" text="aliqua firmiora aut grauiora quaere[n]da sunt?"/>
        
