            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tria genera furtoru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Tria furtoru[m] genera.
                    </marginalia_text>
                </position>
            </language>
