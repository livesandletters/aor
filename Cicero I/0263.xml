    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="qu&agrave;m authoritate vestrae obtemperare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Plus saluti q[uam] authoritati est obtemperandum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                More the demands of prosperity than those of authority should be complied with.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="fretus vobis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fretus vobis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Reliant on you.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="victorem exercitu[m] deportauit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        victorem exercitu[m] deportauit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He led his army home victoriously.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad bellum maximu[m], formidolosissim&uacute;mque">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        bellu[m] maximu[m] formidoliss[imum]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A war, very great and formidable.
            </translation>
        </marginalia>
        
