    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="vitae, quam ant&egrave; praecipitem, &amp; lubricam esse ducebat, huic planae &amp; stabili">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vita pr[a]eceps lubrica ] plana stabilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Life, steep and slippery, firm and stable.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="taedet">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        tedet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He is offended.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sensum iam percipere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        sensu[m] percipere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To gain the feeling.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="casus infestus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Casus infestus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unkind fate.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="improbi ac perditi ciues edomiti atque victi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Ciues improbi perditi ] edomiti victi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Citizens, unprincipled and abandoned, subdued and defeated.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="propulsatione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        propulsatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Rebutting.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="de mea authoritate diripuisset">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        de authoritate deripere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Deprive of prestige. 
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Nolite Iudices existimare eos, quibus integrum est, qui"/>
        <underline method="pen" type="straight" language="LA" text="non expectare huius exitum iudicii"/>
        <underline method="pen" type="straight" language="LA" text="misero puero, vestro, ac liberorum vestrorum supplici Iudices, hoc iudicio viuendi praecepta dabitis. cui"/>
        <underline method="pen" type="straight" language="LA" text="sensum iam percipere possit ex moerore patrio"/>
        <underline method="pen" type="straight" language="LA" text="quanquam ex huius incommodis magnam animo molestiam capio"/>
        <underline method="pen" type="straight" language="LA" text="facile patior oblatum mihi tempus, in quo boni viri lenitatem meam, misericordi&aacute;mque nota[m] omnibus quondam"/>
        <underline method="pen" type="straight" language="LA" text="quasi intermissam agnoscerent"/>
        <underline method="pen" type="straight" language="LA" text="praecipitante Repub. vehementem me fuisse atque fortem, conseruata mitem ac misericordem faterentur"/>
        <underline method="pen" type="straight" language="LA" text="de mea authoritate diripuisset"/>
        
