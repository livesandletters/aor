        <underline method="pen" type="straight" language="LA" text="muta est huius temporis accusatio"/>
        <underline method="pen" type="straight" language="LA" text="etiam ab inimicis laudatur"/>
        <underline method="pen" type="straight" language="LA" text="genere vniuerso &amp; de conditione omniu[m] nostrum"/>
        <underline method="pen" type="straight" language="LA" text="Ipsi coniectura nihil iudicabitis"/>
        <underline method="pen" type="straight" language="LA" text="is est reus auaritiae, qui in vberrima re turpe compendium, in maledicentissima ciuitate, in suspiciosissimo negocio maledictum omne, non modo crimen effugit"/>
        <underline method="pen" type="straight" language="LA" text="ego hosce possum refutare"/>
        <underline method="pen" type="straight" language="LA" text="vos modestissimum adolescentem, prouinciae maximae"/>
        <underline method="pen" type="straight" language="LA" text="sanctissimum virum, nostri exercitus fortissimum miletem, diligentissimum ducem, temperatissimum legatum ,quaestor&eacute;mque cognouerunt"/>
        <underline method="pen" type="straight" language="LA" text="constantissimu[m]"/>
        <underline method="pen" type="straight" language="LA" text="iustissimum"/>
        <underline method="pen" type="straight" language="LA" text="ama[n]tissimum"/>
        <underline method="pen" type="straight" language="LA" text="Sed sunt in illo numero multi boni, docti, prude[n]tes, qui"/>
        <underline method="pen" type="straight" language="LA" text="impudentes, illiterati, leues"/>
        <underline method="pen" type="straight" language="LA" text="Graecorum, tribuo illis literas, do multarum artium disciplinam: non adimo sermonis leporem, ingeniorum acumen, dicendi copiam: denique etiam, siqua sibi alia sumu[n]t, 
            no[n] repugno"/>
        <underline method="pen" type="straight" language="LA" text="testimoniorum religionem &amp; fidem, nunqua[m] ista natio coluit: toti&uacute;sque huiusce rei quae sit vis, quae authoritas, quod pondus ignora[n]t. 
            Vnde illud est, Da mihi testimonium mutuum? num Gallorum, num Hispanorum putatur? totum istud Graecorum est: vt etia[m] qui Graece nesciunt, hoc quibus verbis &agrave; Graecis dici soleat, sciant"/>
        <underline method="pen" type="straight" language="LA" text="Nunquam nobis ad rogatum"/>
        <underline method="pen" type="straight" language="LA" text="quemadmodum probent quod dicunt, sed quemadmodum se explicent dicendo"/>
        <underline method="pen" type="straight" language="LA" text="quanto cu[m] pudore, quo tremore &amp; pallore dixit"/>
        <underline method="pen" type="straight" language="LA" text="tamen haesitabat, tamen eius iracundiae religio nonnunqua[m] repugnabat"/>
        <underline method="pen" type="straight" language="LA" text="nefas esse publicanum iudicare contra publicanum, sublatus"/>
        <underline method="pen" type="straight" language="LA" text="mores ac disciplina plus valere[n]t, qu&agrave;m dolor ac simultas, omnes se spoliatos, vexatos, fortunis euersos esse dixissent"/>
        <underline method="pen" type="straight" language="LA" text="vinci, refelli, coargui putat esse turpissimum"/>
        <underline method="pen" type="straight" language="LA" text="Itaque non optimus quisque nec grauissimus, sed impude[n]tissimus, loquacissim&uacute;sque deligitur"/>
        <underline method="pen" type="straight" language="LA" text="dilige[n]ter expenditis: etiam si formam hominis, si nomen, si tribum nostis, mores tamen exquire[m]"/>
        <underline method="pen" type="straight" language="LA" text="timet nequid cupide, nequid iracunde, nequid plus min&uacute;sve"/>
        
