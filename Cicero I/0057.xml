    <annotation> 
        
        <marginalia hand="Italian" anchor_text="gaudeo Iudices, vehement&eacute;rque laetor">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        gaudeo letor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I am glad, I am delighted.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="non recreatus, neque restitutus, sed subactus oppress&uacute;sque Populus Romanus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Populus recreatus restitutus subactus oppressus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The people, remade, reestablished, subdued, crushed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nobiles, nisi vigilantes, &amp; boni, &amp; fortes, &amp; misericordes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        nobiles vigilantes boni fortes misericordes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nobles: watchful, kind, brave, merciful.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Repetitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Repetition.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Querimonia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Complaint.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nobilitas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        NOBILITAS
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nobility.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="culpa liberatus, &amp; crimine nefario solutus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        culpa liberatus cr[imin]e solutus]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Freed of blame, acquitted of accusation.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="concessit, adnumerauit, appendit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]cedere adnumerare appe[n]dere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To hand over, to count, to weigh up.
            </translation>
