        <underline method="pen" type="straight" language="LA" text="tibi ego idem reponam"/>
        <underline method="pen" type="straight" language="LA" text="laudationem ex vltimus terris miseris"/>
        <underline method="pen" type="straight" language="LA" text="Vatinii fuit etiam ille stimulus"/>
        <underline method="pen" type="straight" language="LA" text="atque hilare amplexarentur"/>
        <underline method="pen" type="straight" language="LA" text="illorum animos mediocriter lacessitus leuiter repungere"/>
        <underline method="pen" type="straight" language="LA" text="diis hominib&uacute;sque approbantibus"/>
        <underline method="pen" type="straight" language="LA" text="mihi cum illo magna iam graita"/>
        <underline method="pen" type="straight" language="LA" text="obliuione contriueram"/>
        <underline method="pen" type="straight" language="LA" text="in me iniuriarum, quod ego effudisse me omne abritrabar, residiuu[m] tamen insciente"/>
        <underline method="pen" type="straight" language="LA" text="fuisset, omne repe[n]te apparuit"/>
        <underline method="pen" type="straight" language="LA" text="saepe nutu, signification&eacute;que appello, cum"/>
        <underline method="pen" type="straight" language="LA" text="Crasso redirem in gratiam"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; meis laribus in prouinciam"/>
        <underline method="pen" type="straight" language="LA" text="Nam cum mihi condixisset, coenauit"/>
        <underline method="pen" type="straight" language="LA" text="in mei generi Crassipedis"/>
        <underline method="pen" type="straight" language="LA" text="eadem sensuru[m] fuisse"/>
        <underline method="pen" type="straight" language="LA" text="integra omnia ac libera"/>
        <underline method="pen" type="straight" language="LA" text="principatum"/>
        <underline method="pen" type="straight" language="LA" text="temporibus affectie[n]dum NVNQVAM ENIM PRAESTANTIBUS"/>
        <underline method="pen" type="straight" language="LA" text="gubernanda viris laudata est in vna sententia perpetua permansio. sed"/>
        <underline method="pen" type="straight" language="LA" text="si portu[m] tenere"/>
        <underline method="pen" type="straight" language="LA" text="possis mutata velificatione"/>
        <underline method="pen" type="straight" language="LA" text="esse debeat id, quod &agrave; me saepissime dictu[m] est, cum dignitate otium: no[n] idem semper dicere, sed idem semper spectare debemus"/>
        <underline method="pen" type="straight" language="LA" text="omnia mihi solutissima"/>
        <underline method="pen" type="straight" language="LA" text="non ita illustri gratia exceperit"/>
        
