        <underline method="pen" type="straight" language="LA" text="Sat&iacute;sne vobis Iudices, videor ad singulos testes accedere, neque vt prim&ograve; constituera[m], tantu[m]modo cu[m] vniuerso genere co[n]fligere"/>
        <underline method="pen" type="straight" language="LA" text="Itaque etsi teneo quid sit dicere paratus, nihil tame[n] contra disputabo, prius qu&agrave;m dixerit: totum enim co[n]uertet, atque alia finget"/>
        <underline method="pen" type="straight" language="LA" text="seruet quod parauit, &amp; ego me ad id quod attulerit, integrum co[n]seruabo"/>
        <underline method="pen" type="straight" language="LA" text="eam ciuitate[m], in quam ego multa &amp; magna officia"/>
        <underline method="pen" type="straight" language="LA" text="quae si ciuitas"/>
        <underline method="pen" type="straight" language="LA" text="magnifica &amp; gloriosa"/>
        <underline method="pen" type="straight" language="LA" text="iam inde &agrave; patre, atque"/>
        <underline method="pen" type="straight" language="LA" text="quasi ministrator"/>
        <underline method="pen" type="straight" language="LA" text="subiiciens, quid in suos ciues ciuitat&eacute;mque"/>
        <underline method="pen" type="straight" language="LA" text="nihil enim illo homine leuius, nihil egentius, nihil inquinatius"/>
        <underline method="pen" type="straight" language="LA" text="remittant spiritus, co[m]primant animos suos, sedent arrogantiam, fateantur in Meandrii persona esse expressam faciem ciuitatis"/>
        <underline method="pen" type="straight" language="LA" text="domi protere[n]dum esse &amp; co[n]culcandum putaueru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="esse in eo testimonio, cuius author inue[n]tus est nemo"/>
        <underline method="pen" type="straight" language="LA" text="rem ista ciuitas neque seuere Flaccu[m]"/>
        <underline method="pen" type="straight" language="LA" text="hinc totu[m] odiu[m], hinc omnis offensio"/>
        <underline method="pen" type="straight" language="LA" text="vulnus dice[n]do refricuisset"/>
        <underline method="pen" type="straight" language="LA" text="cuius linguae quasi flabello seditionis illa tum est ege[n]tium co[n]cio ve[n]tilata"/>
        <underline method="pen" type="straight" language="LA" text="istum dolore[m], querel&aacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="cognoscite"/>
        
