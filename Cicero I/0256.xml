    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="insignia victoriae, non victoriam reportare[n]t">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        insignia victori[a]e no[n] victoria[m] reportare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To pring home trappings of victory, not victory.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tempus, non ad obliuionem veteris belli, sed ad comparationem noui contulit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]ferre temp[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To devote time.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="aedificasset, ornass&eacute;tque classes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        aedificare ornare ] classes magnas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To build and fit out a mighty fleet.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in locis disiunctissimis, maxim&eacute;que diuersis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        loci disiunctiss[imi] maxim&egrave; diuersi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most disjointed frots, greatly diverse.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ancipiti conte[n]tione distracti">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ancipite co[n]se[n]tio[n]e distracti
                    </marginalia_text>
                </position>
            </language>
            <translation>
                With the distraction of a twofold attack.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="multo plus firmamenti ac roboris habebat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        multo plus firmame[n]ti ac roboris habere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be much more serious and substantial.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="is est exorsus orationis meae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        is est exorsus or[ati]o[n]is me[a]e.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That is the beginning of my speech.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="iniuriosius tractatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        iniuriosi[us] tractari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be treated wrongfully.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Corinthum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Corinthus
                    </marginalia_text>
                    <location name="Ancient Corinth"/>
                </position>
            </language>
            <translation>
                Corinth.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        A maiori
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="regno expulsus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        regno expulsus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Driven out from the kingdom.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="quo etiam carent aegrius">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        carere aliq[uo] [a]egrius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be without someone more reluctantly.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="impetus hostium repressos esse intelligunt ac retardatos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        impetus hostium rep[re]ssi retardati
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To check and restrain the onslaught of enemies.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Laus pompei
                    </marginalia_text>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>
                Praise of Pompey.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &Agrave; maiori
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Asia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Asia
                    </marginalia_text>
                    <location name="Asia"/>
                </position>
            </language>
            <translation>
                Asia.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="magnitudine pastionis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        magnitudo pastionis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The extent of pastures.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="emergere &egrave; patrio regno"/>
        <underline method="pen" type="straight" language="LA" text="in Aisae luce versari"/>
        <underline method="pen" type="straight" language="LA" text="insignia victoriae, non victoriam reportare[n]t"/>
        <underline method="pen" type="straight" language="LA" text="duo fortissimi viri &amp; summi Imperatores: sed ita triumpharunt vt ille pulsus superat&uacute;sque regnaret"/>
        <underline method="pen" type="straight" language="LA" text="Veruntamen illis Imperatoribus laus est tribuenda, qu&ograve;d egerunt: venia danda"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d reliquerunt: propterea qu&ograve;d ab eo bello Syllam"/>
        <underline method="pen" type="straight" language="LA" text="Mithridates autem omne reliquum tempus, non ad obliuionem veters belli, sed ad comparationem noui contulit"/>
        <underline method="pen" type="straight" language="LA" text="Bosphoranis finitimis suis bellum inferre simulasset"/>
        <underline method="pen" type="straight" language="LA" text="bellum gerebamus"/>
        <underline method="pen" type="straight" language="LA" text="vt cum duobus in loci disiunctissimis"/>
        <underline method="pen" type="straight" language="LA" text="maxim&eacute;que diuersis vno consilio, ab vnis hostium copiis bellum terra, mar&iacute;que gereretur, vox ancipiti conte[n]tione distracti, de imperio dimicaretis"/>
        <underline method="pen" type="straight" language="LA" text="plus firmamenti ac roboris habebat"/>
        <underline method="pen" type="straight" language="LA" text="Pompeii diuino consilio ac singulari virtute depulsum est"/>
        <underline method="pen" type="straight" language="LA" text="Sed de Lucullo dica[m] alio loco"/>
        <underline method="pen" type="straight" language="LA" text="vera laus ei detracta oratione nostra, neque falsa afficta esse videatur. sed"/>
        <underline method="pen" type="straight" language="LA" text="is est exorsus orationis meae"/>
        <underline method="pen" type="straight" language="LA" text="era[n]t appellati superibus"/>
        <underline method="pen" type="straight" language="LA" text="totius Graeciae lumen extinctu[m] esse volueru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="Videte, ne vt illis pulcherrimum fuit, ta[m]tam vobis imperii gloriam relinquere: sic"/>
        <underline method="pen" type="straight" language="LA" text="vobis turpissimu[m] sit, illud quod accaepistis, tueri &amp; conseruare non posse"/>
        <underline method="pen" type="straight" language="LA" text="regno expulsus est"/>
        <underline method="pen" type="straight" language="LA" text="Imperatorem &agrave; vobis certum deposcere"/>
        <underline method="pen" type="straight" language="LA" text="vident &amp; sentiunt hoc idem quod &amp; vos"/>
        <underline method="pen" type="straight" language="LA" text="quo etiam carent"/>
        <underline method="pen" type="straight" language="LA" text="Hi vos, quoniam libere loqui non licet, tacite rogant vt se quoque, sicut caeterarum prouinciarum socios, dignos existimetis quorum salutem tali viro commendetis"/>
        <underline method="pen" type="straight" language="LA" text="tamen ipsorum aduentus in vrbes sociorum non multum ab hostili expugnatione differant"/>
        <underline method="pen" type="straight" language="LA" text="vident, tanta temperantia, tanta mansuetudine, tanta humanitate, vt ii beatissimi esse videantur, apud quod ille diuertissime commoratur"/>
        <underline method="pen" type="straight" language="LA" text="gesserunt: quanto vos studio conuenit iniuriis prouocatos, sociorum salute[m] vn&agrave; cum imperii cestri dignitate defendere, 
            presertim cu[m] de vestris maximis vestigalibus agatur? nam caeteraru[m]"/>
        <underline method="pen" type="straight" language="LA" text="Asia ver&ograve;tam opima est &amp; fertilis, vt &amp; vbertate agrorum &amp; varietate fructuum, &amp; magnitudine pastionis, 
            &amp; multitudine earum rerum quae exportan[tur]"/>
        
