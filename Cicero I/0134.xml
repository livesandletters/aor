    <page filename="0134.tif" reader="John Dee" pagination="106"/>
    <annotation> 
        
        <marginalia hand="Italian" anchor_text="quid te futurum est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        quid te futuru[m] est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What becomes of you?
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cum &agrave; Praetore prohibere[n]tur, &agrave; quatuor Quaestoribus impedire[n]tur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        prohiberi impediri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be hindered, to be impeded.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="legatus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        legatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Deputy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nimium tenuiter Siculorum erga te voluntatis argumenta colligere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        nimiu[m] tenuiter argume[n]ta colligere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To collect arguments in too great detail.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="monstri simile">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        monstri simile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Like a monstrosity.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="apud omnes Graecos hoc mos est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        apud o[mn]es hic mos est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For all here it is custom.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="prop&egrave; soli">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        prop&egrave; soli
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Almost single-handed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="praeter caeteros">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        p[rae]t[er] ceteros
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Besides others.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quem ipsum hominem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        que[m] ip[su]m ho[m]i[ne]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That man himself.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="magna res est"/>
        <underline method="pen" type="straight" language="LA" text="quid te futurum"/>
        <underline method="pen" type="straight" language="LA" text="tame[n] id more potius suo, qu&agrave;m merito tuo facere viderentur"/>
        <underline method="pen" type="straight" language="LA" text="vt multo maluerint de suo more decedere, qu&agrave;m de tuis moribus non dicere?"/>
        <underline method="pen" type="straight" language="LA" text="Co[n]fitendum est igitur tibi necessario"/>
        <underline method="pen" type="straight" language="LA" text="omnium minas, atque omnia pericula prae salute sua leuia duxerunt"/>
        <underline method="pen" type="straight" language="LA" text="vt posset multas istius, &amp; varias iniurias"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;mapertissime vobis, planissim&eacute;que"/>
        <underline method="pen" type="straight" language="LA" text="numerare omnes no[n] est necesse"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m multi, &amp; qu&agrave;m multa, priore actione dixerunt"/>
        <underline method="pen" type="straight" language="LA" text="denique hoc in hac causa intelligent, hoc animo esse Siculos: vt si in istum animaduersum non sit, sibi relinquendas domos, ac sedes suas: &amp; ex Sicilia decedendum"/>
        <underline method="pen" type="straight" language="LA" text="nimium tenuiter Siculorum erga te coluntatis argumenta colligere"/>
        <underline method="pen" type="straight" language="LA" text="ecquo de homine"/>
        <underline method="pen" type="straight" language="LA" text="Graecis monstri simile"/>
        <underline method="pen" type="straight" language="LA" text="hic mos est"/>
        <underline method="pen" type="straight" language="LA" text="copias, acerim&uacute;mque eius impetum, &agrave; moenibus, litoribus, classib&uacute;sque suis"/>
        <underline method="pen" type="straight" language="LA" text="quem ipsum hominem cuperent euertere, eius effigem, simulacr&uacute;mque seruare"/>
        <underline method="pen" type="straight" language="LA" text="Videtis igitur consuetudinem, religion&eacute;mque Graecorum, quae monimenta hostium in bello ipso soleat defendere, eam summa"/>
        
