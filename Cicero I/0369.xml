    <page filename="0369.tif" reader="John Dee" pagination="341" signature="yy.iii."/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="no[n] enim omnia quae dolemus, eade[m] iure queri possumus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Non enim o[mn]ia qu[a]e dolem[us] eadem iure queri possum[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For we cannot reasonably complain about everything that we do not like [Loeb 324, 505].
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="grauis, locuples, ornata ciuitas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ciuitas grauis locuples ornata
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A city, responsible, wealthy and distinguished.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="multis p&ograve;st annis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        multis p&ograve;st annis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                After many years.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="leuitas propria Graecoru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        leuitas gra[e]cor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The shallowness of the Greeks.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ciuitas grauiss[im]a modertaiss[im]a
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A city, most responsible and controlled.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &agrave; co[n]trario
                    </marginalia_text>
                </position>
            </language>
            <translation>
                On the contrary.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="quantu[m] in ipsis fuit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        qua[n]tu[m] in ip[s]is fuit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As much as it was in their power.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Athenienses">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Athenienses
                    </marginalia_text>
                </position>
            </language>
            <translation>
                People from Athens.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Positio athenaru[m]
                    </marginalia_text>
                    <location name="Athens"/>
                </position>
            </language>
            <translation>
                The position of Athens.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Lacedaemonii">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Laceda[e]monij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Spartans.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="videro. sed primu[m] illud tenebo"/>
        <underline method="pen" type="straight" language="LA" text="spoliatam se dicit, qu&ograve;d id non habet, quod eius no[n] fuit, quid hoc impudentius"/>
        <underline method="pen" type="straight" language="LA" text="amissum est enim praeter spem, quod erat spe deuoratu[m] lucru[m]"/>
        <underline method="pen" type="straight" language="LA" text="no[n] enim omnia quae dolemus, eade[m] iure queri possumus"/>
        <underline method="pen" type="straight" language="LA" text="quo loco etia[m] atq[ue] etia[m] facite vt recordemini quae sit temeritas multitudinis, quae leuitas propria Graecoru[m], quid in co[n]cione seditiosa valeat oratio"/>
        <underline method="pen" type="straight" language="LA" text="atque obsidet rostra vindex temeritatis, &amp; moderatrix officii Curia"/>
        <underline method="pen" type="straight" language="LA" text="vt amicitiam Pop. Rom. fidem suam, iura omnia officii, humanitat&iacute;sque violarent, qu&agrave;m vt filium testimonio laederent, cuius patre[m] armis pellendum"/>
        <underline method="pen" type="straight" language="LA" text="quos enim hostes haec familia contempsit, nun qua[m] eosdem testes pertimescet"/>
        <underline method="pen" type="straight" language="LA" text="quae quidem &agrave; me si vt dice[n]da sunt, dicere[n]tur, grauius agerem Iudices, qu&agrave;m adhuc egi, quanta[m]"/>
        <underline method="pen" type="straight" language="LA" text="reuocarem animos vestros ad Mithridatici belli memoria[m]"/>
        <underline method="pen" type="straight" language="LA" text="Liceat haec nobis, si obliuisci no[n] possumus, at tacere"/>
        <underline method="pen" type="straight" language="LA" text="quantu[m] in ipsis fuit"/>
        <underline method="pen" type="straight" language="LA" text="In hac igitur vrbe se iactant qua[m] oderu[n]t, apud eos quos inuiti vident, in ea Rep. ad quam opprimendam non animus eis, sed vires defuerunt. aspiciant"/>
        <underline method="pen" type="straight" language="LA" text="de quorum vrbis positione"/>
        <underline method="pen" type="straight" language="LA" text="pulchritudine[m] etiam inter deos certame[n] fuisse proditu[m] est. quae vetustate ea est, vt ipsa ex sese suos ciues genuisse dicatur: 
            &amp; eoru[m] eade[m] terra pare[n]s, altrix, patria dicatur. authoritate aute[m] tanta est, vt iam fractu[m] prop&egrave; ac debilitatu[m] Graeciae nome[n] huius vrbis laude nitatur."/>
        <underline method="pen" type="straight" language="LA" text="cuius ciuitatis spectata ac nobilitata virtus, no[n] solu[m] natura coroborata, veru[m]etiam disciplina putatur: qui soli toto orbe terraru[m] septinge[n]tos iam annos"/>
        
