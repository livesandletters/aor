    <annotation>
        
        <marginalia hand="Italian" anchor_text="accessionem decumae, vt nostri facere coactores solent, ce[n]tesima, an decessionem de summa fecerit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>facere accessio[n]em decume facere decessione[m] de summa</marginalia_text>
                </position>
            </language>
            <translation>To make an addition of tenth. To make a deduction from the total.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="reconciliatio gratiae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>reco[n]ciliatio gr[atia]e</marginalia_text>
                </position>
            </language>
            <translation>A reconciliation for goodwill.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="praestigiae, illinc inquam omnes fallaciae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>praestigie fallaci[a]e</marginalia_text>
                </position>
            </language>
            <translation>Tricks. Deceits.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="insulsitate[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>insulsitas</marginalia_text>
                </position>
            </language>
            <translation>Dullness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="peierauerit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>peiuratio</marginalia_text>
                </position>
            </language>
            <translation>False testimony.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="praedes dedisset">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>pra[e]des dare</marginalia_text>
                </position>
            </language>
            <translation>To offer sureties.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="decumae, vt nostri facere coactores solent, ce[n]tesima, an decessionem de summa fecerit"/>
        <underline method="pen" type="straight" language="LA" text="Gabinii, remittere tantu[m] de suo: nec Regis, imponere tantu[m] pati suis"/>
        <underline method="pen" type="straight" language="LA" text="me ver&ograve; poenitet mortales inimicitias, sempiternas amicitias habere"/>
        <underline method="pen" type="straight" language="LA" text="enim Po[m]peius me sua causa quicqua[m] facere"/>
        <underline method="pen" type="straight" language="LA" text="quia nunc aiunt, quod tum negabant. quid"/>
        <underline method="pen" type="straight" language="LA" text="vbi semel quis peierauerit, ei credi postea, etia[m] si per pluris deos iuret, non oportet"/>
        
