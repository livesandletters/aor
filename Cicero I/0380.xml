    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="attulerint neruos &amp; industriam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        afferre [ neruos industriam
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring vigour and drive.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="excutient tibi istam verboru[m] iactione[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        excutere verboru[m] iactio[n]em
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To shake off throwing of words.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ex somno saepe excitabunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ex somno excitare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wake up from sleep.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Regium
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Royal.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="replices annalium memoriam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        replicare animaliu[m] memoria[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To search through historical records.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        no[n] su[m] nescius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I do not know.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="efferatur immoderatione verborum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        efferri immoderatio[n]e v[er]bor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be carried away by exaggerated language.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cum suppressa voce de scelere P. Lentuli, de audacia coniuratorum omnium dixisset">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        dicere supp[re]ssa voce
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To speak in a low voice.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="queribu[n]da voce">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        queribunda vox
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Plaintive voice.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="mihi crede, excutient tibi istam verboru[m] iactatione[m]"/>
        <underline method="pen" type="straight" language="LA" text="te ex somno saepe excitabunt, nec patientur se abs te nisi virtute vince[n]tur, honore superari"/>
        <underline method="pen" type="straight" language="LA" text="genere municipalis honestissimi ac nobilissimi"/>
        <underline method="pen" type="straight" language="LA" text="Quare neque me peregrinum"/>
        <underline method="pen" type="straight" language="LA" text="grauius refutere: neque regem, ne derideare. Nisi forte regium tibi videtur, ita viuere, vt non modo homini nemini, sed ne cupiditati quidem vlli seruias: 
            contenere omnes libidines, non auri, non arge[n]ti, non caeteraru[m] reru[m] indigere, in Senatu sentire libere, populi vtilitati magis co[n]sulere, qu&agrave;m volu[n]tati: nemini cedere, multis obsistere"/>
        <underline method="pen" type="straight" language="LA" text="conspectu tranquillo animo &amp; quieto frui"/>
        <underline method="pen" type="straight" language="LA" text="spiritus attuleru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="vestris ceruicibus depuli Iudices, &agrave;"/>
        <underline method="pen" type="straight" language="LA" text="Horum ego faces eripere de manibus, &amp; gladios extorquere potui, sicut feci"/>
        <underline method="pen" type="straight" language="LA" text="coluntas vero consceleratas ac nefarias nec sanare potui nec"/>
        <underline method="pen" type="straight" language="LA" text="conatus impios, &amp; furore[m] consceleratu[m] repressi"/>
        <underline method="pen" type="straight" language="LA" text="siquid ab improbis de me improbe dicitur? cum"/>
        
