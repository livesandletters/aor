    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="in huiusce legis periculum includatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        includi in periculum legis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be included in the danger of law.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ii qui se volunt posse omnia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ii qui se volunt posse o[mn]ia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Those who want it all in their power.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &AElig;quu[m] iudiciu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An even judgement.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="C. Flauius Pusio, Cn. Titinnius, C. Mecoenas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
