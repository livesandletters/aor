    <page filename="0585.tif" reader="John Dee" pagination="557" signature="[mmm.vii.]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="depuls&oacute;rque">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>depulsor</marginalia_text>
                </position>
            </language>
            <translation>One who repels.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cicerone[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Accusatio Antonij in Ciceronem.</marginalia_text>
                    <person name="Mark Antony"/>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Anthony's accusation against Cicero.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quid enim interest inter suasorem facti &amp; probatorem?">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quid interest inter suasore[m] facti &amp; probatorem?</marginalia_text>
                </position>
            </language>
            <translation>'What difference is there between one who advises an action and one who approves of it?' [Loeb 189, 83].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="stillante[m] prae se pugione[m] tulit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>stillante[m] pugione[m] ferre.</marginalia_text>
                </position>
            </language>
            <translation>To carry a dripping dagger.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="edormi crapula[m] inquam &amp; exhala">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>edormi crapula[m] &amp; exhala</marginalia_text>
                </position>
            </language>
            <translation>Sleep off the intoxication and breathe out.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="siquide[m] est atrocius patriae parente[m], qu&agrave;m suu[m] occidere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Atrocius est patri[a]e paren[n]te[m] qua[m] suu[m] occidere</marginalia_text>
                </position>
            </language>
            <translation>'It is a more atrocious crime to kill the “father of the fatherland” than one’s own parent' [Loeb 189, 85].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tertium nihil">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>nihil mediu[m] tertium</marginalia_text>
                </position>
            </language>
            <translation>No middle [nor] third [possibility].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="distinctius dicuntur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>distinctius dici.</marginalia_text>
                </position>
            </language>
            <translation>To be said more lucidly.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="retexo oratione[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>retexere or[ati]o[n]em</marginalia_text>
                </position>
            </language>
            <translation>To cancel a speech.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in equu[m] Troianu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Equus troianus.</marginalia_text>
                </position>
            </language>
            <translation>The Trojan Horse.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="affari">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>affari.</marginalia_text>
                </position>
            </language>
            <translation>To address.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ascribe me tale[m] in numerum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>ascribe in numeru[m]</marginalia_text>
                </position>
            </language>
            <translation>Insert in number.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="interest inter suasorem facti &amp; probatorem"/>
        <underline method="pen" type="straight" language="LA" text="fieri, an gaudea[m] factum"/>
        <underline method="pen" type="straight" language="LA" text="Etenim omnes boni, quantu[m] in ipsis fuit, Caesare[m] occiderunt"/>
        <underline method="pen" type="straight" language="LA" text="stupore[m] hominis, vel dica[m] pecudis"/>
        <underline method="pen" type="straight" language="LA" text="statuendu[m] tibi esse, vtru[m] illi, qui ista[m] rem gesseru[n]t, homicidii rei sint, an vindices liberatis? atte[n]de"/>
        <underline method="pen" type="straight" language="LA" text="cogiationemque sobrii hominis ad punctu[m] te[m]poris suscipe"/>
        <underline method="pen" type="straight" language="LA" text="siquide[m] est atrocius patriae parente[m], qu&agrave;m suu[m] occidere"/>
        <underline method="pen" type="straight" language="LA" text="no[n] igitur homicidae"/>
        <underline method="pen" type="straight" language="LA" text="tertium nihil"/>
        <underline method="pen" type="straight" language="LA" text="Sed tamen haec est summa co[n]clusionis meae, quonia[m] scelere &agrave; te liberati sunt, ab eodem te amplissimis praemiss dignissimi iudicati sunt. Itaque iam retexo oratione[m] mea[m]"/>
        <underline method="pen" type="straight" language="LA" text="in equu[m] Troianu[m]"/>
        <underline method="pen" type="straight" language="LA" text="quam tu in me vis concitare"/>
        <underline method="pen" type="straight" language="LA" text="cu[m] laude no[m] compare[m]"/>
        <underline method="pen" type="straight" language="LA" text="quae eoru[m] gloria[m] no[n] immortalitatis memoria prosequantur"/>
        
