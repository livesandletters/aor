        <underline method="pen" type="straight" language="LA" text="literis definiri vides"/>
        <underline method="pen" type="straight" language="LA" text="certe maior est virtutis, victori&aacute;eque iucunditas, qu&agrave;m ista voluptas quae percipitur ex libidine"/>
        <underline method="pen" type="straight" language="LA" text="vidi collucere omnia furtis tuis, praeda"/>
        <underline method="pen" type="straight" language="LA" text="vidit enim eos, qui in iudiciorum dominos se dici volebant, harum cupiditatum esse seruos"/>
        <underline method="pen" type="straight" language="LA" text="Nihil esse quod quisquam dubitaret de exitio sociorum, atque amicorum: qum quidem viderent in foro Populi Romani, 
            quo in loco antea qui sociis per scelus ablata, erept&aacute;que essent"/>
        <underline method="pen" type="straight" language="LA" text="solet haec quae rapuit, &amp; furatus est, nonnunquam"/>
        <underline method="pen" type="straight" language="LA" text="sed ex tempore aliquo"/>
        <underline method="pen" type="straight" language="LA" text="hoc vero nouum, &amp; ridiculum est: quod hic nobis respondit"/>
        <underline method="pen" type="straight" language="LA" text="vicisti"/>
        <underline method="pen" type="straight" language="LA" text="solis rebus indomitas cupiditates, atque effrenatas habebat, caeterae libidines eius ratione aliqua"/>
        <underline method="pen" type="straight" language="LA" text="ecquo in oppido pedem posuit, vbi non plura stuproru[m], flagitior&uacute;mque suoru[m], qu&agrave;m aduentus sui vestigia reliquerit?"/>
        <underline method="pen" type="straight" language="LA" text="praeterea maxime sedati &amp; quieti"/>
        
