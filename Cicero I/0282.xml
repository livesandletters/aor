        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="immortales, quae superbia, quanta ignoratio sui, qu&agrave;m grauis atque intolerabilis arroga[n]tia"/>
        <underline method="pen" type="straight" language="LA" text="quasi non satis signi esse debuerit"/>
        <underline method="pen" type="straight" language="LA" text="patronorum hominum eloquentissimorum atque amplissimorum, quorum certe aliquis defendisset"/>
        <underline method="pen" type="straight" language="LA" text="qui, qu&ograve;d Rostra iandiu vacua, loc&uacute;mque illum post aduentum"/>
        <underline method="pen" type="straight" language="LA" text="Syllae, &agrave; Tribunitia voce desertum oppresserat, multitudin&eacute;mque iam desuefacta[m] &agrave; concionibus, ad veteris consuetudinis similitudinem"/>
        <underline method="pen" type="straight" language="LA" text="Facite enim vt non solum mores eius &amp; arrogantiam, sed etiam vultum atque amictum, atque illam vsque ad talos demissam purpura[m] recordemini"/>
        <underline method="pen" type="straight" language="LA" text="ita viuat, vt nobilitatis dignitatem virtte tueri posse videatur: vsque eo peruenit"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m si idem ille esset cu[m] iisdem suis vitiis nobilissimus"/>
        <underline method="pen" type="straight" language="LA" text="quis eum cum illa superbia"/>
        <underline method="pen" type="straight" language="LA" text="intolerantia ferre portuisse"/>
        <underline method="pen" type="straight" language="LA" text="superbiam autem atque arrogantiam eius deridendam magis arbitrarentur propter humilitatem hominis, qu&agrave;m pertimesce[n]dam"/>
        <underline method="pen" type="straight" language="LA" text="saepe vexatus"/>
        <underline method="pen" type="straight" language="LA" text="igitur omnia Quintiana, iniqua, falsa, turbulenta, popularia, seditiosa iudicia fuerunt. Esto"/>
        <underline method="pen" type="straight" language="LA" text="Verum si innocens Falcula, quaeso quis nocens"/>
        <underline method="pen" type="straight" language="LA" text="quanquam satis magno argumento esse"/>
        <underline method="pen" type="straight" language="LA" text="tamen hac plaga periret, cur in tanta multitudine accuasatorum"/>
        <underline method="pen" type="straight" language="LA" text="cuius rei quae consuetudo sit, quoniam apud homines peritissimos dico, pluribus verbis docere non debeo"/>
        
