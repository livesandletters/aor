        <underline method="pen" type="straight" language="LA" text="ciuitates proportione in prouincias descripsisset"/>
        <underline method="pen" type="straight" language="LA" text="Non refert vero"/>
        <underline method="pen" type="straight" language="LA" text="fratris mei laude delector, sed"/>
        <underline method="pen" type="straight" language="LA" text="Citat"/>
        <underline method="pen" type="straight" language="LA" text="vnus Asclepiades"/>
        <underline method="pen" type="straight" language="LA" text="eti&aacute;mne praecone[m] me[n]tiri coegisti"/>
        <underline method="pen" type="straight" language="LA" text="quas ego no[n] solu[m] propter longitudine[m], sed eita[m] propter turpissima[m] obscoenitatem verboru[m] praetereundas puto. dixit"/>
        <underline method="pen" type="straight" language="LA" text="fortuna egens, vita turpis, existimatione damnatus, impude[n]tia atque audacia fretus, sine tabulis, sine authore legerit, id nos quasi"/>
        <underline method="pen" type="straight" language="LA" text="quide[m] laudationis iactura exoptanda nobis"/>
        <underline method="pen" type="straight" language="LA" text="Flacci vita, &amp; digitas nititur"/>
        <underline method="pen" type="straight" language="LA" text="mihi quod haec causa maxime postulat, nulla[m] grauitate[m], nulla[m] consta[n]tiam, nullu[m] firmu[m] in Graecis hominibus consiliu[m], 
            nulla[m] denique esse testimonii fide[m]. nisi vero hactenus ista formula testimonii atq[ue] orationis tuae describi ac distingui potest, vt Flacco absenti aliquid ciuitates tribuisse dicantur"/>
        <underline method="pen" type="straight" language="LA" text="Equidem in minimus rebus saepe res magnas vidi Iudices deprehe[n]di ac teneri, vt in hoc Asclepiade"/>
        <underline method="pen" type="straight" language="LA" text="obsignata erat creta illa Asiatica"/>
        
