        <underline method="pen" type="straight" language="LA" text="vt tum habebatur"/>
        <underline method="pen" type="straight" language="LA" text="mater enim &agrave; me nominis causa, tametsi in hu[n]c hostili odio &amp; crudelitate est"/>
        <underline method="pen" type="straight" language="LA" text="Quo enim est ipsum nome[n] amantius, indulgenti&uacute;sque maternum: hoc illius matris quae multos iam annos, &amp; nun cum maxime filium interfectum cupit, 
            singulare scelus maiore odio dignum esse ducetis"/>
        <underline method="pen" type="straight" language="LA" text="deinde ita flagrare coepit amentia, sic inflammata ferri libidine, vt eam no[n] pudor, non pudicitia, non pietas, non macula familiae, non hominum fama, non filii dolor, 
            non filiae moeror, &agrave; cupiditate reuocaret. Animum adolescentis nondum consilio ac ratione firmatum, pellexit iis omnibus rebus quibus illa aetas capi ac deliniri potest"/>
        <underline method="pen" type="straight" language="LA" text="nefarium matris pellicatum"/>
        <underline method="pen" type="straight" language="LA" text="de quo ne quaeri quidem"/>
        <underline method="pen" type="straight" language="LA" text="sui fratris manibus &amp; gremio, moerore &amp; lachrymis consenescebat"/>
        <underline method="pen" type="straight" language="LA" text="vt in tantis iniuriis, non inuita: vt &agrave; viro, non libenter"/>
        <underline method="pen" type="straight" language="LA" text="tum vero illa egragia ac praeclara"/>
        <underline method="pen" type="straight" language="LA" text="vitrix filiae, no[n] libidinis. Itaque"/>
        <underline method="pen" type="straight" language="LA" text="nubit genero socrus, nullis auspiciis, nullis authoribus, funestis omnibus omnium"/>
        <underline method="pen" type="straight" language="LA" text="O mulieris scelus incredibile, &amp; praeter ha[n]c vnam in omni vita inauditum"/>
        <underline method="pen" type="straight" language="LA" text="libidinem effrenata[m] &amp; indomitam"/>
        <underline method="pen" type="straight" language="LA" text="O audaciam singularem, no[n] timuisse, si minus"/>
        <underline method="pen" type="straight" language="LA" text="Perfregit, ac prostrauit omnia cupiditate ac furore: vicit pudore[m] libido, timorem audacia, rationem amentia"/>
        <underline method="pen" type="straight" language="LA" text="augebatur autem eius molestia quotidianis querimoniis &amp; assiduo fletu sororibus: statuit"/>
        <underline method="pen" type="straight" language="LA" text="Initium quod huic cum matre"/>
        <underline method="pen" type="straight" language="LA" text="pertinuisse hoc ad causam, tunc cum reliqua cognoueritis"/>
        <underline method="pen" type="straight" language="LA" text="Non essem ad vllam causam idoneus Iudices, si hoc quod in communibus hominum sensibus, atque in ipsa natura positu[m] atque infixum est, id ego, 
            cum ad amici pericula depellenda adhiberer, non viderem"/>
        <underline method="pen" type="straight" language="LA" text="Facile intelligo"/>
        <underline method="pen" type="straight" language="LA" text="modo reticere homines parentum iniurias, sed etiam animo aequo ferre oportere. sed ego ea quae ferri possunt, ferenda: quae taceri, tacenda esse arbitror"/>
        <underline method="pen" type="straight" language="LA" text="adiit, nihil mali timuit quod non totum &agrave; matre"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; matre initio est adornata, &agrave; matre hoc te[m]pore instruitur, atque omnibus"/>
        <underline method="pen" type="straight" language="LA" text="praest&ograve; est mulier, audax, pecuniosa, crudelis"/>
        <underline method="pen" type="straight" language="LA" text="squalore huius &amp; sordibus laetatur, exitium exoptat, sanguine[m] suum profunde[re]"/>
        
