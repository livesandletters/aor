    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="qu&agrave;m iuste? qu&agrave;m sapienter? qu&agrave;m diligenter?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        iuste sapie[n]ter diligenter ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Justly, wisely, diligently.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="pueri paruuli">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        puer paruulus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A little boy.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="mare quod sua natura tranquillum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Mare sua natura tra[n]quillu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A sea, calm by its nature.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Comparatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Comparison.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="turpiter pen&egrave; praeterii">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        turpit[er] pen&egrave; p[rae]terij.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To my shame, I will go past.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="perinde quasi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        perinde q[ua]si
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Just as if.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="M. Antonium aiu[n]t solitu[m] esse dicere, idcirco se nullam vnquam orationem scripsisse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quin Marc[us] antoni[us] nu[n]qua[m] or[ati]o[n]em scripsit 
                    </marginalia_text>
                    <person name="Mark Antony"/>
                </position>
            </language>
            <translation>
                In fact, Mark Antony never wrote his speeches.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="facile illis ipsis iudicibus &amp; falsae suspitioni &amp; inuidiae"/>
        <underline method="pen" type="straight" language="LA" text="Vnum etia[m] est quod me maxime conturbat: cui loco respondere posse vix videor"/>
        <underline method="pen" type="straight" language="LA" text="honestissimi videlicet"/>
        <underline method="pen" type="straight" language="LA" text="cum vir optimus &amp; homo"/>
        <underline method="pen" type="straight" language="LA" text="Senatus decreuerat ad illud inuidiae praesens incendiu[m] restinguendum, id"/>
        <underline method="pen" type="straight" language="LA" text="vt mare quod sua natura tranquillum sit, ventorum vi agitari atque turbari: sic &amp; Populum Ro. sua sponte esse placatum, hominum seditiosorum vocibus, 
            vt violentissimis tempestatibus concitari"/>
        <underline method="pen" type="straight" language="LA" text="perinde quasi"/>
        <underline method="pen" type="straight" language="LA" text="Ego vero siquid eiusmodi dixi, neque cognitum co[m]memoraui, neque pro testimonio dixi: &amp; illa oratio potius te[m]poris mei, 
            qu&agrave;m iudicii &amp; authoritatis fuit"/>
        <underline method="pen" type="straight" language="LA" text="Sed errat veheme[n]ter, siquis"/>
        <underline method="pen" type="straight" language="LA" text="nostris quas in"/>
        <underline method="pen" type="straight" language="LA" text="omnes"/>
        <underline method="pen" type="straight" language="LA" text="Nam si causae ipsae pro se loqui possent, nemo adhiberet oratorem. nunc"/>
        <underline method="pen" type="straight" language="LA" text="Homine[m] ingeniosum M. Antonium aiu[n]t solitu[m] esse dicere, idcrico se nullam vnquam orationem scripsisse, vt siquid aliquando quod non opus esset, 
            ab se esset dictum, posset se negare dixisse. perinde quasi quid &agrave; nobis dictum aut actum sit, id nisi literis mandaueri[mus]"/>
        
