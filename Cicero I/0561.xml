        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tanta vis, tanta copia, quae no[n] ferro">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vis Ferru[m]</marginalia_text>
                </position>
            </language>
