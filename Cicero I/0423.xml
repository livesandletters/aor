    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="nihil ab iis, quod sit iniustum ac inhonestum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        nihil iniustum aut inhonestu[m] &agrave; deo est petendum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nothing unjust or dishonest is ought to be asked for by a god.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="domus vniuscuiusque ciuium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Domus Ciuium.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The house of the citizens.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Tanagraea quaedam meretrix">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Tanagraea meretrix
                    </marginalia_text>
                    <location name="Tanagra"/>
                </position>
            </language>
            <translation>
                A courtesan from Tanagra.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="libertatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        LIBERTAS
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Freedom.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Nec est vlla erga deos pietas, nisi honesta de numine eoru[m], ac mente opinio: cum expeti nihil ab iis, quod sit iniustum ac inhonestum, arbitrere"/>
        <underline method="pen" type="straight" language="LA" text="hominis sceleratissimi nefarium latrocinium immigrare voluisse"/>
        <underline method="pen" type="straight" language="LA" text="Quid est sanctius, quid omni religione munitius, qu&agrave;m domus vniuscuiusque ciuium? H&icirc;c arae sunt, h&icirc;c foci, h&icirc;c dii penates, h&icirc;c sacrae religionis cerimoniae contine[n]tur: hoc perfugium est ira ita sanctum omnibus, vt inde abripi neminem fas sit"/>
        <underline method="pen" type="straight" language="LA" text="nobis &amp; sancta esse voluerunt, ea iste non solum co[n]tra religione[m] labefactauit, sed etiam ipsius religionis nomine"/>
        <underline method="pen" type="straight" language="LA" text="pedissequis conculcari"/>
        <underline method="pen" type="straight" language="LA" text="crudelissimi dominatus"/>
