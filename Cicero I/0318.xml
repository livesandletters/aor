        <marginalia hand="English_secretary" anchor_text="agros no[n]nullos, inquit, socer meus desertos atque linginquos, ve[n]det eos mea lege quantivolet: habet incertos ac nullo iure possessos, co[n]firmabu[n]tur optimo iure. 
            habet publicos, redda[m] priuatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ager { disertus longinquus incertus posessus publicus priuatus ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
               A field: waste, remote, uncertain, inherited, public, private.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="non est meae consuetudinis initio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Exordiu[m] no[n] est me[a]e co[n]suetudinis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                For the beginning it is not my custom.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in hac defensione capitis, famae, fortunar&uacute;mque omnium C. Rabirii">
