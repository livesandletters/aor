        <underline method="pen" type="straight" language="LA" text="quo de homine nihil etia[m] nunc dicere nobis est necesse"/>
        <underline method="pen" type="straight" language="LA" text="annos octo meditati accusatores collegeru[n]t, qu&agrave;m leuia genere ipso, qu&agrave;m falsa re, qu&agrave;m breuia responsu"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m paucis verbis haec causa perorari potuerit, &amp; qu&agrave;m multa &agrave; me dicta sint, quae ad huius voluntate[m] maxime ad vestrupm] iudicium minime pertinerent"/>
        <underline method="pen" type="straight" language="LA" text="Opportune adest homo summa fide &amp; omni"/>
        <underline method="pen" type="straight" language="LA" text="adolesce[n]ti pudentissimo, &amp; in primis honesto Equiti Ro. data[m]"/>
        <underline method="pen" type="straight" language="LA" text="intercaepisse, bibisse"/>
        <underline method="pen" type="straight" language="LA" text="hoc ego si sic agere[m], tanquam mihi crime[n] esset diluendum, haec pluribus verbis dicere[m], quae nu[n]c paucis percurrit oratio mea"/>
        <underline method="pen" type="straight" language="LA" text="Multa sunt quae dici possunt, sed non co[m]mittam vt videar non dice[n]do voluisse dicere"/>
        <underline method="pen" type="straight" language="LA" text="magnum &amp; impude[n]s mandaciu[m]. perspicite"/>
        <underline method="pen" type="straight" language="LA" text="crudior venisset"/>
        <underline method="pen" type="straight" language="LA" text="paulisper exurge, &amp; perfer hunc dolorem commemorationis necessariae, in qua ego diutius non morabor: quoniam quod"/>
        <underline method="pen" type="straight" language="LA" text="tuus calamitatem &amp; falsum crimen"/>
        <underline method="pen" type="straight" language="LA" text="Sed homines, inimicos suos morte affici volunt, cel qu&ograve;d metuunt, vel qu&ograve;d oderunt: quo tandem"/>
        
