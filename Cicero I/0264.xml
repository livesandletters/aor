            <translation>
                Conclusion
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ne sit periniquum"/>
        <underline method="pen" type="straight" language="LA" text="authoritate[m] improbari"/>
        <underline method="pen" type="straight" language="LA" text="propterea qu&ograve;d istis reclamantibus vos vnum illum ex omnibus delegistis quem bello praedonum praeponeretis"/>
        <underline method="pen" type="straight" language="LA" text="vidistis, vos his repugnantibus"/>
        <underline method="pen" type="straight" language="LA" text="vosmetipsos dignitate[m] huic imperio, salutem orbi terraru[m] attulistis: aliquando isti"/>
        <underline method="pen" type="straight" language="LA" text="Difficile est dictu"/>
        <underline method="pen" type="straight" language="LA" text="qua[n]to in odio simus"/>
        <underline method="pen" type="straight" language="LA" text="Quod enim"/>
        <underline method="pen" type="straight" language="LA" text="putatis in illis terris, nostris magistratibus religiosum, quam ciuitate[m] sanctam, quam domu[m] satis clausam ac munitam fuisti"/>
        <underline method="pen" type="straight" language="LA" text="Libe[n]ter haec cor&agrave;m cu[m] Q. Catulo, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="Hortensio disputare[m] summis &amp;"/>
        <underline method="pen" type="straight" language="LA" text="querimonias audiunt"/>
        <underline method="pen" type="straight" language="LA" text="animos ac spiritus capere possit?"/>
        <underline method="pen" type="straight" language="LA" text="siquem habetis, qui collatis signis exercitus regios superare posse videatur, tamen nisi erit idem qui se &agrave; pecuniis socioru[m], 
            qui ab eorum coniugibus ac liberis, qui ab auro, gaz&aacute;que regia, manus, oculos, animum cohibere possit, non erit idoneus qui ad bellum Asiaticum, regi&uacute;mque mittatur. Ecquam"/>
        <underline method="pen" type="straight" language="LA" text="locupletari"/>
        <underline method="pen" type="straight" language="LA" text="detrime[n]tis accipiendis maiore affici turpitudine videremur"/>
        <underline method="pen" type="straight" language="LA" text="summo ingenio &amp; prudentia praeditus"/>
        <underline method="pen" type="straight" language="LA" text="Quare videte, num horu[m] authoritatibus illorum orationi qui dissentiunt, respo[n]dere posse videamur"/>
        <underline method="pen" type="straight" language="LA" text="cuiusquam vim aut minas pertimescas"/>
        <underline method="pen" type="straight" language="LA" text="te satis esse animi"/>
        
