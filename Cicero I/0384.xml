        <marginalia hand="English_secretary" anchor_text="At accusat C. Cornelii filius, id&eacute;mque valere debet, ac si pater indicaret">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Eade[m] est r[ati]o accusatio[n]is filij &amp; patris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                This is the reasoning for the accusation of son and father.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tectus praesidio firmo amicoru[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        tectus pr[a]esidio firmo amicoru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Protected by a stout guard of friends {Loeb 324, 365].
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="copias, &amp; conatum repressi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        reprimere copias conatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
