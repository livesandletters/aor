    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Decurion&eacute;sque">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Decurio[n]es.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Decurions.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="commendationis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[m]mendatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Entrusting.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad vrbe[m] confestim incredibili celeritate aduolauit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ad vrbe[m] co[n]festim incredibili celeritate aduolauit.
                    </marginalia_text>
                    <location name="Rome"/>
                </position>
            </language>
            <translation>
                He immediately hastened to Rome with incredible speed.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="militum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Milites
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Soldiers.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="stimulos admouerit homini fortassis studioso victoriae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        admouere stimulos studiosus victori[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring up incentives, eager for victory.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="mirificus vsus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        mirific[us] vsus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Amazing skill.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad excitandum Antonium, cohortandum, impellendum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        excitare cohortare impellere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To excite, to encourage, to persuade.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="&egrave; pruina Apennini, atque &egrave; niuibus illis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        emergere &egrave; pruina niuibus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To emerge from the frost and snow.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="contento studi, curs&uacute;que veniamus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        venire co[n]tento studio cursuq[ue]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come with content, eagerness and direction.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="dolentius deplorandum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        dolentius deplorare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lament with sorrow.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ad vrbe[m] confestim incredibili celeritate aduolauit. Atque vt illius te[m]poris attrocitate[m] recordari"/>
        <underline method="pen" type="straight" language="LA" text="ad timoris praeteriti cogitationem excitate"/>
        <underline method="pen" type="straight" language="LA" text="sed hoc breue dica[m]"/>
        <underline method="pen" type="straight" language="LA" text="pruina Apennini, atque &egrave; niuibus illis emersisset"/>
        <underline method="pen" type="straight" language="LA" text="Macedoniae"/>
        <underline method="pen" type="straight" language="LA" text="vidi vestigia non pressa leuiter ad exigui praedicationem temporis, sed fixa ad memoria[m] illius prouinciae sempiternam. Verum haec ita praetereamus, vt tamen intue[n]tes &amp; respectantes relinquamus"/>
        <underline method="pen" type="straight" language="LA" text="quia ipse ad sese iandudu[m] vocat, &amp; quodammodo absorbet oratione[m] mea[m], contento studio, curs&uacute;que veniamus"/>
        <underline method="pen" type="straight" language="LA" text="non defensionem modo criminum videretur continere, sed etiam memoria digna vti, &amp; Reipublicae capessendae authoritatem disciplin&aacute;mque praescribere"/>
        <underline method="pen" type="straight" language="LA" text="si non subtilius disputandum, at certe dolentius deplorandum. Qua in oratione si asperius in quosdam homines inuehi vellem"/>
        <underline method="pen" type="straight" language="LA" text="concederet, vt eos quorum sceleris furore violatus essem, vocis libertate"/>
        <underline method="pen" type="straight" language="LA" text="Sed vt agam moderate, &amp; huic potius tempori seruia[m], qu&agrave;m dolori meo, siqui occulte &agrave; salute nostra dissentiunt, lateant: si qui fecerunt aliquid aliquando, atque iidem nunc tacent &amp; quiescunt, nos quoque simus obliti: si qui se offerunt insolenter &amp; insecta[n]tur, quoad ferri potuerunt perferemus: neque que[m]quam offendet oratio mea, nisi qui se ita obtulerit, vt in eum non inuasisse, sed incurrisse videamur"/>
        <underline method="pen" type="straight" language="LA" text="Sestii dicere incipiam, me totum superioris anni Reipub. naufragium exponere. in quo colligendo"/>
        
