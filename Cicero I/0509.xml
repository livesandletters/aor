    <page filename="0509.tif" reader="John Dee" pagination="481" signature="hhh.i."/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="Cn. Pompeii">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cn Pompeius
                    </marginalia_text>
                    <person name="Gnaeus Pompeius"/>
                </position>
            </language>
            <translation>
                Gnaeus Pompeius.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in Prouincias [...] recisas, partim repressas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        prouincia[e] reris[a]e repress[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Provinces, cut off, driven back.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="gens [...] subacta [...] domita [...] pacata">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        ge[n]tes subact[a]e domit[a]e pacat[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Peoples, subjugated, subdued, pacified.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="influentes in Italiam Gallorum maximas copias repressit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        influententes in Italia[m] Gallo maximas copias rep[re]ssit
                    </marginalia_text>
                    <location name="Italy"/>
                    <location name="Gaul"/>
                </position>
            </language>
            <translation>
                He drove back vast hordes that streaned into Italy from Gaul.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="domui [...] contentus [...] par&ecirc;re assuefecit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        par&ecirc;re assuefacere frangere domare co[n]serere co[m]pellere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To obey, to accustom, to dishearten, to subdue, to engage, to force.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="bellandum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        bellare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To fight.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in nostram ditionem esse redigendam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in n[ost]ram ditione[m] redigere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To bring under our sway.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="&agrave; gentibus, aut inimicis huc Imperio, aut infidis, aut incognitis, aut certe immanibus &amp; barbaris &amp; bellicosis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Gentes inimic[a]e infid[a]e incognit[a]e immanes barbare bellicos[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The peoples, hostile, faithless, unknown, brutal, uncivilised, belligerent.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Restitimus semper lacessiti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Resistim[us] lacesiti
                    </marginalia_text>
                </position>
            </language>
            <translation>
                We withstood when provoked.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Alpibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Alpes
                    </marginalia_text>
                    <location name="Alps"/>
                </position>
            </language>
            <translation>
                The Alps.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="impolitae vero res, &amp; acerbae si erunt relictae (qua[m]quam sunt abscissae)">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        res impolita acerba relicta abscissa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A matter, unpolished, rough, abandoned, torn.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="reuirescent">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        reuirescere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To revive.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in eius tutela">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in ei[us] tutela
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In his guardianship.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="vrbes sed&eacute;sque penetrauit. Modo"/>
        <underline method="pen" type="straight" language="LA" text="quas nationes nemo vnquam fuit quin frangi, domar&iacute;que cuperet: nemo"/>
        <underline method="pen" type="straight" language="LA" text="Restitimus semper lacessiti"/>
        <underline method="pen" type="straight" language="LA" text="Alpibus Italiam munierat ant&egrave; natura non sine aliquo diuino"/>
        <underline method="pen" type="straight" language="LA" text="Nam illae sente[n]tiae virorum clarissimorum minime probandae sunt, quorum alter vlteriorem Galliam decernit cum Syria, alter"/>
        
