    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="solicitationes seruorum &amp; minis &amp; promissis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        solicitatioes seruoru[m] minis promissis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Overtures to the slaves, by threat and promisses.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="conscelerata exectio linguae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]scelerata exectio lingu[a]e
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dishonourable cutting out of the tongue.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="propter vicinitatem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        propter vicinitate[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Because of the vicinity.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Nemo erat illorum pen&egrave; dicam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        nemo erat illoru[m] pen&egrave; dica[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No one of them was there, I almost say.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="fidem precio labefactare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fide[m] precio labefactare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To undermine loyalty with a bribe.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Deus malis aduersatur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God is against evil.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        pare[n]s pro liberis arguatur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A parent arguing for their childs.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Petitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Petition.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="caeteri nouis affinitatibus adducti"/>
        <underline method="pen" type="straight" language="LA" text="veteres inimicitias saepe deponunt"/>
        <underline method="pen" type="straight" language="LA" text="confirmandas inimmicitias"/>
        <underline method="pen" type="straight" language="LA" text="quibus finem aliquando no[n] mulieris modus, sed amicorum"/>
        <underline method="pen" type="straight" language="LA" text="confestim huc aduolauit: ne aut accusatoribus"/>
        <underline method="pen" type="straight" language="LA" text="dilige[n]tia, aut pecunia testibus deesset: aut ne fort&egrave; mater hoc sibi optatissimum"/>
        <underline method="pen" type="straight" language="LA" text="quos concursus in his oppidis? quantos &amp; viroru[m] &amp; mulierum gemitus esse factos, mulierem quandam Larino, 
            atque illam vsque &agrave; mari supero Romam proficisci"/>
        <underline method="pen" type="straight" language="LA" text="cum magno comitatu &amp; pecunia"/>
        <underline method="pen" type="straight" language="LA" text="quin expediandu[m] illum locum esse arbitraretur, quacunque illa iter fecisset: nemo quin terram ipsam violari, quae"/>
        <underline method="pen" type="straight" language="LA" text="mater est omniu[m], vestigiis consceleratae matris putaret"/>
        <underline method="pen" type="straight" language="LA" text="co[n]tagionem aspectus"/>
        <underline method="pen" type="straight" language="LA" text="nocti se potius ac solitudini, qu&agrave;m vlli aut vrbi, aut hospiti co[m]mittebat"/>
        <underline method="pen" type="straight" language="LA" text="quorum fidem precio labefactare conata sit"/>
        <underline method="pen" type="straight" language="LA" text="quinetiam nocturna sacrificia, quae putat occultiora esse, scelerat&aacute;sque eius preces, &amp; nefaria vota cognouimus, 
            quibus illa etiam deos immortales de suo scelere testatur: neque intelligit pietate &amp; religione, &amp; iustis precibus, deoru[m] mentes, non contaminata superstitione, neque ad scelus perficiendum caesis hostiis posse placari"/>
        <underline method="pen" type="straight" language="LA" text="immprtales &agrave; suis aris, atque templis aspernatos esse co[n]fido"/>
        <underline method="pen" type="straight" language="LA" text="Clue[n]tio quosdam alios deos ad omne vitae tempus fortuna esse voluit, huius importunitatem matris &agrave; filii capite depellite. multi saepe"/>
        <underline method="pen" type="straight" language="LA" text="vos ne huius honestissime actam vitam matris crudelitati co[n]donetis, rogamus"/>
        <underline method="pen" type="straight" language="LA" text="incredibile dictu est, sed &agrave; me verissime dicetur) omnes"/>
        <underline method="pen" type="straight" language="LA" text="pueris illud hoc tempore, &amp; mulieribus oppidum scitote"/>
        <underline method="pen" type="straight" language="LA" text="quae dum laudatio recitatur, vos quaeso qui eam detulistis, assurgite"/>
        <underline method="pen" type="straight" language="LA" text="Ex lachrymis horum Iudices, existimare potestis, omnes haec Decuriones decreuisse lachrymantes"/>
        
