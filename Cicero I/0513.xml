        <marginalia hand="English_secretary" anchor_text="extremu[m] principium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        extremu[m] principiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The extreme of the beginning.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="de authoritate">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Authoritas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Authority.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Audiui hoc de parente meo puer">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        audiui hoc de pare[n]te meo puer
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As a boy, I heard this from my father.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="remoueret oculos, &amp; se totum auerteret">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        remouere oculos se totu[m] auertere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To avert the gaze and turn away completely.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sanct&egrave; grauit&eacute;rque vixisset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        sanct&egrave; grauiter viuere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live more piously and seriously.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="omnia cum plena laudis, tum propria Cornelii, nec in his rebus crimen est vllum"/>
        <underline method="pen" type="straight" language="LA" text="nisi honos"/>
        <underline method="pen" type="straight" language="LA" text="Est enim aliquid quod non oporteat, etiam si licet: quicquid vero non licet, certe non oportet. H&icirc;c"/>
        <underline method="pen" type="straight" language="LA" text="terrarum: tot victorias bellicas, quot sunt in reru[m] natura genera belloru[m]? an"/>
        <underline method="pen" type="straight" language="LA" text="etia[m] ipse casus euent&uacute;sque rerum, no[n] duces, sed comites eius consilioru[m]"/>
        <underline method="pen" type="straight" language="LA" text="ita summa fortuna cum summa virtute certauit"/>
        <underline method="pen" type="straight" language="LA" text="cui patriae salus dulcior qu&agrave;m conspetus fuit"/>
        <underline method="pen" type="straight" language="LA" text="uitate decedere qu&agrave;m de sententia maluit: hoc"/>
        <underline method="pen" type="straight" language="LA" text="veritate fidem esse constrictam"/>
        <underline method="pen" type="straight" language="LA" text="mutae regiones imploro, &amp; sola terrarum vltimarum: vos maria, portus, insulae, litor&aacute;que. quae est enim ora, quae sedes, qui locus, in quo non extent huius cum fortituinis, tum vero humanitatis, tum animi, tum consilii impressa vestigia?"/>
        <underline method="pen" type="straight" language="LA" text="inaudita grauitate, virtute, constantia praeditum foedera scie[n]tem neglixisse, violasse, rupisse, dicere audebit?"/>
        <underline method="pen" type="straight" language="LA" text="acerrimu[m] &amp; maximu[m] gesserat: quo"/>
        
