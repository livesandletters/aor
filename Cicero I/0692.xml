        <underline method="pen" type="straight" language="LA" text="in quo omnia mea posita esse decreui"/> <!-- NBW: Mark leading into this underlining -->
        <underline method="pen" type="straight" language="LA" text="Dux nobis &amp; author opus"/>
        <underline method="pen" type="straight" language="LA" text="tuum studium dices"/>
        <underline method="pen" type="straight" language="LA" text="si complecti hominem volueris,"/>
        <underline method="pen" type="straight" language="LA" text="quantum officii sustinerem"/>
        <underline method="pen" type="straight" language="LA" text="non enim mihi tam mea salus chara fuit, in qua praecipue sum"/>
        <underline method="pen" type="straight" language="LA" text="adiutus, qu&agrave;m pietas erit in referenda gratia iucunda"/>
        <underline method="pen" type="straight" language="LA" text="SERA gratulatio reprehe[n]di non solet"/>
        <underline method="pen" type="straight" language="LA" text=", &amp; vt sempiternae laudi tibi sit iste Tribunatus, exopto"/>
        <underline method="pen" type="straight" language="LA" text="Nemo est qui tibi sapientius suadere possit te ipso: nunquam lab&ecirc;re, si te audies"/>
        <underline method="pen" type="straight" language="LA" text="si ea defendes quae ipse recta esse senties."/>
        <underline method="pen" type="straight" language="LA" text="tempus no[n] incideris sed veneris"/>
        <underline method="pen" type="straight" language="LA" text="temporum sit, qua[n]ta varietas rerum, qu&agrave;m incerti exitus, qu&agrave;m flexibiles hominu[m] voluntates,"/>
        <underline method="pen" type="straight" language="LA" text="vanitatis in vita, non dubito quin cogites"/>
        <underline method="pen" type="straight" language="LA" text="loquere &amp; te adhibe in co[n]silium, te audi, tibi obte[m]pera."/>
        <underline method="pen" type="straight" language="LA" text="particeps, vel socius,"/>
        <underline method="pen" type="straight" language="LA" text="vis amoris mei"/>
        <underline method="pen" type="straight" language="LA" text="consilio te vt possem iuuare"/>
        <underline method="pen" type="straight" language="LA" text="Scribam ad te plura alias"/>
        <underline method="pen" type="straight" language="LA" text="enim diebus era[m] missurus domesticos"/>
        <underline method="pen" type="straight" language="LA" text="vnis literis totius aestatis res gestas ad Senatu[m] perscriberem"/>
        <underline method="pen" type="straight" language="LA" text="Te mi Curio pro tua incredibili in me beneuolentia, me&aacute;que item in te singulari"/>
        <underline method="pen" type="straight" language="LA" text="Tribuno, non vt decernatur aliquid noui"/>
        <underline method="pen" type="straight" language="LA" text="solet esse difficilus: sed vt nequid noui decernatur:"/>
        
