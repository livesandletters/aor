    <page filename="0181.tif" reader="John Dee" pagination="153"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="laesit grauissime">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        laesit grauiss[im&egrave;]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It harms most severely.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="naui faciundae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        facere naue[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make a boat.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iniurias plurimas, contumeli&aacute;sque imposuisti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        imponere iniurias co[n]tumelias
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wrong, to insult.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in perpetuum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in perpetuu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Again and again.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="detrimentum fecit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        detrime[n]tu[m] facere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To suffer.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de summa frumenti detractum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        de su[m]ma destractu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be deducted from the total.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="praedones">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Praedones
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Robbers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="instituunt amicos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        instituere amicos
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To make friends.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="se locis reciperent">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        recipere se.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To recover himself.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ciuitas improba antea non erat, etiam erat inimica improborum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ciuitas no[n] improba inimica improbor[um]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A city, not wicked, the enemy of rascals.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vacatio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vacatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Exemption.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt opinio mea fert">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vt opinio mea fert
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As I do believe.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vacui, expertes, soluti ac liberi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vacuus expers ] liber solutus ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Empty, immune, free, unbound.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Laudatum etiam vos quenquam venire">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        laudatu[m] venire
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To come praising.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ecquae ciuitas est non modo in prouinciis nostris, verum in vltimis nationibus, aut tam pote[n]s, aut tam libera, aut etiam tam immanis ac barbara">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ciuitas [ potens libera immanis barbara ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A powerful, free, inhumane, [and] uncivilised city.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="reticebunt, quae poterunt, libenter: dicent quae necesse erit, gratis"/>
        <underline method="pen" type="straight" language="LA" text="vtina[m] negent. sunt etiam caetera, quae modo integra reseruare volui: vt quamminimum sit illis temporis ad meditandum"/>
        <underline method="pen" type="straight" language="LA" text="qui te neque debent adiuuare, si possint: neque possunt, si velint"/>
        <underline method="pen" type="straight" language="LA" text="in perpetuum infames tuis sturpis, flagiti&iacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="imperii vna in ciuitate imminutum est. Siculi"/>
        <underline method="pen" type="straight" language="LA" text="detractum"/>
        <underline method="pen" type="straight" language="LA" text="&amp; hoc plus impositu[m]"/>
        <underline method="pen" type="straight" language="LA" text="per istos"/>
        <underline method="pen" type="straight" language="LA" text="quae volebat, in nauem clam imponenda, occulte exportanda curabat"/>
        <underline method="pen" type="straight" language="LA" text="vacui, expertes, soluti ac liberi fuerunt"/>
        <underline method="pen" type="straight" language="LA" text="quem obtorta gula de conuiuio in vincula, atque in tenebras abripi iussit"/>
        <underline method="pen" type="straight" language="LA" text="rex denique"/>
        
