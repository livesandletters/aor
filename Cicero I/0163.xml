        <marginalia hand="Italian" anchor_text="inuidiosum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Invidiosus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Invidious.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="adductus erat in sermonem, inuidiam, vituperationem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        adductus erat in sermone[m] inuidia[m] vituperatio[n]em
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He had been made the subject of scandal, of unpopularity, of denunciation.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="venerat res in iudicium, atque discrimen">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        re[m] venire in [ iudiciu[m] discrime[n]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The matter came to court [and] investigation.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sese grauissima leuaret infamia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        sese g[ra]uiss[ima] lenare infamia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To free oneself from the greatest disgrace.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="poena? quid animaduersionis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        poena animadu[er]sio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Penalty, punishment.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="crimen domesticu[m] ac vernaculum: non ex tempore periculi tui constitutum: sed vetus excogitatum iam, &amp; te Praetore iactatu[m]: &amp; no[n] ab inimicis Romae compositu[m], 
            sed Romam de prouincia exportatum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        crime[n] { domesticu[m] ac vernaculu[m] co[n]stitutu[m] excogitatu[m] iactatu[m] co[m]positu[m] exportatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An offense: domestic, homegrown, established, devised, discussed, composed, carried out.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Iudices ta[m] seueros, ta[m] diligentes, ta[m] religiosos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Iudex seuer[us] diligens religiosus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A strict, diligent, [and] dutiful judge.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dissolutior in iudica[n]do">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        dissolutior in indicando
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Loser in indicating.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sedente[m] in faucibus macelli">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        sedere in faucib[us] macelli
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To sit at the entrance to the provision market.
            </translation>
        </marginalia>
        
