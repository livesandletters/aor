        <underline method="pen" type="straight" language="LA" text="Quid ego de praestantissimo viro T. Annio dicam? aut"/>
        <underline method="pen" type="straight" language="LA" text="ciuitatis nullas sibi inimicitias, nullam vim, nullos impetus, nullum vitae discrimen vitandu[m] vnquam putauit. qui"/>
        <underline method="pen" type="straight" language="LA" text="officiis iuxta, ac"/>
        <underline method="pen" type="straight" language="LA" text="quem ad salutem meam voluntas impulit, vis retardauit, authoritas vestra reuocauit"/>
        <underline method="pen" type="straight" language="LA" text="quae cu[m] libenter co[m]memoro, tum no[n] inuitus nonnulloru[m] in me nefarie co[m]missa praetereo"/>
        <underline method="pen" type="straight" language="LA" text="vita est, vt bene de me meritis referam gratiam, cum apertis hostibus bellum geram, timidis amicis ignoscam, proditoribus meis non indicem dolorem profectionis meae, 
            defensores, reditus dignitate consoler"/>
        <underline method="pen" type="straight" language="LA" text="dextrer&aacute;mque porrexit: qui me &agrave; morte ad vitam, &agrave; desperatione ad spem, ab exitio ad salutem reuocauit: qui tanto amore in me, 
            studio in Rempublicam fuit"/>
        <underline method="pen" type="straight" language="LA" text="enim magnifice[n]tius, quid praeclarius mihi"/>
        <underline method="pen" type="straight" language="LA" text="me vnu[m] hominem fractum, &amp; prop&egrave; dissipatum restituendum"/>
        
