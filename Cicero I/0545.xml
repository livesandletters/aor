    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="[nihil enim est tam molle tam tenerum, tam aut fragile, aut flexibile">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        nihil tam { molle teneru[m] fragile flexibile ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nothing so soft, tender, fragile and flexible.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="argumentor? quid plura disputo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        argume[n]tor disputo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I argue, I discuss.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &agrave; maiore
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Generally.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="anteuertit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        anteuertere. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To anticipate.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="insanissima concio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        insaniss[ima] co[n]cio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The most insane assembly.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cognomine">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        cognomine Schola.
                    </marginalia_text>
                    <person name="Gaius Causinius Schola"/>
                </position>
            </language>
            <translation>
                By the third name Schola.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="animam efflantem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        anima[m] efflare.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To breathe out the soul.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="properato opus esset">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        properato opus esset
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Hurry is needed.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quod caput audaciae est Iudices, quis ignorat, maxima[m] illecebram esse peccandi, impunitatis spem? In"/>
