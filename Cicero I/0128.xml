        <marginalia hand="Italian" anchor_text="vita[m] sese meo praesidio victuros esse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vivere vita[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To live life.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pecuniae coge[n]dae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Cogere pecunia[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To extort money.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nundination&eacute;mque hominu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        nu[n]dinatio ho[m]i[nu]m
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Trafficking of humans.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pecunia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Money.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="faciu[n]t hoc homines, quos in summa nequitia non solu[m] libido &amp; voluptas, verumetia[m] ipsius nequitiae fama delectat"/>
        <underline method="pen" type="straight" language="LA" text="Putarem me idoneu[m], qui exemplu[m] sequerer humanitateis atque officii, proponer&eacute;mque spe[m] meis hospitibus ac necessariis, 
            quo tutiorem vita[m] sese meo praesidio victuros esse arbitrare[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="vereri non debeo, nequis hoc quod facio, non existimet sumi officii ratione"/>
        <underline method="pen" type="straight" language="LA" text="coact&uacute;mque"/>
        <underline method="pen" type="straight" language="LA" text="quid ego dispute[m]"/>
        <underline method="pen" type="straight" language="LA" text="quid porr&ograve; argume[n]ter, qua de re dubitare nemo possit?"/>
        <underline method="pen" type="straight" language="LA" text="an hoc dubitabit quisqua[m] omnium, quin"/>
        <underline method="pen" type="straight" language="LA" text="quod enim est praeterea genus pecuniae coge[n]dae, quod praeteriit, quod non ab omnibus aliis praeteritum excogitauit? ecquae"/>
        <underline method="pen" type="straight" language="LA" text="quicu[m]que Senator voluerit fieri, qua[m]uis puer, qua[m]uis indignus, quamuis ex eo loco ex quo non liceret: si is precio apud istum fieret idoneus vt vinceret, factum esse"/>
        
