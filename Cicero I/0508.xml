    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="cumulus dierum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        cumulus dierum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A number of days.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="caeteribus omnibus esset omni honore antelatus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ceteris o[mn]ib[us] esse o[mn]i honore a[n]telatu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He was preferred to all distinction beyond all others.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tempus differebant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        differre tempus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put off a matter.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vt ea quae per eum affecta sunt, perfecta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vt ea que affecta su[n]t p[ro]fecta reddantur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That the accomplished matters that he aspired to do be returned.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="quid Oceano longius inueniri potest?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        quid oceano longius inveniri p[otes]t.
                    </marginalia_text>
                    <person name="Oceanus"/>
                </position>
            </language>
            <translation>
                What can be found more distant than the Ocean?
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="dies">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Dies
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Day.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="de prouincia deuocent">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        de p[ro]vincia deuocare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To recall from a province.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="populi charitatem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        populi charitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The charity of the people.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quada[m] nota atque ignominia noua co[n]demnastis"/>
        <underline method="pen" type="straight" language="LA" text="Vos sequor Patres Co[n]scripti, vobis obtempero, vobis assentior, qui qua[m]diu C. Caesaris co[n]silia in Rep. non maxime diligebatis, 
            me quoque cum illo minus coniunctum videbatis"/>
        <underline method="pen" type="straight" language="LA" text="comitem esse sententiae vestrae, sed etiam laudatorem"/>
        <underline method="pen" type="straight" language="LA" text="nisi vt ea quae per eum affecta sunt, perfecta"/>
        <underline method="pen" type="straight" language="LA" text="locorum, vrbium pulchritudo, hominum nation&uacute;mque illarum humanitas &amp; lepos, victoriae cupiditas, finium imperii propagatio retinet"/>
        <underline method="pen" type="straight" language="LA" text="quid Oceano"/>
        <underline method="pen" type="straight" language="LA" text="An dies auget eius desyderium, an magis obliuionem? ac laurea illa"/>
        
