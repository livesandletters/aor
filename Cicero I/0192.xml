    <page filename="0192.tif" reader="John Dee" pagination="164" signature="[ll.ii.[v]]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="praecerpo fructum officii">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        precepere fructu[m] officij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To anticipate the reward of the duty.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Est aliqua mea pars virilis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        est aliq[u]a me pars virilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In this right I have a share.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ciuitas sum quam ille amplam, illustrem, clar&aacute;mque">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Ciuitas [ ampla illustris clara ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A great, glorious, [and] famous community.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Cicero P. S. Affricano similis
                    </marginalia_text>
                    <person name="Cicero"/>
                    <person name="Scipio Africanus"/>
                </position>
            </language>
            <translation>
                Cicero is similar to Publius Scipio Africanus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="inuictissimi Imperatoris">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Invi[n]ctiss[im][us] imperator
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most invoncible commander.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="bonos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        BONI
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Honest men.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qu&agrave;m audacter, qu&agrave;m libidinose, qu&agrave;m impude[n]ter">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        audacter libidinios&egrave; impude[n]ter
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unscrupulously, wantonly, shamelessly.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="anniuersariis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        anniuersari[us]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Yearly festival.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Ne multa">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ne multa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Well!
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aliqua[n]to post">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        aliquanto post
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Some time later.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Quid aut his respo[n]dere honest&egrave;"/>
        <underline method="pen" type="straight" language="LA" text="omnia in te sunt quae aut fortuna"/>
        <underline method="pen" type="straight" language="LA" text="Non praecerpo fructum officii tui, no[n] alienam mihi laudem appeto"/>
        <underline method="pen" type="straight" language="LA" text="pudoris mei, P. Scipione flore[n]tissimo adolescente viuo &amp; incolumi, me propugnatorem monimentorum P. Scipionis, defensor&eacute;mque profiteri"/>
        <underline method="pen" type="straight" language="LA" text="No[n] est querendu[m] in ea ciuitate, quae propter virtute[m] omnibus nationibus imperat, virtutem plurimum posse. sit apud"/>
        <underline method="pen" type="straight" language="LA" text="Est aliqua mea pars virilis, qu&ograve;d eius ciuitatis"/>
        <underline method="pen" type="straight" language="LA" text="praecipue qu&ograve;d in his artibus pro mea parte"/>
        <underline method="pen" type="straight" language="LA" text="quarum ille princeps fuit, aequitate, industria, temperantia, defensione miserorum, odio"/>
        <underline method="pen" type="straight" language="LA" text="quae cognatio studioru[m] &amp; artium propemodu[m] non minus est coniuncta, qu&agrave;m ista qua vos delectamini, generis &amp; nominis. Repto"/>
        <underline method="pen" type="straight" language="LA" text="Non vereor ne hoc officiu[m] meum P. Seruilio iudici non probe[m], qui cum res maximas gesserit, monime[n]t&aacute;que"/>
        <underline method="pen" type="straight" language="LA" text="vereor ne tibi"/>
        <underline method="pen" type="straight" language="LA" text="quamplurimos esse custodes monimentoru[m], &amp; putare omnes bonos alienae gloriae defensione[m], ad officiu[m] pertinere"/>
        
