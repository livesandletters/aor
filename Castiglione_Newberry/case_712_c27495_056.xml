        <underline method="pen" type="straight" language="EN" text="the ignoraunte"/>
        <underline method="pen" type="straight" language="EN" text="disdeigne"/>
        <underline method="pen" type="straight" language="EN" text="should lye buried"/>
        <underline method="pen" type="straight" language="EN" text="he open it"/>
        <underline method="pen" type="straight" language="EN" text="defrauded of the estimation"/>
        <underline method="pen" type="straight" language="EN" text="auncient writers"/>
        <underline method="pen" type="straight" language="EN" text="muche excelleth"/>
        <underline method="pen" type="straight" language="EN" text="forbeare praisyng hymself"/>
        <underline method="pen" type="straight" language="EN" text="hauyng no skill"/>
        <underline method="pen" type="straight" language="EN" text="praysynge of a mans selfe impudently and withoute respecte"/>
        <underline method="pen" type="straight" language="EN" text="skilfull man that priaseth hymselfe dyscretely, but"/>
        <underline method="pen" type="straight" language="EN" text="for a more certaine witnes"/>
        <underline method="pen" type="straight" language="EN" text="out of an other mans mouth"/>
        <underline method="pen" type="straight" language="EN" text="praising himselfe falleth not into errour"/>
        <underline method="pen" type="straight" language="EN" text="purchaseth himself lothsomenes or hatred of the hearers"/>
        <underline method="pen" type="straight" language="EN" text="moste discrete"/>
        <underline method="pen" type="straight" language="EN" text="himselfe"/>
        <underline method="pen" type="straight" language="EN" text="very harde"/>
        <underline method="pen" type="straight" language="EN" text="auntient writers"/>
        <underline method="pen" type="straight" language="EN" text="taught"/>
        <underline method="pen" type="straight" language="EN" text="after a sort"/>
        <underline method="pen" type="straight" language="EN" text="not rehearse to that ende"/>
        <underline method="pen" type="straight" language="EN" text="come so to purpose"/>
        <underline method="pen" type="straight" language="EN" text="not refrayne tellyng"/>
        <underline method="pen" type="straight" language="EN" text="tell the trueth"/>
        <underline method="pen" type="straight" language="EN" text="open their mouthe"/>
        <underline method="pen" type="straight" language="EN" text="beyng pusshed"/>
        
