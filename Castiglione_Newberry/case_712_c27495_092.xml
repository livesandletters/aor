    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="true glorye"/>
        <underline method="pen" type="straight" language="EN" text="is stores vp"/>
        <underline method="pen" type="straight" language="EN" text="holy treasure of letters"/>
        <underline method="pen" type="straight" language="EN" text="so base a courage"/>
        <underline method="pen" type="straight" language="EN" text="actes and greatnesse"/>
        <underline method="pen" type="straight" language="EN" text="not incensed with a most feruent"/>
        <underline method="pen" type="straight" language="EN" text="be like them"/>
        <underline method="pen" type="straight" language="EN" text="perpetuall fame"/>
        <underline method="pen" type="straight" language="EN" text="lasteth twoo dayes"/>
        <underline method="pen" type="straight" language="EN" text="sauoureth not the sweetnesse of"/>
        <underline method="pen" type="straight" language="EN" text="greatnesse of glorye"/>
        <underline method="pen" type="straight" language="EN" text="perserued by them"/>
        <underline method="pen" type="straight" language="EN" text="this shorte glorye"/>
        <underline method="pen" type="straight" language="EN" text="is euerlastinge"/>
        <underline method="pen" type="straight" language="EN" text="not barred from"/>
        <underline method="pen" type="straight" language="EN" text="neuer hasard"/>
        <underline method="pen" type="straight" language="EN" text="hymself so much to come by it"/>
        <underline method="pen" type="straight" language="EN" text="the contrarye effectes"/>
        <underline method="pen" type="straight" language="EN" text="the Italians with"/>
        <underline method="pen" type="straight" language="EN" text="their knowleage of letters"/>
        <underline method="pen" type="straight" language="EN" text="small prowesse in armes"/>
        <underline method="pen" type="straight" language="EN" text="offence of a few"/>
        <underline method="pen" type="straight" language="EN" text="euerlasting reproche vnto all"/>
        <underline method="pen" type="straight" language="EN" text="our confusion"/>
        <underline method="pen" type="straight" language="EN" text="neglecting of vertue"/>
        <underline method="pen" type="straight" language="EN" text="dead"/>
        <underline method="pen" type="straight" language="EN" text="to publishe"/>
        <underline method="pen" type="straight" language="EN" text="Frenchmen the ignoraunce in letters"/>
        <underline method="pen" type="straight" language="EN" text="whom in letter I will haue"/>
        <underline method="pen" type="straight" language="EN" text="more then"/>
        
        <underline method="pen" type="straight" language="EN" text="true glorye"/>
        <underline method="pen" type="straight" language="EN" text="Noble courages enflamed in readyng the actes of famous captaines"/>
