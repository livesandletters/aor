        <underline method="pen" type="straight" language="EN" text="the fairest condicioned man in"/>
        <underline method="pen" type="straight" language="EN" text="thus baited and iested"/>
        <underline method="pen" type="straight" language="EN" text="ignoraunt"/>
        <underline method="pen" type="straight" language="EN" text="and beehauiours"/>
        <underline method="pen" type="straight" language="EN" text="tymes commended with acclamation"/>
        <underline method="pen" type="straight" language="EN" text="at his boording"/>
        <underline method="pen" type="straight" language="EN" text="certein cartarlike iestes"/>
        <underline method="pen" type="straight" language="EN" text="to vomite"/>
        <underline method="pen" type="straight" language="EN" text="then to laugh"/>
        <underline method="pen" type="straight" language="EN" text="fauoures and disfauoures of great men"/>
        <underline method="pen" type="straight" language="EN" text="worthinesse"/>
        <underline method="pen" type="straight" language="EN" text="himselfe with witt"/>
        <underline method="pen" type="straight" language="EN" text="and art"/>
        <underline method="pen" type="straight" language="EN" text="where he is straunge"/>
        <underline method="pen" type="straight" language="EN" text="not knowen"/>
        <underline method="pen" type="straight" language="EN" text="him procure there goe first a good opinion of him"/>
        <underline method="pen" type="straight" language="EN" text="beefore he come in person"/>
        <underline method="pen" type="straight" language="EN" text="so woork"/>
        <underline method="pen" type="straight" language="EN" text="they maie vnderstand"/>
        <underline method="pen" type="straight" language="EN" text="howe he is in other places with Lordes"/>
        <underline method="pen" type="straight" language="EN" text="Ladyes and"/>
        <underline method="pen" type="straight" language="EN" text="in good estimation"/>
        <underline method="pen" type="straight" language="EN" text="fame"/>
        <underline method="pen" type="straight" language="EN" text="of the iudgementes of many"/>
        <underline method="pen" type="straight" language="EN" text="assured confidence of a mans worthinesse"/>
        <underline method="pen" type="straight" language="EN" text="easily with deedes mainteined"/>
        <underline method="pen" type="straight" language="EN" text="eased of the trouble"/>
        <underline method="pen" type="straight" language="EN" text="who I am"/>
        <underline method="pen" type="straight" language="EN" text="what is my name"/>
        <underline method="pen" type="straight" language="EN" text="Bibiena"/>
        <underline method="pen" type="straight" language="EN" text="great perfection beefore"/>
        <underline method="pen" type="straight" language="EN" text="feinted"/>
        <underline method="pen" type="straight" language="EN" text="muche deceiued in"/>
        <underline method="pen" type="straight" language="EN" text="geuing to muche credit to fame and"/>
        <underline method="pen" type="straight" language="EN" text="the effecte, thoughe"/>
        
        <underline method="pen" type="straight" language="EN" text="Ignoraunt"/>
        <underline method="pen" type="straight" language="EN" text="Report deceiueth"/>
        
