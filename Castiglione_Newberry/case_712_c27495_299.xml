    <annotation>  
        
        <underline method="pen" type="straight" language="EN" text="he be discreete"/>
        <underline method="pen" type="straight" language="EN" text="his blockishenes"/>
        <underline method="pen" type="straight" language="EN" text="an other"/>
        <underline method="pen" type="straight" language="EN" text="be namelesse for the honour of men"/>
        <underline method="pen" type="straight" language="EN" text="tell vs at the least"/>
        <underline method="pen" type="straight" language="EN" text="priuilye to"/>
        <underline method="pen" type="straight" language="EN" text="where she laye"/>
        <underline method="pen" type="straight" language="EN" text="time and leyser"/>
        <underline method="chalk" type="straight" language="EN" text="That she woulde discharge his ynn, for sins he came thither at her request, he thought 
            meete [that] he should not stand to [the] charges of his beeing there himself."/>
        <underline method="chalk" type="straight" language="EN" text="beegan all the Ladies to laugh, and said that"/>
        <underline method="pen" type="straight" language="EN" text="most vnwoorthy"/>
        <underline method="pen" type="straight" language="EN" text="a Gentilman"/>
        <underline method="pen" type="straight" language="EN" text="for the honour of women"/>
        <underline method="pen" type="straight" language="EN" text="naming of him for the honour of"/>
        <underline method="pen" type="straight" language="EN" text="tht great Ladie"/>
        <underline method="chalk" type="straight" language="EN" text="louinge so vnreasonable a creature"/>
        <underline method="pen" type="straight" language="EN" text="she chose him for the"/>
        <underline method="pen" type="straight" language="EN" text="showinge ill wil"/>
        <underline method="pen" type="straight" language="EN" text="woorthie to wayte vpon"/>
        <underline method="pen" type="straight" language="EN" text="was out of the waye onlye about ynnes"/>
        <underline method="pen" type="straight" language="EN" text="for ouermuch loue men committ"/>
        <underline method="pen" type="straight" language="EN" text="of good felowshippe"/>
        <underline method="pen" type="straight" language="EN" text="vs not discouer oure wone ousersightes"/>
        <underline method="pen" type="straight" language="EN" text="Blockish"/>
        <underline method="pen" type="straight" language="EN" text="commit"/>
        <underline method="pen" type="straight" language="EN" text="folies."/>
        
