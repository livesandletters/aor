        <underline method="pen" type="straight" language="EN" text="[the] female that hath not a male"/>
        <underline method="pen" type="straight" language="EN" text="one kinde alone"/>
        <underline method="pen" type="straight" language="EN" text="betokeneth an imperfection"/>
        <underline method="pen" type="straight" language="EN" text="of olde"/>
        <underline method="pen" type="straight" language="EN" text="referr both the one and"/>
        <underline method="pen" type="straight" language="EN" text="to God"/>
        <underline method="pen" type="straight" language="EN" text="Iupiter was both male and femae"/>
        <underline method="pen" type="straight" language="EN" text="facioned male and female to his"/>
        <underline method="pen" type="straight" language="EN" text="meddle the kindes"/>
        <underline method="pen" type="straight" language="EN" text="these subtill pointes"/>
        <underline method="pen" type="straight" language="EN" text="that I am in the wrong"/>
        <underline method="pen" type="straight" language="EN" text="is likened to the Fourme"/>
        <underline method="pen" type="straight" language="EN" text="woman to the Mattier"/>
        <underline method="pen" type="straight" language="EN" text="Fourme"/>
        <underline method="pen" type="straight" language="EN" text="perfecter then the"/>
        <underline method="pen" type="straight" language="EN" text="him his beeing"/>
        <underline method="pen" type="straight" language="EN" text="in certein Problemes"/>
        <underline method="pen" type="straight" language="EN" text="naturally"/>
        <underline method="pen" type="straight" language="EN" text="the man"/>
        <underline method="pen" type="straight" language="EN" text="bine the first to receiue of her"/>
        <underline method="pen" type="straight" language="EN" text="amorous pleasures"/>
        <underline method="pen" type="straight" language="EN" text="hateth the woman"/>
        <underline method="pen" type="straight" language="EN" text="to coople"/>
        <underline method="pen" type="straight" language="EN" text="this act"/>
        <underline method="pen" type="straight" language="EN" text="woman receyueth of the man"/>
        <underline method="pen" type="straight" language="EN" text="perfection"/>
        <underline method="pen" type="straight" language="EN" text="the woman imperfection"/>
        <underline method="pen" type="straight" language="EN" text="him perfect"/>
        <underline method="pen" type="straight" language="EN" text="maketh him vnperfect"/>
        <underline method="pen" type="straight" language="EN" text="euerye woman wisheth she were a man"/>
        <underline method="pen" type="straight" language="EN" text="wishe for"/>
        <underline method="pen" type="straight" language="EN" text="wish not"/>
        <underline method="pen" type="straight" language="EN" text="a man to"/>
        <underline method="pen" type="straight" language="EN" text="haue libertye,"/>
        
        <underline method="pen" type="straight" language="EN" text="male can not"/>
        <underline method="pen" type="straight" language="EN" text="female."/>
        
        <mark name="quotation_mark" method="pen" place="head"/>           
        
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="male can not be without female"/>          
        <mark name="plus_sign" method="pen" place="left_margin"/>      
        <mark name="X_sign" method="pen" place="left_margin"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="For that in this act, the woman receyueth of the man perfection, and the man of the woman 
            imperfection: and therefore euerie man naturallye loueth that maketh him perfect, and hateth that maketh him vnperfect."/>
        <mark name="plus_sign" method="pen" place="left_margin" language="EN" text="euerye woman wisheth she were a man"/>      
        
