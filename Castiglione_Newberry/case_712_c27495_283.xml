    <annotation>  
        
        <marginalia hand="Italian" anchor_text="threw herselfe into the riuer">
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        How [the] Gentlewoman is to Behaue herself towards Her Loouers.
                    </marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="malapert"/>
        <underline method="pen" type="straight" language="EN" text="smalle respect to her in his"/>
        <underline method="pen" type="straight" language="EN" text="shall shape him such"/>
        <underline method="pen" type="straight" language="EN" text="she is not pleased withall"/>
        <underline method="pen" type="straight" language="EN" text="demure and vseth"/>
        <underline method="pen" type="straight" language="EN" text="facions"/>
        <underline method="pen" type="straight" language="EN" text="woordes of loue couertlie"/>
        <underline method="pen" type="straight" language="EN" text="suche honest"/>
        <underline method="pen" type="straight" language="EN" text="beeleaue the Courtier"/>
        <underline method="pen" type="straight" language="EN" text="not to vnderstand him"/>
        <underline method="pen" type="straight" language="EN" text="woordes to another sense"/>
        <underline method="pen" type="straight" language="EN" text="discretion snd wisdome"/>
        <underline method="pen" type="straight" language="EN" text="stray from"/>
        <underline method="pen" type="straight" language="EN" text="feigne not to vnderstande"/>
        <underline method="pen" type="straight" language="EN" text="a meerie diuise"/>
        <underline method="pen" type="straight" language="EN" text="to honour her withall"/>
        <underline method="pen" type="straight" language="EN" text="debasinge her desertes"/>
        <underline method="pen" type="straight" language="EN" text="courtesie the prayses"/>
        <underline method="pen" type="straight" language="EN" text="counted discreete"/>
        <underline method="pen" type="straight" language="EN" text="surer hande for beeinge deceiued"/>
        <underline method="pen" type="straight" language="EN" text="Thus me seemeth the Gentilwoman of the Palaice ought to behaue herself"/>
        <underline method="pen" type="straight" language="EN" text="communication of loue"/>
        <underline method="pen" type="straight" language="EN" text="speake with women"/>
        <underline method="pen" type="straight" language="EN" text="shoulde tell lyes"/>
        <underline method="pen" type="straight" language="EN" text="say your lessons"/>
        <underline method="pen" type="straight" language="EN" text="enterteineth"/>
        <underline method="pen" type="straight" language="EN" text="in very deere"/>
        <underline method="pen" type="straight" language="EN" text="feeleth the passion that"/>
        <underline method="pen" type="straight" language="EN" text="at no time you"/>
        <underline method="pen" type="straight" language="EN" text="shall beeleaue him in any thinge"/>
        <underline method="pen" type="straight" language="EN" text="Shall othes, teares, and so"/>
        <underline method="pen" type="straight" language="EN" text="beeside the naturall crueltye whiche"/>
        <underline method="pen" type="straight" language="EN" text="them yet more. The"/>
        <underline method="pen" type="straight" language="EN" text="not of him that loueth"/>
        <underline method="pen" type="straight" language="EN" text="him that enterteineth"/>
        
        <mark name="equal_sign" method="pen" place="left_margin" language="EN" text="malapert"/>
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="[She] shall shape him such an answere, that he shall plainly vnderstande she is not pleased 
            withall."/>
