    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="without him all the other"/>
        <underline method="chalk" type="straight" language="EN" text="myndes were well tylled, and of theym speake I"/>
        <underline method="chalk" type="straight" language="EN" text="albe to frame their minde to a right trade"/>
        <underline method="pen" type="straight" language="EN" text="vertue"/>
        <underline method="pen" type="straight" language="EN" text="consisteth in doing &amp; practise"/>
        <underline method="pen" type="straight" language="EN" text="iustice"/>
        <underline method="pen" type="straight" language="EN" text="noble courage"/>
        <underline method="pen" type="straight" language="EN" text="his greatnesse"/>
        <underline method="pen" type="straight" language="EN" text="put in cse"/>
        <underline method="pen" type="straight" language="EN" text="make it custome"/>
        <underline method="pen" type="straight" language="EN" text="whiche the Courtier"/>
        <underline method="pen" type="straight" language="EN" text="no meanes to practise"/>
        <underline method="pen" type="straight" language="EN" text="the Prince incluned to vertue"/>
        <underline method="pen" type="straight" language="EN" text="more vertuous then the"/>
        <underline method="chalk" type="straight" language="EN" text="that, you muste conceyue that the whettstone which cutteth not a whitt, doeth yet make a toole sharpe: therefore althrough the 
            Courtier instructeth his Prince"/>
        <underline method="pen" type="straight" language="EN" text="the whettstone"/>
        <underline method="pen" type="straight" language="EN" text="a toole sharpe"/>
        <underline method="pen" type="straight" language="EN" text="ende of this Courtier is harde"/>
        <underline method="pen" type="straight" language="EN" text="vnpossible"/>
        <underline method="pen" type="straight" language="EN" text="hardenesse, bicause"/>
        <underline method="pen" type="straight" language="EN" text="out so excellent a Courtier"/>
        <underline method="pen" type="straight" language="EN" text="by such an ende"/>
        <underline method="pen" type="straight" language="EN" text="many thinges to be"/>
        <underline method="pen" type="straight" language="EN" text="yonge"/>
        <underline method="pen" type="straight" language="EN" text="Prince t be so wise"/>
        <underline method="pen" type="straight" language="EN" text="needeth no exhortations"/>
