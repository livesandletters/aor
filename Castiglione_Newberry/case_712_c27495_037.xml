        <marginalia hand="Italian">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Egregia Praxis perferendi malam, fortunam: ut s[upr]a, in Duce marito, Guidubaldo.</marginalia_text>
                    <person name="Guidobaldo da Montefeltro"/>
                    <internal_ref text="s[upr]a">
                        <target filename="case_712_c27495_035.xml" book_id="Newberry27495" text="[Aiii]"/>
                    </internal_ref>
                </position>
            </language>
            <translation>The wonderful practice of enduring bad fortune: as above, in the married Duke, Guidobaldo.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="contentacyon"/>
        <underline method="pen" type="straight" language="EN" text="into the dutchesse"/>
        <underline method="pen" type="straight" language="EN" text="a chaine that kept all lincked together in loue"/>
        <underline method="pen" type="straight" language="EN" text="agrement of wyll or hearty loue greater betweene brethren"/>
        <underline method="pen" type="straight" language="EN" text="was there beetweene vs all"/>
        <underline method="pen" type="straight" language="EN" text="the women"/>
        <underline method="pen" type="straight" language="EN" text="free and honest conuersation"/>
        <underline method="pen" type="straight" language="EN" text="myght commune, syt, daly, and laugh with whome he had lusted"/>
        <underline method="pen" type="straight" language="EN" text="Dutchesse wyll"/>
        <underline method="pen" type="straight" language="EN" text="libertye was"/>
        <underline method="pen" type="straight" language="EN" text="great bridle"/>
        <underline method="pen" type="straight" language="EN" text="there anye that"/>
        <underline method="pen" type="straight" language="EN" text="greatest pleasure he coulde haue in the worlde"/>
        <underline method="pen" type="straight" language="EN" text="please her"/>
        <underline method="pen" type="straight" language="EN" text="the greatest griefe to offende her"/>
        <underline method="pen" type="straight" language="EN" text="deuises of pastimes"/>
        <underline method="pen" type="straight" language="EN" text="laughine matters tempred in her"/>
        <underline method="pen" type="straight" language="EN" text="wyttye iestes"/>
        <underline method="pen" type="straight" language="EN" text="comelye and graue a"/>
        <underline method="pen" type="straight" language="EN" text="moode and greatnesse that"/>
        <underline method="pen" type="straight" language="EN" text="iesting and laughynge"/>
        <underline method="pen" type="straight" language="EN" text="seene her in their lief before"/>
        <underline method="pen" type="straight" language="EN" text="count her a verye greate Ladye"/>
        <underline method="pen" type="straight" language="EN" text="had made them to her becke"/>
        <underline method="pen" type="straight" language="EN" text="and ensample"/>
        <underline method="pen" type="straight" language="EN" text="faire condicions"/>
        <underline method="pen" type="straight" language="EN" text="presence of so greate and so vertuous a Lady"/>
        <underline method="pen" type="straight" language="EN" text="fortune"/>
        <underline method="pen" type="straight" language="EN" text="wondreth at so rare vertues"/>
        <underline method="pen" type="straight" language="EN" text="with many aduersities and temptatyons of miseries to disclose them"/>
        <underline method="pen" type="straight" language="EN" text="trial thereby"/>
        
