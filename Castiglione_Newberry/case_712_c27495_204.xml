    <annotation>  
        
        <underline method="pen" type="straight" language="EN" text="in Meerie Pranckes"/>
        <underline method="pen" type="straight" language="EN" text="to expectacion"/>
        <underline method="pen" type="straight" language="EN" text="more delite and"/>
        <underline method="pen" type="straight" language="EN" text="wittie and modest"/>
        <underline method="pen" type="straight" language="EN" text="without respect"/>
        <underline method="pen" type="straight" language="EN" text="the places that"/>
        <underline method="pen" type="straight" language="EN" text="verie same that"/>
        <underline method="pen" type="straight" language="EN" text="there be two kyndes of Meerie Pranckes"/>
        <underline method="pen" type="straight" language="EN" text="any man whoeuer he be"/>
        <underline method="pen" type="straight" language="EN" text="deceyued wittilie, and"/>
        <underline method="pen" type="straight" language="EN" text="after a feat maner"/>
        <underline method="pen" type="straight" language="EN" text="manne layeth"/>
        <underline method="pen" type="straight" language="EN" text="a nett"/>
        <underline method="pen" type="straight" language="EN" text="and showeth a piece of a bayte so"/>
        <underline method="pen" type="straight" language="EN" text="deceyued of himself"/>
        <underline method="pen" type="straight" language="EN" text="The first is"/>
        <underline method="pen" type="straight" language="EN" text="of a Spaniarde called Castilio"/>
        <underline method="pen" type="straight" language="EN" text="you not name them"/>
        <underline method="pen" type="straight" language="EN" text="them to take it in yll"/>
        <underline method="pen" type="straight" language="EN" text="againste good maner"/>
        <underline method="pen" type="straight" language="EN" text="to vse"/>
        <underline method="pen" type="straight" language="EN" text="with great men"/>
        <underline method="pen" type="straight" language="EN" text="bine played to Duke Fridericke"/>
        <underline method="pen" type="straight" language="EN" text="kinge Alphonsus"/>
        <underline method="pen" type="straight" language="EN" text="Queene Isabel of Spaine"/>
        <underline method="pen" type="straight" language="EN" text="manie other great Princis, and not"/>
        <underline method="pen" type="straight" language="EN" text="rewarded very largely them"/>
        <underline method="pen" type="straight" language="EN" text="neyther vpon this hope do I entend"/>
        <underline method="pen" type="straight" language="EN" text="name"/>
        <underline method="pen" type="straight" language="EN" text="Court that I meane"/>
        <underline method="pen" type="straight" language="EN" text="Bergamo"/>
        <underline method="pen" type="straight" language="EN" text="with garmentes and so finely clad"/>
        <underline method="pen" type="straight" language="EN" text="in keapinge"/>
        <underline method="pen" type="straight" language="EN" text="had not hearde"/>
        
        <symbol name="SS" place="head" language="EN" text="to expectacion"/>
        
        <symbol name="SS" place="left_margin"/>
        
        <mark name="plus_sign" method="pen" place="head" language="EN" text="doe"/>
        
        <mark name="bracket" method="pen" place="left_margin" language="EN" text="The one is, whan any man whoeuer he be, is deceyued wittilie, and after a feat maner and with 
            pleasantnesse. The other, whan a manne layeth (as it were) a nett, an showeth a piece of a bayte so, that a man renneth to be deceyued of himself."/>
        <mark name="plus_sign" method="pen" place="left_margin"/>
        <mark name="vertical_bar" method="pen" place="left_margin" language="EN" text="And I haue heard of manie that haue bine played to Duke Fridericke, to kinge Alphonsus of Aragon, 
            to Queene Isabel of Saine, and to manie other great Princis, and not onlie they tooke it not in ill part, but rewarded very largely them that plaied them these partes. M Bernarde 
            answered: neyther vpon this hope to I entend to name them."/>
        <mark name="equal_sign" method="pen" place="left_margin" language="EN" text="with great men"/>
        <mark name="straight_quotation_mark" method="pen" place="left_margin"/>
        
        <mark name="comma" method="pen" place="right_margin"/> <!-- from -->
        <mark name="quotation_mark" method="pen" place="right_margin" language="EN" text="neyther vpon this hope to I entend to name them."/>
        <mark name="quotation_mark" method="pen" place="right_margin" language="EN" text="with garmentes and so finely clad"/>
        
        <mark name="vertical_bar" method="chalk" place="intext"/>
        <mark name="dot" method="pen" place="intext"/> <!-- laughter, so -->
        <mark name="equal_sign" method="pen" place="intext" language="EN" text="wittie and modest"/>
        <mark name="equal_sign" method="pen" place="intext" language="EN" text="without respect"/>
        <mark name="comma" method="pen" place="intext"/> <!-- offende, and -->
        <mark name="dot" method="pen" place="intext"/> <!-- Pranckes, euerye -->
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="deceyued wittilie, and"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="deceyued of himself"/>
        <mark name="dot" method="pen" place="intext"/> <!-- Princis, and -->
        <mark name="vertical_bar" method="chalk" place="intext"/>
        <mark name="dot" method="pen" place="intext" language="EN" text="it"/>
        <mark name="dot" method="pen" place="intext" language="EN" text="in"/> 
        <mark name="dot" method="pen" place="intext" language="EN" text="ill"/> 
        <mark name="dot" method="pen" place="intext" language="EN" text="this"/>
        
        <numeral place="left_margin" language="EN" text="The one is, whan any man whoeuer he be, is deceyued wittilie, and after a feat maner and with pleasantnesse.">1.</numeral>        
        <numeral place="left_margin" language="EN" text="The other, whan a manne layeth (as it were) a nett, an showeth a piece of a bayte so, that a man renneth to be deceyued 
            of himself.">2.</numeral>  
        
