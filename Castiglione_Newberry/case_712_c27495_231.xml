        <underline method="pen" type="straight" language="EN" text="ought to entertein"/>
        <underline method="pen" type="straight" language="EN" text="those many thinges be whiche"/>
        <underline method="pen" type="straight" language="EN" text="whether wisedome"/>
        <underline method="pen" type="straight" language="EN" text="and those manye"/>
        <underline method="pen" type="straight" language="EN" text="ouerseeing onlie of her house"/>
        <underline method="pen" type="straight" language="EN" text="els to entertein, and"/>
        <underline method="pen" type="straight" language="EN" text="with a good grace"/>
        <underline method="chalk" type="straight" language="EN" text="good felowship take heede ye put not"/>
        <underline method="pen" type="straight" language="EN" text="these seelie vertues"/>
        <underline method="pen" type="straight" language="EN" text="vttre youre yll stomake againste"/>
        <underline method="pen" type="straight" language="EN" text="concernynge the eexercises of the"/>
        <underline method="pen" type="straight" language="EN" text="woman"/>
        <underline method="chalk" type="straight" language="EN" text="women wrastled naked with men"/>
        <underline method="pen" type="straight" language="EN" text="seene women"/>
        <underline method="pen" type="straight" language="EN" text="practice geates of armes, ride, hunt"/>
        <underline method="pen" type="straight" language="EN" text="not to practise these manlie exerices"/>
        <underline method="pen" type="straight" language="EN" text="so sturdie and boisterous"/>
        <underline method="pen" type="straight" language="EN" text="with heedefulnesse"/>
        <underline method="pen" type="straight" language="EN" text="soft mildenesse that we haue said is comelie"/>
        <underline method="pen" type="straight" language="EN" text="in daulsynge"/>
        <underline method="pen" type="straight" language="EN" text="vse so swift &amp;"/>
        <underline method="pen" type="straight" language="EN" text="in singinge"/>
        <underline method="pen" type="straight" language="EN" text="those harde and often diuisions that declare"/>
        <underline method="pen" type="straight" language="EN" text="counninge then sweetenesse"/>
        <underline method="pen" type="straight" language="EN" text="Daunsing. Singinge. Speculation of musike."/>
        <underline method="pen" type="straight" language="EN" text="Instrumentes of musike."/>
        
