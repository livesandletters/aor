    <page filename="017.tif" reader="John Dee" signature="[Aiiij[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="om[n]e copula[n]s p[er] modu[m] co[n]tinuatio[n]is. indiuisibile est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Omne copulans p[er] modu[m] co[n]tinuationis, indiuisibile e[st].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Everything connecting through a method of continuation, is inseparable.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="linee se tangu[n]t in punctis indiuisibilibus et sup[er]ficies lineis et corp[or]a sup[er]ficiebus et t[em]p[or]a instantaneis, et loca reple[n]dis corporib[us]"> <!-- FSF: not sure about tempora -->
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Nota, loca se tangere mutuo p[er] reple[n]da corpora
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Note that the places go back to meet themselves in order to complete the structures.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qui[n]q[ue] regularia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quinq[ue] regularia corpora
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Five regular bodies.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tempus esse q[ua]ntitate[m] co[n]tinua[m] q[uo]d quide[m] methaphisici est">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Temp[us] est Metaphysici 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Time for methaphysics.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="c[om]munis t[er]minus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Terminus co[m]munis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The common term.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tange[n]cia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Tange[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Tangents.          
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt infra declarabit[ur]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        infra.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Below.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="locu[m] mathematicu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Locus mathematicus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A mathematical locus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Locus vero phisic[us]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Locus physicus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A physical locus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="puncto repletionis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Punctu[m] repletionis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A point of completion.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="replere locu[m] mathematice">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Replere locu[m] mathematice
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To complete a place mathematically.         <!-- DO: Again, do you think all these instances of 'locum' refer to a (mathematical) locus? --> <!-- FSF: I am not sure about this! -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Locus dupl[e]x
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A twofold place.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="positio p[ro]prie p[ar]tium mathematice">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Positio partiu[m] mathematic&egrave;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A mathematical positioning of parts.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quide[m] methaphisici est"/>
        <underline method="pen" type="straight" language="LA" text="et est aliquid vtriusq[ue] et neutrius per se, et"/>
        <underline method="pen" type="straight" language="LA" text="om[n]e"/>
        <underline method="pen" type="straight" language="LA" text="co[n]tinuatio[n]is. indiuisibile est"/>
        <underline method="pen" type="straight" language="LA" text="Sunt aut[em] tange[n]cia eor[um] quor[um] vltima sunt simul et p[ri]ma [et] neutri[us] per se. vt dixim[us]"/>
        <underline method="pen" type="straight" language="LA" text="reple[n]dis corporib[us]"/>
        <underline method="pen" type="straight" language="LA" text="p[er]manentia[m] et positione[m] no[n] habeat in p[ar]tibus que[m]admodu[m] numer[us] poci[us] [et] ordine[m]. postea"/>
        <underline method="pen" type="straight" language="LA" text="tanq[uam] eunde[m] replentes s[e]c[un]d[u]m"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mune[m] t[er]minu[m] corp[or]is"/>
        <underline method="pen" type="straight" language="LA" text="Sic loci particule que sunt particule corp[or]is locati co[n]tinuant[ur]"/>
        <underline method="pen" type="straight" language="LA" text="mediante puncto"/>
        <underline method="pen" type="straight" language="LA" text="loci"/>
        <underline method="pen" type="straight" language="LA" text="distantie"/>
        <underline method="pen" type="straight" language="LA" text="intersecant tres linee dyametrales ad vigintiquatuor rectos angulos et s[e]c[un]d[u]m totidem sup[er]ficies"/>
        <underline method="pen" type="straight" language="LA" text="spaciu[m] circu[m]stans q[uo]dlibet punctu[m] in sublimi localiter"/>
        <underline method="pen" type="straight" language="LA" text="cubos solu[m] replere locum quare locu[m] trib[us] dimensio[n]ibus"/>
        <underline method="pen" type="double" language="LA" text="et no[n]"/>
        <underline method="pen" type="double" language="LA" text="in de celo"/>
        <underline method="pen" type="straight" language="LA" text="q[uod] positio p[ro]prie p[ar]tium mathematice co[n]siderat[ur]"/>

