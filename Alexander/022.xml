        <underline method="pen" type="straight" language="LA" text="si est scibilis"/>
        <underline method="pen" type="straight" language="LA" text="ex quindecima tercij geometrie"/>
        <underline method="pen" type="straight" language="LA" text="quia est"/>
        <underline method="pen" type="straight" language="LA" text="Si quadratura circuli scibilis est necessario ab aduersario dicat[ur] curuum recto vniri per lieam"/>
        <underline method="pen" type="straight" language="LA" text="media et non per equalem angulum rectum"/>
        <underline method="pen" type="straight" language="LA" text="et itur per omnia media et nunq[uam] per equale. S[ed]"/>
        <underline method="pen" type="straight" language="LA" text="si angulus contingentie esset diuisibilis per lineam rectam aliquod scibile esse de quadratura quare dicit ph[ilosoph]us in p[ro]posito si est scibilis"/>
        <underline method="pen" type="straight" language="LA" text="q[uo]d tame[n] non est"/>
        <underline method="pen" type="straight" language="LA" text="multi male opinant[ur] de quadratura et diuersas opiniones"/>
        <underline method="pen" type="straight" language="LA" text="fieri per lineam elicam"/>
        <underline method="pen" type="straight" language="LA" text="cuse"/>
        <underline method="pen" type="straight" language="LA" text="differunt speciebus. Ita etiam figuris"/>
        <underline method="pen" type="straight" language="LA" text="etia[m] est vna figura"/>
        <underline method="pen" type="straight" language="LA" text="Quadratura negati[u]e euclidis"/>
        
