    <page filename="025.tif" reader="John Dee" signature="[Bij[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="quare dixeru[n]t impares semper esse i[m]mutabilis sub[stantia]e">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Impares numeri q[ua]re i[m]mutabilis substa[n]tia[e] putaba[n]tur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Odd numbers were therefire considered unchangeable substances.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="alteru[m] vero nihil factum est">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        [id est] nihil est inde alteratu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                That is, nothing was changed thereupon.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="gnomo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Gnomo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A gnomon.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        2&o_super; Euclidis
                    </marginalia_text>
                    <person name="Euclid"/>
                </position>
            </language>
            <translation>
                Euclid's second defintion [of geometry].               
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="antiqui omnes impares numeros quos gnomones dicebant">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Impares numeri ab antiquis gnomones diceba[n]tur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Odd numbers were called gnomons by the old [masters].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pithagoricos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pithagoras
                    </marginalia_text>
                    <person name="Pythagoras"/>
                </position>
            </language>
            <translation>
                Pythagoras.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Omnes aute[m] pares numeros s[e]c[un]d[u]m pithagoricos esse causas alterat[i]o[n]is">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pares numeri vt su[n]t alteratio[n]is causa[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Equal numbers are like causes of change.  <!-- DO: Not sure about translation. 'are like causes of change'? --> <!-- FSF: not 100% sure either -->
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omnis silogismus ad impossibile falsum silogisat ex dato opposito co[n]clusionis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Syllogismus ad impossibile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A syllogism to the impossible.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dyameter">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Diameter
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Diameter.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="linea transuersalis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Diagonalis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Diagonal.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Costa">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Costa
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Sides.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="sed ip[s]a relatio [et] respect[us] est causa vtriusq[ue] [et] sic naturaliter"/>
        <underline method="pen" type="straight" language="LA" text="alteru[m]"/>
        <underline method="pen" type="straight" language="LA" text="quia de figura in figura[m] siue de forma in formam qualitatis ta[m] in angulis q[uam] lateribus non est mutatu[m]"/>
        <underline method="pen" type="straight" language="LA" text="alterationis causa[m] dixerunt antiqui omnes impares numeros quos gnomones dicebant"/>
        <underline method="pen" type="straight" language="LA" text="quare dixeru[n]t impares semper esse i[m]mutabilis sub[stantia]e. et"/>
        <underline method="pen" type="straight" language="LA" text="re naturam eiusdem immutabilis substantie"/>
        <underline method="pen" type="straight" language="LA" text="esse causas alterat[i]o[n]is"/>
        <underline method="pen" type="straight" language="LA" text="qui prim[us] nu[m]erus alter factus ab vnitate"/>     
        <underline method="pen" type="straight" language="LA" text="vnitatis naturam immutabilis sub[stantia]e"/>
        <underline method="pen" type="straight" language="LA" text="p[ar]ticipes [et] omnes quadratos et gnomones qui"/>
        <underline method="pen" type="straight" language="LA" text="ostendere aliquid sequi impossibile. p[ro]pter priorem datam ypothesim in opposito prime conclusionis. vt si"/>
        <underline method="pen" type="straight" language="LA" text="sunt imparia sunt equalia perfectis id est parib[us]"/>
        <underline method="pen" type="straight" language="LA" text="linea tra[n]suersalis p[ro]tracta in q[ua]drato equilatere"/>
        <underline method="pen" type="straight" language="LA" text="eq[ua]les ex q[ua]rta"/>
        
