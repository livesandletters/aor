        <underline method="pen" type="straight" language="LA" text="est q[ua]n[do] totu[m] p[re]dicatu[m] est q[uod] quid est v[e]l totu[m] vel pars quidditatis subiecti id est"/>
        <underline method="pen" type="straight" language="LA" text="sic linea p[er] se inest triangulo"/>
        <underline method="pen" type="straight" language="LA" text="om[n]i hic"/>
