<?xml version="1.0" encoding="UTF-8"?>
 
<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="024.tif" reader="John Dee" signature="Bij"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="p[ar]te altera lo[n]gior semp[er] mediu[m] p[ro]portio[n]ale int[er] duo q[ua]drata">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Altera parte lo[n]gior se[m]per mediu[m] proportionale inter duo q[ua]drata
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the other part the proportional middle is always longer between two squares.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>[..air] dispositio.</marginalia_text>
                </position>
            </language>
            <translation>[...] disposition.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="rep[re]hendit[ur] in seque[n]tib[us] opinio antipho[n]tis qui dicebat vltima[m] polligonia[m] figura[m] inscriptam circulo">  
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Antipho[n]tis r[ati]o improbatur
                    </marginalia_text>
                    <person name="Antiphon"/>
                </position>
            </language>
            <translation>
                Antiphon's reasoning is rejected.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="similitudo in figuris mathematicis decimo methaphisice disputabit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Similitudo in figuris mathematicis   
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Similarity in the mathematical figures.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Na[m] equale [et] ineq[ua]le adinuice[m] vt p[er] modu[m] quanti. 
            simile vero [et] dissimile p[er] modu[m] vt quale">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        qua[n]tu[m] quale
                    </marginalia_text>
                </position>
            </language>
            <translation>
                How much, what kind.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt sexto geometrie sunt o[mn]es figure mathematice similes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Similes figura[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Similar figures.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="que non sumunt[ur] hic p[ro]prie quia tunc habe[n]t mediu[m] sc[ilicet] esse diminutu[m] alique[m]. 
            Sumit[ur] nanq[uam] hic perfectus p[ro] pari nu[m]ero [et] habu[n]dans p[ro] impari">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        numera { abunda[n]s perfect[us] diminitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Abundant, perfect and diminished numbers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Perfecti sunt qui co[m]parati suis partib[us] aliquotis redduntur equales">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Numerus p[er]fect[us]  
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A perfect number.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sup[er]habu[n]dantes aut[em] dicunt[ur] qui suis partibus aliquotis comp[ar]ati">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sup[er]habundans 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Superabundant.                   
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="diminuti vero minores vt in sequentib[us] ostendet[ur]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Diminuti.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Diminished.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="altera lo[n]gior semp[er] mediu[m] p[ro]portio[n]ale int[er] duo q[ua]drata vt dice[n]do thetragonizare
            p[ar]te altera lo[n]giore[m] est inuenire mediu[m] q[ua]dratu[m] int[er] duo"/>
        <underline method="pen" type="straight" language="LA" text="ex p[ar]te altera lo[n]giori"/>
        <underline method="pen" type="straight" language="LA" text="cu[m] maiore[m] dispositione[m] [et]"/>
        <underline method="pen" type="straight" language="LA" text="qui dicebat vltima[m] polligonia[m] figura[m] inscriptam circulo. per lunulas esse eq[ua]le[m] circulo"/>
        <underline method="pen" type="straight" language="LA" text="dicat[ur] similitudo in figuris mathematicis decimo methaphisice disputabit[ur]"/>
        <underline method="pen" type="straight" language="LA" text="modu[m] quanti"/>
        <underline method="pen" type="straight" language="LA" text="S[ed] dissimile dicit[ur] a diuersis formis. vt circul[us] [et] q[ua]dratu[m] in formis [et] figuris sunt dissimilia"/>
        <underline method="pen" type="straight" language="LA" text="Sumit[ur] nanq[uam] hic perfectus p[ro] pari nu[m]ero [et] habu[n]dans p[ro] impari"/>
        <underline method="pen" type="straight" language="LA" text="[et] sic intra que[m]libet limitem tantu[m] vnus p[er]fectus est"/>
        
