    <annotation>
        
        <marginalia hand="Italian" anchor_text="septe[m] r[ati]ones">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Septem r[ati]o[n]es pra[e]sta[n]tiam vniversalis demo[n]stratio[n]is co[n]firma[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Seven reasons confirming the excellence of universal demonstrations.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cui[us] r[ati]o demo[n]stratio[n]is a p[ri]ori est talis [et] agis magistralis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Alia demo[n]str[ati]o
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The other demonstration.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de co[m]paratio[n]e scie[n]tar[um]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Scientiaru[m] Co[m]paratio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The comparison of sciences.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qu[ae] sunt effectus demo[n]strationu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Scientia[e] su[n]t effectus demo[n]strationu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Sciences are the effect of demonstrations.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="maxime scim[us] v[niversa]le si no[n] ampli[us] est p[ro]pter aliq[ui]d aliud"/>
        <underline method="pen" type="straight" language="LA" text="huc deest q[uae]rere p[ro]pter quid vt q[ui]a tria[n]gulus"/>
        <underline method="pen" type="straight" language="LA" text="co[n]ueniat q[ue] deue[n]tu[m] est ad vltimu[m] genus"/>
        <underline method="pen" type="straight" language="LA" text="q[uod] q[uae]libet figura resoluat[ur] in tot tria[n]gulos qua[n]tu[m] ip[s]a distiterit a p[ri]ma sup[er]ficie vt q[ua]dratu[m] s[e]c[un]da sup[er]ficies a tria[n]gulo. ergo"/>
        <underline method="pen" type="straight" language="LA" text="Cui[us] r[ati]o demo[n]stratio[n]is a p[ri]ori est talis [et] magis magistralis. ponat[ur]"/>
        <underline method="pen" type="straight" language="LA" text="S[ed] cu[m] qui[n]q[ue] tria[n]guli valeant decem rectos. hij vero q[ui] sunt ad ce[n]tru[m] valent quatuor q[ui]"/>
        <underline method="pen" type="straight" language="LA" text="si figura haberet mille angulos tu[n]c om[n]es extrinsici p[ar]iter accepti sunt"/>
        <underline method="pen" type="straight" language="LA" text="co[m]paratio[n]e scie[n]tiar[um] q[uae] sunt effectus demo[n]strationu[m]"/>
        <underline method="pen" type="straight" language="LA" text="c[on]siderat q[ue] [et] p[ro]pter quid"/>
        <underline method="pen" type="straight" language="LA" text="q[uae] no[n] est de subiecto sensibili"/>
        
        <mark name="comma" method="pen" place="intext"/> <!-- q[uae]rere p[ro]pter -->
