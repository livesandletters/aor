    <page filename="067.tif" reader="John Dee" signature="[Ev[v]]"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="duplices sunt modi p[rae]dicandi p[er] se">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Duo modi predica[n]di p[er] se.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Two methods of predicting in itself.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Et huiusm[od]i silogistice rationes sunt in p[ri]mo m[od]o certitudinis 
            na[m] p[ro]cedu[n]t ex p[ri]oribus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        huiusmodi su[n]t in p[ri]mo modo certitudinis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Of this kind are [the reasons] in the first method of certainty.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="frequentissime">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        frequentissim&egrave;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Most frequently.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="p[rae]dicatio v[niversa]lis s[e]c[un]d[u]m q[uod] sub se habet p[rae]dicatione[m] p[er] se">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pra[e]dicatio vniversalis p[er] se
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Itself a universal prediction.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nu[m]erus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Numerus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A number.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="multitudo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Multitudo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Amount.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="triangulus causa passio[n]is"/>
        <underline method="pen" type="straight" language="LA" text="subiecto. Et huiusmo[d]i silogistice rationes sunt in p[ri]mo m[od]o certitudinis na[m] p[ro]cedu[n]t ex p[ri]oribus i[d est] a causis ad effectus"/>
        <underline method="pen" type="straight" language="LA" text="rectis q[ua]re p[er] causam scit mathematic[us] effectu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vt effectus siue passio"/>
        <underline method="pen" type="straight" language="LA" text="frequentissime silogisantes in prima figura mathematici silogisant maxime scire"/>
        <underline method="pen" type="straight" language="LA" text="ide[m] analetice i[d est] p[er] rationes"/>
        <underline method="pen" type="straight" language="LA" text="i[d est] ad demo[n]stratiua[m] scie[n]tia[m] p[er]tinet p[rae]dicatio q[uae] est p[er] se. sed ad"/>
        <underline method="pen" type="straight" language="LA" text="p[rae]dicatio v[niuersa]lis"/>
        <underline method="pen" type="straight" language="LA" text="que p[er] se insunt rebus"/>
        <underline method="pen" type="straight" language="LA" text="vt par [et] impar p[rae]dicant[ur] de numero in secu[n]do m[od]o p[er] se"/>
        <underline method="pen" type="straight" language="LA" text="vt nu[m]erus est multitudo ex vnitatib[us] p[ro]fusa"/>
        <underline method="pen" type="straight" language="LA" text="tandu[m] q[uod] multitudo est qua[n]titas ex diuisibilib[us] sep[ar]atis coaceruata et differt a diuisibili s[e]c[un]d[u]m actu[m] Nam"/>
        <underline method="pen" type="straight" language="LA" text="decrescentia[m] multitudo qua[n]titas coaceruata diuisibile v[er]o qua[n]titas decresce[n]da v[e]l segrega[n]da"/>
        <underline method="pen" type="straight" language="LA" text="diuisibile p[ro]ponit actu[m] nu[m]er[um] m[u]ltitudo v[er]o nu[m]er[um] pote[n]ciale[m] augme[n]tandu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Na[m] om[n]is demo[n]stratio h[abet] fieri in p[ri]mo aut secu[n]do m[od]o vt in p[re]missis est ostensum"/>
        <underline method="pen" type="straight" language="LA" text="in per se passio[n]ib[us]"/>
        <underline method="pen" type="straight" language="LA" text="q[uod] subiectu[m] sc[ilicet] nu[m]erus cui statim inest passio erit om[n]ib[us]"/>
        <underline method="pen" type="straight" language="LA" text="q[ue] quicq[ui]d est in r[ati]one diffinie[n]tis est in ratio[n]e diffiniti"/>
        
