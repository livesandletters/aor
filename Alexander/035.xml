        <marginalia hand="Italian" anchor_text="p[re]cognoscit[ur]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pra[e]cogniitio { affectionis dignitatu[m] subiecti.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foreknowledge of affection, of dignities and of the subject.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cu[m] mathematica ex immediatis p[ri]ncipijs necessarijs no[n] aliter se habe[n]tibus p[ro]cedit
            et q[uae] maxi[m]e p[er] alijs p[ro]porcionata est intellectui nostro. cum semp[er] effectum ex causa demo[n]strat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Demo[n]strat&iacute;o mathemat&iacute;ca
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mathematical demonstration.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="q[ui]a est [...] q[ui]d est">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Quid est Quia est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What it is, because it is.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="no[n] potest p[ro]bari de ip[s]is sicut sunt principia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Principia no[n] possunt p[ro]bari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Principles cannot be proven.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="p[rae]cognitio">  
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pra[e]cognitio Principij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foreknowledge of the principle.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="principiata">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Principiatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Principiate.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="p[rae]cognoscere vtraq[ue] cognitio[n]e siue vtroq[ue] m[od]o">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pra[e]cognoscere vtroq[ue] modo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To foresee by both methods. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vnitas principiu[m] et principiatu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Vnitas principiu[m] [et] principiatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Unity in the principle and the principiate.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="p[rae]cognosca[n]tur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pra[e]cognitio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foreknowledge.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="minore[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Minor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Minor.
            </translation>
        </marginalia>
        
