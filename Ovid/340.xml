        <underline method="pen" type="straight" language="LA" text="Ocia si tollas, periere cupidinis arcus, Contemptae&apos;q[ue] iacent, &amp; sine luce faces"/>
        <underline method="pen" type="straight" language="LA" text="Tam Venus ocia amat"/>
        <underline method="pen" type="straight" language="LA" text="Cedit amor rebus, res age, tutus eris"/>
        <underline method="pen" type="straight" language="LA" text="Eripiunt omnes animo sine vulnere"/>
        <underline method="pen" type="straight" language="LA" text="Affluit incautis insidiosus amor"/>
        <underline method="pen" type="straight" language="LA" text="Da vacuae menti, quo teneatur, opus"/>
        <underline method="pen" type="straight" language="LA" text="Uince cupidineas"/>
        <underline method="pen" type="straight" language="LA" text="Quaeritur, aegisthus qua re sit factus adulter"/>
        <underline method="pen" type="straight" language="LA" text="In promptu causa est, desidiosus erat"/>
        <underline method="pen" type="straight" language="LA" text="Quod potuit, fecit. ne nil ageretur, amauit"/>
        
