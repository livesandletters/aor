<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0823.tif" reader="John Dee" pagination="265"/> 
    <annotation>
                
        <marginalia hand="English_secretary" anchor_text="contrarium rationi &amp; constantiae qu&agrave;m fortuna">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>fortuna. R[ati]o. consta[n]tia</marginalia_text>
                </position>
            </language>
            <translation>Fortune, reason, constancy.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dei pra[e]scie[n]tia</marginalia_text>
                </position>
            </language>
            <translation>The prescience of god.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="omnia que fiu[n]t, qu&aacute;[e]que futura sunt, ex omni aeternitate definita dicis esse fataliter">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>An o[m]nia qua[e] futura sunt, ex o[mn]i a[e]ternitate definita sunt fataliter</marginalia_text>
                </position>
            </language>
            <translation>Whether all things which there are in the future, have been inevitably determined from all eternity.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Anile san&egrave; plenu[m] supersitionis fati nomen">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Anile nome[n] fatum.</marginalia_text>
                </position>
            </language>
            <translation>The name fate is like an old woman.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Deiotarum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Deiotarus</marginalia_text>
                    <person name="Deiotarus"/>
                </position>
            </language>
            <translation>Deiotarus.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="classes Populi R. bello Punico">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Classes Pop[uli] Ro[mani]</marginalia_text>
                </position>
            </language>
            <translation>Roman fleets of the nation.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="lacum Trasime">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>lacum Trasimenum.</marginalia_text>
                    <person name="Lake Trasimeno"/>
                </position>
            </language>
            <translation>Lake Trasimeno.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="mutari enim fata non possunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mutari fata no[n] possu[n]t</marginalia_text>
                </position>
            </language>
            <translation>Destinies are not able to be altered.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nihil nos admonere potest vt cautiores simus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nihil nos admonere p[otes]t [fatum] vt cautiores simus Diniu</marginalia_text> <!-- M.B.'fatu[m]' appears to be written and crossed out. Although not entirely certain. -->
                </position>
            </language>
            <translation>It does us no good to admonish us to be on guard against the gods.</translation> <!-- M.B. Not certain what Dee means exactly, this translation is a little loose.. -->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vtilem quidem arbitror esse nobis futuraru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Non esse vtile[m] Diuinatione[m]</marginalia_text>
                </position>
            </language>
            <translation>Divination will not be useful.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Priamo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Priamus.</marginalia_text>
                </position>
            </language>
            <translation>Priam.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="M. Crassus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>M[arcus]. Crassus.</marginalia_text>
                    <person name="Marcus Licinius Crassus"/>
                </position>
            </language>
            <translation>Marcus Crassus.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Cn. Pompeium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cn. Pompeius</marginalia_text>
                    <person name="Gnaeus Pompeius"/>
                </position>
            </language>
            <translation>Cnaeus Pomeius.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Caesre[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ca[e]sar.</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>Caesar.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ignoratio futurorum malorum vtilior est qu&agrave;m scientia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ignoratio futuroru[m] maloru[m] vtilior q[uam] scientia.</marginalia_text>
                </position>
            </language>
            <translation>Ignorance of future ills is more profitable than the knowledge of them.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="omnia fato: nihil ergo illis profuisset diuinare">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>si fato o[m]nia euenia[n]t nihil prodest diuinare.</marginalia_text>
                </position>
            </language>
            <translation>If so, then, the deaths that befell these men were not determined by fate.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="fortuna">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>fortuna</marginalia_text>
                </position>
            </language>
            <translation>Fortunes.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="&agrave; fabulis">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text> historis troiana fabula est</marginalia_text> <!-- M.B. Not sure of the first word, but I think Dee is referencing 'Historia Troiana' by  Guido delle Colonne. See: https://en.wikipedia.org/wiki/Historia_destructionis_Troiae  -->
                    <book title="Historia Troiana"/>
                </position>
            </language>
            <translation>It is the fable of the history of Troy.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cooptasset">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>coopto.</marginalia_text>
                </position>
            </language>
            <translation>I co-opt.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ex omni aeternitate definita dicis esse fataliter"/>
        <underline method="pen" type="straight" language="LA" text="omni aeternitate certum fuerit esse"/>
        <underline method="pen" type="straight" language="LA" text="Anile san&egrave; &amp; plenu[m] superstitionis fatis nomen ipsum"/>
        <underline method="pen" type="double" language="LA" text="sed tamen"/>
        <underline method="pen" type="straight" language="LA" text="apud"/>
        <underline method="pen" type="double" language="LA" text="at id neque si fatum fuerat effugisset: nec fi no[n] fuerat, in eum casum incidisset. Quid"/>
        <underline method="pen" type="double" language="LA" text="Poenis depressam"/>
        <underline method="pen" type="double" language="LA" text="fiet tamen illud quod futurum est. Sin autem id potest flecti, nullum est fatum: ita ne diuinatio quidem, 
            quoniam ea rerum futurarum est. Nihil autem est pro certo futurum quod potest aliqua procuratione accidere ne fiat. Atque ego ne vtilem quidem arbitror esse nobis 
            futuraru[m] rerum scientiam. Quae enim"/>
        <underline method="pen" type="double" language="LA" text="&agrave; fabulis"/>
        <underline method="pen" type="double" language="LA" text="ipse cooptasset"/>
        <underline method="pen" type="double" language="LA" text="Quid enim posset iis esse laetum, exitus suos cogitantibus? Ita"/>
        <underline method="pen" type="double" language="LA" text="eorum solertia"/>
        
