            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ex rebus timiditas no[n] ex vocabulis, nascitur</marginalia_text>
                </position>
            </language>
            <translation>Timidity is born out of matters, not out of vocabulary.</translation>
        </marginalia>
        
