    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0409.tif" reader="John Dee" pagination="139"/>
    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="aut crudior fuit"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d ita maluerit"/>
        <underline method="pen" type="straight" language="LA" text="subimus"/>
        <underline method="pen" type="straight" language="LA" text="non co[n]tinuo existimatur nescire gestum"/>
        <underline method="pen" type="straight" language="LA" text="reprehensum est, aut aeterna in eo, aut certe diuturna valet opinio tarditatis"/>
        <underline method="pen" type="straight" language="LA" text="qui cum mercede doceret"/>
        <underline method="pen" type="straight" language="LA" text="operam apud sese perdere"/>
        <underline method="pen" type="straight" language="LA" text="impellere"/>
        <underline method="pen" type="straight" language="LA" text="similem esse hominis"/>
        <underline method="pen" type="straight" language="LA" text="tradatur"/>
        <underline method="pen" type="straight" language="LA" text="inculcetur"/>
        <underline method="pen" type="straight" language="LA" text="sit tardior, posse percipere animo, &amp; memoria custodire"/>
        <underline method="pen" type="straight" language="LA" text="mobilitas linguae"/>
        <underline method="pen" type="straight" language="LA" text="celeritas verboru[m]"/>
        <underline method="pen" type="straight" language="LA" text="nobis non possumus fingere"/>
        <underline method="pen" type="straight" language="LA" text="In oratore autem acumen dialecticoru[m]"/>
        <underline method="pen" type="straight" language="LA" text="sententiae philosophoru[m], verba prop&egrave; po&euml;tarum, memoria iurisconsultoru[m], vox tragoedorum"/>
        <underline method="pen" type="straight" language="LA" text="gestus pen&egrave; summoru[m] actorum est requirendus"/>
        <underline method="pen" type="straight" language="LA" text="nihil in hominu[m] genere rarius perfecto oratore inueniri potest: quae enim singularu[m] rerum artifices singula si mediocriter adepti sunt, probantur, ea nisi omnia summa sunt in oratore, probari no[n] possunt"/>
        <underline method="pen" type="straight" language="LA" text="in artificio &amp; opera qu&agrave;m tenui &amp; leui"/>
        <underline method="pen" type="straight" language="LA" text="soleo"/>
        <underline method="pen" type="straight" language="LA" text="probabiles"/>
        <underline method="pen" type="straight" language="LA" text="ad hanc similitudine[m] huius histrionis, oratoriam laudem dirigamus"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m nihil"/>
        <underline method="pen" type="straight" language="LA" text="moueat"/>
        <underline method="pen" type="straight" language="LA" text="vt in quo quisque artificio excelleret, is in suo genere Roscius diceretur"/>
        <underline method="pen" type="straight" language="LA" text="absolutionem"/>
        <underline method="pen" type="straight" language="LA" text="desydera[n]s, &agrave; qua ipse longe absum, facio impudenter"/>
        <underline method="pen" type="straight" language="LA" text="mihi enim volo ignosci, caeteris ipse no[n] ignosco"/>
        <underline method="pen" type="straight" language="LA" text="qui non potest"/>
        <underline method="pen" type="straight" language="LA" text="qui vitiose facit"/>
        <underline method="pen" type="straight" language="LA" text="quem denique non decet"/>
        <underline method="pen" type="straight" language="LA" text="detrudendum puto"/>
        <underline method="pen" type="straight" language="LA" text="summa atque"/>
        <underline method="pen" type="straight" language="LA" text="indolem"/>
        <underline method="pen" type="straight" language="LA" text="nec magis ad eos deterre[n]dos qui no[n] possent, qu&agrave;m ad vos qui possetis, exacuendos acco[m]modaui"/>
        <underline method="pen" type="straight" language="LA" text="haec quae sunt in specie posita"/>
        <underline method="pen" type="straight" language="LA" text="diuina sunt"/>
        <underline method="pen" type="straight" language="LA" text="ipso habitu"/>
        <underline method="pen" type="straight" language="LA" text="voce pleniore[m]"/>
        <underline method="pen" type="straight" language="LA" text="quae quibus &agrave; natura minora data sunt"/>
        <underline method="pen" type="straight" language="LA" text="modice"/>
        <underline method="pen" type="straight" language="LA" text="scienter"/>
        <underline method="pen" type="straight" language="LA" text="&amp; vt ne dedeceat"/>
        <underline method="pen" type="straight" language="LA" text="minime est facile praecipere"/>
        <underline method="pen" type="straight" language="LA" text="sicut vnus paterfamili&acirc;s his"/>
        <underline method="pen" type="straight" language="LA" text="caput esse artis decere: quod tamen vnu[m] id esse, quod tradi arte non possit"/>
        <underline method="pen" type="straight" language="LA" text="sermone[m] ali&ograve; transferamus"/>
        <underline method="pen" type="straight" language="LA" text="retines nos in hoc studio, nec ad aliam dimittis artem"/>
        <underline method="pen" type="straight" language="LA" text="quicquid est"/>
        <underline method="pen" type="straight" language="LA" text="potes"/>
        <underline method="pen" type="straight" language="LA" text="neque enim sumus nimis auidi, ista tua mediocri eloquentia conte[n]ti sumus"/>
        <underline method="pen" type="straight" language="LA" text="quantulum"/>
        <underline method="pen" type="straight" language="LA" text="expetenda"/>
        <underline method="pen" type="straight" language="LA" text="assume[n]dum"/>
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen"/>
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen"/>
        
