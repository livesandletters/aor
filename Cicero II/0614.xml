        <underline method="pen" type="straight" language="LA" text="vtilitatem, acutum, versutum, veteratore[m], facile vt excogitet quomodo occulte sine"/>
        <underline method="pen" type="straight" language="LA" text="vllo conscio fallat. An"/>
        <underline method="pen" type="straight" language="LA" text="sicarios exercuisset"/>
        <underline method="pen" type="straight" language="LA" text="Numa[n]tino inficiando"/>
        <underline method="pen" type="straight" language="LA" text="qui animi conscientiam non curet, quam scilicet comprimere nihil"/>
        <underline method="pen" type="straight" language="LA" text="Is enim qui occultus &amp; tectus dicitur, tantum abest vt se indicet, perficiet etiam vt dolere alterius improbe facto videatur"/>
        <underline method="pen" type="straight" language="LA" text="er&aacute;tque verisimilius hunc mentiri cuius interesset, qu&agrave;m illum qui id se rogasse scripsisset, quod debuisset rogare"/>
        <underline method="pen" type="straight" language="LA" text="si secutus esset eorum sententiam"/>
        <underline method="pen" type="straight" language="LA" text="numum nullum attigisset"/>
        <underline method="pen" type="straight" language="LA" text="quidem vel cum periculo est quaere[n]da vobis. est enim effectric multaru[m] &amp; magnarum voluptatu[m]"/>
        <underline method="pen" type="straight" language="LA" text="quum pecunia voluptates pariantur plurimae"/>
        <underline method="pen" type="straight" language="LA" text="ad honestatem enim ille omnem conatum suum referebat, non"/>
        <underline method="pen" type="straight" language="LA" text="impatibilem facitis"/>
        <underline method="pen" type="straight" language="LA" text="tolerabilem"/>
        <underline method="pen" type="straight" language="LA" text="quanuis vellet iustus, iniquus poterat impune. Qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="nostrum reliquit effigiem &amp; humanitatis &amp; probitatis suae filium, tum doctus, tum"/>
