        <underline method="pen" type="straight" language="LA" text="Nihil grauius, nil philosophia"/>
        <underline method="pen" type="straight" language="LA" text="honeste, sapienter"/>
        <underline method="pen" type="straight" language="LA" text="Occupaui te, inquit, fortuna atque cepi, omn&eacute;sque aditus tuos interclusi, vt ad me aspirare no[n] posses. 
            Praeclare si Aristo Chius, aut si Stoicus Zeno diceret, qui nisi quod turpe esset, nihil malum duceret"/>
        <underline method="pen" type="straight" language="LA" text="qui omne bonum in visceribus medull&iacute;sque condideris"/>
        <underline method="pen" type="straight" language="LA" text="expoliari"/>
        <underline method="pen" type="straight" language="LA" text="Acute autem disputantis illud est, non quid quisque dicat, sed quid cuique dicendum sit videre. Velut"/>
        <underline method="pen" type="straight" language="LA" text="nisi secretis malis omnibus cumulata bonorum complexio"/>
        <underline method="pen" type="straight" language="LA" text="turba quaedam, paupertas, ignobilitas, humilitas, solitudo, amissio suorum, graues dolores corporis, 
            perdita valetudo, debilitas, caecitas, interitus patriae, exilium, seruitus"/>
        <underline method="pen" type="straight" language="LA" text="insignis &amp; pulcher"/>
        <underline method="pen" type="straight" language="LA" text="omni&aacute;que sibi in sese"/>
        <underline method="pen" type="straight" language="LA" text="magni cuiusdam &amp; alti"/>
        <underline method="pen" type="straight" language="LA" text="Non igitur ex singulis vocibus philosophi spectandi sunt, sed ex perpetuitate atque constantia"/>
        <underline method="pen" type="straight" language="LA" text="nihil praeter verborum nouitatem interesse"/>
        <underline method="pen" type="straight" language="LA" text="Rem enim opinor spectari oportere, no[n] verba"/>
        <underline method="pen" type="straight" language="LA" text="Tu quidem tabellis obsignatis agis mecum"/>
        <underline method="pen" type="straight" language="LA" text="quodcunque nostros animos probabilitate percussit, id dicimus"/>
        
