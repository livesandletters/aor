        <marginalia hand="Italian" anchor_text="Aristoteli, Speusippo, Xenocrati, Polemoni ... Pythagora, Socrate, Platone">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Aristoteles. Speusippus. Xenocrates. Polemo. Pythagoras. Plato Socrates.
                    </marginalia_text>
                    <person name="Aristotle"/>
                    <person name="Speusippus"/>
                    <person name="Xenocrates of Chalcedon"/>
                    <person name="Polemon"/>
                    <person name="Pythagoras"/>
                    <person name="Plato"/>
                    <person name="Socrates"/>
                </position>
            </language>
            <translation>
                Aristoteles, Speusippus, Xenocrates, Polemon, Pythagoras, Plato, Socrates.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Non igitur ex singulis vocibus philosophi spectandi sunt, sed ex perpetuitate atque constantia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Non ex singulis vocibus, Philosophi spectandi su[n]t sed ex perpetuitate et constantia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Philosophers are to be looked at not from a single voice, but from continuity and perseverance.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Rem enim opinor spectari">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Res, no[n] verba spectanda
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A thing, not a words are to be looked at.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nos in diem viuimus">
            <language ident="LA">
