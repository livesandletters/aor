        <marginalia hand="English_secretary" anchor_text="inducitis in eas captiones">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>inducere in captiones. se mirifica[m] in latebra[m] co[n]iecisti premere alique[m].</marginalia_text>
                </position>
            </language>
            <translation>To lead into these sophistries. You throw yourself in a wonderful hiding place. To overwhelm someone.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="qu&agrave;m quu[m] foruito hostiae adducantur">
