        <underline method="pen" type="straight" language="LA" text="posito, &amp; omnium assensu approbato"/>
        <underline method="pen" type="straight" language="LA" text="altus &amp; excelle[n]s"/>
        <underline method="pen" type="straight" language="LA" text="eoru[m] dico"/>
        <underline method="pen" type="straight" language="LA" text="qui mancam fore putauerunt sine aliqua accessione virtutem"/>
