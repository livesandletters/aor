        <underline method="pen" type="straight" language="LA" text="assens&uacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="Circunscriptis igitur his sententiis"/>
        <underline method="pen" type="straight" language="LA" text="bonum sit viuere: scientiam adhibentem earum rerum quae natura eueniant, seligentem quae secundum naturam, 
            &amp; siquae contra naturam sunt, reiicientem, id est conuenienter congruent&eacute;rque naturae viuere"/>
        <underline method="pen" type="straight" language="LA" text="id apprime rectissime dicitur"/>
        <underline method="pen" type="straight" language="LA" text="expletum esse omnibus suis"/>
        <underline method="pen" type="straight" language="LA" text="est patriam prodere, parentes violare, fana depeculari"/>
        <underline method="pen" type="straight" language="LA" text="paulum oppid&ograve; inter"/>
        <underline method="pen" type="straight" language="LA" text="id quod esset natura absolutu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Qu&uacute;mque rerum notiones in animis fiant, si aut vsu aliquid cognitum sit, aut coniunctione, aut similitudine, aut collatione rationis, hoc quarto"/>
        <underline method="pen" type="straight" language="LA" text="Quum enim ab his rebus, quae sunt secundum naturam, ascendit animus collatione rationis, tum ad notionem boni peruenit"/>
        <underline method="pen" type="straight" language="LA" text="non comparatione cum aliis dulce"/>
        <underline method="pen" type="straight" language="LA" text="sed ea aestimatio genere valet, non magnitudine"/>
        <underline method="pen" type="straight" language="LA" text="vitam insipientium miseram acerb&aacute;mque reddunt"/>
        
