                </position>
            </language>
            <translation>Virtue is abundantly full of toil</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nullum esse ius naturale">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nullu[m] ius naturale</marginalia_text>
                </position>
            </language>
