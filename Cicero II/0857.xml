            <translation>To kill a wrongful person, it is stupid to be killed.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ciuilem esse dicens, alteram naturalem, vtranque subuertit quod illa ciuilis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iustitia { ciuilis naturalis</marginalia_text>
                </position>
            </language>
            <translation>Civil and natural justice.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="non potuerit refellere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cic[ero] ha[e]c refellere no[n] potuit</marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Cicero could not refute these things.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sapie[n]tiam quidem esse concesserat, sed iniustam">
