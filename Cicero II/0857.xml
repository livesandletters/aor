        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nihil percipi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Carneades nihil p[er]cipib[at]</marginalia_text> <!--JG: Unsure about last word-->
