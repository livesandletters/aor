        <marginalia hand="English_secretary" anchor_text="stultus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>stultus.</marginalia_text>
                </position>
            </language>
            <translation>Foolish.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="&eacute;ine parcet, vt ipse occidatur">
            <language ident="LA">
