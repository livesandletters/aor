    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="desyderabile; optabilem">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        desyderabile
                        optabilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Desirable.
                Appealing.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="excruciant, &amp; semper solicitant; animi noct&eacute;sque di&eacute;sque exeduntur">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                       excruciare.
                       solicitare.
                       a[nim]i noctes diesq[ue] exedunter
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To torture.
                To agitate.
                Minds are consumed for nights and days.
            </translation>
        </marginalia>
         
        <marginalia hand="English_secretary" anchor_text="dedoce[n]di">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        dedocere
                    </marginalia_text>
                </position>
            </language>
            <translation>To unlearn.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="temeritas &amp; libido &amp; ignauia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Libido. Temeritas
                        Ignauia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Wantonness. Rashness. Idleness.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="improboru[m] facta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Improboru[m] facta. 
                    </marginalia_text>
                </position>
            </language>
            <translation>Deeds of the wicked.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ipsi se indicaueru[n]t">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Ho[m]i[n]es seip[s]os p[er]de[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>Men destroying themselves.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="conscientiam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Conscientia
                    </marginalia_text>
                </position>
            </language>
            <translation>Awareness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="opes; qua qui vta[n]tur, beneuolentiam sibi conciliant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        opu[m] vsus
                    </marginalia_text>
                </position>
            </language>
            <translation>The use of resources.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cupiditates &agrave; natura proficiscuntur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Cupiditates naturales
                    </marginalia_text>
                </position>
            </language>
            <translation>Natural desires.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iniuria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Iniuria
                    </marginalia_text>
                </position>
            </language>
            <translation>Injury.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="diligi &amp; charum esse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Diligi et charu[m] esse.
                    </marginalia_text>
                </position>
            </language>
            <translation>To be esteemed and to be cared for.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Itaque non ob ea solum inco[m]moda quae eueniunt improbis, fugiendum improbitate[m] putamus, sed multo etiam magis qu&ograve;d cuius in animo versatur, 
            nunquam sinit eum respirare, nunqua[m] acquiescere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Improbitatis poena.
                    </marginalia_text>
                </position>
            </language>
            <translation>The punishment of wickedness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="no[n] potest esse dubium quin id sit summum atque extremu[m] bonorum omnium">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Conclusio loquitur ip[s]am voluptate[m] su[m]mu[m] bonu[m] esse.
                    </marginalia_text>
                </position>
            </language>
            <translation>The conclusion states that pleasure itself is the greatest good.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Huic certae stabil&iacute;que sententiae quae sint coniuncta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Huic sententia[e] co[n]iu[n]cta
                    </marginalia_text>
                </position>
            </language>
            <translation>Points related to this doctrine.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Animi aute[m] voluptates &amp; dolores nasci fatemur &egrave; corporis voluptatibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animi voluptates nasci fatemur &egrave; corporis volup.
                    </marginalia_text>
                </position>
            </language>
            <translation>We acknowledge that pleasures of the mind arise from pleasures of the body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec ob eam causam no[n] multo maiores esse &amp; voluptates &amp; dolores animi qu&agrave;m corporis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        A[nim]i voluptas, maior.
                    </marginalia_text>
                </position>
            </language>
            <translation>Pleasure of the mind, greater.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nam corpore nihil nisi praesens, &amp; quod adest sentire possumus: animo autem &amp; praeterita &amp; futura">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Solum pra[e]sens corpore se[n]tim[us], a[n]i[m]o pra[e]terita et futura.
                    </marginalia_text>
                </position>
            </language>
            <translation>We only feel the present in the body; in the mind we feel the past and the future.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quanta voluptas non dolere sit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        No[n] dolere, magna volup.
                    </marginalia_text>
                </position>
            </language>
            <translation>Not feeling grief, a great pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sed vt his bonis erigimur quae expectamus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                       Spes. 
                    </marginalia_text>
                </position>
            </language>
            <translation>Hope.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="memoria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Memoria.
                    </marginalia_text>
                </position>
            </language>
            <translation>Memory.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cadere causa">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        cadere causa.
                    </marginalia_text>
                </position>
            </language>
            <translation>To fall from the cause.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="detrimenti; inco[m]moda">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        detrime[n]tum
                        incommodu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Detriment. Trouble.
            </translation>
        </marginalia>
        
