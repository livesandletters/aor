        <underline method="pen" type="straight" language="LA" text="verborum collocatione"/>
        <underline method="pen" type="straight" language="LA" text="tractu orationis leni"/>
        <underline method="pen" type="straight" language="LA" text="aequabili perpoliuit illud opus"/>
        <underline method="pen" type="straight" language="LA" text="dolauit"/>
        <underline method="pen" type="straight" language="LA" text="illustrata"/>
        <underline method="pen" type="straight" language="LA" text="eluceat"/>
        <underline method="pen" type="straight" language="LA" text="remoti &agrave;"/>
        <underline method="pen" type="straight" language="LA" text="princeps"/>
        <underline method="pen" type="straight" language="LA" text="post illum"/>
        <underline method="pen" type="straight" language="LA" text="qui ita creber est rerum frequentia, vt verborum prop&egrave; numerum sententiarum numero consequatur: ita porro verbis aptus &amp; pressus, vt nescias vtrum res oratione, an verba sententiis illustrentur"/>
        <underline method="pen" type="straight" language="LA" text="ex numero accepimus eorum, qui causas dictitarunt"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; repub. remotus"/>
        <underline method="pen" type="straight" language="LA" text="id quod optimo cuique Athenis accidere solitum est, in exilium pulsus esset"/>
        <underline method="pen" type="straight" language="LA" text="ex clarissima rhetoris officina duo"/>
        <underline method="pen" type="straight" language="LA" text="causas omnino nunquam attigerunt"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; philosophia profectus princeps"/>
        <underline method="pen" type="straight" language="LA" text="leniore quoda[m] sono est vsus: &amp; qui illum impetu[m] oratoris non habeat, vehemens fortasse minus, sed aliquanto tamen est, vt mihi quidem videtur, dulcior"/>
        <underline method="pen" type="straight" language="LA" text="Minimus natu"/>
        <underline method="pen" type="straight" language="LA" text="rerum copia"/>
        <underline method="pen" type="straight" language="LA" text="sententiarum varietate abunda[n]tissimus"/>
        <underline method="pen" type="straight" language="LA" text="ipsa compositione verborum no[n] impolitus"/>
        <underline method="pen" type="straight" language="LA" text="magnam eloquentiam ad scribe[n]dum attulit, sed nullum vsum forensem"/>
        <underline method="pen" type="straight" language="LA" text="Graece negant scire"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m scienter? qu&agrave;m proprie de vnoquoque dixit"/>
        <underline method="pen" type="straight" language="LA" text="admirans illud iam mirari desino, quod"/>
        <underline method="pen" type="straight" language="LA" text="posse tantum"/>
        <underline method="pen" type="straight" language="LA" text="aucupans"/>
        <underline method="pen" type="straight" language="LA" text="fatebor aliquid tamen, vt cum in sole ambulem, etiam si aliam ob causam ambulem, fieri natura tame[n] vt colorer: sic cum istos libros ad Misenum (nam Romae vix licet) studiosius legerim, sentio orationem meam illorum cantu quasi colorari"/>
        <underline method="pen" type="straight" language="LA" text="latius hoc vobis patere"/>
        <underline method="pen" type="straight" language="LA" text="voluerunt &agrave; vulgo intelligi"/>
        <underline method="pen" type="straight" language="LA" text="deceptus indicibus"/>
        <underline method="pen" type="straight" language="LA" text="angustis"/>
        <underline method="pen" type="straight" language="LA" text="co[n]cisis disputationibus illigati"/>
        <underline method="pen" type="straight" language="LA" text="aliena quadam lingua locutos no[n]"/>
        <underline method="pen" type="straight" language="LA" text="vt videantur voluisse esse nobis, qui non sumus eruditissimi, familiares"/>
        <underline method="pen" type="straight" language="LA" text="Sed illuc redeo"/>
        
