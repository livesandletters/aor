        <underline method="pen" type="straight" language="LA" text="addidit historiae maiore[m] sonum vocis vir"/>
        <underline method="pen" type="straight" language="LA" text="no[n] exornatores rerum"/>
        <underline method="pen" type="straight" language="LA" text="tantu[m]modo narratores"/>
