        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        stoicorum Dij nec otiosi vt epicureor[um] nec voluptatibus aut co[m]messationibus vaca[n]tes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The gods of the Stoics, neither ilde like the Epicureans, nor free of pleasures or revelry.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Epicurus, monogrammos deos">
