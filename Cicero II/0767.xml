        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        suscepit aute[m] vita ho[m]i[n]u[m] 
                        co[n]suetudo&apos;q[ue] r[ati]o[n]is
                    </marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="tail" book_orientation="0">
