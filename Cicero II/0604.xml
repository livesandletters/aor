        <underline method="pen" type="straight" language="LA" text="progrediens familiaritate[m]"/>
        <underline method="pen" type="straight" language="LA" text="amorem efflorescere"/>
        <underline method="pen" type="straight" language="LA" text="vt ne minus"/>
