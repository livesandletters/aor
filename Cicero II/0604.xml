    <page filename="0604.tif" reader="John Dee" pagination="46"/>  
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="tria vix amicorum paria">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Tria paria</marginalia_text>
                </position>
            </language>
            <translation>Three pairs.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="despicationes">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>despicatio</marginalia_text>
                </position>
            </language>
            <translation>Contempt.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cernimus, id omne oritur &agrave; sensibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quicquid a[n]i[m]o cernimus id omne oritur &agrave; se[n]sibus.</marginalia_text>
                </position>
            </language>
            <translation>Whatever we discern in the mind, each one arises from the senses.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quos qui tollunt, &amp; nihil posse percipi dicu[n]t, ii remotis sensibus, ne idipsum quidem expedire possunt quod disserunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Contra, Socratis nihil sciri</marginalia_text>
                    <person name="Socrates"/>
                </position>
            </language>
            <translation>Againt the opinion of Socrates, that nothing is known.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sic &egrave; Physicis &amp; fortitudo sumitur contra mortis timorem, &amp; constantia contra metum religionis, 
            &amp; sedatio animi omnium rerum occultaru[m] ignoratione sublata, &amp; moderatio, natura cupiditaru[m], 
            generib&uacute;sque explicatis: &amp; vt modo docui cognitionis regula &amp; iudicio, ab eade[m] illa co[n]stituto, veri &agrave; falso distinctio traditur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Natura[e] cognitio qua[n]tu[m] iuuet</marginalia_text>
                </position>
            </language>
            <translation>How far knowledge of nature may help.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="amicitia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Amicitia</marginalia_text>
                </position>
            </language>
            <translation>Friendship.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fictae veterum fabulae declarant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>ficta[e] fabula[e].</marginalia_text>
                </position>
            </language>
            <translation>Fictional stories.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Epicuri amicitia</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>The friendship of Epicurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="stabilitas amicitiae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>stabilitas amicitia[e]</marginalia_text>
                </position>
            </language>
            <translation>The stability of friendship.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="solitudo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Solitudo.</marginalia_text>
                </position>
            </language>
            <translation>Solitude.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="amicitia cum voluptate connectitur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Amicitia cu[m] voluptate co[n]nectitur.</marginalia_text>
                </position>
            </language>
            <translation>Frienship connected to pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quocirca eodem modo sapiens erit affectus erga amicum, quo in seipsum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Amicus alter idem</marginalia_text>
                </position>
            </language>
            <translation>A friend, a second self.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Praeclare enim Epicurus his pen&egrave; verbis, Eadem, inquit, scientia confirmauit animu[m], ne quod aut sempiternum, aut diuturnum timeret malum, 
            quae perspexit in hoc ipso vitae spatio, amicitiae praesidium esse firmissimu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Epicuri dictum.</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>A saying of Epicurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="congressus, copulation&eacute;sque">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Congressus. copulatio[n]es</marginalia_text>
                </position>
            </language>
            <translation>Meetings. Pairings.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="familiaritate[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>familiaritas</marginalia_text>
                </position>
            </language>
            <translation>Intimacy.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="foedus quoddam esse sapientum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapientu[m] foedus</marginalia_text>
                </position>
            </language>
            <translation>An agreement of wise people.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Conclusio</marginalia_text>
                </position>
            </language>
            <translation>The conclusion.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vacillare; claudicare">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        vacillare
                        claudicare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To stagger.
                To waver.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="sole ipso illustriora &amp; clariora">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>sole ip[s]o illustriora et clariora</marginalia_text>
                </position>
            </language>
            <translation>Brighter and clearer than the sun itself.</translation>
        </marginalia>
        
