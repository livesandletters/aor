        <underline method="pen" type="straight" language="LA" text="AEgyptum peragrauit"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; sacerdotibus barbaris numeros &amp; caelestia"/>
        <underline method="pen" type="straight" language="LA" text="vt quum Socratem expressisset"/>
        <underline method="pen" type="straight" language="LA" text="AEgyptum lustrauit"/>
        <underline method="pen" type="straight" language="LA" text="cur tantas regiones barbarorum pedibus obiit, tot maria transmisit? Cur haec eadem Democritus, 
            qui (vere fals&oacute;ne quaereremus) dicitur oculis se priuasse?"/>
        <underline method="pen" type="straight" language="LA" text="nisi beatam vitam"/>
        <underline method="pen" type="straight" language="LA" text="id est animu[m] terrore liberum"/>
        <underline method="pen" type="straight" language="LA" text="haec etsi praeclare nondum, tame[n] &amp; perpolita"/>
        <underline method="pen" type="straight" language="LA" text="in actionibus praescribi solet"/>
        <underline method="pen" type="straight" language="LA" text="Inconstantiae crimen ille effugit, nos effugere non"/>
        <underline method="pen" type="straight" language="LA" text="noua quaerunt, quibus idem dicant"/>
        <underline method="pen" type="straight" language="LA" text="si homines essent, vsitate loquere[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="obscurari, nec apparere, &amp; in virtutes tanquam in solis radios incurrere. At enim qua in"/>
        <underline method="pen" type="straight" language="LA" text="seges quidem igitur spicis vberibus &amp; crebris, si auenam vspiam videris: nec mercatura qaestuosa, 
            si in maximis lucris parum aliquid damni contraxerit"/>
        <underline method="pen" type="straight" language="LA" text="vt reliquas obruat?"/>
        <underline method="pen" type="straight" language="LA" text="nec fraudere suo veteri nomine"/>
        <underline method="pen" type="straight" language="LA" text="in altera librae lance ponere. Terram, mihi crede, ea lanx &amp; maria deprimet. Semper enim ex eo quod maximas partes co[n]tinet, 
            latissim&eacute;que funditur, tota res appellatur"/>
        <underline method="pen" type="straight" language="LA" text="mare abiecit"/>
        
