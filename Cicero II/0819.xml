        <underline method="pen" type="straight" language="LA" text="Fieri igitur omnia fato ratio cogit fateri. Fatu[m] autem id appello, quod Graeci"/>
        <underline method="pen" type="straight" language="EL" text=""/>
        <underline method="pen" type="straight" language="LA" text="id est, ordine[m] seri&eacute;mque causarum, quu[m] causae causa nexa re[m] ex se gignat. Ea est ex omni aeternitate fluens ceritas sempiterna"/>
        <underline method="pen" type="straight" language="LA" text="supersitiose"/>
        <underline method="pen" type="straight" language="LA" text="colligatione[m]"/>
        <underline method="pen" type="straight" language="LA" text="est quasi rudentis explicatio, sic traductio temporis"/>
        <underline method="pen" type="straight" language="LA" text="noui efficientis, &amp; primu[m] quique replicantis. Quod"/>
        <underline method="pen" type="straight" language="LA" text="progignuntur"/>
        <underline method="pen" type="straight" language="LA" text="diuturnitate pertractata notaureunt, aut semper, aut"/>
        <underline method="pen" type="straight" language="LA" text="euisdemmodi"/>
        <underline method="pen" type="straight" language="LA" text="Vt enim deoru[m] animi sine oculis, sine auribus"/>
        <underline method="pen" type="straight" language="LA" text="Na[m] si obscurior quasi caliginosa stella extiterit, pingue &amp; concretu[m] esse coelum, vt eius aspiratio grauis &amp; pestilens futura sit: 
            sin illustris &amp; perlucida stella apparuerit, significari coelum esse tenue pur&uacute;mque, &amp; propterea salubre. Democritus"/>
        <underline method="pen" type="straight" language="LA" text="Pl&uacute;sque ex alieno iecore sapiunt qu&agrave;m ex suo"/>
        <underline method="pen" type="straight" language="LA" text="Quidquid est hoc, omnia animat, format, alit, auget, creat"/>
        
