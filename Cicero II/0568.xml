        <marginalia hand="Italian" anchor_text="Lucullus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>LAVS LVCVLLI</marginalia_text>
                    <person name="Lucius Licinius Lucullus"/>
                </position>
            </language>
            <translation>Praise of Lucullus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="dicerem, nisi de meipso dicendum esset: quod hoc tempore non est necesse. Ita priuabo potius illum debito testimonio, qu&agrave;m id cum mea laude co[m]municem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cicero no[n] est a[m]bitiosus.</marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Cicero is not ambitious.</translation>
        </marginalia>
        
      
        <marginalia hand="Italian" anchor_text="Maiore enim studio Lucullus cum omni literarum generi, tum philosophiae deditus fuit, qu&agrave;m qui illum ignorabant arbitraba[n]tur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Lucullus qua[n]tum fecit philosophi[a]e studiu[m]</marginalia_text>
                    <person name="Lucius Licinius Lucullus"/>
                </position>
            </language>
            <translation>How much study of philosophy Lucullus did.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="lectione libroru[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Lectio librorum.</marginalia_text>
                </position>
            </language>
            <translation>The reading of books.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Sunt enim multi, qui omnino Graecas non ament literas">
            <language ident="LA">
