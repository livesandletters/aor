        <underline method="pen" type="straight" language="LA" text="[quan]dam memoriam rerum, verboru[m] maiorem"/>
        <underline method="pen" type="straight" language="LA" text="res, qu&agrave;m verba prosunt: hoc"/>
        <underline method="pen" type="straight" language="LA" text="credo, qu&ograve;d haerebant in memoria quaecunque audierat aut viderat. Tali"/>
        <underline method="pen" type="straight" language="LA" text="vt literis consignamus"/>
        <underline method="pen" type="straight" language="LA" text="sic ille in animo res insculptas habebat"/>
        <underline method="pen" type="straight" language="LA" text="maximus, hunc &agrave; se maiorem ducem cognitum, qu&agrave;m quenquam eorum quos legisset fateretur"/>
        <underline method="pen" type="straight" language="LA" text="In eodem tanta prudentia fuit in constituendis temperand&iacute;sque ciuitatibus, tanta aequitas, vt hodie stet Asia Luculli institutis serua[n]dis, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="vestigiis persequendis"/>
        <underline method="pen" type="straight" language="LA" text="vis virtutis, atque ingenii peregrinata abfuit ab oculis &amp; fori, &amp; curiae"/>
        <underline method="pen" type="straight" language="LA" text="inimicoru[m] calumnia"/>
        <underline method="pen" type="straight" language="LA" text="Ita priuabo potius illum debito testimonio, qu&agrave;m id cum mea laude co[m]municem"/>
        <underline method="pen" type="straight" language="LA" text="Graecis literis celebrata &amp; Latinis"/>
        <underline method="pen" type="straight" language="LA" text="externa cum multis, haec interiora cum paucis ex ipso saepe cognouimus"/>
        <underline method="pen" type="straight" language="LA" text="studio Lucullus cum omni literarum generi, tum philosophiae deditus fuit, qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="ignorabant arbitraba[n]tur: nec vero ineunte aetate solum, sed &amp; Quaestore"/>
        <underline method="pen" type="straight" language="LA" text="Imperatori sub ipsis pellibus otii relinquatur"/>
        <underline method="pen" type="straight" language="LA" text="&egrave; philosophis"/>
        <underline method="pen" type="straight" language="LA" text="facile cognouit, quae vel semel audita meminisse potuisset. Delectabatur"/>
        <underline method="pen" type="straight" language="LA" text="amplificare velim, minuam etia[m] gloriam"/>
        <underline method="pen" type="straight" language="LA" text="literas: plures qui philosophiam"/>
        <underline method="pen" type="straight" language="LA" text="rerum disputatione[m] principibus ciuitatis non ita decoram putent"/>
        <underline method="pen" type="straight" language="LA" text="nec literarum Graecaru[m], nec philosophiae iam vllum authorem"/>
        <underline method="pen" type="straight" language="LA" text="Restat vt his"/>
        <underline method="pen" type="straight" language="LA" text="personas tam graues illigari"/>
        <underline method="pen" type="straight" language="LA" text="vero clarorum viroru[m] aut tacitos congressus esse oporteat, aut ludicros sermones, aut rerum colloquia leuiorum"/>
        <underline method="pen" type="straight" language="LA" text="eius tractatio optimo"/>
        <underline method="pen" type="straight" language="LA" text="de opera publica"/>
        <underline method="pen" type="straight" language="LA" text="sed ne literam quidem vllam facimus nisi forensem"/>
        <underline method="pen" type="straight" language="LA" text="hebescere &amp; languere nolumus, sed etiam vt plurimis prosimus enitimur"/>
        <underline method="pen" type="straight" language="LA" text="non modo non minui, sed etiam augeri arbitramur eorum, quoru[m] ad populares illustresque laudes, has etia[m] minus notas, min&uacute;sque peruulgatas adiungimus"/>
        <underline method="pen" type="straight" language="LA" text="negent his qui in nostris libris dispute[n]t, fuisse earu[m] rerum de quibus disputatur scie[n]tiam"/>
        <underline method="pen" type="straight" language="LA" text="mihi videntur no[n] solum viuis, sed etia[m] mortuis inuidere"/>
        <underline method="pen" type="straight" language="LA" text="disciplinam philosophiae probaret"/>
        <underline method="pen" type="straight" language="LA" text="contra omnes dicere, quae videntur, solemus: non"/>
        
        
