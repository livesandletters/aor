        <marginalia hand="Italian" anchor_text="id quod oportuisset simpliciter &amp; singulari modo docere, illos distribuisse dupliciter bipertito">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>simpl[icite]r singulari modo / x dupl[icite]r bipertito.</marginalia_text>
                </position>
            </language>
            <translation>Simply and by one method / not twofold in different ways.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="modo id quod obest, sed &amp; id quod neque obest, neque iuuat, satius est praeterire"/>
        <underline method="pen" type="straight" language="LA" text="perturbate, nequid co[n]torte, nequid ambigue, nequid noue"/>
        <underline method="pen" type="straight" language="LA" text="Nam quo breuior, eo dilucidior &amp; cognitu facilior narratio fiet"/>
        <underline method="pen" type="straight" language="LA" text="ne refelli possit"/>
        <underline method="pen" type="straight" language="LA" text="Si vera res erit, nihilominus haec omnia narrando co[n]seruanda sunt. Nsm saepe veritas, nisi haec seruata sint, fide[m] facere non potest. Sin erit ficta, e&ograve; magis co[n]seruanda eru[n]t. De"/>
        <underline method="pen" type="straight" language="LA" text="arbitror mihi co[n]stare"/>
        <underline method="pen" type="straight" language="LA" text="vt nihil minus industriae qu&agrave;m rei vtilitas postulabit"/>
        <underline method="pen" type="straight" language="LA" text="Eam plus qu&agrave;m triu[m] partium numero esse non oportet"/>
        <underline method="pen" type="straight" language="LA" text="suspicionem afferet auditori meditationis &amp; artificii, quae res fidem abrogat oratori"/>
        <underline method="pen" type="straight" language="LA" text="noster doctor tres putauit esse"/>
        <underline method="pen" type="straight" language="LA" text="simpliciter &amp; singulari modo docere, illos distribuisse"/>
        <underline method="pen" type="straight" language="LA" text="capitis arcessit"/>
        
        
