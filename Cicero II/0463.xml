    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="educet atque confirmet"/>
        <underline method="pen" type="straight" language="LA" text="tam acri memoria fer&egrave;"/>
        <underline method="pen" type="straight" language="LA" text="dispositis notat&iacute;sque rebus, ordinem verborum aut sententiarum co[m]plectatur"/>
        <underline method="pen" type="straight" language="LA" text="tam hebeti"/>
        <underline method="pen" type="straight" language="LA" text="animis affigi nostris, quae essent &agrave; sensu tradita atque impressa"/>
        <underline method="pen" type="straight" language="LA" text="oculorum commendatione animis traderentur, vt res caecas, &amp; ab aspectus iudicio remotas conformatio quaeda[m] &amp; imago &amp; figura ita notaret, vt ea, quae cogita[n]do complecti vix possemus, intuendo quasi teneremus"/>
        <underline method="pen" type="straight" language="LA" text="formis"/>
        <underline method="pen" type="straight" language="LA" text="corporibus"/>
        <underline method="pen" type="straight" language="LA" text="sub aspectum veniunt"/>
        <underline method="pen" type="straight" language="LA" text="sedes opus est, etenim corpus intelligi sine loco non potest"/>
        <underline method="pen" type="straight" language="LA" text="peruulgata multus &amp; insole[n]s sim"/>
        <underline method="pen" type="straight" language="LA" text="illustribus"/>
        <underline method="pen" type="straight" language="LA" text="explicatis, modicis interuallis"/>
        <underline method="pen" type="straight" language="LA" text="age[n]tibus, acribus, insignitis"/>
        <underline method="pen" type="straight" language="LA" text="quae occurrere celerit&eacute;rque percutere animum possint"/>
        <underline method="pen" type="straight" language="LA" text="co[n]suetudo gignitur"/>
        <underline method="pen" type="straight" language="LA" text="co[n]seruata"/>
        <underline method="pen" type="straight" language="LA" text="immutata"/>
        <underline method="pen" type="straight" language="LA" text="traducta ex parte ad genus notatio"/>
        <underline method="pen" type="straight" language="LA" text="vnius verbi imagine totius sententiae informatio"/>
        <underline method="pen" type="straight" language="LA" text="formarum varietate locos distinguentis"/>
        <underline method="pen" type="straight" language="LA" text="quasi articuli connectunt membra orationis"/>
        <underline method="pen" type="straight" language="LA" text="propria est oratoris"/>
        <underline method="pen" type="straight" language="LA" text="ab inertibus dicitur"/>
        <underline method="pen" type="straight" language="LA" text="opprimi memoria[m] imaginu[m] pondere, &amp; obscurari etia[m] id, quod per se natura tenere potuisset"/>
        <underline method="pen" type="straight" language="LA" text="ta[n]quam literis in cera"/>
        <underline method="pen" type="straight" language="LA" text="non eruenda memoria est, si est nulla naturalis: sed certe, si latet, euocanda est"/>
        <underline method="pen" type="straight" language="LA" text="bene longum hominis vtinam non impudentis, illud quidem certe, non nimis verecundi: qui"/>
        <underline method="pen" type="straight" language="LA" text="insolita[m] mihi loquacitate[m] impulerit"/>
        <underline method="pen" type="straight" language="LA" text="acceperitis"/>
        <underline method="pen" type="straight" language="LA" text="fratre respondeo"/>
        <underline method="pen" type="straight" language="LA" text="facilitatem"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d magno sum leuatus errore"/>
        <underline method="pen" type="straight" language="LA" text="illa tanta tua in causis diuinitas"/>
        <underline method="pen" type="straight" language="LA" text="ista attigisse"/>
        <underline method="pen" type="straight" language="LA" text="vndique collegisse"/>
        <underline method="pen" type="straight" language="LA" text="partim correxisse video, partim comprobasse"/>
        <underline method="pen" type="straight" language="LA" text="id quod dixisti, fore vt tibi"/>
        <underline method="pen" type="straight" language="LA" text="qui te attentissime audieru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="Adimere"/>
        <underline method="pen" type="straight" language="LA" text="omnem recusationem Crasso volui"/>
        <underline method="pen" type="straight" language="LA" text="quem ego paulo scieba[m] vel pude[n]tius vel inuitius"/>
        <underline method="pen" type="straight" language="LA" text="nolo enim dicere de tam suaui homine fastidiosius) ad hoc genus sermonis accedere"/>
        <underline method="pen" type="straight" language="LA" text="An aetate[m] afferet? quadriennio minor est"/>
        <underline method="pen" type="straight" language="LA" text="quae succisiuis operis"/>
        <underline method="pen" type="straight" language="LA" text="tam sui despiciens fuit, qui desperaret aut melius, aut eodem modo se posse dicere"/>
        <underline method="pen" type="straight" language="LA" text="tam arrogans"/>
        
