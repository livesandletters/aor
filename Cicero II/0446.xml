    <annotation>
        
        
        <underline method="pen" type="straight" language="LA" text="renouabam atque reuocaba[m]"/>
        <underline method="pen" type="straight" language="LA" text="in possessione iudicii ac defensionis meae constitisse"/>
        <underline method="pen" type="straight" language="LA" text="cuius ius etia[m] cum seditionis coniunctione defenderam"/>
        <underline method="pen" type="straight" language="LA" text="desyderio"/>
        <underline method="pen" type="straight" language="LA" text="conuertera[m]"/>
        <underline method="pen" type="straight" language="LA" text="admiscere huic generi orationis vehementi atque atroci"/>
        <underline method="pen" type="straight" language="LA" text="decernere: nihil mihi ad existimatione[m] turpius, nihil ad dolore[m] acerbius accidere posse"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m si is, qui saepe alienissimis &agrave; me, sed meis tamen ciuibus saluti existimarer fuisse, sodali meo auxiliu[m] ferre non potuissem"/>
        <underline method="pen" type="straight" language="LA" text="aetati meae"/>
        <underline method="pen" type="straight" language="LA" text="si iusto, si pio dolore me esse affectum"/>
        <underline method="pen" type="straight" language="LA" text="concederent"/>
        <underline method="pen" type="straight" language="LA" text="deprecatum"/>
        <underline method="pen" type="straight" language="LA" text="minuere maiestatem"/>
        <underline method="pen" type="straight" language="LA" text="perquam breuiter perstrinxi atque attigi"/>
        <underline method="pen" type="straight" language="LA" text="altera concitatione[m] habet"/>
        <underline method="pen" type="straight" language="LA" text="altera co[m]mendatione[m], quae minime praeceptis artium sunt perpolitae"/>
        <underline method="pen" type="straight" language="LA" text="vt &amp; acerrimus in Caepionis inuidia renoua[n]da"/>
        <underline method="pen" type="straight" language="LA" text="&amp; in meis moribus erga meos necessarios declarandis mansuetissimus viderer"/>
        <underline method="pen" type="straight" language="LA" text="affectis animis iudicu[m] qu&agrave;m doctis, tua Sulpiti est &agrave; nobis tum accusatio victa"/>
        <underline method="pen" type="straight" language="LA" text="Vere hercule"/>
        <underline method="pen" type="straight" language="LA" text="ista co[m]memoras"/>
        <underline method="pen" type="straight" language="LA" text="nihil vnqua[m] vidi, quod tam"/>
        <underline method="pen" type="straight" language="LA" text="manibus elaberetur, qu&agrave;m mihi tum est elapsa illa causa"/>
        <underline method="pen" type="straight" language="LA" text="non iudicium, sed incendium tradidissem"/>
        <underline method="pen" type="straight" language="LA" text="haesitatio tract&uacute;sque verboru[m]"/>
        <underline method="pen" type="straight" language="LA" text="quod tibi vnum ad ignoscendum homines dabant, tenuisti"/>
        <underline method="pen" type="straight" language="LA" text="munisti"/>
        <underline method="pen" type="straight" language="LA" text="Ecce autem"/>
        <underline method="pen" type="straight" language="LA" text="aliud profecisse"/>
        <underline method="pen" type="straight" language="LA" text="serpere occulte coepisti"/>
        <underline method="pen" type="straight" language="LA" text="miscuisti"/>
        <underline method="pen" type="straight" language="LA" text="ad eunde[m] impetu[m] populi confugiendo refutasti"/>
        <underline method="pen" type="straight" language="LA" text="nulla praecepta desyderaba[m]"/>
        <underline method="pen" type="straight" language="LA" text="sequi"/>
        <underline method="pen" type="straight" language="LA" text="spectare"/>
        <underline method="pen" type="straight" language="LA" text="faces"/>
        <underline method="pen" type="straight" language="LA" text="ita animatis"/>
        <underline method="pen" type="straight" language="LA" text="mentes oratione flectendas"/>
        <underline method="pen" type="straight" language="LA" text="tragoedias agamus in nugis"/>
        <underline method="pen" type="straight" language="LA" text="conuellere adoriamur ea, quae no[n] possint co[m]moueri"/>
        <underline method="pen" type="straight" language="LA" text="molienda"/>
        <underline method="pen" type="straight" language="LA" text="charitatem"/>
        <underline method="pen" type="straight" language="LA" text="Enite[n]dum est"/>
        <underline method="pen" type="straight" language="LA" text="ad vtilitatem suam retulisse"/>
        <underline method="pen" type="straight" language="LA" text="caeteris co[m]modandi fauetur"/>
        <underline method="pen" type="straight" language="LA" text="benefacta"/>
        
        
