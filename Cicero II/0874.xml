        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>co[l]loqui</marginalia_text>
                </position>
            </language>
            <translation>To converse.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="iniuriosa facineros&aacute;que vita">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Vita { iniuriosa facinorosa</marginalia_text>
                </position>
            </language>
            <translation>A life, insulting and wicked.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="obtemperatio">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>obtemp[er]atio.</marginalia_text>
                </position>
            </language>
            <translation>Submission.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="poena, non natura">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Poena. Natura.</marginalia_text>
                </position>
            </language>
            <translation>Punishment, nature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="poena, si metus supplicii, non ipsa turpitudo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Poena[e] metus. Turpitudo</marginalia_text>
                </position>
            </language>
            <translation>Fear of punishment, baseness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vtilitate aliqua atque fructu, callidi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vtilitas. Calliditas</marginalia_text>
                </position>
            </language>
            <translation>Utility, Shrewdness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nisi testem &amp; iudicem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vir natura iustus</marginalia_text>
                </position>
            </language>
            <translation>By nature a just man.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="metuat ne emanet, id est ne malum habeat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>formidine poena[e] bon[us]</marginalia_text>
                </position>
            </language>
            <translation>Good from the fear of punishment.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tyrannicis legibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Tyrannoru[m] Leges.</marginalia_text>
                </position>
            </language>
            <translation>The laws of tyrants.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Interrex * noster tulit, vt Dictator">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Interrex. Dictator.</marginalia_text>
                </position>
            </language>
            <translation>Interrex, dictator.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="lex co[n]stituit vna">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Lex vna.</marginalia_text>
                </position>
            </language>
            <translation>One law.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vtilitate omnia metienda">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vtilitate o[mn]ia metienda</marginalia_text>
                </position>
            </language>
            <translation>Everything is determined by utility.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ex eo qu&ograve;d natura propensi sumus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ex natura[e] fontibus o[mn]es hausta[e] su[n]t virtutes</marginalia_text>
                </position>
            </language>
            <translation>All virtues are derived from nature's fountains.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="bona[m] &agrave; mala">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ex bona. mala.</marginalia_text>
                </position>
            </language>
            <translation>From the good, the bad.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="communis intelligentia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Communis intelligentia</marginalia_text>
                </position>
            </language>
            <translation>A common intelligence.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="virtus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Virtus</marginalia_text>
                </position>
            </language>
            <translation>Virtue.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="bonum non est opinionibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>bonu[m] no[n] est Opinione</marginalia_text>
                </position>
            </language>
            <translation>It is not good by judgement.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="fraud&iacute;sque cruciatu"/>
        <underline method="pen" type="straight" language="LA" text="Qu&ograve;d si poena, si metus supplicii, non ipsa turpitudo deterret ab iniuriosa facineros&aacute;que vita, nemo est iniustus, aut incauti potius habendi 
            sunt, improbi"/>
        <underline method="pen" type="straight" language="LA" text="homo in tenebris, qui nihil timet nisi testem &amp; iudicem?"/>
        <underline method="pen" type="straight" language="LA" text="quem multo auro spoliare possit imbecillum atque solum?"/>
        <underline method="pen" type="straight" language="LA" text="sed qu&ograve;d metuat ne emanet, id est ne malum habeat"/>
        <underline method="pen" type="straight" language="LA" text="iusta esse quae scita sint in populoru[m] institutis, aut legibus. Eti&aacute;mnesiquae"/>
        <underline method="pen" type="straight" language="LA" text="indicita causa"/>
        <underline method="pen" type="straight" language="LA" text="Est enim vnum ius, quo deuincta est societas hominum, &amp; quod lex co[n]stituit vna: quae lex, est recta ratio imperandi atque prohibendi. 
            quam qui ignorat, is est iniustus: siue est illa scripta vspiam, siue nusquam. Qu&ograve;d"/>
        <underline method="pen" type="straight" language="LA" text="leges, e&aacute;sque perrumpet"/>
        <underline method="pen" type="straight" language="LA" text="conuellitur"/>
        <underline method="pen" type="straight" language="LA" text="propensi sumus"/>
        <underline method="pen" type="straight" language="LA" text="quod fundamentu[m]"/>
        <underline method="pen" type="straight" language="LA" text="testamenta falsa supponere"/>
        <underline method="pen" type="straight" language="LA" text="haec suffragiis, aut scitis multitudinis probare[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="naturae norma"/>
        <underline method="pen" type="straight" language="LA" text="intelligentia"/>
        <underline method="pen" type="straight" language="LA" text="nostris inchoauit"/>
        <underline method="pen" type="straight" language="LA" text="catum"/>
        <underline method="pen" type="straight" language="LA" text="Est enim virtus, perfecta ratio, quod cert&egrave; in natura est"/>
        
