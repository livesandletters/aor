            </language>
            <translation>
                Gaius Claudius Centho.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="L. Iunius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        L. Iunius
                    </marginalia_text>
                    <person name="Lucius Junius Pullus"/>
                </position>
            </language>
            <translation>
                Lucius Junius Pullus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Agamemnoni">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Agamemnon.
                    </marginalia_text>
                    <person name="Agamemnon"/>
                </position>
            </language>
            <translation>
                Agamemnon.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dirarum obnunciatione">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Diraru[m] obnunciatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Announcement of omens.
            </translation>
