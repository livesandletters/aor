    <page filename="0450.tif" reader="John Dee" pagination="180" signature="[m.ii[v]]"/>   
    
    <annotation>
        
        
        <underline method="pen" type="straight" language="LA" text="foenu[m] alios aiebat esse oportere"/>
        <underline method="pen" type="straight" language="LA" text="vt ambrosia alendus esse videatur"/>
        <underline method="pen" type="straight" language="LA" text="ad Antonii reliqua redeamus"/>
        <underline method="pen" type="straight" language="LA" text="defessus iam labore atque itinere disputationis meae"/>
        <underline method="pen" type="straight" language="LA" text="requiescam in Caesaris sermone, quasi in aliquo peropportuno diuersorio"/>
        <underline method="pen" type="straight" language="LA" text="non nimium liberale hospitium meum dices: nam te in viam, simul ac perpaululum gustaris, extrudam &amp; eiiciam"/>
        <underline method="pen" type="straight" language="LA" text="vnum"/>
        <underline method="pen" type="straight" language="LA" text="alterum"/>
        <underline method="pen" type="straight" language="LA" text="velle risum mouere"/>
        <underline method="pen" type="straight" language="LA" text="repente erumpat"/>
        <underline method="pen" type="straight" language="LA" text="viderit Democritus"/>
        <underline method="pen" type="straight" language="LA" text="quidem illi scirent, qui pollicerentur"/>
        <underline method="pen" type="straight" language="LA" text="Locus autem &amp; regio quasi ridiculi"/>
        <underline method="pen" type="straight" language="LA" text="turpitudine &amp; deformitate quadam co[n]tinetur"/>
        <underline method="pen" type="straight" language="LA" text="quae notant &amp; designa[n]t turpitudine[m] aliquam no[n] turpiter"/>
        <underline method="pen" type="straight" language="LA" text="acumen vno saepe in verbo positum, maxime respondentis, no[n]nunquam etiam lacessentis"/>
        <underline method="pen" type="straight" language="LA" text="frangit aduersarium"/>
        <underline method="pen" type="straight" language="LA" text="impedit"/>
        <underline method="pen" type="straight" language="LA" text="eleuat"/>
        <underline method="pen" type="straight" language="LA" text="deterret"/>
        <underline method="pen" type="straight" language="LA" text="refutat"/>
        <underline method="pen" type="straight" language="LA" text="seueritatem mitigat &amp; relaxat"/>
        <underline method="pen" type="straight" language="LA" text="dilui no[n] facile est, ioco ris&uacute;que dissoluit"/>
        <underline method="pen" type="straight" language="LA" text="in quarto loco quaere[n]di posueramus"/>
        <underline method="pen" type="straight" language="LA" text="insignis improbitas, &amp; scelere iuncta, nec rursus miseria insignis agitata ridetur. facinorosos enim maiore quada[m] vi qu&agrave;m ridiculi vulnerari volunt: miseros illudi nolunt, nisi si se forte iactant"/>
        <underline method="pen" type="straight" language="LA" text="Parcendum est aute[m] maxime charitati hominu[m], ne temere in eos dicas, qui diligu[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="ad suppliciu[m] rapiendi videntur"/>
        <underline method="pen" type="straight" language="LA" text="e&aacute;que belle agitata ridentur"/>
        <underline method="pen" type="straight" language="LA" text="Est etiam deformitatis &amp; corporis vitiorum satis bella materies ad iocandum"/>
        <underline method="pen" type="straight" language="LA" text="insulse"/>
        <underline method="pen" type="straight" language="LA" text="perridicule possis"/>
        <underline method="pen" type="straight" language="LA" text="scurrilis"/>
        <underline method="pen" type="straight" language="LA" text="mimicus"/>
        <underline method="pen" type="straight" language="LA" text="fabella"/>
        <underline method="pen" type="straight" language="LA" text="de amicula rixatus: salsa, attame[n] &agrave; teipso ficta tota narratio"/>
        <underline method="pen" type="straight" language="LA" text="addidisti clausulam"/>
        <underline method="pen" type="straight" language="LA" text="tum omnibus in parietibus inscriptas fuisse literas, tria LLL, duo MM. Cum quaereres id quid esset, senem tibi que[m]dam oppidanum dixisse, Lacerat lacertum Largii mordax Memmius. Perspicitis"/>
        <underline method="pen" type="straight" language="LA" text="mendaciunculis aspergendum"/>
        <underline method="pen" type="straight" language="LA" text="Est aute[m] haec huius generis virtus"/>
        <underline method="pen" type="straight" language="LA" text="In re est item ridiculu[m], quod ex quada[m] deprauata imitatione sumi solet"/>
        <underline method="pen" type="straight" language="LA" text="extento brachio paululum etiam de gestu addidit"/>
        <underline method="pen" type="straight" language="LA" text="Mimoru[m] est enim ethologoru[m]"/>
        
        
