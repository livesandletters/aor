<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0465.tif" reader="John Dee" pagination="195" signature="n.ii."/>   
    
    <annotation>
        
        
        
        <underline method="pen" type="straight" language="LA" text="haec tibi est excidenda lingua, qua vel euulsa spiritu ipso libidinem tuam libertas mea refutabit"/>
        <underline method="pen" type="straight" language="LA" text="vehementissima contentione animi"/>
        <underline method="pen" type="straight" language="LA" text="quam senatus frequens secutus est"/>
        <underline method="pen" type="straight" language="LA" text="cygnea fuit diuini hominis vox &amp; oratio, qua[m] quasi expecta[n]tes post eius interitum veniebamus in curiam, vt vestigium illud ipsum, in quo ille postremum institisset, contueremur"/>
        <underline method="pen" type="straight" language="LA" text="tum latus ei dicenti condoluisse, sudor&eacute;mque multum co[n]secutum esse audiebamus: ex quo cum cohorruisset, cum febri domu[m] rediit, di&eacute;que septimo est lateris dolore consumptus"/>
        <underline method="pen" type="straight" language="LA" text="quae in medio spatio saepe franguntur &amp; corruunt, &amp; ant&egrave; in ipso cursu obruuntur, qu&agrave;m portum conspicere potuerunt"/>
        <underline method="pen" type="straight" language="LA" text="ambitionis labore vita districta"/>
        <underline method="pen" type="straight" language="LA" text="floruit"/>
        <underline method="pen" type="straight" language="LA" text="fructu amplitudinis aut reip. dignitate"/>
        <underline method="pen" type="straight" language="LA" text="annus primus ab honorum perfunctione aditum omnium co[n]cessu ad summam authoritatem dabat"/>
        <underline method="pen" type="straight" language="LA" text="is eius omnem spem, atque omnia vitae consilia morte peruertit"/>
        <underline method="pen" type="straight" language="LA" text="luctuosum"/>
        <underline method="pen" type="straight" language="LA" text="non erepta"/>
        <underline method="pen" type="straight" language="LA" text="donata mors esse videatur"/>
        <underline method="pen" type="straight" language="LA" text="deformatam"/>
        <underline method="pen" type="straight" language="LA" text="non vagabitur oratio mea longius"/>
        <underline method="pen" type="straight" language="LA" text="definietur"/>
        <underline method="pen" type="straight" language="LA" text="defleta"/>
        <underline method="pen" type="straight" language="LA" text="recordatus"/>
        <underline method="pen" type="straight" language="LA" text="incolumen fortuna[m]"/>
        <underline method="pen" type="straight" language="LA" text="imperatoriis manubiis ornarat"/>
        <underline method="pen" type="straight" language="LA" text="scelere proditu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vt ille qui haec non vidit, &amp; vixisse cum repub. pariter, &amp; cum illa simul extrinctus esse videatur"/>
        <underline method="pen" type="straight" language="LA" text="Crasso se dicarant, horribiles"/>
        <underline method="pen" type="straight" language="LA" text="florentem"/>
        <underline method="pen" type="straight" language="LA" text="depulsis per inuidiam tribunatu, non multis ab eo te[m]pore me[n]sibus eiectus est &egrave; ciuitate"/>
        <underline method="pen" type="straight" language="LA" text="qui in eade[m] inuidiae flamma fuisset"/>
        <underline method="pen" type="straight" language="LA" text="florescenti, ferro erepta"/>
        <underline method="pen" type="straight" language="LA" text="cum vitae flore, tum mortis opportunitate, diuino consilio &amp; ortum &amp; extinctum esse arbitror"/>
        <underline method="pen" type="straight" language="LA" text="subeunda"/>
        <underline method="pen" type="straight" language="LA" text="eadem esse te funerum patriae spectatorem coegisset"/>
        <underline method="pen" type="straight" language="LA" text="admistam"/>
        
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen"/> 
        
        
    </annotation>
    
