        <marginalia hand="English_secretary" anchor_text="ita connectitur">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ita con[n]ectitur</marginalia_text>
                </position>
            </language>
            <translation>It is thus connected.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="magna luctatio est">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>tibi magna luctatio est cu[m] magnu[m] certame[n] est cu[m]</marginalia_text>
                </position>
            </language>
            <translation>It is a great stuggle for you with;  it is  a great contest for your with.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="[astrologoru[m] percepta huiusmodi], Siquis">
