        <marginalia hand="English_secretary" anchor_text="Nihil fieri quod no necesse fuerit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nihil fit q[uam] no[n] necesse fit</marginalia_text>
                </position>
            </language>
            <translation>Nothing happens which is not necessary.</translation>
        </marginalia>
        
