    <page filename="0434.tif" reader="John Dee" pagination="164" signature="[l.ii.[v]]"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="in adolescente foecunditas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>In adolesce[n]te foecu[n]ditas laudabilis.</marginalia_text>
                </position>
            </language>
            <translation>Abundance [is] praiseworthy in the young.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Isocrates">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Isocrates.</marginalia_text>
                    <person name="Isocrates"/>
                </position>
            </language>
            <translation>Isocrates.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="paruula"/>
        <underline method="pen" type="straight" language="LA" text="de quo quaerimus"/>
        <underline method="pen" type="straight" language="LA" text="aute[m] celeri"/>
        <underline method="pen" type="straight" language="LA" text="concitata, quod erat ingenii"/>
        <underline method="pen" type="straight" language="LA" text="efferuescentibus"/>
        <underline method="pen" type="straight" language="LA" text="paulo nimium redunda[n]tibus, quod erat aetatis"/>
        <underline method="pen" type="straight" language="LA" text="Volo enim se efferat in adolescente foecunditas"/>
        <underline method="pen" type="straight" language="LA" text="facilius, sicut in vitibus, reuocantur ea quae sese nimium profuderunt, qu&agrave;m si nihil valet materies, noua sarmenta cultura excitantur: ita volo esse in adolesce[n]te vnde aliquid amputem: no[n] enim potest in eo esse succus diuturnus, quod nimis celeriter est maturitatem assecutu[m]"/>
        <underline method="pen" type="straight" language="LA" text="indolem"/>
        <underline method="pen" type="straight" language="LA" text="dimisi te[m]pus"/>
        <underline method="pen" type="straight" language="LA" text="vt foru[m] sibi ludum putaret esse ad discendu[m]"/>
        <underline method="pen" type="straight" language="LA" text="me quidem si audiret"/>
        <underline method="pen" type="straight" language="LA" text="arripuit"/>
        <underline method="pen" type="straight" language="LA" text="atque etia[m] addidit"/>
        <underline method="pen" type="straight" language="LA" text="intercesserat ab hoc sermone"/>
        <underline method="pen" type="straight" language="LA" text="cu[m]"/>
        <underline method="pen" type="straight" language="LA" text="no[n] est credibile quid interesse mihi sit visum inter eum, qui tum erat, &amp; qui anno ant&egrave; fuerat"/>
        <underline method="pen" type="straight" language="LA" text="magnificu[m]"/>
        <underline method="pen" type="straight" language="LA" text="praeclaru[m] natura"/>
        <underline method="pen" type="straight" language="LA" text="eode[m] studio atque imitatione incidisset"/>
        <underline method="pen" type="straight" language="LA" text="hoc sit primu[m] in praeceptis meis"/>
        <underline method="pen" type="straight" language="LA" text="diligentissime persequatur"/>
        <underline method="pen" type="straight" language="LA" text="que[m] ant&egrave; delegerit, imitando effingat"/>
        <underline method="pen" type="straight" language="LA" text="atque ita exprimat"/>
        <underline method="pen" type="straight" language="LA" text="co[n]sectentur imita[n]do"/>
        <underline method="pen" type="straight" language="LA" text="Nihil est facilius, qu&agrave;m amictum imitari alicuius, aut statu[m], aut motum"/>
        <underline method="pen" type="straight" language="LA" text="amissa voce furit in"/>
        <underline method="pen" type="straight" language="LA" text="neruos in"/>
        <underline method="pen" type="straight" language="LA" text="oris prauitatem"/>
        <underline method="pen" type="straight" language="LA" text="verborum latitudine[m] imitatur"/>
        <underline method="pen" type="straight" language="LA" text="primum vigilet necesse est"/>
        <underline method="pen" type="straight" language="LA" text="quem probauit"/>
        <underline method="pen" type="straight" language="LA" text="censetis esse, cur aetates extulerint singulae singula prop&egrave; genera dicendi"/>
        <underline method="pen" type="straight" language="LA" text="quae dicendi ratio volu[n]t&aacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="quidem scripta co[n]stent"/>
        <underline method="pen" type="straight" language="LA" text="eadem aetate"/>
        <underline method="pen" type="straight" language="LA" text="subtiles"/>
        <underline method="pen" type="straight" language="LA" text="acuti, breues, sententiis magis qu&agrave;m verbis abunda[n]tes"/>
        <underline method="pen" type="straight" language="LA" text="proponerent ad imitandum"/>
        <underline method="pen" type="straight" language="LA" text="retineba[n]t illum Periclis succum, sed erant paulo vberiore filo. Ecce tibi exortus est Isocrates"/>
        <underline method="pen" type="straight" language="LA" text="cuius &egrave; ludo tanquam ex equo Troiano innumeri principes exierunt: sed eoru[m] partim in pompa, partim in acie illustres esse voluerunt"/>
        <underline method="pen" type="straight" language="LA" text="naturis differunt"/>
        <underline method="pen" type="straight" language="LA" text="ad causas contulerunt"/>
        <underline method="pen" type="straight" language="LA" text="quandiu mansit imitatio"/>
        <underline method="pen" type="straight" language="LA" text="vixit. Posteaqua[m] extinctis"/>
        <underline method="pen" type="straight" language="LA" text="sensim obscurata est &amp; euanuit"/>
        <underline method="pen" type="straight" language="LA" text="dicendi molliora"/>
        <underline method="pen" type="straight" language="LA" text="remissiora"/>
        <underline method="pen" type="straight" language="LA" text="viguerunt"/>
        <underline method="pen" type="straight" language="LA" text="politissimus"/>
        <underline method="pen" type="straight" language="LA" text="vsque ad hoc te[m]pus persequi"/>
        <underline method="pen" type="straight" language="LA" text="multo eius oratio esset pressior"/>
        
