    <page filename="04589.tif" reader="John Dee" pagination="189"/>   
    
    <annotation>
        
        
        <underline method="pen" type="straight" language="LA" text="[spe]ciem valere videantur: quanto hoc magis in oratione expectandum est, in qua non vis potius, sed delectatio postulatur? Nihil est denique in natura rerum omniu[m], quod se vniuersum prefundat, &amp; quod totum repente euolet: sic omnia quae fiunt, qua&eacute;que aguntur acerrime, lenioribus principiis natura ipsa praetexuit"/>
        <underline method="pen" type="straight" language="LA" text="ex ipsis visceribus causae sume[n]da sunt"/>
        <underline method="pen" type="straight" language="LA" text="pertentata"/>
        <underline method="pen" type="straight" language="LA" text="perspecta"/>
        <underline method="pen" type="straight" language="LA" text="inuentis"/>
        <underline method="pen" type="straight" language="LA" text="instructis"/>
        <underline method="pen" type="straight" language="LA" text="momenti aliquid afferent"/>
        <underline method="pen" type="straight" language="LA" text="intima defensione depromptae"/>
        <underline method="pen" type="straight" language="LA" text="penitus ex ea causa, quae tum agatur, effloruisse"/>
        <underline method="pen" type="straight" language="LA" text="vt aedibus ac templis vestibula &amp; aditus, sic causis principia pro portione rerum praeponere"/>
        <underline method="pen" type="straight" language="LA" text="reos appello, quoru[m] res est"/>
        <underline method="pen" type="straight" language="LA" text="infanda"/>
        <underline method="pen" type="straight" language="LA" text="restitui sanar&iacute;que non possit"/>
        <underline method="pen" type="straight" language="LA" text="agendo efficitur melius qu&agrave;m rogando"/>
        <underline method="pen" type="straight" language="LA" text="confundendum"/>
        <underline method="pen" type="straight" language="LA" text="gignuntur"/>
        <underline method="pen" type="straight" language="LA" text="in mediis causis dicuntur"/>
        <underline method="pen" type="straight" language="LA" text="alliciendu[m]"/>
        <underline method="pen" type="straight" language="LA" text="incitandum"/>
        <underline method="pen" type="straight" language="LA" text="impelli primo iudicem leuiter"/>
        <underline method="pen" type="straight" language="LA" text="iam inclinato reliqua incumbat oratio"/>
        <underline method="pen" type="straight" language="LA" text="Connexum autem ita sit principum co[n]sequenti orationi, vt non ta[n]quam citharoedi prooemium affictum aliqui=od, sed cohaerens cum omni corpore membrum esse videatur"/>
        <underline method="pen" type="straight" language="LA" text="meditati ediderunt"/>
        <underline method="pen" type="straight" language="LA" text="audientiam"/>
        <underline method="pen" type="straight" language="LA" text="Atque eiusmodi illa prolusio debet esse, non vt Samnitum, qui vibrant hastas ante pugnam, quibus in pugnando nihil vtuntur: sed vt ipsis sententiis, quibus proluserunt, vel pugnare possint"/>
        <underline method="pen" type="straight" language="LA" text="verbum nullum redu[n]det"/>
        <underline method="pen" type="straight" language="LA" text="obscuritatem affert"/>
        <underline method="pen" type="straight" language="LA" text="quae narrationis est maxima"/>
        <underline method="pen" type="straight" language="LA" text="seruilis percontatio"/>
        <underline method="pen" type="straight" language="LA" text="peruarie iucund&eacute;que"/>
        <underline method="pen" type="straight" language="LA" text="totum conficere"/>
        <underline method="pen" type="straight" language="LA" text="concisum est, ita vt non breuitati seruitum sit, sed magis venustati"/>
        <underline method="pen" type="straight" language="LA" text="festiuitatem habet narratio distincta personis &amp; interpuncta sermonibus"/>
        <underline method="pen" type="straight" language="LA" text="multo apertius ad intelligendu[m] est, si sic consistitur aliquando, ac non ista breuitate percurritur"/>
        
        
