            <translation>Aristotle.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vnum in locum conduxeru[n]t">  
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        vnu[m] in locu[m]
                        conducere
                        cogere.
                        co[n]ferre.
                    </marginalia_text>
                </position>
            </language>
            <translation>To assemble, to collect, to gather in one place.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="muneratur">
