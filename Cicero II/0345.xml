    <annotation>
        
        <marginalia hand="Italian" anchor_text="vt excellente[m] muliebris formae pulchritudine[m] muta in sese imago contineret, Helenae se pingere simulacru[m] velle dixit">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        muta imago                  
                        mutu[m] simulachru[m]. ]
                    </marginalia_text>
                </position>
            </language>
            <translation>A mute image; a mute likeness. ]</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="excellente[m] muliebris formae pulchritudine[m]; magna praeditos dignitate; corporu[m] viribus &amp; dignitatibus antesteterunt; ex istis virginibus formosissimas">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        excellens muliebris forma[e] pulchritudo
                        virgo formosa.
                        preditus magna dignitate
                        corporis dignitatibus antestare.
                    </marginalia_text>
                </position>
            </language>
            <translation>The excellent beauty of the female form. A beautiful girl. Bestowed with great merit. To excel in merit of the body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="longe caeteris excellere; plurimu[m] aliis praestare; multum omnibus corporu[m] viribus &amp; dignitatibus antesteterunt">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        longe ca[e]teris excellere
                        plurimu[m] alijs pr[a]estare
                        multis o[mn]ib[us] antestare ]
                    </marginalia_text>
                </position>
            </language>
            <translation>To surpass the others by far. To stand ahead of most others. To excel before all.</translation>   
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="CROTONIATAE">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
