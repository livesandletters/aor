    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="multa omnibus modis confusa &amp; variata">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>multis modis co[n]fusa variata</marginalia_text>
                </position>
            </language>
            <translation>Confused and varied in many ways.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="inusitata, tam nulla">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ta[m] invsitata ta[m] nulla</marginalia_text>
                </position>
            </language>
            <translation>So unseen and so void.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="superstitione &amp; deprauatione">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>sup[er]stitio deprauatio.</marginalia_text>
                </position>
            </language>
            <translation>Superstition and depravity.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Plena sunt imaginum omnia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plena imaginu[m] o[mn]ia.</marginalia_text>
                </position>
            </language>
            <translation>All things are full of phantoms.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="informata habeamus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>informata habem[us] multa.</marginalia_text>
                </position>
            </language>
            <translation>We have fashioned many things.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="omnia igitur quae volumus, nota nobis esse possunt.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>O[mn]ia qua[e] volum[us] nota nobis esse possu[n]t</marginalia_text>
                </position>
            </language>
            <translation>It is possible for us to know everything we wish to know [Loeb 154, 525].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Animorum est ea vis e&aacute;que natura, vt vigeant vigilantes nullo adue[n]titio pulsu, sed suo motu">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animoru[m] motus</marginalia_text>
                </position>
            </language>
            <translation>The disturbance of souls.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="maxima&eacute;que reliquiae reru[m] earum mouentur in animis &amp; agitanturm de quibus vigilantes aut cogitauimus aut egimus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Maxime reliquia[e] earu[m] reru[m] moue[n]tur in ani[mi]s de q[ui]b[us] vigila[n]tos aut cogitamim[us] aut egim[us]</marginalia_text>
                </position>
            </language>
            <translation>The remnants of our waking thoughts in particular, move and excite the soul [Loeb 154, 527].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="animis vigilantium cogitationum vestigia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Insu[n]t in a[nim]is vigila[n]tiu[m] cogitationu[m] vestigia</marginalia_text>
                </position>
            </language>
            <translation>Within our souls are traces of our waking thoughts.</translation>
        </marginalia>
        
