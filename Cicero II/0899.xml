        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cum Graecis Latina coniunxi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cum gra[e]cis latina co[n]inuge.</marginalia_text>
                </position>
