        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qua[m]diu voles: tandiu aute[m] velle debebis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Qua[m]diu quis velle discere debet</marginalia_text>
                </position>
            </language>
            <translation>Why you are bound to study as long as you wish.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="oratoris propriu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>oratoris propriu[m].</marginalia_text>
                </position>
            </language>
            <translation>Perculiar to an orator.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cicero, vt non[n] solum orationes meas, sed hos etia[m] de philosophia libros">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>or[ation]es Ciceronis. Philosophia</marginalia_text>
                </position>
            </language>
            <translation>Cicero's orations. Philosophical</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fore[n]se dicendi, &amp; hoc quietu[m] disputandi genus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>fore[n]se dicendi &amp; quietum disputandi genus.</marginalia_text>
                </position>
            </language>
            <translation>Declamatory speaking and calm manner of arguing.</translation>
        </marginalia>
 
        
        <marginalia hand="Italian" anchor_text="Demosthen[m], si illa quae &agrave; Platone didicerat, tenuisset, &amp; pronunciare voluisset, ornat&egrave; sp;endid&eacute;que facere potuisse. 
            Eod&eacute;,que modo de Aristotele &amp; Isocrate">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plato. Demosthenes Aristoteles Isocrates</marginalia_text>
                    <person name="Plato"/>
                    <person name="Demosthenes"/>
                    <person name="Aristotle"/>
                    <person name="Isocrates"/>
                </position>
            </language>
            <translation>Plato, Demosthenes, Aristotle, Isocrates.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de officiis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>De officijs</marginalia_text>
                    <book title="De officis"/>
                </position>
            </language>
            <translation>De officis.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="officii praeceptis tradendis philosophu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Philosophus officij pra[e]cepta tradire debet.</marginalia_text>
                </position>
            </language>
            <translation>A philosopher ought to have transmitted precepts on duty.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="voluptate[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>voluptas</marginalia_text>
                </position>
            </language>
            <translation>Pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dolorem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dolor.</marginalia_text>
                </position>
            </language>
            <translation>Pain.</translation>
        </marginalia>
        
        <marginalia hand="Italian"> <!-- M.B. The anchor text appears on the followig page 'praeceptio Stoicorum, Academicorum'. -->
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Academicoru[m] pra[e]ceptio.</marginalia_text>
                </position>
            </language>
            <translation>Precept of academics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Demetrius Phalereus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Demetrius</marginalia_text>
                    <person name="Demetrius of Phalerum"/>
                </position>
            </language>
            <translation>Demetrius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="grauissime &amp; copiosissime potuisse dicere">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>dicere grauissime copiosissimeq[ue]</marginalia_text>
                </position>
            </language>
            <translation>Spoken most powerfully and most copiously.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ornat&egrave; spendid&eacute;que">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>facere ornato splendidi</marginalia_text> <!-- M.B. Not sure of first word. -->
                </position>
            </language>
            <translation>Performed gracefully and elegantly.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="accurat&egrave; copios&eacute;que &agrave; philosophis disputata">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>disp[utata] accuratio copiosio</marginalia_text>
                </position>
            </language>
            <translation>Accurately and copiously argued.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="id mihi assumo, videor id meo iure quoda[n]modo vendicare">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>assumo vindico.</marginalia_text>
                </position>
            </language>
            <translation>I assume, I claim.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="officii praecepta firma, stabilia, co[n]iuncta naturae">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>officij pr[aec]epta { firma stabilia co[n]iuncta natur[a]e</marginalia_text>
                </position>
            </language>
            <translation>Precepts on duties { solid, constant, steadfast to nature. </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="si sibi consentaneae velint esse">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>conse[n]teri sibi si sibi co[n]sentaneus esse veli[nt]</marginalia_text>
                </position>
            </language>
            <translation>Consistent with, if it would be consistent with itself.</translation> 
        </marginalia>
       
        
        <underline method="pen" type="straight" language="LA" text="abundare oportet praeceptis"/>
        <underline method="pen" type="straight" language="LA" text="faciendum, vt par sis in vtriusque orationis facultate"/>
        <underline method="pen" type="straight" language="LA" text="principe huius aetatis philosophoru[m]"/>
        <underline method="pen" type="straight" language="LA" text="Latina[m] efficies"/>
        <underline method="pen" type="straight" language="LA" text="nostris pleniore[m]"/>
        <underline method="pen" type="straight" language="LA" text="Vis enim dice[n]di maior est in illis: sed hoc quoque colendum est &amp; aequabile &amp; temperatum orationis genus"/>
        <underline method="pen" type="straight" language="LA" text="in hoc numero haberi"/>
        <underline method="pen" type="straight" language="LA" text="subtilis, orator parum veheme[n]s, dulcis tamen, vt Theophrasti discipulu[m] possis agnoscere"/>
        <underline method="pen" type="straight" language="LA" text="volui, quod &amp; aetati tuae esset aptissimu[m], authoritati meae"/>
        <underline method="pen" type="straight" language="LA" text="latissim&egrave; patere"/>
        <underline method="pen" type="straight" language="LA" text="tradita ab illis &amp; praecepta"/>
        <underline method="pen" type="straight" language="LA" text="in e&oacute;que cole[n]do sita vitae est honestas omnis, &amp; negligendo turpitudo"/>
        <underline method="pen" type="straight" language="LA" text="bonu[m] sic"/>
        <underline method="pen" type="straight" language="LA" text="quanquam ita sint in promptu, vt res disputatione no[n] egeat, sunt"/>
        <underline method="pen" type="straight" language="LA" text="consentanea"/>
        <underline method="pen" type="straight" language="LA" text="est ea"/>
        
