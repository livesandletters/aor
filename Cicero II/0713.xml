        <underline method="pen" type="straight" language="LA" text="Quae enim venientia metuuntur, eadem efficiunt aegritudinem insta[n]tia. Laetitia autem &amp; libido in bonoru[m] opinione versantur, quum"/>
        <underline method="pen" type="straight" language="LA" text="illecta &amp; inflammata raoiatur, laetitia"/>
        <underline method="pen" type="straight" language="LA" text="Voluntas est, quae quid cu[m] ratione desyderat"/>
        <underline method="pen" type="straight" language="LA" text="vel cupiditas"/>
        <underline method="pen" type="straight" language="LA" text="examinatione humili atque fracta, nominetur"/>
        <underline method="pen" type="straight" language="LA" text="igitur metus rationi aduersa cautio"/>
        <underline method="pen" type="straight" language="LA" text="anim&oacute;sque demittunt &amp; contrahunt rationi non"/>
        <underline method="pen" type="straight" language="LA" text="definiunt pressius"/>
        <underline method="pen" type="straight" language="LA" text="in quo demitti contrah&iacute;que animo"/>
        <underline method="pen" type="straight" language="LA" text="aegritudo quasi morsum aliquem doloris efficiat: metus recessum quendam animi &amp; fugam: laetitia profusam hilaritatem: libido effrenatam appetentiam"/>
        <underline method="pen" type="straight" language="LA" text="vtendum est enim docendi causa verbo minus vsitato, quoniam"/>
        <underline method="pen" type="straight" language="LA" text="lacta[n]s malo"/>
        
