        <underline method="pen" type="straight" language="LA" text="sed intento opus est"/>
        <underline method="pen" type="straight" language="LA" text="alteram rationis participem faciunt"/>
        <underline method="pen" type="straight" language="LA" text="alteram expertem"/>
        <underline method="pen" type="straight" language="LA" text="in hac quaestione versari acutissime"/>
        <underline method="pen" type="straight" language="LA" text="auersa &agrave; recta ratione contra naturam animi co[m]motio. Quidam breuius, perturbationem esse appetitum vehementiorem"/>
