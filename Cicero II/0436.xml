    <annotation>
        
        <marginalia hand="Italian" anchor_text="Iure aute[m] omnia defenduntur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iure facta.</marginalia_text>
                </position>
            </language>
            <translation>Done by law.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dialectici">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dialectici.</marginalia_text>
                </position>
            </language>
            <translation>Dialecticians.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Iure aute[m] omnia defenduntur, quae sunt eius generis"/>
        <underline method="pen" type="straight" language="LA" text="aut imprude[n]tia"/>
        <underline method="pen" type="straight" language="LA" text="casu"/>
        <underline method="pen" type="straight" language="LA" text="quid vocetur quaeritur"/>
        <underline method="pen" type="straight" language="LA" text="cu[m] quo verbo quid appellandu[m] sit, co[n]tenditur"/>
        <underline method="pen" type="straight" language="LA" text="maiestatem imminutam"/>
        <underline method="pen" type="straight" language="LA" text="pendebat"/>
        <underline method="pen" type="straight" language="LA" text="quod causam facit"/>
        <underline method="pen" type="straight" language="LA" text="perqu&agrave;m puerile"/>
        <underline method="pen" type="straight" language="LA" text="vt neque absit quicqua[m], neque supersit"/>
        <underline method="pen" type="straight" language="LA" text="dilatauit"/>
        <underline method="pen" type="straight" language="LA" text="maiestate[m] minuere"/>
        <underline method="pen" type="straight" language="LA" text="reprehenso verbo vno, aut addito, aut dempto, saepe extorquetur &egrave; manibus"/>
        <underline method="pen" type="straight" language="LA" text="doctrinam redolet, exercitation&eacute;mque pen&egrave; puerilem"/>
        <underline method="pen" type="straight" language="LA" text="in sensum"/>
        <underline method="pen" type="straight" language="LA" text="in mentem iudicis intrare"/>
        <underline method="pen" type="straight" language="LA" text="praeterlabitur"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m percepta est"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; sententia discrepat"/>
        <underline method="pen" type="straight" language="LA" text="suggesta sunt"/>
        <underline method="pen" type="straight" language="LA" text="non nouu[m] genus nascitur, sed superioris generis causa duplicatur"/>
        <underline method="pen" type="straight" language="LA" text="referendis praeteritis verbis id scriptu[m], quodcu[m]que defendimus, suppleatur"/>
        <underline method="pen" type="straight" language="LA" text="Iterum aute[m] peccant"/>
        <underline method="pen" type="straight" language="LA" text="disceptatur"/>
        <underline method="pen" type="straight" language="LA" text="seiungu[n]t"/>
        <underline method="pen" type="straight" language="LA" text="separatu[m] est"/>
        <underline method="pen" type="straight" language="LA" text="quae in disceptatione[m] &amp; controuersia[m] cadere possunt, quid fiat, factu[m], futur&uacute;mve sit, aut quale sit, aut quomodo nominetur"/>
        <underline method="pen" type="straight" language="LA" text="adiungunt"/>
        <underline method="pen" type="straight" language="LA" text="totum in eo est, quod quale sit, quaerimus"/>
        <underline method="pen" type="straight" language="LA" text="ad institutu[m] reuertar meu[m]"/>
        <underline method="pen" type="straight" language="LA" text="accepto"/>
        <underline method="pen" type="straight" language="LA" text="nihil prius constituo"/>
        <underline method="pen" type="straight" language="LA" text="referenda"/>
        <underline method="pen" type="straight" language="LA" text="propria quaestionis &amp; iudicii"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mendationem habet nostra[m]"/>
        <underline method="pen" type="straight" language="LA" text="Ita ratio omnis dice[n]di tribus ad persuadendu[m] rebus est nixa: vt probemus vera esse ea quae defendimus: vt co[n]ciliemus nobis eos qui audiu[n]t: vt animos eoru[m] ad quencunque causa postulabit motu[m], vocemus"/>
        <underline method="pen" type="straight" language="LA" text="oratori subiecta materies"/>
        <underline method="pen" type="straight" language="LA" text="vna rerum earum"/>
        <underline method="pen" type="straight" language="LA" text="quae no[n] excogitantur ab oratore, sed in re proposita ratione tractantur"/>
        <underline method="pen" type="straight" language="LA" text="pariuntur"/>
        <underline method="pen" type="straight" language="LA" text="deferentur"/>
        <underline method="pen" type="straight" language="LA" text="argumentatione"/>
        <underline method="pen" type="straight" language="LA" text="Ita in superiore genere de tractandis argumentis, in hoc autem etia[m] de inueniendis cogitandum est"/>
        <underline method="pen" type="straight" language="LA" text="causas in plura genera secueru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="argumentorum copiam suggerunt"/>
        <underline method="pen" type="straight" language="LA" text="posita sit"/>
        <underline method="pen" type="straight" language="LA" text="habeant quo se referant, vnde statim expedita possint argumenta depromere"/>
        <underline method="pen" type="straight" language="LA" text="riuulos consectari"/>
        <underline method="pen" type="straight" language="LA" text="fontes rerum non videre"/>
        <underline method="pen" type="straight" language="LA" text="etiam aetatis est vs&uacute;sque nostri, &agrave; capite quod velimus arcessere, &amp;"/>
        
