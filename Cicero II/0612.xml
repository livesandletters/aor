        <marginalia hand="English_secretary" anchor_text="&egrave; duplici genere voluptatis coniunctus est">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>co[n]iunctus est &egrave; duplici genere voluptatis.</marginalia_text>
                </position>
            </language>
            <translation>He is joined by a double kind of pleasure.</translation>
        </marginalia>

        <marginalia hand="English_secretary" anchor_text="iacere vestra omnia">
            <language ident="LA">
