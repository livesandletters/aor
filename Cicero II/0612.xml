        <marginalia hand="Italian" anchor_text="Honestum igitur id intelligimus, quod tale est vt detracta omni vtilitate sine vllis praemiis fructib&uacute;sve per seipsum possit iure laudari">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Honestum quid</marginalia_text>
                </position>
            </language>
            <translation>What honour is.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Homines enim etsi multis aliis, tamen hoc vno &agrave; bestiis plurimu[m] differunt: qu&ograve;d rationem habeant &agrave; natura datam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Hominis praesta[n]tia</marginalia_text>
                </position>
            </language>
            <translation>The excelling of man.</translation>
        </marginalia>
        
