            <translation>The elements.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quinta quaedam natura">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quinta qua[e]da[m] natura</marginalia_text>
                </position>
            </language>
