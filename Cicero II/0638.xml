        <marginalia hand="Italian" anchor_text="&agrave; Xenocrate atque ab Aristotele">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Xenocrates.
                        Aristoteles.
                    </marginalia_text>
                    <person name="Xenocrates of Chalcedon"/>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>
                Xenocrates. Aristotle.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Omnis natura vult esse conseruatrix sui, vt &amp; salua sit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Omnis natura est co[n]seruatrix sui vt salua</marginalia_text>
                </position>
            </language>
            <translation>Every natural thing is its own conservator so that it is safe.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hominis in animu[m] &amp; corpus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Homo est a[n]i[m]a et Corpus.</marginalia_text>
                </position>
            </language>
            <translation>A man is a soul and a body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sapientia[m] totius hominis custodem &amp; procuratricem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapie[n]tia totius ho[min]is custos et procuratrix</marginalia_text>
                </position>
            </language>
            <translation>Wisdom is the guard and manager of the whole man.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iustitiae semina">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iusticia[e] Semina</marginalia_text>
                </position>
            </language>
            <translation>The seeds of justice.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hi qui procreati essent, &agrave; procreatoribus amare[n]tur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Procreati &agrave; procreatoribus amantur.</marginalia_text>
                </position>
            </language>
            <translation>Offspring are loved by the parents.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="procuratricem">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>procuratrix.</marginalia_text>
                </position>
            </language>
            <translation>A manager.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="infinita laude anteponerent">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>infinita laude anteponere.</marginalia_text>
                </position>
            </language>
            <translation>To value above with infinite praise.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="simpliciter; subtilius; accuratius">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Simpl[icite]r X acurat&egrave;
                                          subtilius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Simply. Accurately; exactly.  
            </translation>
        </marginalia>
        
        <underline method="pen" type="triple" language="LA" text="Inest in eadem explicatione naturae, insatiabilis quaedam &egrave; cognoscendis rebus voluptas, in qua vna confectis rebus necessariis, 
            vacui negotiis honeste ac liberaliter possumus"/>
        <underline method="pen" type="triple" language="LA" text="ignem, nonnulla deinde"/>
        <underline method="pen" type="triple" language="LA" text="eos copiam exilem, apud illos vberrimam"/>
        <underline method="pen" type="triple" language="LA" text="multa ab his conquisita &amp; collecta sunt, de omnium animantium genere, ortu, membris, aetatibus"/>
        <underline method="pen" type="triple" language="LA" text="omni copia, plurima &amp; certissima argumenta sumuntur ad cuiusque"/>
        <underline method="pen" type="triple" language="LA" text="Quod item quum"/>
        <underline method="pen" type="triple" language="LA" text="magnam tribuunt inue[n]toribus gratiam"/>
        <underline method="pen" type="triple" language="LA" text="viuere, summu[m] bonum esse dixisset, his verbis tria significari Stoici dicunt"/>
        <underline method="pen" type="triple" language="LA" text="eiusmodi, viuere adhibente[m] scientiam earum rerum, quae natura euenirent. Hunc"/>
        <underline method="pen" type="triple" language="LA" text="completur enim"/>
        <underline method="pen" type="triple" language="LA" text="e&aacute;que vita quae ex summo bono degitur"/>
        <underline method="pen" type="triple" language="LA" text="Omnis natura vult"/>
        <underline method="pen" type="triple" language="LA" text="quae est viue[n]di"/>
        <underline method="pen" type="triple" language="LA" text="hominis in animu[m]"/>
        <underline method="pen" type="triple" language="LA" text="animu[m] quadam infinita laude anteponerent corpori, virtutes quoque animi bonis"/>
        <underline method="pen" type="triple" language="LA" text="totius hominis custodem &amp; procuratricem esse vellent, quae esset naturae comes"/>
        <underline method="pen" type="triple" language="LA" text="adiutrix, hoc sapientiae munus esse dicebant"/>
        <underline method="pen" type="triple" language="LA" text="accuratius"/>
        <underline method="pen" type="triple" language="LA" text="prim&iacute;que ex omnibus philosophis natura tributu[m] esse docuerunt, vt hi qui procreati essent, &agrave; procreatoribus amare[n]tur: &amp;"/>
        <underline method="pen" type="triple" language="LA" text="quod temporum ordine antiquius"/>
        
