        <marginalia hand="Italian" anchor_text="Zenonis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Zeno.</marginalia_text>
                    <person name="Zeno of Citium"/>
                </position>
