        <marginalia hand="Italian" anchor_text="inue[n]toribus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Inuentores rerum</marginalia_text>
                </position>
            </language>
            <translation>The discoverers of things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Polemo">
            <language ident="LA">
