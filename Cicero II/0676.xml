        <underline method="pen" type="straight" language="LA" text="pocula ministrante"/>
        <underline method="pen" type="straight" language="LA" text="Fingebat haec Homerus, &amp; humana ad deos transferebat"/>
        <underline method="pen" type="straight" language="LA" text="vigere, sapere, inueniere, meminisse"/>
        <underline method="pen" type="straight" language="LA" text="&amp; terra vacat &amp; humore"/>
        <underline method="pen" type="straight" language="LA" text="aut flabile, aut igneum"/>
        <underline method="pen" type="straight" language="LA" text="inuenietur vnquam vnde ad hominem venire possint nisi &agrave; deo"/>
        <underline method="pen" type="straight" language="LA" text="sentit, quod sapit, quod vult, quod viget, caeleste &amp; diuinum est"/>
        <underline method="pen" type="straight" language="LA" text="mens soluta quaedam &amp; libera segregata ab omni concretione mortali omnia sentiens"/>
        <underline method="pen" type="straight" language="LA" text="mouens, ips&aacute;que praedita motu sempiterno"/>
        <underline method="pen" type="straight" language="LA" text="at vt oculus, sic animus sese non videns alia cernit"/>
        <underline method="pen" type="straight" language="LA" text="Qua facie quidem"/>
        <underline method="pen" type="straight" language="LA" text="habitet, ne quaere[n]dum quidem est: vt quum videmus speciem prim&ugrave;m candor&eacute;mque caeli"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mutation&eacute;sque temporum quadripartitas ad"/>
        <underline method="pen" type="straight" language="LA" text="maturitatem frugum &amp; temperatione[m] corporum aptas, eor&uacute;mque omniu[m]"/>
        <underline method="pen" type="straight" language="LA" text="solem, lun&aacute;mque accretione &amp; diminutione luminis quasi fastorum"/>
        <underline method="pen" type="straight" language="LA" text="nocturn&aacute;mque caeli formam"/>
        <underline method="pen" type="straight" language="LA" text="Tum globum terrae"/>
        <underline method="pen" type="straight" language="LA" text="frigore rigeant, aut vrantur calore"/>
        <underline method="pen" type="straight" language="LA" text="Ramos baccarum vbertate incuruescere, Segetes largiri fruges, florere omnia, Fontes scatere, herbis prata conuestirier"/>
        <underline method="pen" type="straight" language="LA" text="caeli ac deorum, ipsor&uacute;mque cultorem, atque hominis vtilitati agros omnes &amp; maria parentia. Haec"/>
        <underline method="pen" type="straight" language="LA" text="praesit aliquis conditor vel effector, si haec nata sunt, vt Platoni videtur: vel si semper"/>
        
