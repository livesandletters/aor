        <marginalia hand="English_secretary" anchor_text="paratissimis vesci voluptatibus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>paratissimis vesci voluptatibus.</marginalia_text>
                </position>
            </language>
            <translation>To enjoy the most ready pleasures.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Videmus igitur vt conquiescere ne infantes quidem possint">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Infantu[m] vita.</marginalia_text> 
                </position>
            </language>
