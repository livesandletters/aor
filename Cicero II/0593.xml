    <page filename="0593.tif" reader="John Dee" pagination="35" signature="Cc.ii."/>   
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="certum, comprehe[n]sum, perceptum, ratum, firmum, fixum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        certum
                        co[m]prehe[n]su[m]
                        p[rae]ceptum
                        ratum            <!-- DO: Looks more like 'ratium' though -->
                        firmu[m]
                        fixu[m]
                        stabile. }
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Certain. Comprehended. Perceived. Established. Firm. Fixed. Stable.
            </translation>
        </marginalia>
        <marginalia hand="Italian" anchor_text="iudicia ista dialecticae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dialectica[e] iudicia</marginalia_text>
                </position>
            </language>
            <translation>Dialectic decisions.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plato in Sophista.</marginalia_text>
                    <person name="Plato"/>
                    <book title="Sophist"/>
                </position>
            </language>
            <translation>Plato in the Sophist.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Plato aute[m] omne iudicium veritatis, veritat&eacute;mque ipsam abducta[m] ab opinionibus, &amp; &agrave; sensibus cogitationis ipsius &amp; me[n]tis esse voluit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Platonis opinio</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato's opinion.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Xenocrate[m] sequitur, cuius libri sunt de ratione loquendi multi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Xenocrates de r[ati]o[n]e loque[n]di.</marginalia_text>
                    <person name="Xenocrates of Chalcedon"/>
                </position>
            </language>
            <translation>Xenocrates of Chalcedon on the rationale of speaking.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="elementis Dialectici">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dialecticoru[m] eleme[n]ta.</marginalia_text>
                </position>
            </language>
            <translation>The elements of the dialecticians.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cleanthe">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cleanthes.</marginalia_text>
                </position>
            </language>
            <translation>Cleanths.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Antipater &amp; Archidemus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antipater. Archidemus</marginalia_text>
                    <person name="Antipater of Tarsus"/>        <!-- DO: Not sure if either of these two people are correct -->
                    <person name="Archidamus II"/>
                </position>
            </language>
            <translation>Antipater. Archidamus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="occludi tabernas iubes">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>occludi tabernas iubes</marginalia_text>
                </position>
            </language>
            <translation>You order the shops to be closed.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Negat enim vos Zeno, negat Antiochus scire quicquam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Zeno. Antiochus nihil sciri affirmant.</marginalia_text>
                    <person name="Zeno of Citium"/>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>Zeno [and] Antiochus maintain that nothing is known.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Zeno gestu">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Zenonis gestus.</marginalia_text>
                    <person name="Zeno of Citium"/>
                </position>
            </language>
            <translation>Zeno's gesture.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>κατάληψις </marginalia_text>   
                </position>
            </language>
            <translation>Direct apprehension.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="scie[n]tiam tale[m] esse dicebat, cuius co[m]potem nisi sapientem esse neminem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>solus sapie[n]s scie[n]tia[e] co[m]po[t]</marginalia_text>
                </position>
            </language>
            <translation>Only a wise person has control of knowledge.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="refero arte[m] sine scientia esse no[n] posse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ars sine scie[n]tia no[n] est.</marginalia_text>
                </position>
            </language>
            <translation>There is no art without knowledge.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Zeuxis, aut Phidias, aut Polycletus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Zeuxis. Polycletus
                        Phidias.
                    </marginalia_text>
                    <person name="Zeuxis"/>
                    <person name="Polykleitos"/>
                    <person name="Phidias"/>
                </position>
            </language>
            <translation>
                Zeuxis. Polykleitos. Phidias.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="inscientia multa versaretur in vita">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Inscie[n]tia multa in vita versatur.</marginalia_text>
                </position>
            </language>
            <translation>Much that was unwitting happens in life.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Veru[m] quonia[m] no[n] solu[m] nauta significat, sed etiam Fauonis ipse insusurrat nauigandi nobis Luculle te[m]pus esse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nauta significat nauigandi tempus.</marginalia_text>
                </position>
            </language>
            <translation>The sailor signifies the time for sailing.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="plus vno verum esse non possit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plus vno veru[m] esse no[n] potest.</marginalia_text>
                </position>
            </language>
            <translation>There cannot be more than one truth.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Tam moueor qu&agrave;m tu Luculle: nec"/>
        <underline method="pen" type="straight" language="LA" text="me minus hominem qu&agrave;m te putaueris"/>
        <underline method="pen" type="straight" language="LA" text="neque pelli, neque moueri potes"/>
        <underline method="pen" type="straight" language="LA" text="est, qui putet id cuique verum esse quod cuique videatur"/>
        <underline method="pen" type="straight" language="LA" text="ipsam abducta[m] ab opinionibus, &amp; &agrave; sensibus cogitationis ipsius &amp; me[n]tis esse voluit"/>
        <underline method="pen" type="straight" language="LA" text="multi, &amp; multu[m]"/>
        <underline method="pen" type="straight" language="LA" text="quo profecto nihil est acutius, nihil politius? &agrave; Chrysippo"/>
        <underline method="pen" type="straight" language="LA" text="abutimer gloria nominis"/>
        <underline method="pen" type="straight" language="LA" text="in elementis Dialectici"/>
        <underline method="pen" type="straight" language="LA" text="opinosissimi homines"/>
        <underline method="pen" type="straight" language="LA" text="in inuidiam, &amp; tanquam in concione[m] vocas"/>
        <underline method="pen" type="straight" language="LA" text="occludi tabernas"/>
        <underline method="pen" type="straight" language="LA" text="spectat illud, quum artificia tolli"/>
        <underline method="pen" type="straight" language="LA" text="Exproma[m] primu[m] illa inuidiosa"/>
        <underline method="pen" type="straight" language="LA" text="Zeno gestu co[n]ficiebat"/>
        <underline method="pen" type="straight" language="LA" text="aduersam manu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Assensus huiusmodi"/>
        <underline method="pen" type="straight" language="LA" text="plane co[m]presserat"/>
        <underline method="pen" type="straight" language="LA" text="arcte veheme[n]t&eacute;rque"/>
        <underline method="pen" type="straight" language="LA" text="nec tame[n] nimis elega[n]ter, illa subtilius"/>
        <underline method="pen" type="straight" language="LA" text="tibi refero arte[m] sine"/>
        <underline method="pen" type="straight" language="LA" text="quum didicissent id tollere nos quod nusqua[m] esset: quod aute[m] satis esset"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d inscientia"/>
        <underline method="pen" type="straight" language="LA" text="no[n] solu[m] nauta significat, sed"/>
        <underline method="pen" type="straight" language="LA" text="Fauonius ipse insusurrat nauigandi nobis Luculle te[m]pus"/>
        <underline method="pen" type="straight" language="LA" text="est mihi perora[n]dum"/>
        <underline method="pen" type="straight" language="LA" text="vt quum plus vno verum esse non possit, iac&ecirc;re necesse"/>
        <underline method="pen" type="straight" language="LA" text="tam nobiles disciplinas, qu&agrave;m de oculorum sensu&uacute;mque reliquorum mendaciis"/>
        
