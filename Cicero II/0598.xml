    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="&egrave; philosophorum choro sustilisti">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>&egrave; philosophoru[m] choro tollere.</marginalia_text>
                </position>
            </language>
            <translation>To expel from the chorus of philosophers.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="de caetero">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>de c[a]etero</marginalia_text>
                </position>
            </language>
            <translation>Regarding the rest.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="illa perdiscere ludus esset">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>illa p[er]discere ludus esset &id_est;</marginalia_text>
                </position>
            </language>
            <translation>To study these things may be child's play.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad haec">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ad hec.</marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="head" book_orientation="0">
                    <marginalia_text>as co[n]cerning these things</marginalia_text> 
                </position>
            </language>
            <translation>As concerning these things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="honesta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Honesta.</marginalia_text>
                </position>
            </language>
            <translation>Moral integrity.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="per se esset &amp; virtus &amp; cognitio reru[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>virtus no[n] p[er] se expete[n]da, nec rerum cognitio</marginalia_text>
                </position>
            </language>
            <translation>Virtue must not be desired in itself, nor is the knowledge of things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="non satis politus his artibus, quas qui tene[n]t, eruditi appella[n]tur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Artibus politi, Eruditi</marginalia_text>
                </position>
            </language>
            <translation>Those polished in the arts, the learned.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="disserendi artem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dissere[n]di Ars.</marginalia_text>
                </position>
            </language>
            <translation>The art of discussion.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aristippus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aristippus</marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>Aristippus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dicas quid non probes eius &agrave; quo dissentias">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dic quid no[n] probes eius priusq[uam] dissentias.</marginalia_text>
                </position>
            </language>
            <translation>'Say which of his ideas you don't approve of before' [Loeb 40, 31].</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="reprehensiones; disputa[n]do pertinaces">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Reprehensio et Pertinacia in disput.</marginalia_text>
                </position>
            </language>
            <translation>Criticism and obstinacy in dispute.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Democriti errata ab Epicuro reprehe[n]sa &amp; correcta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Democriti errata ab Epicuro reprehensa et correcta.</marginalia_text>
                    <person name="Democritus"/>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>The errors of Democritus reprimanded and corrected by Epicurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="oratione perpetua">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>O[rati]o perpetua.</marginalia_text>
                </position>
            </language>
            <translation>Continuous oration.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quid sit extremum, quid vltimum bonorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>O[mn]i[u]m Bonoru[m] extremu[m] quid sit.</marginalia_text>
                </position>
            </language>
            <translation>What may be the end of all good things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="natura incorrupte atque integre iudicante">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Natura incorrupt&egrave; et integr&egrave; iudicat.</marginalia_text>
                </position>
            </language>
            <translation>Nature judges in an unbiased and genuine way.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Maniefeste vera.</marginalia_text>  
                </position>
            </language>
            <translation>Apparent truths.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="animaduersione[m] atque admonitionem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animaduersio
                        Admonitio. ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An observation. A reminder. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="voluptate[m] &amp; dolore[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        voluptas
                        Dolor.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pleasure. Grief.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="si mihi probabis ea quae dice">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Si mihi probabis ea qu[a]e dices.
                    </marginalia_text>
                </position>
            </language>
            <language ident="EN">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        yf ye will proue me those things you say.
                    </marginalia_text>
                </position>
            </language>
            <translation>If you will prove to me those things you say.</translation>
        </marginalia>
        
