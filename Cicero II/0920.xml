        <marginalia hand="Italian" anchor_text="offficio saepe inter se comparentur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>officioru[m] comparatio.</marginalia_text>
                </position>
            </language>
            <translation>Comparison of duties.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Princ&eacute;psque omnium virtutum est illa sapientia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapientia o[mn]i[u]m virtutu[m] p[ri]nceps</marginalia_text>
                </position>
            </language>
            <translation>Wisdom is the chief of all virtues.</translation>
        </marginalia>
        
