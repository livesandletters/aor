            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Prudentia.</marginalia_text>
                </position>
            </language>
            <translation>Prudence.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="maxima est, vt est cert&egrave;, necesse est quod &agrave; co[m]munitate ducatur officium">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Maximu[m] officiu[m] a co[mmun]itate ductu[m].</marginalia_text>
                </position>
            </language>
            <translation>The greatest duty from a community is leadership.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="actio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Actio.</marginalia_text>
                </position>
            </language>
            <translation>Action.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cognoscend&aacute;que rerum natura">
            <language ident="LA">
