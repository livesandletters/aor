    <annotation>

        <marginalia hand="English_secretary" anchor_text="[diuinis duo gene]ra motus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuinis a[n]i[m]alib[us] duo insunt genera motus</marginalia_text>
                </position>
            </language>
            <translation>The divine beings have two kinds of emotions.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="quae infixa caelo non mounetur loco">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>fixa sydera</marginalia_text>
                </position>
            </language>
            <translation>The fixed stars.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>De planetis loq[ui]tur et eor[um] ut vulgo dicunt passionibus</marginalia_text>
                </position>
            </language>
            <translation>It is said of the planets and theirs as if they usually speak with passions.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="δαίμουας">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>δαίμ[ο]υας</marginalia_text>
                </position>
            </language>
            <translation>Daemon.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Oceanum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Oceanus.</marginalia_text>
                </position>
            </language>
            <translation>Ocean.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="deorum ortus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Deoru[m] duplex genus</marginalia_text>
                </position>
            </language>
            <translation>Twofold kind of gods.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Vos qui deorum satu ori estis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dei or[ati]o ad creaturas suas ca[e]listes.</marginalia_text>
                </position>
            </language>
            <translation>The speech of the god to his heavenly creatures.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="quasi in currum vniuersitatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[e]lu[m], currus vniversitatis</marginalia_text>
                </position>
            </language>
            <translation>Heaven, chariot of the universe.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="in antiquam partem"/>
        <underline method="pen" type="straight" language="LA" text="ex quo genere ea sunt sidera, quae infixa caelo non mouentur loco, quae sunt animantia, e&aacute;que diuina, ob e&aacute;mque causam"/>
        <underline method="pen" type="straight" language="LA" text="vt supr&agrave;"/>
        <underline method="pen" type="straight" language="LA" text="quae traiecto axi"/>
        <underline method="pen" type="straight" language="LA" text="generatores suos"/>
        <underline method="pen" type="straight" language="LA" text="quanquam nec argumentis"/>
        <underline method="pen" type="straight" language="LA" text="deorum satu orti estis"/>
        <underline method="pen" type="straight" language="LA" text="&amp; indissolubiles non potestis"/>
        <underline method="pen" type="straight" language="LA" text="Vt igitur mortali conditione genere[n]tur vos"/>
        <underline method="pen" type="straight" language="LA" text="illa gignatis,"/>
        <underline method="pen" type="double" language="LA" text="imitemin&iacute;que vim meam qua me &amp; in vestro ortu"/>
        <underline method="pen" type="straight" language="LA" text="vsum esse meministis"/>
