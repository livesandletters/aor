    <page filename="0579.tif" reader="John Dee" pagination="21" signature="Bb.iii."/>
    <annotation>
        
        
        <marginalia hand="Italian" anchor_text="improbus Trib. pleb.">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Trib. pleb. improbus</marginalia_text>
                </position>
            </language>
            <translation>A wicked Tribune of the People.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Antiochum ipsum cum annos multos talia sensisset, simul ac visum sit sente[n]tia destitisse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antiochus post multos annos sente[n]tia destitit,</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>After many years Antiochus left the opinion.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="pertinacia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pertinacia</marginalia_text>
                </position>
            </language>
            <translation>Obstinacy.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="disputatione">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Disputatio qualis e&sharp-S;e debet.</marginalia_text>
                </position>
            </language>
            <translation>How a debate should be.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="probare falsa, turpissimu[m] est">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>falsa probare turpissimu[m].</marginalia_text>
                </position>
            </language>
            <translation>It is the ugliest to approve false things.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Aratus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Aratus.</marginalia_text>
                    <person name="Aratus"/>
                </position>
            </language>
            <translation>Aratus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Nihil enim arbitror posse percipi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nihil arbitror posse percipi.</marginalia_text>
                </position>
            </language>
            <translation>I judge that nothing can be perceived.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Sapientis autem hanc censet Arcesilas vim esse maximam (Zenoni assentiens) cauere ne capiatur, ne fallatur videre">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sapientis munus.</marginalia_text>
                </position>
            </language>
            <translation>The duty of a wise person.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Arcesilas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Arcesilas.</marginalia_text>
                    <person name="Arcesilaus"/>
                </position>
            </language>
            <translation>Arcesilaus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Stoici dicu[n]t, &amp; eoru[m] astipulator Antiochus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Stoicoru[m] Astipulator Antiochus</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>Antiochus, a follower of the Stoics.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="sustinenda est potius omnis assensio ne praecipitet, si temere processerit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sustine[n]da potius est omnis assensio, ne pra[e]cipitet, si temere proce&sharp-S;erit.</marginalia_text>
                </position>
            </language>
            <translation>It is more potent to withhold all assent, so that one does not fall down, if they proceed rashly.</translation>
        </marginalia>
        
      
        <marginalia hand="English_secretary" anchor_text="acriter mente[m]">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        acriter [ me[n]te[m]
                                  a[n]im[um] ] pellere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To sharply strike the mind [and] the spirit.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="errem &amp; vager">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        [ errare
                          vagari.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To wander; to roam.
            </translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="vt praepostere tecu[m] agam">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                      vt prepostere tecu[m] agam.
                    </marginalia_text>
                </position>
            </language>
            <translation>To deal with you out of turn.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="directius gubernant">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        directius gubernare
                    </marginalia_text>
                </position>
            </language>
            <translation>To steer more directly.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="accurate, copiose, taceo: neque te quo minus (si tibi ita videatur) sententiam mutes, deterrendum puto. Illud vero no[n] censuerim, vt eius authoritate moueare"/>
        <underline method="pen" type="straight" language="LA" text="arriperet te, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="qu&icirc; tibi constares"/>
        <underline method="pen" type="straight" language="LA" text="Antiochum ipsum cum annos multos talia sensisset, simul"/>
        <underline method="pen" type="straight" language="LA" text="Me Catule oratio Luculli de ipsa re ita mouit, vt docti hominis, &amp; copiosi, &amp; parati, &amp; nihil praetereuntis eorum quae pro illa causa dici possent: non tamen vt ei respondere posse diffiderem"/>
        <underline method="pen" type="straight" language="LA" text="Nam si in minimis rebus pertinacia reprehenditur, calumnia etia[m] coercetur"/>
        <underline method="pen" type="straight" language="LA" text="aut certare cum aliis pugnaciter"/>
        <underline method="pen" type="straight" language="LA" text="aut frustrari cum alios"/>
        <underline method="pen" type="straight" language="LA" text="iurarem per Iouem de&oacute;sque penates, me &amp; ardere studio veri reperie[n]di, &amp; ea sentire quae dicere[m]. Qu&icirc; enim possum no[n] cupere veru[m] inuenire, quum gaudea[m] si simile veri quid inuenerim"/>
        <underline method="pen" type="straight" language="LA" text="Nec tame[n] ego is sum qui nihil vnquam falsi approbem, qui nunquam assentiar, qui nihil opiner. Sed"/>
        <underline method="pen" type="straight" language="LA" text="Ego vero ipse &amp; magnus quidem sum opinator"/>
        <underline method="pen" type="straight" language="LA" text="Non enim sum sapiens, &amp; meas cogitationes sic dirigo, no[n] ad illam paruulam Cynosuram"/>
        <underline method="pen" type="straight" language="LA" text="directius gubernant"/>
        <underline method="pen" type="straight" language="LA" text="rationes has latiore specie, non ad tenue elimatas"/>
        <underline method="pen" type="straight" language="LA" text="vt errem &amp; vager"/>
        <underline method="pen" type="straight" language="LA" text="sens&uacute;mve pepuleru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="assentiens) cauere ne capiatur, ne fallatur"/>
        <underline method="pen" type="straight" language="LA" text="temeritate diiunctius. Quid"/>
        <underline method="pen" type="straight" language="LA" text="praepostere tecu[m] agam"/>
        <underline method="pen" type="straight" language="LA" text="secu[n]dum illud dabat"/>
        <underline method="pen" type="straight" language="LA" text="id assequebatur etia[m]"/>
        <underline method="pen" type="straight" language="LA" text="&amp; eoru[m] astipulator Antiochus"/>
        <underline method="pen" type="straight" language="LA" text="tame[n] ipsa co[n]suetudo assentiendi periculosa esse videtur &amp; lubrica. Qua[m]obrem"/>
        <underline method="pen" type="straight" language="LA" text="sustinenda est potius omnis assensio ne praecipitet"/>
        <underline method="pen" type="straight" language="LA" text="si temere processerit"/>
        <underline method="pen" type="straight" language="LA" text="Ita enim finitima sunt falsa veris, e&aacute;que quae percipi possunt (si modo ea sunt quaeda[m]"/>
        <underline method="pen" type="straight" language="LA" text="iam enim"/>
        <underline method="pen" type="straight" language="LA" text="vt tame[n] in praecipite[m] locum no[n] debeat se sapie[n]s co[m]mittere"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; me sumpsero"/>
        <underline method="pen" type="straight" language="LA" text="etenim de eo omnis est controuersia"/>
        
