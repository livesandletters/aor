        <underline method="pen" type="straight" language="LA" text="minutatim interroga[n]ti"/>
        <underline method="pen" type="straight" language="LA" text="gradatim"/>
        <underline method="pen" type="straight" language="LA" text="dicitur"/>   <!-- Greek needed -->
        <underline method="pen" type="straight" language="LA" text="Cuius generis error"/>
        <underline method="pen" type="straight" language="LA" text="manat, vt non videam qu&ograve; non possit accedere"/>
        <underline method="pen" type="straight" language="LA" text="veniam equos sustinebo"/>
        <underline method="pen" type="straight" language="LA" text="ferentur equi praeceps"/>
        <underline method="pen" type="straight" language="LA" text="nec diutius captiose interroganti"/>
        <underline method="pen" type="straight" language="LA" text="Si habes quod liqueat, neque respondes: superbis. si non habes, ne tu quidem percipis, si quia obscura, concedo. 
            sed negas te vsque ad obscura progredi. illustribus"/>
        <underline method="pen" type="straight" language="LA" text="rebus insistis"/>
        <underline method="pen" type="straight" language="LA" text="eadem illa ars quasi Penelope, telam retexens tollit"/>
        <underline method="pen" type="straight" language="LA" text="Nempe fundamentum Dialecticae est"/>
        <underline method="pen" type="straight" language="LA" text="Si te mentiri dicis, &iacute;dque veru[m] dicis: me[n]tiri veru[m] dicis"/>
        <underline method="pen" type="straight" language="LA" text="scilicet inexplicabilia esse"/>
        <underline method="pen" type="straight" language="LA" text="quod est odiosius"/>
        <underline method="pen" type="straight" language="LA" text="Effatum esse id quod aut verum aut falsum sit? Rebus sumptis"/>
        <underline method="pen" type="straight" language="LA" text="mentiris: dicis autem te mentiri, ver&uacute;mqu edicis, mentiris igitur"/>
        <underline method="pen" type="straight" language="LA" text="connexi, quum concesseris superius, cogit inferius concedere"/>
        <underline method="pen" type="straight" language="LA" text="nec approbare nec improbare: qu&icirc;"/>
        <underline method="pen" type="straight" language="LA" text="ars, si ratio, si via, si vis denique conclusionis valet, eadem est in vtroque"/>
        <underline method="pen" type="straight" language="LA" text="postulant vt excipiantur haec inexplicabilia"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; me istam exceptionem nunquam impetrabunt"/>
        <underline method="pen" type="straight" language="LA" text="omne quod ita disiunctum sit, quasi aut etiam, aut non, non"/>
        <underline method="pen" type="straight" language="LA" text="autem est in natura rerum talis necessitas"/>
        <underline method="pen" type="straight" language="LA" text="autem ea dico quum alterum ait, alterum neget) si talis disiunctio falsa potest"/>
        
