        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ante lucuem enim iussit efferri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ante luce[m] efferenda funera</marginalia_text>
                </position>
            </language>
            <translation>Funerals should be carried out before daybreak.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fruges">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>fruges</marginalia_text>
