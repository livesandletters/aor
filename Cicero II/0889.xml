        <marginalia hand="Italian" anchor_text="NENIAE">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nenia[e]</marginalia_text>
                </position>
            </language>
            <translation>A funeral song.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sepulcrorum magnificentia[m] esse minuendam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sepulchrou[m] magnifice[n]tia minuenda.</marginalia_text>
                </position>
            </language>
            <translation>The greatness of tombs is diminished.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="corpus terra huma[n]di">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Corpora terra humanda</marginalia_text>
