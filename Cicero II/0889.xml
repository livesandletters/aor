        </marginalia>
        
        <marginalia hand="Italian" anchor_text="laudem mortui incisam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Laus mortui incisa</marginalia_text>
                </position>
            </language>
            <translation>The inscribed praise of the deceased.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="post mortem tranquillitate bonorum, poenis impiorum">
            <language ident="LA">
