    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="si haec accendi, aut co[m]moueri arte possint: inseri quidem &amp; donari ab arte no[n] possunt"/>
        <underline method="pen" type="straight" language="LA" text="cum ipso homine nascuntur? linguae solutio, vocis sonus, latera, vires, conformatio quaedam &amp; figura totius oris &amp; corporis"/>
        <underline method="pen" type="straight" language="LA" text="vt ars aliquid limare non possit"/>
        <underline method="pen" type="straight" language="LA" text="aut ita lingua haesita[n]tes, aut ita voce absoni, aut ita vultu mot&uacute;que corporis vasti atque agrestes"/>
        <underline method="pen" type="straight" language="LA" text="venire"/>
        <underline method="pen" type="straight" language="LA" text="ita naturae muneribus in iisdem rebus habiles"/>
        <underline method="pen" type="straight" language="LA" text="vt non nati"/>
        <underline method="pen" type="straight" language="LA" text="sed ab aliquo deo ficti esse videantur"/>
        <underline method="pen" type="straight" language="LA" text="offenditur"/>
        <underline method="pen" type="straight" language="LA" text="obruit"/>
        <underline method="pen" type="straight" language="LA" text="in eam sententiam"/>
        <underline method="pen" type="straight" language="LA" text="naturale"/>
        <underline method="pen" type="straight" language="LA" text="aequali"/>
        <underline method="pen" type="straight" language="LA" text="qua[m]cunque assequi potuerit in dicendo mediocritatem"/>
        <underline method="pen" type="straight" language="LA" text="vastum homine[m] atque foedum"/>
        <underline method="pen" type="straight" language="LA" text="quaerimus, fingendus"/>
        <underline method="pen" type="straight" language="LA" text="detractis omnibus vitiis orator atque omni laude cumulatus"/>
        <underline method="pen" type="straight" language="LA" text="si multitudo litium"/>
        <underline method="pen" type="straight" language="LA" text="turba &amp; barbaria forensis dat locu[m] vel vitiosissimis oratoribus"/>
        <underline method="pen" type="straight" language="LA" text="libera quaedam oblectatio, qu&agrave;m dilige[n]ter, &amp; qu&agrave;m prop&egrave; fastidiose iudicamus"/>
        <underline method="pen" type="straight" language="LA" text="sicut in foro non bonos oratores, item in theatro actores malos perpeti"/>
        <underline method="pen" type="straight" language="LA" text="admirabilis esse videatur"/>
        <underline method="pen" type="straight" language="LA" text="enuntiabo"/>
        <underline method="pen" type="straight" language="LA" text="timide ad dice[n]dum accedunt, &amp; in exordienda oratione perturba[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="dignum re"/>
        <underline method="pen" type="straight" language="LA" text="efficere"/>
        <underline method="pen" type="straight" language="LA" text="edere"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mouetur in dicendo"/>
        <underline method="pen" type="straight" language="LA" text="non enim pudendo, sed non faciendo id, quod non decet, impudentiae nomen effugere debemus"/>
        <underline method="pen" type="straight" language="LA" text="animaduertere"/>
        <underline method="pen" type="straight" language="LA" text="exalbescam"/>
        <underline method="pen" type="straight" language="LA" text="tota mente atque omnibus artubus contremiscam"/>
        <underline method="pen" type="straight" language="LA" text="initio accusationis exanimatus sum, vt hoc summum beneficium Q. Maximo debuerim, qu&ograve;d continuo consilium dimiserit, simulac me fractum ac debilitatu[m] metu viderit. H&icirc;c omnes assensi, significare inter sese, &amp; colloqui coeperunt"/>
        <underline method="pen" type="straight" language="LA" text="quoque oratore plurimu[m] esset, ita"/>
        <underline method="pen" type="straight" language="LA" text="euentum dicendi procedere"/>
        <underline method="pen" type="straight" language="LA" text="id quod aliquando posset accidere, ne tum accideret, timere"/>
        <underline method="pen" type="straight" language="LA" text="spectati"/>
        <underline method="pen" type="straight" language="LA" text="probati"/>
        <underline method="pen" type="straight" language="LA" text="valetudine impediti no[n]"/>
        
