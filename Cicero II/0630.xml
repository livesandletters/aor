    <page filename="0630.tif" reader="John Dee" pagination="72" signature="[Ee.iiii.[v]]"/>   
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="perfacilis &amp; perexpedita defensio">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>perfacilis et p[er]expedita defensio</marginalia_text>
                </position>
            </language>
            <translation>A very easy and very much obtainable defence.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="hae perturbationes vi aliqua naturali moue[n]tur: omn&eacute;sque hae sunt genere quatuor">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Perturbationu[m] 4 genera.</marginalia_text>
                </position>
            </language>
            <translation>The four kinds of passion.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text></marginalia_text>  <!-- Greek needed -->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="iudicia leuitatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Perturbationes sunt iudicia leuitatis.</marginalia_text>
                </position>
            </language>
            <translation>The passions are frivolous judgements.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Honesta per se expete[n]da, Turpia p[er] se fugienda.</marginalia_text>
                </position>
            </language>
            <translation>Honour must be sought through the self; dishonour must be fled through the self.</translation> 
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Aphricanorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aphricani.</marginalia_text>
                </position>
            </language>
            <translation>The Africani.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Aut quis est qui maiorum, aut Aphricanorum, aut eius quem tu in ore semper habes proaui mei, caeteror&uacute;mque viroru[m] fortium, atque omni virtute praestantium, facta, dicta, consilia cognoscens, nulla animo afficiatur voluptate">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Historia</marginalia_text>
                </position>
            </language>
            <translation>History.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="turpitudine[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Turpitudo.</marginalia_text>
                </position>
            </language>
            <translation>Dishonour.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="media">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Media</marginalia_text>
                </position>
            </language>
            <translation>Neutral things.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text></marginalia_text>       <!-- Greek needed -->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Itaque mihi videris Latine docere philosophiam, &amp; ei quasi ciuitatem dare, quae quidem adhuc peregrinari Romae videbatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Philosophia[e] ciuitas est data, prius p[er]igrinata[e]</marginalia_text>
                </position>
            </language>
            <translation>Citizenship is given to philosophy, previously to a wanderer.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in rebus minime obscuris non multus est apud eos disserendi labor">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&Iuml;n rebus minime obscuris no[n] sit multus dissere[n]di labor</marginalia_text> 
                </position>
            </language>
            <translation>In matters which are in no way obscure, there will not be much labor in discussing [them].</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vituperabile">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>vituperabile.</marginalia_text>
                </position>
            </language>
            <translation>Blameworthy.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text></marginalia_text>    <!-- Greek needed -->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ignoratione[m] dialecticae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Peripatetici dialecticam ignoraba[n]t.</marginalia_text>
                </position>
            </language>
            <translation>The Peripatetics ignore logic.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dialectica[e] fructus.</marginalia_text>
                </position>
            </language>
            <translation>The fruits of logic.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="quasi gestientis animi elatione[m] voluptariam"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mune nobis est cum multorum aliorum philosophoru[m]"/>
        <underline method="pen" type="straight" language="LA" text="Sed haec quide[m] est perfacilis &amp; perexpedita defensio"/>
        <underline method="pen" type="straight" language="LA" text="aut auaritia tam ardenti, aut tam effraenatis cupiditatibus"/>
        <underline method="pen" type="straight" language="LA" text="multis partibus malit"/>
        <underline method="pen" type="straight" language="LA" text="tam agrestibus institutis"/>
        <underline method="pen" type="straight" language="LA" text="aut quis co[n]tra studia naturae tam veheme[n]ter obdurauit"/>
        <underline method="pen" type="straight" language="LA" text="abhorreat, e&aacute;sque sine voluptate aut vtilitate aliqua no[n] requirat"/>
        <underline method="pen" type="straight" language="LA" text="quem tu in ore semper"/>
        <underline method="pen" type="straight" language="LA" text="honesta in familia institutus"/>
        <underline method="pen" type="straight" language="LA" text="&amp; educatus ingenue"/>
        <underline method="pen" type="straight" language="LA" text="eum, quem impure ac flagitiose putet viuere"/>
        <underline method="pen" type="straight" language="LA" text="varios, leues, futiles"/>
        <underline method="pen" type="straight" language="LA" text="quo minus homines tenebras &amp; solitudine[m] nacti, nullo dedecore se abstineant, nisi eos per se foeditate sua turpitudo ipsa deterreat"/>
        <underline method="pen" type="straight" language="LA" text="Constituto autem illo, de quo ant&egrave; diximus, quod"/>
        <underline method="pen" type="straight" language="LA" text="Stultitiam autem &amp; temeritatem &amp; iniustitiam &amp; intemperantiam"/>
        <underline method="pen" type="straight" language="LA" text="Cato verbis illustribus, &amp; id quod vis declara[n]tibus"/>
        <underline method="pen" type="straight" language="LA" text="docere philosophiam, &amp; ei quasi ciuitatem dare, quae quidem adhuc peregrinari Romae videbatur, nec offerre sese nostris sermonibus, &amp; ista maxime propter limatam"/>
        <underline method="pen" type="straight" language="LA" text="&amp; rerum &amp; verborum tenuitatem. Scio enim esse quosdam"/>
        <underline method="pen" type="straight" language="LA" text="qui quanuis lingua philosophari possint, nullis tamen partitionibus"/>
        <underline method="pen" type="straight" language="LA" text="nullis definitionibus"/>
        <underline method="pen" type="straight" language="LA" text="se modo probare, quibus natura tacita assentiatur"/>
        <underline method="pen" type="straight" language="LA" text="de quibus hic sermo est, nomina imponis, memoriae mando"/>
        <underline method="pen" type="straight" language="LA" text="Peripateticis mollius"/>
        <underline method="pen" type="straight" language="LA" text="dicendi non satis acuta propter ignoratione[m] dialecticae"/>
        <underline method="pen" type="straight" language="LA" text="exercitatione in dialecticis, summ&aacute;que eloquentia"/>
        
        
