<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0912.tif" reader="John Dee" pagination="354" signature="[Z.z.i[v]]"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="separari">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>distingui separari.</marginalia_text>
                </position>
            </language>
            <translation>To be distinguished, to be separated.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cum virtute confusum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>confusu[m] cu[m] virtute</marginalia_text>
                </position>
            </language>
            <translation>Combined with virtue.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>hoc ip[s]o, qu&ograve;d in eo, in q[uo].</marginalia_text>
                </position>
            </language>
            <translation>This itself, which, in it, in which.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="reco[n]dita quadam ratione">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>reco[n]dita r[ati]o[n]e cerni</marginalia_text>
                </position>
            </language>
            <translation>To be examined by a hidden reasoning.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="cum specie quadam liberali">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>cu[m] sp[eci]e q[ua]da[m] liberali</marginalia_text> 
                </position>
            </language>
            <translation>With a certain generous hope.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="EL">
                <position place="head" book_orientation="0">
                    <marginalia_text></marginalia_text> <!--JG: Greek, to be done-->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quid deceat apparet">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quid decet, quid dedecet</marginalia_text>
                </position>
            </language>
            <translation>What is fitting, what is unsuitable.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="venustas; valetudine">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>valetudo. venustas.</marginalia_text>
                </position>
            </language>
            <translation>Good health. Attractiveness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="duplex. nam &amp; generale quoddam decorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Decorum duplex.</marginalia_text>
                </position>
            </language>
            <translation>The divide of beaty.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Decorum generale &amp; particulare</marginalia_text>
                </position>
            </language>
            <translation>The general and particular of beauty.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="seruare illud poetas quod deceat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Poeta[e] decorum serua[n]t</marginalia_text>
                </position>
            </language>
            <translation>The poets of beauty watch over.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nobis autem personam imposuit ipsa natura">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nobis p[er]sona[m] imposuit Na[m]</marginalia_text>
                </position>
            </language>
            <translation>For [nature itself] imposed a mask on us.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nobis autem quum &agrave; naturam constantiae, moderationism tempera[n]tiae, verecundiae, partes datae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qua[e] partes nobis su[n]t &agrave; natura data[e].</marginalia_text>
                </position>
            </language>
            <translation>Which parts are given by nature to us.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pulchritudo corporis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Comparatio pulchritudinis &amp; temperantia[e]</marginalia_text>
                </position>
            </language>
            <translation>A combination of beauty and temperance.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="reuerentia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Reuerentia</marginalia_text>
                </position>
            </language>
            <translation>Reverence.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fama &amp; nome[n] bonu[m]</marginalia_text> 
                </position>
            </language>
            <translation>Fame and a good name.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Iustitiae partes sunt, non violare homines: verecundiae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iustiti[a]e et et verecundi[a]e discrimen.</marginalia_text>
                </position>
            </language>
            <translation>The dividing line of justice and modesty.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="conuenientiam co[n]seruation&eacute;mque naturae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Convenientia &amp; conservatio natura[e].</marginalia_text>
                </position>
            </language>
            <translation>The harmony and preservation of nature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Duplex est enim animoru[m] vis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Duplex animoru[m] vis</marginalia_text>
                </position>
            </language>
            <translation>The twofold force of mind.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ratio praesit, appetitus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Appetitus. R[ati]o.</marginalia_text>
                </position>
            </language>
            <translation>Desire. Reason.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>approbatio</marginalia_text>
                </position>
            </language>
            <translation>Approbation.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="consociationem">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>co[n]sociatio</marginalia_text>
                </position>
            </language>
            <translation>Association.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aduersus homines">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>aduersus ho[m]i[n]es</marginalia_text>
                </position>
            </language>
            <translation>Against men.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="acutum &amp; perspicax natura est, &amp; id quod ad hominum consociationem accomoodatum est, &amp; id quod vehemens atque forte">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>acutu[m] p[er]spicax vehemens forte</marginalia_text>
                </position>
            </language>
            <translation>Sharpened. Penetrating. Vehement. Strong.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="facilius intelligi qu&agrave;m explanari potest"/>
        <underline method="pen" type="straight" language="LA" text="&amp; agere quod agas considerate"/>
        <underline method="pen" type="straight" language="LA" text="dedecet, qu&agrave;m delirare &amp; mente captu[m] esse"/>
        <underline method="pen" type="straight" language="LA" text="pertinet, vt non reco[n]dita quadam ratione"/>
        <underline method="pen" type="straight" language="LA" text="quod cogitatione magis &agrave; virtute potest, qu&agrave;m re separari"/>
        <underline method="pen" type="straight" language="LA" text="cum virtute consusum"/>
        <underline method="pen" type="straight" language="LA" text="Decorum id esse, quod consentaneum sit hominis excellentiae in eo, in quo natura eius &agrave; reliquis animantibus differat"/>
        <underline method="pen" type="straight" language="LA" text="cum specie quadam"/>
        <underline method="pen" type="straight" language="LA" text="sona dignum est, &amp; fit"/>
        <underline method="pen" type="straight" language="LA" text="personam imposuit ipsa natura"/>
        <underline method="pen" type="straight" language="LA" text="magna cum excellentia praestanti&aacute;que animantium reliquorum"/>
        <underline method="pen" type="straight" language="LA" text="constantiae"/>
        <underline method="pen" type="straight" language="LA" text="moderationis"/>
        <underline method="pen" type="straight" language="LA" text="tempera[n]tiae, verecundiae, partes"/>
        <underline method="pen" type="straight" language="LA" text="aduersus homines"/>
        <underline method="pen" type="straight" language="LA" text="apta compositione membrorum"/>
        <underline method="pen" type="straight" language="LA" text="Nam negliere quid de se quisque sentiat, non solum arrogantis est, sed etiam omnino dissoluti"/>
        <underline method="pen" type="straight" language="LA" text="conuenientiam co[n]seruation&eacute;mque naturae"/>
        
