            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nobis autem quum &agrave; naturam constantiae, moderationism tempera[n]tiae, verecundiae, partes datae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qua[e] partes nobis sut &agrave; natura data[e].</marginalia_text>
