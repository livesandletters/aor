        <underline method="pen" type="straight" language="LA" text="imman&iacute;que vita exculti ad humanitatem &amp; mitigati sumus"/>
        <underline method="pen" type="straight" language="LA" text="cogitatam libidinem intulit, qu&ograve; ne imprudentiam quidem oculorum adiici fas fuit"/>
        <underline method="pen" type="straight" language="LA" text="initient&uacute;rque eo ritu Cereri, quo Romae initiantur"/>
        <underline method="pen" type="straight" language="LA" text="peruigilationes"/>
        <underline method="pen" type="straight" language="LA" text="sunt cauea, circ&oacute;que diuisi, sint corporum certationes"/>
        
