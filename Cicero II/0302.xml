                <position place="left_margin" book_orientation="0">
                    <marginalia_text>figurarum diuisio</marginalia_text>
                </position>
            </language>
            <translation>The division of styles.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vnam graue[m], alteram mediocre[m], tertiam extenuata[m] vocamus">
