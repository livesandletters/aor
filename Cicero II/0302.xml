        <marginalia hand="Italian" anchor_text="Nunc omnino aliunde sic intelligemus sumenda non fuisse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Exempla o[mn]i[n]o aliu[n]de no[n] sume[n]da.</marginalia_text>
                </position>
            </language>
            <translation>Examples are not to be taken from elsewhere at all.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="aceruos se dicant tritici habere, &amp; eoru[m] exemplum pugno no[n] habeant, quod ostendant">
