    <annotation>
        
        <marginalia hand="Italian" anchor_text="animus, an tanta sit eius tenuitas vt fugiat aciem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animus aciem fugit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The mind flees sight.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in liberu[m] coelum quasi in domu[m] suam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Co[e]lum a[n]i[m]oru[m] n[ost]ror[um] domus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The heavens are the house of our minds.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="deum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Deus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="maximu[m], animo ipso animu[m] videre">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animu[m] a[n]i[m]o videre, maximu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'The mind sees the mind alone' [Loeb 141, 61].
            </translation>
        </marginalia>
        
