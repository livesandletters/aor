        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Corporis co[n]formatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The conformity of the body.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animus quid secu[m] affert?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What does the mind carry with itself?
