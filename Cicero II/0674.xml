        <underline method="pen" type="straight" language="LA" text="possent in homine vno cerni omnia, quae nunc tecta sunt, casur&uacute;sne in conspectu[m] videatur animus, an tanta sit eius tenuitas vt fugiat"/>
        <underline method="pen" type="straight" language="LA" text="intuenti mult&ograve; difficilior occurrit cogitatio, mult&oacute;que obscurior qualis"/>
        <underline method="pen" type="straight" language="LA" text="corpore sit, tanqua[m] alienae domui"/>
        <underline method="pen" type="straight" language="LA" text="&amp; in liberu[m] coelum"/>
        <underline method="pen" type="straight" language="LA" text="Si enim quod nunqua[m] vidimus, id quale sit intelligere no[n] possumus, certe &amp; deum ipsum &amp; diuinu[m] animu[m] corpore liberatu[m], 
            cogitatione co[m]plecti no[n] possumus"/>
        <underline method="pen" type="straight" language="LA" text="neque ego haec dice[n]s corpori tuo dico"/>
        <underline method="pen" type="straight" language="LA" text="Principii autem nulla est origo"/>
        <underline method="pen" type="straight" language="LA" text="Nam principiu[m] extinctum, nec ipsum ab alio renascetur"/>
        <underline method="pen" type="straight" language="LA" text="Quum pateat igitur aeternum id esse quod"/>
        <underline method="pen" type="straight" language="LA" text="hominis vitam sustentari"/>
        <underline method="pen" type="straight" language="LA" text="quidem Plato recordatione[m] esse vult vitae superioris"/>
        <underline method="pen" type="straight" language="LA" text="de dime[n]sione quadrati"/>
        
