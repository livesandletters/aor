    <page filename="0909.tif" reader="John Dee" pagination="351"/>
    <annotation>
        
        <marginalia hand="Italian" anchor_text="imperium Lacedaemonis dilitatum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>imp[er]iu[m] dilatare.</marginalia_text>
                </position>
            </language>
            <translation>To enlarge the empire.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="parentiores habuerunt exercitus &amp; fortiores">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>exercitus paretiores fortiores</marginalia_text>
                </position>
            </language>
            <translation>Armies, better disciplined, braver.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="adiumenta">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>adiume[n]tu[m]</marginalia_text>
                </position>
            </language>
            <translation>Assistance.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="singularis &amp; vir &amp; Impertator">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Singularis et vir imp[er]ator</marginalia_text>
                </position>
            </language>
            <translation>Extraordinary man and general.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vir abundans bellicis laudibus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>vir abundans bellicis laudib[us]</marginalia_text>
                </position>
            </language>
            <translation>A man abounding with military honours.</translation>
        </marginalia>
       
        
        <marginalia hand="Italian" anchor_text="abiecta omni cunctatione">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>abiecta o[mn]i cunctatio[n]i</marginalia_text>
                </position>
            </language>
            <translation>All hesitation cast aside.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="philosophis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Philosophis.</marginalia_text>
                </position>
            </language>
            <translation>Philosophers.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="magnificentia &amp; despicientia abhibenda sit rerum humanurum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Magnifice[n]tia, cu[m] rerum humanaru[m] despicie[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>Greatness with contempt for humane things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mangitudo animi est adhibe[n]da, &amp; vacuitas an angoribus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Magnitudo a[nim]i &amp; vacuitas ab angoribus abhibenda.</marginalia_text>
                </position>
            </language>
            <translation>Elevation of the mind and exemption from being possessed by perplexities.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="negociis prius qu&agrave;m aggrediare, abhidenda est praeparatio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>pra[e]paratio a[nim]i ad o[mn]e negocii[s]</marginalia_text>
                </position>
            </language>
            <translation>preparation of the mind in all affairs.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="res bellicas maiores esse qu&agrave;m vrbanas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vrbanas res Bellicis maiores esse</marginalia_text>
                </position>
            </language>
            <translation>Military things are greater than civil ones.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Themistocles iure laudetur, &amp; sit eius nomen qu&agrave;m Solonis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Themistocles. Solon.</marginalia_text>
                    <person name="Themistocles"/>
                    <person name="Solon"/>
                </position>
            </language>
            <translation>Themistocles, Solon. </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="no[n] minus praeclarum hoc qu&agrave;m illud iudicandu[m] est"> <!-- M.B. Not sure of anchor text? -->
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>A te[m]poris diuturnitate</marginalia_text>
                </position>
            </language>
            <translation>A long duration of time.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pausania Lysandr&oacute;que">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pausanias. Lysander</marginalia_text>
                    <person name="Pausanias of Sparta"/>
                    <person name="Lysander"/>
                </position>
            </language>
            <translation>Pausanias, Lysander.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Lycurgi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lycurgus.</marginalia_text>
                    <person name="Lycurgus of Sparta"/>
                </position>
            </language>
            <translation>Lycurgus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="M. Scaurus C. Mario, neque quu[m] versaremur in Rep. Q. Catulus Cn. Pompeio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>M. Scaurus. C. Marius Q Catulus. Cn. Pompeius</marginalia_text>
                    <person name="Marcus Aemilius Scaurus"/>
                    <person name="Gaius Marius"/>
                    <person name="Quintus Lutatius Catulus"/>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>Marcus Scaurus, Caius Marius, Quintus Catulus, Cnaeus Pompeius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="P. Nascia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Scipio. P. Nascia</marginalia_text>
                    <person name="Publius Cornelius Scipio Nasica Serapio"/>
                </position>
            </language>
            <translation>Scipio Publicus Nasica.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Ti. Gracchu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>T. Gracchus</marginalia_text>
                    <person name="Tiberius Gracchus"/>
                </position>
            </language>
            <translation>Tiberius Gracchus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cedant arma togae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ca[e]daut arma toga[e]</marginalia_text>
                </position>
            </language>
            <translation>Arms yield to the gown.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="haereditsa huius gloriae, &amp; factorum imitatio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Gloria[e] ha[e]reditas. factorum imitatio.</marginalia_text>
                </position>
            </language>
            <translation>The heritage of this glory and the imitation of my actions.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cn. Pompeius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cn. Pompeius</marginalia_text>
                    <person name="Pompey"/>
                </position>
            </language>
            <translation>Cnaeus Pompeius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ex animo excelso magnific&ograve;que quaerimus, animi efficitur, no[n] corporis viribus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vires { Animi Corporis</marginalia_text>
                </position>
            </language>
            <translation>Powers { mind, body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Exercendu[m] tamen corpus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Corporis exercitatio qualis</marginalia_text>
                </position>
            </language>
            <translation>What kind of exercise of the body</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="M. Catonis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>M. Ca[e]to.</marginalia_text>
                    <person name="Cato the Elder"/>
                </position>
            </language>
            <translation>Marcus Cato.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="abiecta omni cunctatione adipiscendi"/>
        <underline method="pen" type="straight" language="LA" text="minus multa patent in eorum vita quae fortuna feriat, &amp; quo"/>
        <underline method="pen" type="straight" language="LA" text="Ad rem gerendam aute[m] qui accedit, caueat ne id modo consideret, qu&agrave;m illa res honesta sit, sed etia[m] vt habeat 
            efficiendi facultate[m]. In quo"/>
        <underline method="pen" type="straight" language="LA" text="enim bella saepe quaesiuerunt propter"/>
        <underline method="pen" type="straight" language="LA" text="vrbanae maiores clarior&eacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="no[n] minus praeclarum"/>
        <underline method="pen" type="straight" language="LA" text="quanquam imperium"/>
        <underline method="pen" type="straight" language="LA" text="dilatatum putatur"/>
        <underline method="pen" type="straight" language="LA" text="causas &amp; parentiores"/>
        <underline method="pen" type="straight" language="LA" text="neque pueris nobis"/>
        <underline method="pen" type="straight" language="LA" text="Parua enim sunt foris arma, nisi est consilium domi"/>
        <underline method="pen" type="straight" language="LA" text="res no[n] solum ex domestica est ratione, sed attingit etia[m]"/>
        <underline method="pen" type="straight" language="LA" text="solere ad inuidis &amp; improbis audito"/>
        <underline method="pen" type="straight" language="LA" text="nostra celerier de manibus audacissimoru[m] ciuium delapsa arma ipsa ceciderunt"/>
        <underline method="pen" type="straight" language="LA" text="Marce fili"/>
        <underline method="pen" type="straight" language="LA" text="abundans bellicis laudibus"/>
        <underline method="pen" type="straight" language="LA" text="triumphu[m] deportaturum fuisse"/>
        <underline method="pen" type="straight" language="LA" text="his operae studi&iacute;que ponendu[m] est. Omnino"/>
        <underline method="pen" type="straight" language="LA" text="Exercendu[m] tamen corpus, &amp; ita efficiendum est, vt obedire consilio ration&iacute;que possit in exequendis negociis, 
            &amp; in labore tolerando. Honestum"/>
        
