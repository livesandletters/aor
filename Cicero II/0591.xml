        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Diodorus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Diodorus.</marginalia_text>
                    <person name="Diodorus of Tyre"/>
                </position>
