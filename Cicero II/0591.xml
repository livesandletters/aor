                    <marginalia_text>voluptas.</marginalia_text>
                </position>
            </language>
            <translation>Pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Voluptatem autem &amp; honestatem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Voluptas cu[m] Honestate</marginalia_text>
                </position>
            </language>
            <translation>Pleasure with honour.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Hieronymus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Hieronymus.</marginalia_text>
                    <person name="Hieronymus of Rhodes"/>
                </position>
            </language>
