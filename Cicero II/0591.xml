        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aristoteles">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Aristoteles.</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Aristotle.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="summum bonum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Summu[m] Bonu[m] carneadis</marginalia_text>
                    <person name="Carneades"/>
                </position>
            </language>
            <translation>The greatest good of Carneades.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Zeno statuit finem esse bonorum, qui inue[n]tor &amp; princeps Stoicorum fuit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Zeno stoicoru[m] princeps.</marginalia_text>
                    <person name="Zeno of Citium"/>
                </position>
            </language>
            <translation>Zeno of Citium, the founder of the Stoics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aristotelem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Aristoteles.</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Aristotle.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Antiochum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antiochus</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>Antiochus of Ascalon.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omnis ratio vitae definitione summi boni continetur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>O[mn]is vita[e] r[ati]o summi Boni definitione co[n]tinetur</marginalia_text>
                </position>
            </language>
            <translation>The entire reasoning of life is bound up with the definition of the greatest good.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quid, quum ipse Antiochus dissentit quibusdam in rebus ab his quos amat Stoicis, n&oacute;nne indicat no[n] posse illa proba[n]da esse sapienti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antiochus cur disse[n]tiat.</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                </position>
            </language>
            <translation>Why Antiochus disagrees.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Placet Stoicis omnia peccata esse paria">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>peccata o[mn]ia paria secu[n]dum stoicos</marginalia_text>
                </position>
            </language>
            <translation>All sins are equal according to the Stoics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Scelus enim dicebas esse Luculle dogma prodere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dogma prodere, scelus e[sse].</marginalia_text>
                </position>
            </language>
            <translation>To abandon a dogma is a crime.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Etiam, inquit, beatam, sed non beatissimam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>vita Beata, beatissima</marginalia_text>
                </position>
            </language>
            <translation>A charmed life; the most charmed life.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Theophrasto">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Theophrastus.</marginalia_text>
                    <person name="Theophrastus"/>
                </position>
            </language>
            <translation>Theophrastus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Distrahor">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Distrahor.</marginalia_text>
                </position>
            </language>
            <translation>I am dragged.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Zenoni">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Zeno.</marginalia_text>
                    <person name="Zeno of Citium"/>
                </position>
            </language>
            <translation>Zeno of Citium.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sed Zenoni necessarium, cui praeter honestu[m] nihil est in bonis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Honestum solu[m], Bonu[m].</marginalia_text>
                </position>
            </language>
            <translation>Only honourable, good.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Tibi vero Antioche minime, cui praeter honestatem multa bona, praeter turpitudinem multa mala videntur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antiochus aliter</marginalia_text>
                </position>
            </language>
            <translation>Antiochus [thinks] otherwise.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="homuncio">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Homuncio.</marginalia_text>
                </position>
            </language>
            <translation>A little man.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Praecide, inquit: statue">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        pra[e]cide
                        statue.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Cut it short.
                Make up your mind.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="dicebant, qu&ograve;d esset"/>
        <underline method="pen" type="straight" language="LA" text="quorum omne bonum in mente positum, &amp;"/>
        <underline method="pen" type="straight" language="LA" text="acie, qua verum"/>
        <underline method="pen" type="straight" language="LA" text="illis similia, sed opinor explicata vberius &amp;"/>
        <underline method="pen" type="straight" language="LA" text="Nihil esse bonum, nisi virtutem"/>
        <underline method="pen" type="straight" language="LA" text="nec malu[m], nisi quod virtuti esset contrarium. In"/>
        <underline method="pen" type="straight" language="LA" text="haec videamus quae nunc diu mult&uacute;mque defensa sunt"/>
        <underline method="pen" type="straight" language="LA" text="molestia Hieronymus"/>
        <underline method="pen" type="straight" language="LA" text="Honeste autem viuere fruentem rebus his, quas primas homini natura conciliet, &amp; vetus Academia censuit, 
            vt indicant scripta Polemonis, quae Antiochus probat maxime &amp; Aristoteles, ei&uacute;sque amici nu[n]c proxime videntur accedere"/>
        <underline method="pen" type="straight" language="LA" text="vt opponeret"/>
        <underline method="pen" type="straight" language="LA" text="bonum esse frui his rebus, quas primas natura conciliauisset"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; conciliatione naturae"/>
        <underline method="pen" type="straight" language="LA" text="bonoru[m], quos"/>
        <underline method="pen" type="straight" language="LA" text="Ad vos nunc refero"/>
        <underline method="pen" type="straight" language="LA" text="tam ineruditum absurd&uacute;mque respondeat"/>
        <underline method="pen" type="straight" language="LA" text="dici inconsyderatius"/>
        <underline method="pen" type="straight" language="LA" text="per ipsum Aristotelem"/>
        <underline method="pen" type="straight" language="LA" text="germanissimus Stoicis"/>
        <underline method="pen" type="straight" language="LA" text="inter eos non de terminis, sed de tota possessione contentio"/>
        <underline method="pen" type="straight" language="LA" text="veheme[n]tissime displicet"/>
        <underline method="pen" type="straight" language="LA" text="n&oacute;nne caueam ne scelus faciam"/>
        <underline method="pen" type="straight" language="LA" text="dogma prodere"/>
        <underline method="pen" type="straight" language="LA" text="Etiam, inquit, beatam, sed non beatissimam"/>
        <underline method="pen" type="straight" language="LA" text="homuncio hic"/>
        <underline method="pen" type="straight" language="LA" text="praesertim Theophrasto multa diserte copios&eacute;que dicente"/>
        <underline method="pen" type="straight" language="LA" text="Distrahor, tu[m] hoc mihi probabilius, tum illud videtur: &amp; tamen nisi alterutru[m] sit, virtute[m] iacere plane puto"/>
        
