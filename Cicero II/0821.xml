    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="munus Reipub. afferre maius meli&uacute;sve possumus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Maximu[m] munis quod reip[ublicae]. adferre possumus</marginalia_text>
                </position>
            </language>
            <translation>What [is] greatest service to the commonwealth we can render.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Inue[n]tus</marginalia_text>
                </position>
            </language>
            <translation>Discovered.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Mangificu[m] illud etiam, Roman&iacute;sque hominibus gloriosum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Magnificu[m] et gloriosu[m]</marginalia_text>
                </position>
            </language>
            <translation>Splendid and glorious.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="casus grauis ciuitatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Casus grauis Ciuitatis</marginalia_text>
                </position>
            </language>
            <translation>The grave condition of the state.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Dabunt igitur mihi veniam mei ciues, vel gratiam potius habebu[n]t, qu&ograve;d quu[m] esset in vnius potestate Respub. neque ego me abdidi, neque 
            deserui, neque afflixi, neque ita gessi, quasi homini aut te[m]poribus iratus: neque porr&ograve; ita aut adulatus, aut admiratus fortuna[m] sum alterius, vt me meae poeniteret">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cic[ero] Constantia</marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Cicero's constancy.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Platone">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plato.</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="naturales esse quasfam conuersiones Reru[m]p[ublicum]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Naturales co[n]uersiones rerump[ublicum].</marginalia_text>
                </position>
            </language>
            <translation>Revolutions in government are to be expected [Loeb 154, 377].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="In libris enim sententia[m] dicebamus, concionabamur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In libris se[n]tentia[m] dicam[us] concionamur [etc].</marginalia_text> 
                </position>
            </language>
            <translation>'It was in my books that I made my senatorial speeches and my forensic harangues', et cetera [Loeb 154, 377].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Quintus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quintus Cicero.</marginalia_text>
                    <person name="Quintus Tullius Cicero"/>
                </position>
            </language>
            <translation>Quintus Cicero.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Stoicorum sententiam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Stoicoru[m] s[e]n[tent]ia.</marginalia_text>
                </position>
            </language>
            <translation>The opinions of the Stoics.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nihil vt affirmem: quaeram omnia">
