                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nihil certi</marginalia_text>
                </position>
            </language>
            <translation>Nothing is certain.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Carneades">
            <language ident="LA">
