        <underline method="pen" type="straight" language="LA" text="retuli me Brute, te hortante maxime"/>
        <underline method="pen" type="straight" language="LA" text="mihi Latinis literis illustra[n]dum"/>
        <underline method="pen" type="straight" language="LA" text="Graecis &amp; literis &amp; doctoribus"/>
        <underline method="pen" type="straight" language="LA" text="quae quidem digna statuissent in quibus elaborarent"/>
        <underline method="pen" type="straight" language="LA" text="temperauerunt &amp; institutis &amp; legibus"/>
        <underline method="pen" type="straight" language="LA" text="sunt, neque cum Graeci, neque vlla cum gente sunt confere[n]da"/>
        <underline method="pen" type="straight" language="LA" text="quae tanta constantia, magnitudo animi, probitas, fides, quae tam excellens in"/>
        <underline method="pen" type="straight" language="LA" text="virtus in vllis fuit, vt sit cum maioribus nostris compara[n]da?"/>
        <underline method="pen" type="straight" language="LA" text="literarum genere"/>
        <underline method="pen" type="straight" language="LA" text="antiquissimum &egrave; doctis genus"/>
        <underline method="pen" type="straight" language="LA" text="serius poeticam nos accepimus"/>
        <underline method="pen" type="straight" language="LA" text="qui fuit maior"/>
        <underline method="pen" type="straight" language="LA" text="solitos esse"/>
        <underline method="pen" type="straight" language="LA" text="qua obiecit vt probrum"/>
        <underline method="pen" type="straight" language="LA" text="Nobiliori, qu&ograve;d is in prouinciam poetas duxisset"/>
        <underline method="pen" type="straight" language="LA" text="igitur honoris erat poetis e&ograve; minora studia fuerunt"/>
        <underline method="pen" type="straight" language="LA" text="nobilissimo homini laudi datu[m]"/>
        <underline method="pen" type="straight" language="LA" text="incenduntur ad studia gloria"/>
        <underline method="pen" type="straight" language="LA" text="iac&eacute;ntque ea semper uae apud quosque improbantur"/>
        <underline method="pen" type="straight" language="LA" text="in neruorum vec&uacute;mque"/>
        <underline method="pen" type="straight" language="LA" text="in epulis"/>
        <underline method="pen" type="straight" language="LA" text="satis excultus doctrina"/>
        <underline method="pen" type="straight" language="LA" text="metiendi ratiocinand&iacute;sque vtilitate, huius artis terminauimus"/>
        <underline method="pen" type="straight" language="LA" text="magnos"/>
        <underline method="pen" type="straight" language="LA" text="nec vllum habuit lumen literarum"/>
        <underline method="pen" type="straight" language="LA" text="illustranda &amp; excita[n]da nobis est, vt si occupati profuimus aliquid ciuibus nostris, prosimus etiam si possumus otiosi. In"/>
        
