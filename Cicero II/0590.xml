<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0590.tif" reader="John Dee" pagination="32"/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="magnitudinem quasi decempeda">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>dece[m]peda magnitudo</marginalia_text>
                </position>
            </language>
            <translation>A size of ten feet.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Vrgebor iam omnium vestru[m] co[n]uitio">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>o[mn]i[u]m co[n]vitio vrgeri</marginalia_text>
                </position>
            </language>
            <translation>To be oppressed by the scorn of all.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="rapior in inuidiam">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>in invidia[m] rapi</marginalia_text>
                </position>
            </language>
            <translation>To be dragged into disrepute.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Solis autem magnitudo (ipse enim hic radiatus me intueri videtur)">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>sol radiatus</marginalia_text>
                </position>
            </language>
            <translation>The radiating sun.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="inane">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Inane</marginalia_text>
                </position>
            </language>
            <translation>Inane.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="atomos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Atomi</marginalia_text>
                </position>
            </language>
            <translation>Atoms.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Et quum in vno mundo ornatus hic tam sit mirabilis, innumerabilis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mundi innumerabiles</marginalia_text>
                </position>
            </language>
            <translation>Innumerable worlds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="imagines">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Imagines.</marginalia_text>
                </position>
            </language>
            <translation>Images.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nihil sentire est melius, qu&agrave;m tam praua sentire">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nihil sentire melius qu&agrave;m tam praua sentire.</marginalia_text>
                </position>
            </language>
            <translation>It is better to think nothing than to think such perverse things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nec enim diuinationem, quam probatis, vllam esse arbitror">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuinatio nulla</marginalia_text>
                </position>
            </language>
            <translation>No divination.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fat&uacute;mque illud etiam, quo omnia contineri dicitis, conte[m]no">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>fatum nullum.</marginalia_text>
                </position>
            </language>
            <translation>No fate.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aether videtur summus Deus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>&AElig;ther summus DEVS</marginalia_text>
                </position>
            </language>
            <translation>The aether, the highest deity.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Solem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sol.</marginalia_text>
                </position>
            </language>
            <translation>The sun.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cogimur dissensione sapientum dominum nostrum ignorare">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dissentione sapie[n]tum dominu[m] n[ost]r[u]m ignorare cogimur</marginalia_text>
                </position>
            </language>
            <translation>We are compelled by the disagreement of the wise to be ignorant of our lord.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec tamen istas quaestiones Physicorum exterminandas puto">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ista[e] physicoru[m] qua[e]stiones non exterminanda[e].</marginalia_text>
                </position>
            </language>
            <translation>Those questions of the natural philosophers are not to be expelled.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pabulum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Animoru[m] pabulu[m].</marginalia_text>
                </position>
            </language>
            <translation>The nourishment of minds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quaeret igitur haec &amp; vester sapie[n]s, &amp; hic noster">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sapiens                  
                        Noster &amp; vester
                        dissentiu[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>Our wise person and your wise person disagree.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quum cornix cecinerit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quum cornix cecinerit.</marginalia_text>
                </position>
            </language>
            <translation>When a crow caws.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="qu&agrave;m solem, quem metiri non possunt plus qu&agrave;m  duodeuiginti partibus maiore[m] esse qu&agrave;m terra[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>An ta[n]tum &Sun; 18 terras co[n]tinuet</marginalia_text>
                    <symbol_in_text name="Sun"/>
                </position>
            </language>
            <translation>Whether the sun covers as much space as 18 Earths.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vna definitio co[m]prehendendi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>vna est definitio co[m]prehendendi.</marginalia_text>
                </position>
            </language>
            <translation>The is one definition to be comprehended.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Herillum qui in cognitione &amp; scientia summu[m] bonu[m] ponit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Herillus summu[m] bonu[m] in cognitio[n]e et scie[n]tia posuit.</marginalia_text>
                    <person name="Herillus"/>
                </position>
            </language>
            <translation>Herillus put the greatest good into understanding and knowledge.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Xenophanes; Parmenides &amp; Zeno; Euclides">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Xenophanes
                        Parmenides
                        Zeno.
                        Euclides.
                    </marginalia_text>
                    <person name="Xenophanes"/>
                    <person name="Parmenides"/>
                    <person name="Zeno of Citium"/>
                    <person name="Euclid"/>
                </position>
            </language>
            <translation>
                Xenophanes.
                Parmenides.
                Zeno of Citium.
                Euclid.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="paululum ant&egrave; dicendum est">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        paululu[m] ant&egrave; dice[n]du[m] est
                        [superius - deleted] repete[n]du[m] altius.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'A few words must be said first' [Loeb 268, 633].
                [A few words] must be repeated higher up.            
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Vrgebor iam omnium vestru[m]"/>
        <underline method="pen" type="straight" language="LA" text="praeclara[m]? Et quum in vno mundo ornatus hic tam sit mirabilis, innumerabilis"/>
        <underline method="pen" type="straight" language="LA" text="nominibus, honoribus"/>
        <underline method="pen" type="straight" language="LA" text="rebus gestis, ingeniis"/>
        <underline method="pen" type="straight" language="LA" text="si etiam dormientes aliquid animo videre videamur, imagines"/>
        <underline method="pen" type="straight" language="LA" text="animos nostros per corpus irrumpere"/>
        <underline method="pen" type="straight" language="LA" text="Nihil sentire est melius, qu&agrave;m tam praua sentire"/>
        <underline method="pen" type="straight" language="LA" text="diuinationem, quam probatis, vllam esse arbitror: fat&uacute;mque illud etiam, quo"/>
        <underline method="pen" type="straight" language="LA" text="contineri dicitis, conte[m]no: ne exaedificatu[m] quidem hunc mundu diuino co[n]silio existimo"/>
        <underline method="pen" type="straight" language="LA" text="haud scio an ita sit"/>
        <underline method="pen" type="straight" language="LA" text="Sed quum rapior in inuidiam"/>
        <underline method="pen" type="straight" language="LA" text="videtur summus Deus, mente praeditus, qua omnia regantur"/>
        <underline method="pen" type="straight" language="LA" text="cogimur dissensione sapientum dominum nostrum ignorare"/>
        <underline method="pen" type="straight" language="LA" text="quippe qui nesciamus Soli, an aetheri seruiamus"/>
        <underline method="pen" type="straight" language="LA" text="ipse enim hic radiatus me intueri videtur"/>
        <underline method="pen" type="straight" language="LA" text="admonet, vt crebro faciam mentionem sui"/>
        <underline method="pen" type="straight" language="LA" text="decempeda"/>
        <underline method="pen" type="straight" language="LA" text="exterminandas puto. Est enim animoru[m] ingenior&uacute;mque naturale quoddam quasi pabulum consyderatio contemplati&oacute;que naturae. 
            Erigimur, latiores fieri videmur, humana despicimus: cogitant&eacute;sque supera atque caelestia, haec nostra vt exigua &amp; minima contemnimus. Indagatio ipsa reru[m] cum maximaru[m], 
            tum etiam occultissimarum habet oblectatione[m]. Si vero aliquid occurret quod verisimile videatur, humanissima co[m]pletur animus"/>
        <underline method="pen" type="straight" language="LA" text="assentiatur, credat, affirmet: noster, vt vereatur"/>
        <underline method="pen" type="straight" language="LA" text="eiusmodi rebus verisimile quod sit inuenerit"/>
        <underline method="pen" type="straight" language="LA" text="&amp; paululum ant&egrave; dicendum"/>
        <underline method="pen" type="straight" language="LA" text="quum cornix cecinerit, tum"/>
        <underline method="pen" type="straight" language="LA" text="co[n]stituendi sunt ad quos &amp;"/>
        <underline method="pen" type="straight" language="LA" text="cognitione &amp; scientia summu[m] bonu[m] ponit"/>
        
