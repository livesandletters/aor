    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="expedito, soluto, libero, nulla re implicato">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        expeditus
                        solutus
                        liber
                        nulla re implicatus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dispatched.
                Released.
                Free.
                Entangled in nothing.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="multa falsa probabilia sint">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Multa falsa probabilia</marginalia_text>
                </position>
            </language>
            <translation>Many false things [are] probable.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Itaque ait vehementer errare eos qui dicant ab Academia sensus eripi, &agrave; quibus no[n]nunquam dictum sit, aut colorem, aut saporem, aut sonum nullum esse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Academica[e] opinio</marginalia_text>
                </position>
            </language>
            <translation>The opinion of the Academy.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="adiungit dupliciter dici assensus sustinere sapiente[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Dupl[icite]r cohibetur asse[n]sus</marginalia_text>
                </position>
            </language>
            <translation>Agreement is withheld in two ways.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="visa quibus ad actionem excitemur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>visa quibus ad actione[m] excitamur.</marginalia_text>
                </position>
            </language>
            <translation>The visions by which we are called to action</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sed ea quae vos percipi comprehe[n]d&iacute;que">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>p[er]cipi. .videri.</marginalia_text>
                </position>
            </language>
            <translation>To be perceived. To be seen.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mare illus quod nunc Fauonio nascente purpureum videtur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mare fauonio nasce[n]te purpureu[m] videtur.</marginalia_text>
                </position>
            </language>
            <translation>The sea seems purple with the emerging west wind.</translation>
        </marginalia>

        <marginalia hand="Italian" anchor_text="quia nobismetipsis modo caeruleu[m] videbatur, mane flauum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Maris varij colores.</marginalia_text>
                </position>
            </language>
            <translation>The various colours of the sea.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="memoria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Memoria</marginalia_text>
                </position>
            </language>
            <translation>Memory.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Polyaenus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Polya[e]nus.</marginalia_text>
                    <person name="Polyaenus of Lampsacus"/>
                </position>
            </language>
            <translation>Polyaenus of Lampsacus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="falsum quod est, id percipi no[n] potest">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>falsum percipi no[n] p[otes]t.</marginalia_text>
                </position>
            </language>
            <translation>A false thing cannot be perceived.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="memoria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Memoria</marginalia_text>
                </position>
            </language>
            <translation>Memory.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Scyron">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Scyron.</marginalia_text>
                    <person name="Siro the Epicurean"/>
                </position>
            </language>
            <translation>Siro the Epicurean.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Panaetius">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pana[e]tius.</marginalia_text>
                    <person name="Panaetius"/>
                </position>
            </language>
            <translation>Panaetius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="auspicia, oracula, somnia, vaticinationes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        auspicia
                        oracula
                        somnia
                        vaticinatio[n]es
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Diviners.
                Oracles.
                Dreams.
                Prophecies.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="veri similitudinem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Probabilitas veri similitudo</marginalia_text>
                </position>
            </language>
            <translation>Probability, an appearance of truth.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Dicunt enim Stoici sensus ipsos assensus esse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sensus esse asse[n]sus dicu[n]t Stoici</marginalia_text>
                </position>
            </language>
            <translation>'The Stoics say that the sensations are themselves acts of assent' [Loeb 268, 605].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="assensus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Assensus lubrici</marginalia_text>
                </position>
            </language>
            <translation>'Slippery assents' [Loeb 268, 607].</translation>  
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vide superiora" id="RCP10549-50v2:m0001">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>vide superiora</marginalia_text>
                </position>
            </language>
            <translation>See above.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d multa falsa probabilia sint, nihil"/>
        <underline method="pen" type="straight" language="LA" text="inesse in his propriam, quae nusquam alibi esset, veri &amp; certi notam"/>
        <underline method="pen" type="straight" language="LA" text="adiungit dupliciter dici"/>
        <underline method="pen" type="straight" language="LA" text="vt seque[n]s probabilitatem, vbicunque haec aut occurrat"/>
        <underline method="pen" type="straight" language="LA" text="aut deficiat"/>
        <underline method="pen" type="straight" language="LA" text="Non enim lucem eripimus"/>
        <underline method="pen" type="straight" language="LA" text="percipi comprehe[n]d&iacute;que, eadem nos si modo probabilia sint, videri dicimus"/>
        <underline method="pen" type="straight" language="LA" text="eo quide[m] expedito, soluto, libero, nulla re implicato"/>
        <underline method="pen" type="straight" language="LA" text="Luculle iacere iam illud tuum perspecuitatis patrocinium"/>
        <underline method="pen" type="straight" language="LA" text="caeruleu[m]"/>
        <underline method="pen" type="straight" language="LA" text="mane flauum"/>
        <underline method="pen" type="straight" language="LA" text="tota[m] Geometriam falsam esse credidit, num illa etiam quae sciebat oblitus"/>
        <underline method="pen" type="straight" language="LA" text="comprehe[n]sar&uacute;mque rerum est"/>
        <underline method="pen" type="straight" language="LA" text="Hoc per me licet"/>
        <underline method="pen" type="straight" language="LA" text="putant, vera esse aruspicum auspicia, oracula, somnia, vaticinationes, s&eacute;que ab assensu sustineat. Quod"/>
        <underline method="pen" type="straight" language="LA" text="veri similitudinem non impeditum"/>
        <underline method="pen" type="straight" language="LA" text="Dicunt enim Stoici sensus ipsos assensus"/>
        <underline method="pen" type="double" language="LA" text="vide" id="RCP10549-50v2:u0001"/>
        <underline method="pen" type="straight" language="LA" text="superiora: sed breui res potest tota confici"/>
        
