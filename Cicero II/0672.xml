        <underline method="pen" type="straight" language="LA" text="venisse, &amp; in ea cum alios multos, tum Archytam, Timae&uacute;mque cognouisse, &amp; didicisse Pythagorea omnia: 
            prim&uacute;mque de animorum aeternitate non solum sensisse idem quod Pyuthagoras, sed rationem etiam attulisse, quam (nisi quid dicis) praetermittamus, &amp; hanc totam spem immortalitatis relinquamus"/>
        <underline method="pen" type="straight" language="LA" text="quem ex tuo ore admiror: qu&agrave;m cum istis vera"/>
        <underline method="pen" type="straight" language="LA" text="M. Macte virtute"/>
        <underline method="pen" type="straight" language="LA" text="mundi sitam ad vniuersi caeli complexum, quasi puncti instar obtinere, quod –––GREEK NEEDED––– illi vocant"/> <!-- FSF: Greek needed --> 
        <underline method="pen" type="straight" language="LA" text="inter se &amp; diuisa momenta, vt terrena, &amp; humida suopte"/>
        <underline method="pen" type="straight" language="LA" text="siue ipsa natura superiora"/>
        <underline method="pen" type="straight" language="LA" text="grauioribus"/>
        <underline method="pen" type="straight" language="LA" text="illi sint animales, id"/>
        <underline method="pen" type="straight" language="LA" text="quod subtiliter magis qu&agrave;m dilucide dicitur, aut quinta illa non nominata magis qu&agrave;m non intellecta natura, 
            mult&ograve; etiam integriora ac puriora sunt vt &agrave; terra longissime se"/>
        <underline method="pen" type="straight" language="LA" text="nec tam vegata mens, aut in corde cerebr&oacute;ve, aut in Empedocleo sanguine demersa iaceat"/>
        <underline method="pen" type="straight" language="LA" text="aequali &amp; condiscipulo"/>
        <underline method="pen" type="straight" language="LA" text="ne condoluisse quidem vnquam videtur, qui animum se"/>
        <underline method="pen" type="straight" language="LA" text="non sentiat: alter ita delectatur suis cantibus, vt eos etiam ad haec transferre conetur"/>
        <underline method="pen" type="straight" language="LA" text="eruditus sit, sicut est, haec magistro concedat Aristoteli: canere ipse doceat"/>
        <underline method="pen" type="straight" language="LA" text="prouerbio praecipitur"/>
        <underline method="pen" type="straight" language="LA" text="Illam vero funditus eiiciamus indiuidoru[m] corporum leuium"/>
        <underline method="pen" type="straight" language="LA" text="concursionem fortuita[m], quam"/>
        <underline method="pen" type="straight" language="LA" text="concalefacta[m] &amp; spirabilem"/>
        <underline method="pen" type="straight" language="LA" text="Nihil enim habent haec duo genera proni"/>
        <underline method="pen" type="straight" language="LA" text="his perrumpatur &amp; diuidatur crassus"/>
        <underline method="pen" type="straight" language="LA" text="concretus aer qui est terrae proximus"/>
        <underline method="pen" type="straight" language="LA" text="nostra terreno principiorum genere confecta, ardore animi concalescunt"/>
        <underline method="pen" type="straight" language="LA" text="diuidat omne caelum hoc, in quo nubes, imbres, vent&iacute;que coguntur"/>
        <underline method="pen" type="straight" language="LA" text="caliginosum est propter exhalationes terrae"/>
        <underline method="pen" type="straight" language="LA" text="ignibus insistit"/>
        <underline method="pen" type="straight" language="LA" text="tanquam paribus examinatus ponderibus"/>
        <underline method="pen" type="straight" language="LA" text="nulla re egens aletur, &amp; sustentabitur iisdem rebus quibus astra sustentantur"/>

