        <marginalia hand="English_secretary" anchor_text="Solis defectiones, it&eacute;mque lunae praedicu[n]tur in multos annos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Solis defectio[n]es et lun[a]e in multos predicu[n]t[u]r annos</marginalia_text>
                </position>
            </language>
            <translation>Eclipses of the sun and the moon are predicted for many years.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="naturae necessitas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Natura[e] necessitas</marginalia_text>
                </position>
            </language>
