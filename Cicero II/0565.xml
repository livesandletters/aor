        <underline method="pen" type="straight" language="LA" text="immutabilem continuationem ordinis sempiterni"/>
        <underline method="pen" type="straight" language="LA" text="Nonnunqua[m] quidem eandem Fortuna[m], qu&ograve;d efficiat multa improuisa haec nec opinata nobis, propter obscuritatem ignoration&eacute;mque causaru[m]"/>
        <underline method="pen" type="straight" language="LA" text="Quanqua[m] oriretur &agrave; sensibus, tamen no[n]"/>
        <underline method="pen" type="straight" language="LA" text="Mentem voleba[n]t rerum esse iudicem: solam"/>
        <underline method="pen" type="straight" language="LA" text="quia sola cerneret id quod semper esset simplex, &amp; vnius modi, &amp; tale quale esset"/>
        <underline method="pen" type="straight" language="LA" text="hebetes &amp; tardos"/>
        <underline method="pen" type="straight" language="LA" text="mobiles &amp; concitatae"/>
        <underline method="pen" type="straight" language="LA" text="quia continenter laberentur &amp; fluerent"/>
        <underline method="pen" type="straight" language="LA" text="nisi in animi motionibus atque rationibus"/>
        <underline method="pen" type="straight" language="LA" text="definitiones rerum"/>
        <underline method="pen" type="straight" language="LA" text="Verboru[m] etiam explicatio probabatur"/>
        <underline method="pen" type="straight" language="LA" text="id est orationis ratione conclusae"/>
        <underline method="pen" type="straight" language="LA" text="ex altera parte"/>
        <underline method="pen" type="straight" language="LA" text="oratoria vis dicendi adhibebatur, explicatrix orationis perpetuae, ad persuadendum accommodatae"/>
        <underline method="pen" type="straight" language="LA" text="Plato erat amplexatus"/>
        <underline method="pen" type="straight" language="LA" text="vir &amp; oratione suauis, &amp; ita moratus, vt prae se probitatem quandam &amp; ingenuitatem ferat, vehementius"/>
        <underline method="pen" type="straight" language="LA" text="Spoliauit enim virtutem suo decore"/>
        <underline method="pen" type="straight" language="LA" text="imbecill&aacute;mque reddidit"/>
        <underline method="pen" type="straight" language="LA" text="fuit acri ingenio"/>
        <underline method="pen" type="straight" language="LA" text="omnino semouendus"/>
        <underline method="pen" type="straight" language="LA" text="posita est in virtute &amp; in moribus"/>
        <underline method="pen" type="straight" language="LA" text="ad inuestigationem naturae"/>
        <underline method="pen" type="straight" language="LA" text="anteiret aetate, vald&eacute;que"/>
        <underline method="pen" type="straight" language="LA" text="&amp; peracute moueretur"/>
        <underline method="pen" type="straight" language="LA" text="significare Po[m]ponium"/>
        <underline method="pen" type="straight" language="LA" text="neruos virtutis inciderit"/>
        
