        <underline method="pen" type="straight" language="LA" text="quam mihi quisque"/>
        <underline method="pen" type="straight" language="LA" text="ancillae s&iacute;tne in"/>
        <underline method="pen" type="straight" language="LA" text="acutu[m] genus est, &amp; ad vsus ciuium no[n] inutile"/>
        <underline method="pen" type="straight" language="LA" text="illa vendibiliora, haec vberiora certe sunt: quanquam"/>
        <underline method="pen" type="straight" language="LA" text="esse his literis"/>
        <underline method="pen" type="straight" language="LA" text="philosophiae disciplinis diceretur"/>
        <underline method="pen" type="straight" language="LA" text="veniat in medium Epicuri"/>
        <underline method="pen" type="straight" language="LA" text="non soleat accuratius explicari: veru[m]"/>
        <underline method="pen" type="straight" language="LA" text="aduersarium aliquem conuincere"/>
        <underline method="pen" type="straight" language="LA" text="in primis grauis &amp; doctus adolesce[n]s ei disputationi interesset. Nam"/>
        <underline method="pen" type="straight" language="LA" text="maxim&iacute;sque erroribus animos hominum liberauisse"/>
        <underline method="pen" type="straight" language="LA" text="Nam illuc quidem adduci vix possum, vt ea quae senserit"/>
        <underline method="pen" type="straight" language="LA" text="philosophi non offendit"/>
        <underline method="pen" type="straight" language="LA" text="nam &amp; complectitur verbis quod"/>
        <underline method="pen" type="straight" language="LA" text="philosopho si afferat eloquentiam, non asperner: si non habeat, no[n] admodum flagite[m]"/>
        <underline method="pen" type="straight" language="LA" text="sed quot homines tot sententiae"/>
        <underline method="pen" type="straight" language="LA" text="praeter sedulitatem probarent"/>
        <underline method="pen" type="straight" language="LA" text="Phaedrum aute[m] etiam amaret"/>
        <underline method="pen" type="straight" language="LA" text="quotidi&eacute;que inter nos ea, quae audiebamus, conferebamus"/>
        <underline method="pen" type="straight" language="LA" text="controuersia quid ego intelligerem, sed quid probarem"/>
        <underline method="pen" type="straight" language="LA" text="quibus maxime gloriatur"/>
        <underline method="pen" type="straight" language="LA" text="quae corrigere vult, mihi quidem deprauare videatur"/>
        <underline method="pen" type="straight" language="LA" text="id est corpora indiuidia propter soliditate[m]"/>
        <underline method="pen" type="straight" language="LA" text="concursionibus inter se cohaerescant, ex"/>
        <underline method="pen" type="straight" language="LA" text="efficiantur ea quae sint, qua&eacute;que cernantur, omnia"/>
        <underline method="pen" type="straight" language="LA" text="sed ex aeterno tempore"/>
        <underline method="pen" type="straight" language="LA" text="vnum quae"/>
        <underline method="pen" type="straight" language="LA" text="illae Epicuri propriae ruinae"/>
        <underline method="pen" type="straight" language="LA" text="illa indiuidua &amp; solida corpora"/>
        
