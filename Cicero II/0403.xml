    <annotation>
        
        
        <marginalia hand="Italian" anchor_text="oratoris">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Orator</marginalia_text>
                </position>
            </language>
            <translation>An orator.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="cognosce[n]dis atque tracta[n]dis"/>
        <underline method="pen" type="straight" language="LA" text="plenum"/>
        <underline method="pen" type="straight" language="LA" text="varie copios&eacute;que"/>
        <underline method="pen" type="straight" language="LA" text="ex obscuriore aliqua scie[n]tia sit promendu[m] &amp; assumendu[m]"/>
        <underline method="pen" type="straight" language="LA" text="legibus iubendis"/>
        <underline method="pen" type="straight" language="LA" text="vetandis"/>
        <underline method="pen" type="straight" language="LA" text="admoueri possit oratio ad sensus animoru[m] atque motus vel infla[m]mandos, vel etia[m] extinguendos, quod vnu[m] in oratore dominatur, sine dilige[n]tissima peruestigatione"/>
        <underline method="pen" type="straight" language="LA" text="minus hoc vobis sim probaturus"/>
        <underline method="pen" type="straight" language="LA" text="qui illa profite[n]tur: illustrare"/>
        <underline method="pen" type="straight" language="LA" text="confugiendu[m] est facultatem. Neque"/>
        <underline method="pen" type="straight" language="LA" text="architectu[m]"/>
        <underline method="pen" type="straight" language="LA" text="armamentariu[m] fecit"/>
        <underline method="pen" type="straight" language="LA" text="architecti potius artificio disertum, qu&agrave;m oratoris fuisse"/>
        <underline method="pen" type="straight" language="LA" text="de naualium opere"/>
        <underline method="pen" type="straight" language="LA" text="non"/>
        <underline method="pen" type="straight" language="LA" text="Neque vero"/>
        <underline method="pen" type="straight" language="LA" text="vincebat"/>
        <underline method="pen" type="straight" language="LA" text="in eo ipso quod"/>
        <underline method="pen" type="straight" language="LA" text="non"/>
        <underline method="pen" type="straight" language="LA" text="illud est probabilius, neque tame[n] verum"/>
        <underline method="pen" type="straight" language="LA" text="omnes in eo quod scirent, satis esse eloquentes"/>
        <underline method="pen" type="straight" language="LA" text="Illud verius, neque quenqua[m] in eo disertum esse posse quod nesciat, neque si id optime sciat, ignar&uacute;sque sit faciundae ac poliendae orationis, diserte id ipsum posse, de quo sciat, dicere"/>
        <underline method="pen" type="straight" language="LA" text="vniuersam &amp; propria[m] oratoris vim definire complect&iacute;que vult"/>
        <underline method="pen" type="straight" language="LA" text="hoc tam graui dignus nomine"/>
        <underline method="pen" type="straight" language="LA" text="dictione explicanda"/>
        <underline method="pen" type="straight" language="LA" text="memoriter"/>
        <underline method="pen" type="straight" language="LA" text="quadam etia[m] actionis dignitate"/>
        <underline method="pen" type="straight" language="LA" text="quod ita proposui"/>
        <underline method="pen" type="straight" language="LA" text="circu[m]c&icirc;dat"/>
        <underline method="pen" type="straight" language="LA" text="amputet"/>
        <underline method="pen" type="straight" language="LA" text="tenebo"/>
        <underline method="pen" type="straight" language="LA" text="sita sunt"/>
        <underline method="pen" type="straight" language="LA" text="in disceptationibus, aut"/>
        <underline method="pen" type="straight" language="LA" text="quae sint in quaque re"/>
        <underline method="pen" type="straight" language="LA" text="cum acceperit, ita pronu[n]tiabit"/>
        <underline method="pen" type="straight" language="LA" text="tecum co[m]municabit, t&eacute;que"/>
        <underline method="pen" type="straight" language="LA" text="co[m]municabit"/>
        <underline method="pen" type="straight" language="LA" text="si me audierit"/>
        <underline method="pen" type="straight" language="LA" text="in naturae obscuritate[m], in dissere[n]di subtilitate[m], in vitam atque mores"/>
        <underline method="pen" type="straight" language="LA" text="&iacute;dque largiamur inertiae nostrae"/>
        <underline method="pen" type="straight" language="LA" text="in quo magnus esse possit"/>
        <underline method="pen" type="straight" language="LA" text="totus est oratori perdiscendus"/>
        <underline method="pen" type="straight" language="LA" text="delata &amp; tradita"/>
        <underline method="pen" type="straight" language="LA" text="ab agro remotissimu[m]"/>
        <underline method="pen" type="straight" language="LA" text="ad certam causam temp&uacute;sque cognorit? Est enim finitimus oratori poeta, numeris astrictior paulo, verboru[m] aute[m] licentia liberior"/>
        
        
