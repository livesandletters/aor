    <annotation>
        
        <marginalia hand="Italian" anchor_text="Socrate">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Socrates
                    </marginalia_text>
                    <person name="Socrates"/>
                </position>
            </language>
            <translation>
                Socrates.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="co[n]tra deos disputandi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Ne co[n]tra deos disputes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                You shall not discuss with gods.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="suo cuique iudicio sit vtendum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Suo cuiq[ue] iudicio vtendum est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'Each man must use his own judgement' [LCL 268:286-87].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Epicurus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Contra Epicurum
                    </marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>
                Against Epicurus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cottam esse &amp; Pontifice[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Cotta es &amp; Pontifex
                    </marginalia_text>
                    <person name="Gaius Aurelius Cotta"/>
                </position>
            </language>
            <translation>
                Gaius Aurelius Cotta was a priest.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="populi R[omani] religio in sacta &amp; in auspicia diuisa sit, tertiu[m] adiunctu[m] sit, 
            siquid praedictionis causa ex porte[n]tis &amp; monstris Sibyllae interpretes, aruspic&eacute;sve mouerunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Romanorum religio in quas secetur partes Sibylla[e]
                    </marginalia_text>
                    <person name="Sibyl"/>
                </position>
            </language>
            <translation>
                The devotion of Romans in parts of the Sibyl.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="philopho rationem accipere debeo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Philosophus vacare r[ati]o[n]e no[n] debet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A philosopher must not be free from reason.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Primum">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        p[ri]mu[m] deinde tum postremo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                First, next, then, finally.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Haec mihi fer&egrave; in mentem veniebant, quae dicenda"/>
        <underline method="pen" type="straight" language="LA" text="Mala enim &amp; impia consetudo est co[n]tra deos disputandi, siue ex animo id sit, siue simulate"/>
        <underline method="pen" type="straight" language="LA" text="meditabar, neque tam refellendi tui causa, qu&agrave;m ea quae minus intelligebam requirendi"/>
        <underline method="pen" type="straight" language="LA" text="bene paratum"/>
        <underline method="pen" type="straight" language="LA" text="nequid inuidiae subeat aut criminis"/>
        <underline method="pen" type="straight" language="LA" text="etiam si minus vera, tamen apta inter se &amp; cohaerentia"/>
        <underline method="pen" type="straight" language="LA" text="sic agamus vt nos"/>
        <underline method="pen" type="straight" language="LA" text="sacra, cerimonias, religion&eacute;sque defenderem"/>
        <underline method="pen" type="straight" language="LA" text="omnis populi R[omana] religio in sacra &amp; in auspicia diuisa sit, tertiu[m] adiunctu[m] sit, 
            siquid praedictionis causa ex porte[n]tis &amp; monstris Sibyllae interpretes"/>
        <underline method="pen" type="straight" language="LA" text="te enim philopho rationem accipere"/>
        <underline method="pen" type="straight" language="LA" text="Primum"/>
        
