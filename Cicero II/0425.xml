       <underline method="pen" type="straight" language="LA" text="probabiliorem"/>
        <underline method="pen" type="straight" language="LA" text="si alter contemnere, alter ne nosse quidem Graecos videretur"/>
        <underline method="pen" type="straight" language="LA" text="nihil san&egrave; ad hoc tempus"/>
        <underline method="pen" type="straight" language="LA" text="institutae scriptionis ac temporis"/>
        <underline method="pen" type="straight" language="LA" text="florere"/>
        <underline method="pen" type="straight" language="LA" text="praestare"/>
        <underline method="pen" type="straight" language="LA" text="definitam aliqua[m] regionem, cuius terminis septa teneatur"/>
        <underline method="pen" type="straight" language="LA" text="in hominum disceptationemn cadere possunt"/>
        <underline method="pen" type="straight" language="LA" text="in tantam prudentia[m] pertinerent"/>
        <underline method="pen" type="straight" language="LA" text="confirmo"/>
        <underline method="pen" type="straight" language="LA" text="feci libentius, vt"/>
        <underline method="pen" type="straight" language="LA" text="ma[n]darem literis"/>
        <underline method="pen" type="straight" language="LA" text="vt"/>
        <underline method="pen" type="straight" language="LA" text="tolleretur"/>
        <underline method="pen" type="straight" language="LA" text="custodirem literis"/>
        <underline method="pen" type="straight" language="LA" text="prope senescentem"/>
        <underline method="pen" type="straight" language="LA" text="ab obliuione hominu[m] atque &agrave; silentio vindicarem"/>
        <underline method="pen" type="straight" language="LA" text="cognosci"/>
        <underline method="pen" type="straight" language="LA" text="deberi"/>
        <underline method="pen" type="straight" language="LA" text="viua[m] illorum memoria[m] teneremus, hanc immortalem redderem, si possem"/>
        <underline method="pen" type="straight" language="LA" text="spe aggredior maiore ad probandu[m]"/>
        <underline method="pen" type="straight" language="LA" text="nullius memoria iam refellente"/>
        <underline method="pen" type="straight" language="LA" text="edo"/>
        <underline method="pen" type="straight" language="LA" text="iis qui neutrum illorum viderint, eorum, quibus ambo illi oratores cogniti sint, viuorum &amp; praesentium, memoria teste, co[m]mendemus"/>
        <underline method="pen" type="straight" language="LA" text="te charissime atque"/>
        <underline method="pen" type="straight" language="LA" text="quos tu agrestes putas"/>
        <underline method="pen" type="straight" language="LA" text="insequor vt erudia[m]"/>
        <underline method="pen" type="straight" language="LA" text="pudore &agrave; dicendo &amp; timiditate ingenua quadam refugisti"/>
        <underline method="pen" type="straight" language="LA" text="vnum putasti satis esse non modo in vna familia rhetorem, sed pen&egrave; in tota ciuitate"/>
        <underline method="pen" type="straight" language="LA" text="ieiunitatem"/>
        <underline method="pen" type="straight" language="LA" text="acerrimis studiis"/>
        <underline method="pen" type="straight" language="LA" text="prudentiam ration&eacute;mque dicendi per teipsum, vsum autem per nos percipere voluisti"/>
        <underline method="pen" type="straight" language="LA" text="non mediocre munus"/>
        <underline method="pen" type="straight" language="LA" text="porticu"/>
        <underline method="pen" type="straight" language="LA" text="co[m]motus"/>
        <underline method="pen" type="straight" language="LA" text="vt ipsorum vsus ferebat, amicissime consalutassent"/>
        <underline method="pen" type="straight" language="LA" text="tu nos ineptos licet, inquit, vel molestos putes"/>
        <underline method="pen" type="straight" language="LA" text="de Tusculano suo"/>
        <underline method="pen" type="straight" language="LA" text="mihi &agrave; se"/>
