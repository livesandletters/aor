    <page filename="0424.tif" reader="John Dee" pagination="154"/>  
    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="non posset dicere, perfecit medita[n]do, vt nemo planius eo locutus putaretur"/>
        <underline method="pen" type="straight" language="LA" text="spiritus eius esset angustior, tantum continenda anima in dicendo est assecutus, vt vna co[n]tinuatione verborum"/>
        <underline method="pen" type="straight" language="LA" text="binae ei co[n]tentiones vocis, &amp; remissiones continerentur"/>
        <underline method="pen" type="straight" language="LA" text="memoriae proditum"/>
        <underline method="pen" type="straight" language="LA" text="coniectus in os calculis, summa voce versus multos vno spiritu pronuntiare consuescebat: neque is consistens in loco"/>
        <underline method="pen" type="straight" language="LA" text="sed inambula[n]s atque ascensu ingrediens arduo"/>
        <underline method="pen" type="straight" language="LA" text="proprio officio atque munere seiuncta esse arbitror"/>
        <underline method="pen" type="straight" language="LA" text="Operarium nobis quenda[m] Antoni oratorem facis"/>
        <underline method="pen" type="straight" language="LA" text="illa mirifica ad refellendum consuetudine, qua"/>
        <underline method="pen" type="straight" language="LA" text="in vtranque partem solent copiosissime dicere"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; me informari"/>
        <underline method="pen" type="straight" language="LA" text="qui habitaret in subselliis"/>
        <underline method="pen" type="straight" language="LA" text="nullius ornamenti experte[m]"/>
        <underline method="pen" type="straight" language="LA" text="quibusdam finibus"/>
        <underline method="pen" type="straight" language="LA" text="circu[m]dedisti"/>
        <underline method="pen" type="straight" language="LA" text="secundum hunc diem"/>
        <underline method="pen" type="straight" language="LA" text="paulum requiescet"/>
        <underline method="pen" type="straight" language="LA" text="se calor frangat"/>
        <underline method="pen" type="straight" language="LA" text="quoniam id temporis est, valetudini demus operam"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d ius nostrum ciuile peruellit, qu&agrave;m iucundus, qu&ograve;d se id nescire confessus est"/>
        <underline method="pen" type="straight" language="LA" text="non plus attigisse doctrinae, qu&agrave;m quantum prima illa puerili institutione potuisset: M."/>
        <underline method="pen" type="straight" language="LA" text="inanis omnis noster esse labor"/>
        <underline method="pen" type="straight" language="LA" text="refutare domesticis testibus"/>
        <underline method="pen" type="straight" language="LA" text="matertera"/>
        <underline method="pen" type="straight" language="LA" text="dilexit ex omnibus plurimu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vel pueri sentire"/>
        <underline method="pen" type="straight" language="LA" text="ea ponere in percontando"/>
        <underline method="pen" type="straight" language="LA" text="sermonibus dedidisset"/>
        <underline method="pen" type="straight" language="LA" text="ineuntis aetatis meae patiebatur pudor"/>
        
        
    </annotation>
