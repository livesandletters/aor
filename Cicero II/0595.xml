    <page filename="0595.tif" reader="John Dee" pagination="37" signature="Cc.iii."/> 
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="ferreum scriptorem">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ferreus scriptor.</marginalia_text>
                </position>
            </language>
            <translation>An iron writer.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ad arbitrium suum scribere">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ad arbitriu[m] scribere.</marginalia_text>
                </position>
            </language>
            <translation>To write to their judgement.</translation>     
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nostra">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nostra</marginalia_text>
                </position>
            </language>
            <translation>Our.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de bene beat&eacute;que viuendo &agrave; Platone">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plato de bene beateq[ue] viue[n]do</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato on living well and living morally well.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Graeca">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Gra[e]ca</marginalia_text>
                    <location name="Greece"/>
                </position>
            </language>
            <translation>Greece.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Stoicis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Stoici</marginalia_text>
                </position>
            </language>
            <translation>The Stoics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Theophrastus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Theophrastus</marginalia_text>
                    <person name="Theophrastus"/>
                </position>
            </language>
            <translation>Theophrastus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="si plane sic verterem Platone[m] aut Aristotele[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Aristotele[m] aut Platone[m] vertere &egrave; gra[e]co</marginalia_text>
                    <person name="Aristotle"/>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>To turn from the Greeks to Aristotle or Plato.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omnes mea legant">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>omnes mea lega[n]t.</marginalia_text>
                </position>
            </language>
            <translation>They all read my works.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Persius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Persij scripta.</marginalia_text>
                    <person name="Persius"/>
                </position>
            </language>
            <translation>The writings of Persius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de virtute">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Brutus de virtute.</marginalia_text>
                    <person name="Marcus Junius Brutus the Younger"/>
                    <book title="On Virtue"/>
                </position>
            </language>
            <translation>Marcus Junius Brutus the Younger's 'On virtue'.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="venire vt abhorreant &agrave; Latinis, qu&ograve;d inciderint in inculta quaedam &amp; horrida, de malis Graecis Latine scripta deterius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cur Ho[m]i[n]es &agrave; latinis abhorreba[n]t scriptis.</marginalia_text>
                </position>
            </language>
            <translation>Why men hated Latin literature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Graecum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Gra[e]cus.</marginalia_text>
                </position>
            </language>
            <translation>Greek.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="domesticarum rerum fastidiu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Reru[m] domesticaru[m] fastidium.</marginalia_text>
                </position>
            </language>
            <translation>The fastidiousness of domestic matters.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="locupletiore[m] etiam esse qu&agrave;m Graeca[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lingua latina locupletior quam gra[e]ca.</marginalia_text>
                </position>
            </language>
            <translation>The Latin language [is] richer than the Greek.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="scribentur fortasse plura si vita suppetet">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Scribe[n]tur plura si vita suppetet.</marginalia_text>
                </position>
            </language>
            <translation>Many things will be written if life is sufficient.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Qui autem alia malunt scribi &agrave; nobis, aequi esse debent">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>a[e]quis sis.</marginalia_text>   
                </position>
            </language>
            <translation>Fair, if you prefer.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ferreum scriptorem"/>
        <underline method="pen" type="straight" language="LA" text="poetis, aut inertissimae"/>
        <underline method="pen" type="straight" language="LA" text="fastidii delicatissimi"/>
        <underline method="pen" type="straight" language="LA" text="nulli satis eruditi vide[n]tur, quibus nostra ignota sunt"/>
        <underline method="pen" type="straight" language="LA" text="e&iacute;sque nostrum iudicium &amp; nostrum scribendi ordinem adiungimus"/>
        <underline method="pen" type="straight" language="LA" text="quid habent cur Graeca"/>
        <underline method="pen" type="straight" language="LA" text="ad arbitrium suum"/>
        <underline method="pen" type="straight" language="LA" text="si ad eoru[m] cognitione[m] diuina illa ingenia transferrem. Sed"/>
        <underline method="pen" type="straight" language="LA" text="locos quide[m] quosda[m] si videbitur transferam, &amp; maxime ab his quos modo nominaui, quum inciderit vt id apte fieri possit, vt ab Homero"/>
        <underline method="pen" type="straight" language="LA" text="Nec vero vt noster Lucilius, recusabo quo minus omnes"/>
        <underline method="pen" type="straight" language="LA" text="iudicium reformidans"/>
        <underline method="pen" type="straight" language="LA" text="neque tam docti tunc erant ad quorum iudicium elaboraret"/>
        <underline method="pen" type="straight" language="LA" text="quem timeam lectorem, quum"/>
        <underline method="pen" type="straight" language="LA" text="in philosophia audeam"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; teipso id quide[m] facio prouocatus"/>
        <underline method="pen" type="straight" language="LA" text="in inculta quaedam &amp; horrida, de malis Graecis Latine scripta deterius"/>
        <underline method="pen" type="straight" language="LA" text="bonas verbis electis grauiter ornat&eacute;que dictatas quis non legat? nisi"/>
        <underline method="pen" type="straight" language="LA" text="locum cum multa venustate &amp; omni sale, idem"/>
        <underline method="pen" type="straight" language="LA" text="vnde hoc sit tam insolens domesticarum rerum fastidiu[m]"/>
        <underline method="pen" type="straight" language="LA" text="linguam no[n] modo non inope[m], vt vulgo putarent, sed locupletiore[m] etiam esse qu&agrave;m Graeca[m]. Quando"/>
        <underline method="pen" type="straight" language="LA" text="vllus orationis vel copiosae, vel elega[n]tis ornatus defuit"/>
        <underline method="pen" type="straight" language="LA" text="istis ta[n]topere pugnare"/>
        <underline method="pen" type="straight" language="LA" text="aequi"/>
        <underline method="pen" type="straight" language="LA" text="sic vt plura nemini &egrave; nostris"/>
        <underline method="pen" type="straight" language="LA" text="plura si vita suppetet"/>
        
