    <page filename="0406.tif" reader="John Dee" pagination="136" signature="[i.iiii.[v]]"/> 
    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="tenerent, &amp; audirentur ab iis, quibus no[n] scientia esset trade[n]da"/>
        <underline method="pen" type="straight" language="LA" text="sed exigui temporis aut falsa aut certe obscura opinio"/>
        <underline method="pen" type="straight" language="LA" text="artificium vllum"/>
        <underline method="pen" type="straight" language="LA" text="me sibi perfacilem in audiendo, te perpugnace[m] in disputando esse visum"/>
        <underline method="pen" type="straight" language="LA" text="imprude[n]te"/>
        <underline method="pen" type="straight" language="LA" text="excidit"/>
        <underline method="pen" type="straight" language="LA" text="acute atque dilucide apud mediocres homines ex co[m]muni quadam hominu[m] opinione dicere"/>
        <underline method="pen" type="straight" language="LA" text="qui mirabilius &amp; magnificentius augere posset, atque ornare quae vellet, omn&eacute;sque omniu[m] reru[m], quae ad dicendu[m] pertinere[n]t, fontes animo ac memoria co[n]tineret"/>
        <underline method="pen" type="straight" language="LA" text="ingressi simus, obruimur ambitione &amp; foro, sit tamen in re positu[m] atque natura"/>
        <underline method="pen" type="straight" language="LA" text="auguror co[n]iectura"/>
        <underline method="pen" type="straight" language="LA" text="in nostris hominibus"/>
        <underline method="pen" type="straight" language="LA" text="studio acriore"/>
        <underline method="pen" type="straight" language="LA" text="maiore ac maturiore"/>
        <underline method="pen" type="straight" language="LA" text="superiore"/>
        <underline method="pen" type="straight" language="LA" text="dediderit, existat"/>
        <underline method="pen" type="straight" language="LA" text="qualem quaerimus"/>
        <underline method="pen" type="straight" language="LA" text="lectitarit"/>
        <underline method="pen" type="straight" language="LA" text="paulu[m] huic aliquid poterit addere"/>
        <underline method="pen" type="straight" language="LA" text="Inspera[n]ti"/>
        <underline method="pen" type="straight" language="LA" text="sed valde optani vtrique nostru[m] cecidit"/>
        <underline method="pen" type="straight" language="LA" text="in istum sermone[m] Crasse delaberemini"/>
        <underline method="pen" type="straight" language="LA" text="memoria dignu[m] excipere possemus"/>
        <underline method="pen" type="straight" language="LA" text="vel studii, vel artificii, vel facultatis disputationem pen&egrave; intima[m] perueniretis"/>
        <underline method="pen" type="straight" language="LA" text="ab ineunte aetate incensus essem studio vtriusque vestru[m], Crassi vero etia[m] amore"/>
        <underline method="pen" type="straight" language="LA" text="elicere"/>
        <underline method="pen" type="straight" language="LA" text="per meipsum egissem"/>
        <underline method="pen" type="straight" language="LA" text="per Drusum saepe tentassem"/>
        <underline method="pen" type="straight" language="LA" text="defuisti"/>
        <underline method="pen" type="straight" language="LA" text="obseruare"/>
        <underline method="pen" type="straight" language="LA" text="patefecit earum rerum ipsarum aditu[m]"/>
        <underline method="pen" type="straight" language="LA" text="princeps Crassus eius sermonis ordiendi fuit, date nobis hanc venia[m]"/>
        <underline method="pen" type="straight" language="LA" text="subtiliter prosequamini"/>
        <underline method="pen" type="straight" language="LA" text="huic palaestrae"/>
        <underline method="pen" type="straight" language="LA" text="tuu[m] hoc suburbanu[m] gymnasium antepona[m]"/>
        <underline method="pen" type="straight" language="LA" text="id quod requiris"/>
        <underline method="pen" type="straight" language="LA" text="refugisse"/>
        <underline method="pen" type="straight" language="LA" text="instanti"/>
        <underline method="pen" type="straight" language="LA" text="inhumanitate"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve; tuo studio rectissimo atque optimo no[n] obsequi velle[m]"/>
        <underline method="pen" type="straight" language="LA" text="maxime natum apt&uacute;mque cognossem"/>
        <underline method="pen" type="straight" language="LA" text="disputationis insolentia, atque earu[m] rerum, quae quasi traduntur in arte, inscitia"/>
        <underline method="pen" type="straight" language="LA" text="omnino"/>
        <underline method="pen" type="straight" language="LA" text="de reliquo ia[m] nostra culpa fuerit"/>
        <underline method="pen" type="straight" language="LA" text="dimiserimus"/>
        <underline method="pen" type="straight" language="LA" text="vt in co[n]ditionibus scribi solt, de quibus scia[m] poter&oacute;que"/>
        <underline method="pen" type="straight" language="LA" text="tam impudens est"/>
        <underline method="pen" type="straight" language="LA" text="scire aut posse postulet"/>
        <underline method="pen" type="straight" language="LA" text="ista conditione, dum mihi liceat negare posse quod non potero, &amp; fateri nescire quod nescia[m], licet"/>
        <underline method="pen" type="straight" language="LA" text="vestro arbitratu perco[n]temini"/>
        <underline method="pen" type="straight" language="LA" text="exposuit"/>
        <underline method="pen" type="straight" language="LA" text="tanquam alicui Graeculo otioso &amp; loquaci, &amp; fortasse docto atque erudito quaestiunculam, de qua meo arbitratu loquar, ponitis"/>
        
