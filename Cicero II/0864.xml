        <marginalia hand="English_secretary" anchor_text="terra quae colitur &agrave; vobis, angustata verticibus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>terra angustata verticibus</marginalia_text>
                </position>
            </language>
            <translation>Land confined by peaks.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="obriguisse pruina">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>obrigere pruina</marginalia_text>
                </position>
            </language>
            <translation>To numb by rime.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="lunaris">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>lunaris</marginalia_text>
                </position>
            </language>
            <translation>Lunar.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>celebritas sermonis ho[m]i[nu]m gloria</marginalia_text>
                </position>
            </language>
            <translation>The fame of a speech is the glory of people.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="terra nona immobilis manens, ima sede semper haeret, complexa medium">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Terra i[m]mobilis ima sede ha[e]ret media</marginalia_text>
                </position>
            </language>
            <translation>The immobile land stays in its deep seat in the centre.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="numerus rerum omnium fer&egrave; nodus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nodus o[mn]i[u]m rerum</marginalia_text>
                </position>
            </language>
            <translation>The knot of all things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="studia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Studia.</marginalia_text>
                </position>
            </language>
            <translation>Pursuits.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nilus ad illa quae Catadupa">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Catadupa Nili</marginalia_text>
                    <location name="Nile"/>
                </position>
            </language>
            <translation>Cataracts of the Nile.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="caelestia semper spectato, illa humana contemnito">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[e]lestia semp[er] spectato, humana contemnito</marginalia_text>
                </position>
            </language>
            <translation>Heavenly matter shall alwys be watched, human matters avoided!</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in terra raris &amp; angustis in locis, &amp; in ipsis quasi maculis vbi habitatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Terra[e] habitatio</marginalia_text>
                </position>
            </language>
            <translation>The habitation of the world.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="terram, quasi quibusdam redimitam &amp; circundatam cingulis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Terra redimita &amp; circundata cingulis</marginalia_text>
                </position>
            </language>
            <translation>The world is encircled and surrounded by a belt.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="australis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Antipodes</marginalia_text>
                </position>
            </language>
            <translation>Antipodes.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Oceanum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Oceanus</marginalia_text>
                </position>
            </language>
            <translation>Ocean.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="eluuiones exustion&eacute;sque terrarum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Eluuiones exustionesq[ue] terrarum futura[e]</marginalia_text>
                </position>
            </language>
            <translation>Inundations and devastation by fire of the future worlds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="meliores">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Maiores posteris meliores</marginalia_text>
                </position>
            </language>
            <translation>Ancestors are better than their descendants.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="annum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Annus</marginalia_text>
                </position>
            </language>
            <translation>A year.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="verte[n]s annus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Annus vertens</marginalia_text>
                </position>
            </language>
            <translation>A changing year.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[e]lum quibus definitu[m]</marginalia_text>
                </position>
            </language>
            <translation>The sky is defined by what?</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sermonibus vulgi dederis te">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sermonibus vulgi ne dederis te.</marginalia_text>
                </position>
            </language>
            <translation>You shall not surrender yourself to the rumours of the people.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>partes orie[n]tes obeuntes solis</marginalia_text>
                </position>
            </language>
            <translation>The rising and falling parts of the sun.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="populariter">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>populariter</marginalia_text>
                </position>
            </language>
            <translation>In everyday language.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Caucasum huc, quem cernis, transcendere potuit, vel illum Gangem transnare">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Caucasu[m] transcendere gange[m] transnare</marginalia_text>
                    <location name="Caucasus"/>
                    <location name="Ganges"/>
                </position>
            </language>
            <translation>To climb the Caucasus, to swim across the Ganges.</translation>
        </marginalia>
        
