        <marginalia hand="Italian" anchor_text="Veram enim fuisse obnunciationem, vt ait idem Augur &amp; Censor, exitus approbauit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        vera[m] esse obnu[n]ciatione[m] exitus comprobat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The death proves that the omen was true.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nunciant euentura nisi prouideris">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Nuncia[n]t futura nisi p[ro]uideris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The shall announce future events unless you foresee them.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="peccatum haereat non in eo">
