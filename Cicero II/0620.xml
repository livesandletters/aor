        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="debilitari, dolere, frangi, succu[m]bere">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        debilitari.
                        Dolere
                        frangi
                        succumbere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be crippled.
                To grieve.
                To be broken.
                To succumb.
            </translation>
