    <annotation>
        
        <marginalia hand="Italian" anchor_text="vna res optima">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Virtus o[mn]i[u]m qua[e] in a[nim]is ponu[n]t vna res optima.</marginalia_text>
                </position>
            </language>
            <translation>They posit that virtue is the best thing of all in the mind.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="res optima; congruentes cum descriptione naturae; inusitatis">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        res optima
                        congruens a[nim]i
                        invsitata [etc]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The best things.
                United with the mind.
                Unusual, etc.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="tripertita">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        tripertita r[ati]o
                        triplex r[ati]o.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A threefold reason.
                A triple reason.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="partitio; diuiderent">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Partitio
                        diuisio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Partition.
                Division.
            </translation>
        </marginalia>
            
            
            
        <marginalia hand="Italian" anchor_text="Iam virtus, animi bonis &amp; corporis cernitur in quibusdam, quae non tam naturae, qu&agrave;m beatae vitae adiuncta sunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                      In quibus rebus cernit[ur] virtus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In which matters virtue is perceived.
            </translation>
        </marginalia>
            
            
        <marginalia hand="Italian" anchor_text="Hominem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                      HOMO
                    </marginalia_text>
                </position>
            </language>
            <translation>
             Man.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Ac de summo quidem atque naturali bono sic agunt: caetera autem pertinere ad id puta[n]t, aut ad augendum, aut ad tuendum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Ca[e]tera bona p[er]tine[n]t su[m]mu[m] vel Ad auge[n]du[m] Tue[n]du[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The other goods pertain to the greatest good, or to its increase; to its preservation.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="maxima">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Maxima qua[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Which are the greatest.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in vna virtute esse positam beatam vitam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        In vna virtute foelicitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Good fortune in one virtue.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="beatam vitam: nec tamen beatissimam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Vita [ beata
                        beatissima
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Life: blessed; most blessed.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="co[n]seruatione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Conservatio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Conservation.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Hinc gignebatur fuga desidiae, voluptat&uacute;mque contemptio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quid desidia[e] fuga 
                        voluptatu[m] co[n]te[m]ptio ] efficit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What the refusal of idleness, and contempt of pleasure bring.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="anteponebantur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        anteponeba[n]tur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                They were preferred.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="In eo quod efficeret, vim esse censebant: in eo autem quod efficeretur, materiam quandam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Natura efficiens [ in hac est vis
                               effectu[m] [ materia q[uan]da[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nature: passive, the force is in this one; 'passive', a certain matter [Loeb 268, 435].
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in vtroque tamen vtrunque">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Materia est in forma et forma in materia, nec vna p[otes]t esse sine altera.
                    </marginalia_text>
                </position>
            </language>
            <translation>
               Matter is in form and form is in matter, and one cannot be without the other.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="corpus, &amp; quasi qualitatem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Corpus q[ua]si q[ua]litas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The body; quality, so to speak.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="si te Latina forte deficie[n]t">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        verba latina te deficie[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Latin words will let you down.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="enitar vt Latine loquar">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        enitar vt latine loquar
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I will struggle as I speak in Latin.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="co[n]suetudo iam vtitur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]suetudo vtitur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The custom is used.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="non est vulgi verbu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        no[n] est vulgi verbu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The word is not common.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Et id quidem commune omnium fer&egrave;e est artium">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        O[mn]is ars habet sua p[ro]p[r]ia verba.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every art has its own words.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="copia rerum auxeris, vt effecisti, sed etiam verborum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Copia [ reru[m] 
                                verboru[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A supply of things; of words.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="principes sunt vnius modi, &amp; simplices; aer quoque (vtimur enim pro Latino) ignis, &amp; aqua, &amp; terra prima sunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        principes [ vni[us] mo[di] et simplices } Aer ignis Aq[ua] terra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The principal things: of one mode and simple. Air, fire, water, earth. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Earum igitur qualitatu[m] sunt aliae principes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Qualitates ali[a]e su[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>
                There are other qualities.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ex iis autem ortae, variae sunt, &amp; quasi multiformes; Ex iis autem ortae animantium formae, ear&uacute;mque rerum quae gignuntur &egrave; terra">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ex his orta[e] [ varia[e]
                                         multiformes
                                         a[n]i[m]antiu[m]
                                         form[a]e
                                         et earu[m] reru[m]
                                         q[uae] gignu[n]t[ue] &egrave; terra.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                From these [are] origins [which are] various; multiform; living beings and things which are produced by the earth.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="illa initia, &amp; vt &egrave; Graeco vertam, elementa dicuntur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Prima reru[m] Initia, Eleme[n]ta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The primary, initial things, the elements.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ignis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ignis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fire.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="aer &amp; ignis, mouendi vim habent &amp; efficiendi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Aer
                        ignis ] move[n]di vim habe[n]t et efficie[n]di.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Air. Fire. The have the power of moving and doing.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="reliquae partes accipiendi, &amp; quasi patiendi, aquam dico &amp; terram">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Aqua 
                        Terra ] accipie[n]di et q[ua]si patie[n]ti vi[m] obtine[n]t
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Water. Earth. They possess the power of accepting and, so to speak, enduring.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Quintu[m] genus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Quintum Genus. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The fifth kind.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="rebatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        rebatur.       <!-- DO: Possibly crossed out? -->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                He judged.
            </translation>
        </marginalia>
        

        <marginalia hand="Italian" anchor_text="nihil est enim quod no[n] alicubi esse cogatur; Dialectiocoru[m] vero verba nulla sunt publica; Aut enim noua sunt rerum nouarum facie[n]da nomina, aut ex eliis transferenda">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Nihil quod no[n] alicubi esse cogatur
                        Dialecticoru[m] verba nulla su[n]t publica
                        Aut su[n]t reru[m] novaru[m] nova facienda no[m]i[n]a aut ex aliis
                        transfere[n]da.
                    </marginalia_text>
                </position>
            </language>
            <translation>                                       
                'Nothing exists that is not necessarily somewhere' [Loeb 268, 435].
                No words of dialecticians are of the popular language [Loeb 268, 437].
                Either new names are to be made for new things or to be transferred from other things [Loeb 268, 437].
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="inauditis; Graecis; Latinis; non est vulgi; publica; noua">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        verba Graeca.
                        inaudita
                        no[n] vulgi
                        publica
                        nova
                        latina } verbu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Words: Greek, unheard, uncommon, popular, new, Latin.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="faciamus enim tractando vsitatius hoc verbum &amp; tritius">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        vsitatius
                        tritius
                    </marginalia_text>
                </position>
            </language>
            <translation>
                More usual.
                More common.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="omnib&uacute;sque modis mutare, atque ex omni parte">   <!-- DO: I can't find 'penitus' in the text - transcription may be wrong. 'Omnino' is at the start of the next page -->
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        o[mn]ibus modis
                        ex o[mn]i parte
                        penitus
                        o[mn]i[n]o
                    </marginalia_text>
                    <emphasis method="pen" type="straight" text="ex o[mn]i parte"/>
                </position>
            </language>
            <translation>
                In all ways.
                From every part.
                Thoroughly.
                Completely.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="secari ac diuidi">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        secari
                        diuidi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be separated.
                To be divided.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="expressa atque effecta">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        expressa
                        effecta
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Formed.
                Made.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="rebatur; putant">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        reor
                        puto
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I judge.
                I think.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="enitar; conamur">  <!-- DO: Can't find 'moliri' in the text... -->
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        eniti
                        conari
                        moliri
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To strive.
                To try.
                To endeavour.
            </translation>
        </marginalia>
        
