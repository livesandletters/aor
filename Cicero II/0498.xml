        <marginalia hand="English_secretary" anchor_text="Sed est mos hominu[m], vt nolint eu[n]dem pluribus rebus excellere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Mos ho[m]i[nu]m: vt nolint eunde[m] plurib[us] rebus excellere.
                    </marginalia_text>
                </position>
            </language>
            <translation>The custom of men: they do not wish to exalt the same man for many things.</translation>
        </marginalia>
        
        
