            <translation>Zaleucus and Charondas.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="legis putauit esse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Legis natura</marginalia_text>
                </position>
            </language>
            <translation>Nature of law.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dominos esse omnium rerum ac moderatores deos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Deus o[mn]i[u]m reru[m] moderator &amp; Dominus</marginalia_text>
                </position>
            </language>
            <translation>God is the lord and ruler of all things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hominum mereri">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Deus ho[m]i[nu]m me[n]tes videt</marginalia_text>
                </position>
            </language>
            <translation>God sees the minds of the people.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="rationem &amp; mentem putet inesse, in caelo mund&oacute;que non putet">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>In mundo mens &amp; r[ati]o</marginalia_text>
                </position>
            </language>
            <translation>In the world mind and reason.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="naturae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Natura</marginalia_text>
                </position>
            </language>
            <translation>Nature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="foederum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>foedus</marginalia_text>
                </position>
            </language>
            <translation>Contract.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="esse vellem meus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vellem esse meus</marginalia_text>
                </position>
            </language>
            <translation>I have my wish.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="summae rerum sententia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Summa[e] reru[m] sente[n]tia[e]</marginalia_text>
                </position>
            </language>
            <translation>The greatest thoughts of things .</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="DEVS IPSE VINDEX ERIT">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Deus vindex sit.</marginalia_text>
                </position>
            </language>
            <translation>God will be the vindicator.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="SEPARATIM NEMO HABESSIT DEOS">
