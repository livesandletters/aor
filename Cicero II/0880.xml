        <marginalia hand="Italian" anchor_text="LVCOS">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Luci.</marginalia_text>
                </position>
            </language>
            <translation>Groves.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="MERITA">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Merita</marginalia_text>
                </position>
            </language>
            <translation>Merits.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="putauit esse, persuadere aliquid, non omnia vi ac minis cogere"/>
        <underline method="pen" type="straight" language="LA" text="dominos esse omnium rerum ac moderatores deos: e&aacute;que quae gerantur, eorum geri ditione atque numine, 
            eosd&eacute;mque optim&egrave; de genere homininum mereri: &amp; qualis quisque sit quid agat, quid in se admittat, qua mente, qua pietate colat religiones, 
            intueri: pior&uacute;que &amp; impiorum habere rationem. His"/>
        <underline method="pen" type="straight" language="LA" text="neminem esse oportere tam stult&egrave; arrogantem, vt in se rationem &amp; mentem putet inesse, in caelo mund&oacute;que non putet? aut"/>
        <underline method="pen" type="straight" language="LA" text="praestare naturae"/>
        <underline method="pen" type="straight" language="LA" text="firmentur iureiurando, quantae salutis sint foederum religiones, qu&agrave;m multos diuini supplicii metus &agrave; scelere reuocarit, qu&agrave;mque 
            sancta sit societas ciuium inter ipsos"/>
        <underline method="pen" type="straight" language="LA" text="versaris atque ille"/>
        <underline method="pen" type="straight" language="LA" text="nisi plan&egrave; esse vellem"/>
        <underline method="pen" type="straight" language="LA" text="enim negotii est, eadem prop&egrave; verbis iisde, co[n]uersa dicere?"/>
        <underline method="pen" type="straight" language="LA" text="SEPARATIM NEMO HABESSIT DEOS"/>
        
