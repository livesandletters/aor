    <annotation>
        
        
        <marginalia hand="English_secretary" anchor_text="lumen extinxeris; euerteris; deprauare">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        lume[n] extinguere
                        euertere
                        deprauare
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To extinguish a light.
                To overturn.
                To distort.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="verborum gloriam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        verboru[m] gloria.
                    </marginalia_text>
                </position>
            </language>
            <translation>The glory of words.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="sine vlla doctrina naturam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Natura sine doctrina.
                    </marginalia_text>
                </position>
            </language>
            <translation>Nature without learning.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="id solum bonum esse quod honestum sit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Id solum bonu[m] q[uod] honestu[m].  <!-- DO: Dots on the I -->
                    </marginalia_text>
                </position>
            </language>
            <translation>That which is honourable is the sole good.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Pyrrhonem aut Aristonem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pyrrho. Aristo.
                    </marginalia_text>
                    <person name="Pyrrho"/>
                    <person name="Aristo of Chios"/>
                </position>
            </language>
            <translation>Pyrrho. Aristo.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Omnes semper beatos esse sapientes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Omnes semper beati su[n]t sapientes.
                    </marginalia_text>
                </position>
            </language>
            <translation>All the wise are always happy.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="virtutis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        virtutis munus.
                    </marginalia_text>
                </position>
            </language>
            <translation>The duty of virtue.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="erit enim notius quale sit, pluribus notatum vocabulis idem declarantibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Notius quid erit, pluribus verbis idem declara[n]tibus notatum.
                    </marginalia_text>
                </position>
            </language>
            <translation>That will be better known which is known by many words denoting the same thing.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Stoicor&uacute;mque sente[n]tiam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Stoicoru[m] se[n]te[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>The opinion of the Stoics.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nomina nouarum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Noua no[m]i[n]a
                    </marginalia_text>
                </position>
            </language>
            <translation>New names.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="interpretes indiserti">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Interpretes indiserti.
                    </marginalia_text>
                </position>
            </language>
            <translation>An inelegant translator.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ephippiis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        ephippium.
                    </marginalia_text>
                </position>
            </language>
            <translation>A saddlecloth.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="fortuna fortis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        fortuna fortis.
                    </marginalia_text>
                </position>
            </language>
            <translation>Fortune [favours] the brave.</translation>   <!-- DO: Not sure if this is the right way to translate -->
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="animal">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animal
                    </marginalia_text>
                </position>
            </language>
            <translation>A living thing.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="co[n]seruantia sunt eius status">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        co[n]serua[n]tia eius status
                    </marginalia_text>
                </position>
            </language>
            <translation>Things which protect its state.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="rebus nomina imponere"/>
        <underline method="pen" type="straight" language="LA" text="virtutis lumen"/>
        <underline method="pen" type="straight" language="LA" text="euerteris"/>
        <underline method="pen" type="straight" language="LA" text="qui omnia exaequent"/>
        <underline method="pen" type="straight" language="LA" text="sine vlla doctrina naturam ipsam secuti multa laudabilia fecerunt, eos melius &agrave; natura institutos fuisse, qu&agrave;m institui potuissent &agrave; philosophia, si vllam aliam probauissent, praeter eam quae nihil aliud in bonis habet, nisi honestum"/>
        <underline method="pen" type="straight" language="LA" text="ipsam deprauare"/>
        <underline method="pen" type="straight" language="LA" text="ego istam gloriosam memorabil&eacute;mque virtutem non"/>
        <underline method="pen" type="straight" language="LA" text="naturam sint, habere delectum, qui omnia sic exaequauerunt, vt in vtranque"/>
        <underline method="pen" type="straight" language="LA" text="paria redderent"/>
        <underline method="pen" type="straight" language="LA" text="vti nulla selectione vterentur eius, virtute[m] ipsam sustulerunt"/>
        <underline method="pen" type="straight" language="LA" text="appellas, rectum, laudabile, decorum"/>
        <underline method="pen" type="straight" language="LA" text="erit enim notius quale sit, pluribus notatum vocabulis idem declarantibus"/>
        <underline method="pen" type="straight" language="LA" text="indecorum, prauum, flagitiosum, foedum"/>
        <underline method="pen" type="straight" language="LA" text="nominibus insigne faciamus"/>
        <underline method="pen" type="straight" language="LA" text="ex mea breui responsione arripere cupienti"/>
        <underline method="pen" type="straight" language="LA" text="ratio difficilius quidda[m] &amp; obscurius"/>
        <underline method="pen" type="straight" language="LA" text="nouarum nouabantur"/>
        <underline method="pen" type="straight" language="LA" text="nunc consuetudo diuturna triuit"/>
        <underline method="pen" type="straight" language="LA" text="quum rem aliquam inuenisset inusitatam, inauditum quoque ei rei nome[n]"/>
        <underline method="pen" type="straight" language="LA" text="Nec tamen exprimi verbum &egrave; verbo necesse erit, vt interpretes indiserti sole[n]t, quum sit verbum quod idem declarat, magis min&uacute;sve vsitatum"/>
        <underline method="pen" type="straight" language="LA" text="etiam quod vno Graeci, si aliter no[n] possum, idem pluribus verbis exponere"/>
        <underline method="pen" type="straight" language="LA" text="proposita recte &amp; reiecta"/>
        <underline method="pen" type="straight" language="LA" text="in caeteris subuenies, si me haerentem"/>
        <underline method="pen" type="straight" language="LA" text="sed fortuna fortis"/>
        <underline method="pen" type="straight" language="LA" text="ipsum sibi conciliari, &amp; co[m]mendari ad"/>
        <underline method="pen" type="straight" language="LA" text="co[n]seruandum, &amp; ad suum statum, &amp; ad ea quae co[n]seruantia sunt eius status dilige[n]da"/>
        
        
