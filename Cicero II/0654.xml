                    <marginalia_text>Ab interitu natura abhorret.</marginalia_text>
                </position>
            </language>
            <translation>Nature recoils from extinction.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="metum mortis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mortis metus</marginalia_text>
                </position>
            </language>
