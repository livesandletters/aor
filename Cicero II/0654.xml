        <marginalia hand="Italian" anchor_text="sibi quenque esse charum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sibi quisq[ue] Charus.</marginalia_text>
                </position>
            </language>
            <translation>Each person is dear to themself.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Quis est enim aut quotus quisque, cui mors quum appropinquet, Non refugiat timido sanguen, atque exalbescat metu?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mortem o[mn]es timemus.</marginalia_text>
                </position>
            </language>
            <translation>We all fear death.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in vitio, dissolutionem naturae tam valde perhorrescere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>In vitio est morte[m] p[er]horrescere.</marginalia_text>
                </position>
            </language>
            <translation>It is a fault to shudder at death.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ab interitu naturam abhorrere">
