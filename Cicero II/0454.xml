<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0454.tif" reader="John Dee" pagination="184" signature="[m.iiii.[v]]"/>   
    <annotation>
        
        
        <underline method="pen" type="straight" language="LA" text="ex aerariis exemit, lustrum condidit, &amp; tauru[m] immolauit"/>
        <underline method="pen" type="straight" language="LA" text="religione ciuitatem obstrinxisse"/>
        <underline method="pen" type="straight" language="LA" text="ignominia leuarit"/>
        <underline method="pen" type="straight" language="LA" text="aurum repensum"/>
        <underline method="pen" type="straight" language="LA" text="ta[n]ta malorum est multitudo ciuium, vt tibi ego hoc confirmem, si Romae manseris, te paucis annis ad maximas pecunias esse venturum"/>
        <underline method="pen" type="straight" language="LA" text=""/>  <!-- Greek needed -->
        <underline method="pen" type="straight" language="LA" text="hac ironia dissimulanti&aacute;que longe lep&ocirc;re &amp; humanitate omnibus praestitisse"/>
        <underline method="pen" type="straight" language="LA" text="no[n] maiora forensium actionum, qu&agrave;m omniu[m] sermonu[m] condimenta sunt"/>
        <underline method="pen" type="straight" language="LA" text="per mihi scitum videtur"/>
        <underline method="pen" type="straight" language="LA" text="P. Mummium cuiuis tempori hominem esse"/>
        <underline method="pen" type="straight" language="LA" text="honesto verbo vitiosa res appellatur: vt"/>
        <underline method="pen" type="straight" language="LA" text="ab eo notaretur"/>
        <underline method="pen" type="straight" language="LA" text="Non amo, inquit, nimium diligentes"/>
        <underline method="pen" type="straight" language="LA" text="aliud excipias atque ille vult"/>
        <underline method="pen" type="straight" language="LA" text="mult&aacute;que ex ea praelia praeclara fecisset, cu[m]"/>
        <underline method="pen" type="straight" language="LA" text="id oppidum recepisset"/>
        <underline method="pen" type="straight" language="LA" text="Quid ni, inquit, meminerim? nunquam ego recepissem, nisi tu perdidisses"/>
        <underline method="pen" type="straight" language="LA" text="subabsurda"/>
        <underline method="pen" type="straight" language="LA" text="mimis perapposita"/>
        <underline method="pen" type="straight" language="LA" text="quodammodo nobis"/>
        <underline method="pen" type="straight" language="LA" text="rem habere coepit"/>
        <underline method="pen" type="straight" language="LA" text="de ambitu postulatum"/>
        <underline method="pen" type="straight" language="LA" text="tibi tuum negotium agere licebit"/>
        <underline method="pen" type="straight" language="LA" text="tardum"/>
        <underline method="pen" type="straight" language="LA" text="Tu igitur nihil vides? Ego vero inquam &agrave; porta Exquilina video villam tuam"/>
        <underline method="pen" type="straight" language="LA" text="ab ostio quaerenti Ennium"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; ianua quaereret"/>
        <underline method="pen" type="straight" language="LA" text="ego cum te quaererem, ancillae tuae credidi te domi non esse, tu mihi non credis ipsi"/>
        <underline method="pen" type="straight" language="LA" text="male audisset, festiuo homini"/>
        <underline method="pen" type="straight" language="LA" text="cum tua colu &amp; lana"/>
        <underline method="pen" type="straight" language="LA" text="ad famosas vetuit"/>
        <underline method="pen" type="straight" language="LA" text="suspicionem ridiculi absconditam"/>
        <underline method="pen" type="straight" language="LA" text="suspe[n]disse se de ficu"/>
        <underline method="pen" type="straight" language="LA" text="Amabo te, inquit, da mihi ex ista arbore quos"/>
        
        
    </annotation>
</transcription>