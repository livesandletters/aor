    <annotation>
        
        <underline method="pen" type="straight" language="LA" text="copiose eloqui"/>
        <underline method="pen" type="straight" language="LA" text="extare no[n]"/>
        <underline method="pen" type="straight" language="LA" text="sicut fertur, &amp; mihi videtur"/>
        <underline method="pen" type="straight" language="LA" text="materies illa fuit physici, de"/>
        <underline method="pen" type="straight" language="LA" text="ornatus vero ipse verboru[m], oratoris putandus est"/>
        <underline method="pen" type="straight" language="LA" text="remotissimis"/>
        <underline method="pen" type="straight" language="LA" text="suaues"/>
        <underline method="pen" type="straight" language="LA" text="propria est huius vnius rationis"/>
        <underline method="pen" type="straight" language="LA" text="ieiune"/>
        <underline method="pen" type="straight" language="LA" text="exiliter"/>
        <underline method="pen" type="straight" language="LA" text="acutissimum"/>
        <underline method="pen" type="straight" language="LA" text="philosophiae non satisfecisse, qu&ograve;d non"/>
        <underline method="pen" type="straight" language="LA" text="hanc dicendi ex arte alienam facultatem"/>
        <underline method="pen" type="straight" language="LA" text="vbertate[m] in dicendo"/>
        <underline method="pen" type="straight" language="LA" text="copia[m] ab eorum exilitate"/>
        <underline method="pen" type="straight" language="LA" text="varietate"/>
        <underline method="pen" type="straight" language="LA" text="afferunt proprium, compositam orationem &amp; ornatam, &amp; artificio quodam &amp; expolitione distinctam"/>
        <underline method="pen" type="straight" language="LA" text="Haec autem oratio, si res non subest ab oratore percepta &amp; cognita, aut nulla sit necesse est, aut omniu[m] irrisione ludatur"/>
        <underline method="pen" type="straight" language="LA" text="ta[m] furiosum"/>
        <underline method="pen" type="straight" language="LA" text="vel"/>
        <underline method="pen" type="straight" language="LA" text="sonitus inanis, nulla subiecta sententia nec scientia"/>
        <underline method="pen" type="straight" language="LA" text="si tanqua[m]"/>
        <underline method="pen" type="straight" language="LA" text="inuentor"/>
        <underline method="pen" type="straight" language="LA" text="artifex"/>
        <underline method="pen" type="straight" language="LA" text="forensibus cancellis circunscriptam"/>
        <underline method="pen" type="straight" language="LA" text="magis assidue versari hanc nostram dictionem"/>
        <underline method="pen" type="straight" language="LA" text="quae isti magistri, qui rhetorici vocantur"/>
        <underline method="pen" type="straight" language="LA" text="vim existere oratoris"/>
        <underline method="pen" type="straight" language="LA" text="permotionibus"/>
        <underline method="pen" type="straight" language="LA" text="reuocandis"/>
        <underline method="pen" type="straight" language="LA" text="v&iacute;mque omne[m] humanitatis"/>
        <underline method="pen" type="straight" language="LA" text="reflectuntur"/>
        <underline method="pen" type="straight" language="LA" text="quod volet"/>
        <underline method="pen" type="straight" language="LA" text="putatur proprius"/>
        <underline method="pen" type="straight" language="LA" text="me authore"/>
        <underline method="pen" type="straight" language="LA" text="vnqua[m] repugnabit, sed cum illis cognitione[m] rerum co[n]cesserit"/>
        <underline method="pen" type="straight" language="LA" text="tractatione[m] orationis"/>
        <underline method="pen" type="straight" language="LA" text="sibi assumet"/>
        <underline method="pen" type="straight" language="LA" text="oratio grauis &amp; ornata, &amp; hominu[m] sensibus ac mentibus acco[m]modata"/>
        <underline method="pen" type="straight" language="LA" text="vide, ne hoc Scaeuola totum sit &agrave; me"/>
        <underline method="pen" type="straight" language="LA" text="sunt oratoris cum illis co[m]munia"/>
        <underline method="pen" type="straight" language="LA" text="mutuor ab illis"/>
        <underline method="pen" type="straight" language="LA" text="inscribu[n]t"/>
        <underline method="pen" type="straight" language="LA" text="inciderint loci"/>
        <underline method="pen" type="straight" language="LA" text="de co[m]muni ciuiu[m], de hominum, de gentiu[m] iure"/>
        <underline method="pen" type="straight" language="LA" text="clamabu[n]t credo omnia gymnasia, atque omnes philosophoru[m] scholae"/>
        <underline method="pen" type="straight" language="LA" text="pertinere"/>
        <underline method="pen" type="straight" language="LA" text="omnibus in angulis co[n]sumendi otii causa"/>
        <underline method="pen" type="straight" language="LA" text="illi tenui quodam &amp; exiguo sermone"/>
        <underline method="pen" type="straight" language="LA" text="cum omni grauitate &amp; iucunditate explicet"/>
        <underline method="pen" type="straight" language="LA" text="nisi ludos nunc faceret"/>
        <underline method="pen" type="straight" language="LA" text="his studiis mirifice deditus"/>
        <underline method="pen" type="straight" language="LA" text="instituendis"/>
        <underline method="pen" type="straight" language="LA" text="generatim in ordines aetat&eacute;sque descripto"/>
        <underline method="pen" type="straight" language="LA" text="censemus in numero eloquentiu[m] repone[n]dos, scisse"/>
        <underline method="pen" type="straight" language="LA" text="perfectos iam homines in dice[n]do &amp; perpolitos"/>
        <underline method="pen" type="straight" language="LA" text="dice[n]di gloria praestitisse"/>
        
