        <marginalia hand="English_secretary" anchor_text="omne animal patibilem naturam habeat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        omne a[n]i[m]al partibilem natura[m] habet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'Every living thing is by its nature capable of feeling' [LCL 268:312-13].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ne indiuiduum quidem, nec quod dirimi distrah&iacute;ve non possit">
