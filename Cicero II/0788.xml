<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0788.tif" reader="John Dee" pagination="230" signature="[Pp.iii.[v]]"/>   
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="dirimi distrah&iacute;ve ... secari ac diuidi">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        secari diuidi dirimi distrahi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be cut, to be divided, to be pulled apart, to be separated.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        partibilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Divisible.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="omne animal patibilem naturam habeat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        omne a[n]i[m]al partibilem natura[m] habet
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'Every living thing is by its nature capable of feeling' [LCL 268:312-13].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ne indiuiduum quidem, nec quod dirimi distrah&iacute;ve non possit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Indiuiduum, quod nec dirimi nec distrahi p[otes]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every being that cannot be pulled or torn apart.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omne animal mortale est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        omne a[n]i[m]al mortale est.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every animal is mortal.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omne animal &amp; dissolubile">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        o[mn]e animal est dissolubile
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every animal is destructible.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mutabilia autem sunt illa &egrave; quibus omnia constant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Mutabilia su[n]t &egrave; quibus cuncta constant.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'All things are composed are liable to change' [LCL 268;314-15].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omne corpus aut aqua, aut aer, aut ignis, aut terra est, aut id quod est concretu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Omne corpus aut eleme[n]tum aut ex elementis co[n]cretum.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every body consist either of elements or of combination of elements.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Mutua generatio eleme[n]toru[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A reciprocal generation of elements.
            </translation>
        </marginalia>

        <marginalia hand="Italian" anchor_text="Omne enim animal sensus habet">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Omne a[n]i[m]al sensus habet.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                All animals have sensations.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Contraria versantur circa idem:
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Opposite things move about. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec voluptatem sentiat, nec dolore[m], id animal">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Omne a[n]i[m]al voluptatis &amp; doloris sensum capit.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Every animal holds the sensation of desire and pain.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="appetitio sit &amp; declinatio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Appetitio. Declinatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Appetite, divergence.
            </translation>
        </marginalia>

        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        obiectum quid veheme[n]tius, interemit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What kills that which is thrown before you more vehemently.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="simplex est natura animantis ... aut condretum ex pluribus naturis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Anima[n]tis natura no[n] simplex sed ex plurimis naturis co[n]creta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The nature of the animals is not simple, but composed of different natures.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Locus rerum naturalium.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The place for all natural things.
            </translation>
        </marginalia>

        <marginalia hand="Italian" anchor_text="Heraclitum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Heraclitus
                    </marginalia_text>
                    <person name="Heraclitus of Ephesus"/>
                </position>
            </language>
            <translation>
                Heraclitus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        sic aristot[eles] in lib[ro] de lo[n]g[itudine]
                    </marginalia_text>
                    <person name="Aristotle"/>
                    <book title="On Length and Shortness of Life"/> 
                </position>
            </language>
            <translation>
                So Aristotle in his book On Length and Shortness of Life. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="animus, ex quo anima">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animus. anima
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Mind. Intellect.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        ex quib[us] { efficitur cogitur
                    </marginalia_text>
                </position>
            </language>
            <translation>
                From these it is brought about, gathered.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ne indiuiduum quidem, nec quod dirimi distrah&iacute;ve non possit"/>
        <underline method="pen" type="straight" language="LA" text="patibilem"/>
        <underline method="pen" type="straight" language="LA" text="extrinsecus, id quasi ferendi &amp; patiendi necessitatem. Et si omne"/>
        <underline method="pen" type="straight" language="LA" text="secari ac diuidi"/>
        <underline method="pen" type="straight" language="LA" text="mortale igitur omne animal &amp; dissolubile &amp; diuiduum sit necesse"/>
        <underline method="pen" type="straight" language="LA" text="igitur si omnia quae sunt &egrave; quibus cuncta constant, mutabilia sunt, nullum corpus esse potest non mutabile"/>
        <underline method="pen" type="straight" language="LA" text="sit. Etenim omne corpus aut aqua, aut aer, aut ignis, aut terra est, aut id quod est concretu[m] ex his, aut ex aliqua parte eoru[m]: 
            horum aute[m] nihil est quin intereat. Na[m] &amp; terrenum omne diuiditur: &amp; humor ita mollis est, vt facile premi collid&iacute;que possit: ignis vero &amp; aer omni impulsu facillime pellitur"/>
        <underline method="pen" type="straight" language="LA" text="qu&uacute;mque eadem vicissim retro commeant"/>
        <underline method="pen" type="straight" language="LA" text="accipere, &amp; non accipere"/>
        <underline method="pen" type="straight" language="LA" text="appetitio sit &amp; declinatio naturalis"/>
        <underline method="pen" type="straight" language="LA" text="sunt ex quibus effici cog&iacute;que possit"/>
        <underline method="pen" type="straight" language="LA" text="vt cetera, quu[m] amplificata"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve; naturae vi efferatur"/>
        <underline method="pen" type="straight" language="LA" text="Haec ad quodda[m] tempis cohaerere possunt"/>
        <underline method="pen" type="straight" language="LA" text="Necesse est enim in suu[m] quaeque locum natura rapiatur"/>
        <underline method="pen" type="straight" language="LA" text="omnes interpretantur vno modo"/>
        <underline method="pen" type="straight" language="LA" text="omnem vim"/>
        <underline method="pen" type="straight" language="LA" text="defecerit tu[m] interire"/>
        <underline method="pen" type="straight" language="LA" text="in omni natura rerum id viuere, id vigere quod caleat. Ego"/>
        
