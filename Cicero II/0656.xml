    <page filename="0656.tif" reader="John Dee" pagination="98" signature="[Gg.i.[v]]"/>   
    
    <annotation>
        
        
        <marginalia hand="Italian" anchor_text="vites">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vites.</marginalia_text>
                </position>
            </language>
            <translation>Vines.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vinitorem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vinitor.</marginalia_text>
                </position>
            </language>
            <translation>The vine dresser.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="expletione naturae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Expletio natura[e].</marginalia_text>
                </position>
            </language>
            <translation>The fulfilment of nature.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vt ad summum perueniret, quod cumulatur ex integritate corporis, &amp; ex mentis ratione perfecta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Summu[m] cumulatur ex integritate corporis et ex me[n]tis r[ati]o[n]e perfecta.</marginalia_text>
                </position>
            </language>
            <translation>The highest point is reached from integrity of the body and complete reasoning of the mind.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="occulta natura est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Natura occulta est.</marginalia_text>
                </position>
            </language>
            <translation>Nature is hidden.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="prima co[m]mendatio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Prima co[m]mendatio.</marginalia_text>
                </position>
            </language>
            <translation>The earliest committal.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Quam similitudinem videmus in bestiis, quae primo in quo loco natae aunt, ex eo se non commouent: deinde suo quaeque appetitu mouetur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Bestiaru[m] appetitus.</marginalia_text>
                </position>
            </language>
            <translation>The inclination of beasts.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="naturam esse ad viuendum duce[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Natura ad viue[n]dum dux.</marginalia_text>
                </position>
            </language>
            <translation>Nature, a guide for living.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Parui">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Parui.</marginalia_text>
                </position>
            </language>
            <translation>Children.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Est enim natura sic generata vis hominis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vis ho[min]is natura q[uo]m[odo] est generata.</marginalia_text>
                </position>
            </language>
            <translation>In what way the power of man is produced.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="virtutis quasi carmen">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Virtutis Carmen.</marginalia_text>
                </position>
            </language>
            <translation>The song of virtue.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="virtutum quasi scintillulas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Virtutu[m] scintillula[e].</marginalia_text>
                </position>
            </language>
            <translation>Flickers of virtues.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vis naturae per caliginem cernitur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vis natura[e] per caligine[m] cernit[ur]</marginalia_text>
                </position>
            </language>
            <translation>The power of nature is seen through the fog.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nosmetipsos nosse">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nosce teip[su]m.</marginalia_text>
                </position>
            </language>
            <translation>Know yourself.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="habere possit si nulla cultura"/>
        <underline method="pen" type="straight" language="LA" text="ea quae per vinitorem"/>
        <underline method="pen" type="straight" language="LA" text="in e&aacute;que expletione"/>
        <underline method="pen" type="straight" language="LA" text="quod cumulatur ex integritate"/>
        <underline method="pen" type="straight" language="LA" text="occulta natura"/>
        <underline method="pen" type="straight" language="LA" text="nobis incerta &amp; obscura est: prim&uacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="serpere anguiculos"/>
        <underline method="pen" type="straight" language="LA" text="nare anaticulas"/>
        <underline method="pen" type="straight" language="LA" text="euolare merulas"/>
        <underline method="pen" type="straight" language="LA" text="cornibus vti videmus boues, nepas aculeis, suam denique cuique naturam"/>
        <underline method="pen" type="straight" language="LA" text="primo ortu sic iacent, ta[n]quam omnino sine animo"/>
        <underline method="pen" type="straight" language="LA" text="accesserit, &amp; animo vtuntur &amp; sensibus, connitunt&uacute;rque vt sese erigant"/>
        <underline method="pen" type="straight" language="LA" text="&amp; eos agnoscunt &agrave; quibus educantur: deinde aequalibus"/>
        <underline method="pen" type="straight" language="LA" text="congregant"/>
        <underline method="pen" type="straight" language="LA" text="d&aacute;ntque se ad ludendum, fabellar&uacute;mque auditione ducuntur"/>
        <underline method="pen" type="straight" language="LA" text="ipsis superat, aliis gratificari volunt, animaduert&uacute;ntque ea quae domi fiunt curiosius"/>
        <underline method="pen" type="straight" language="LA" text="incipi&uacute;ntque commentari aliquid &amp; discere: &amp; eorum quos vident, volunt non ignorare nomina: quib&uacute;sque rebus cum aequalibus decertant, si vincunt, efferunt"/>
        <underline method="pen" type="straight" language="LA" text="debilitantur, anim&oacute;sque demittunt. Quorum sine causa fieri nihil putandum"/>
        <underline method="pen" type="straight" language="LA" text="e&aacute;mque causam parui virtutum simulacris, quarum in se habent semina"/>
        <underline method="pen" type="straight" language="LA" text="mouentur. Sunt enim prima elementa naturae, quibus auctis, virtutis quasi carmen efficitur"/>
        <underline method="pen" type="straight" language="LA" text="Nam quum ita nati fact&iacute;que simus, vt &amp; agendi aliquid, &amp; diligendi"/>
        <underline method="pen" type="straight" language="LA" text="quasi scintillulas"/>
        <underline method="pen" type="straight" language="LA" text="imbecill&aacute;que mente vis naturae per caliginem cernitur"/>
        <underline method="pen" type="straight" language="LA" text="Intra[n]dum est igitur in rerum naturam, &amp; penitus quid ea postulet, peruidendum"/>
        
        
