    <page filename="0849.tif" reader="John Dee" pagination="291"/> 
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Assensiones.</marginalia_text>
                </position>
            </language>
            <translation>Assents.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="causa appetitus est in nostra">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Causa appetitus nu[nc] in nobis</marginalia_text>
                </position>
            </language>
            <translation>The cause of desire is now within us.</translation> <!-- M.B. Not sure ? -->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="nihil vellet sine praepositis causis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nihil sine p[rae]postis causis</marginalia_text> <!-- M.B. Not certain of word following 'sine'. -->
                </position>
            </language>
            <translation>Nothing without preordained causes.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="causarum genera distinguit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Causaru[m] distinctio.</marginalia_text>
                </position>
            </language>
            <translation>Difference of causes.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="asssensio no[n] possit fieri nisi commota viso">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Assensio fit co[m]mota viso</marginalia_text>
                </position>
            </language>
            <translation>Assent prompted by sense-presentation.</translation> <!-- M.B. Not sure how best to render 'commota' viso' into English, according to the Loeb translation Cicero uses this Latin phrase to render the Greek philosphical term'φαντασία'. -->
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Cylindrum volui, &amp; vesari turbinem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cylindrus. Turbo.</marginalia_text>
                </position>
            </language>
            <translation>Cylinder, spinning. </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="causus fia[n]t antecede[n]tibus &amp; necessariis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Causa[e] antecede[n]tes necessaria</marginalia_text>
                </position>
            </language>
            <translation>The necessity of antecedent causes.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="distinctio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Distinctio.</marginalia_text>
                </position>
            </language>
            <translation>Difference.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="protrusit">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>protrudo</marginalia_text>
                </position>
            </language>
            <translation>Protude.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>signari.</marginalia_text>
                </position>
            </language>
            <translation>A sign.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="praecursione">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>pra[e]cursio.</marginalia_text>
                </position>
            </language>
            <translation>Precursor.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="praegressione">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>pra[e]gressio.</marginalia_text>
                </position>
            </language>
            <translation>Precedent.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="impulsionis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>impulsio</marginalia_text>
                </position>
            </language>
            <translation>Impulse.</translation>
        </marginalia>
        
