    <page filename="0407.tif" reader="John Dee" pagination="137"/>   
    
    <annotation>
        
        
        <underline method="pen" type="straight" language="LA" text="in schola assedissent, ex magna hominu[m] frequentia dicere iuberent, siquis quid quaereret"/>
        <underline method="pen" type="straight" language="LA" text="permagnum quiddam suscipere ac profiteri"/>
        <underline method="pen" type="straight" language="LA" text="denuntiaret"/>
        <underline method="pen" type="straight" language="LA" text="improuisa"/>
        <underline method="pen" type="straight" language="LA" text="audire velle"/>
        <underline method="pen" type="straight" language="LA" text="difficile factu est"/>
        <underline method="pen" type="straight" language="LA" text="hominem"/>
        <underline method="pen" type="straight" language="LA" text="nostr&iacute;que cupidissimum"/>
        <underline method="pen" type="straight" language="LA" text="in illo suo genere omniu[m] princeps"/>
        <underline method="pen" type="straight" language="LA" text="gerendus est tibi mos adolescentibus"/>
        <underline method="pen" type="straight" language="LA" text="quotidianam loquacitatem sine vsu, neque ex scholis cantilenam requiru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="in hoc domicilio imperii &amp;"/>
        <underline method="pen" type="straight" language="LA" text="vestigia qui persequi"/>
        <underline method="pen" type="straight" language="LA" text="eius sententiam sciscita[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="deum"/>
        <underline method="pen" type="straight" language="LA" text="defugere"/>
        <underline method="pen" type="straight" language="LA" text="ad quam te duo excellentis ingenii adolescentes cupiunt accedere"/>
        <underline method="pen" type="straight" language="LA" text="grauabor"/>
        <underline method="pen" type="straight" language="LA" text="mihi dicendi aut nullam artem, aut pertenuem videri"/>
        <underline method="pen" type="straight" language="LA" text="contentionem inter homines doctos in verbi controuersia positam"/>
        <underline method="pen" type="straight" language="LA" text="ab opinionis arbitrio seiunctis"/>
        <underline method="pen" type="straight" language="LA" text="scienti&agrave;que comprehe[n]sis"/>
        <underline method="pen" type="straight" language="LA" text="ad vulgarem popular&eacute;mque sensum acco[m]modata omnia genera huius forensis nostrae dictionis"/>
        <underline method="pen" type="straight" language="LA" text="obseruata sunt"/>
        <underline method="pen" type="straight" language="LA" text="callidis ac peritis animaduersa ac nostra, verbis designata, generibus illustrata"/>
        <underline method="pen" type="straight" language="LA" text="partibus distributa sunt"/>
        <underline method="pen" type="straight" language="LA" text="non, si minus"/>
        <underline method="pen" type="straight" language="LA" text="at"/>
        <underline method="pen" type="straight" language="LA" text="similitudo"/>
        <underline method="pen" type="straight" language="LA" text="maiora"/>
        <underline method="pen" type="straight" language="LA" text="ponerent"/>
        <underline method="pen" type="straight" language="LA" text="gratum te his esse Crasse facturu[m], si"/>
        <underline method="pen" type="straight" language="LA" text="has meas ineptias efferatis"/>
        <underline method="pen" type="straight" language="LA" text="moderabor ipse, ne vt quidam magister atque artifex, sed quasi vnus &egrave; togatoru[m] numero atque &egrave; forensi vsu homo mediocris, neque omnino rudis, videar non ipse aliquid &agrave; me prompsisse, sed fortuito in sermonem vestru[m] incidisse"/>
        <underline method="pen" type="straight" language="LA" text="cum peterem magistratus, solebam in prehe[n]sando dimittere &agrave; me Scaeuolam, cum ei ita dicere[m], me velle esse ineptu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Id erat petere blandius"/>
        <underline method="pen" type="straight" language="LA" text="Hunc autem esse vnum homine[m] ex omnibus, quo praesente ego ineptus esse minime vellem, que[m] quide[m] nunc mearu[m] ineptiaru[m] testem &amp; spectatore[m] fortuna constituit"/>
        <underline method="pen" type="straight" language="LA" text="sit no[n] ineptu[m]"/>
        <underline method="pen" type="straight" language="LA" text="culpa[m]"/>
        <underline method="pen" type="straight" language="LA" text="ego praestabo"/>
        <underline method="pen" type="straight" language="LA" text="vim affere maxima[m]"/>
        <underline method="pen" type="straight" language="LA" text="celeres quidam motus esse debe[n]t, qui ad excogitandum acuti, &amp; ad explicandum ornand&uacute;mque sint vberes, &amp; ad memoria[m] firmi atque diuturni"/>
        <underline method="pen" type="straight" language="LA" text="praeclare enim se res"/>
        
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen"/>
       
        
