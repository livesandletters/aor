    <page filename="0634.tif" reader="John Dee" pagination="76"/>  
    <annotation>
        
        <marginalia hand="Italian" anchor_text="vt liberi &agrave; parentibus ame[n]tur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pare[n]tum amor in lib.</marginalia_text>
                </position>
            </language>
            <translation>The love of parents for their children.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="procreandi &agrave; natura habitam esse ratione[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Procrea[n]di, &agrave; natura est habita ratio</marginalia_text>
                </position>
            </language>
            <translation>The reasoning of procreation was maintained by nature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in bestiis vis naturae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>vis natura[e] in bestijs.</marginalia_text>
                </position>
            </language>
            <translation>The force of nature in beasts.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Vt enim in membris alia sunt tanquam sibi nata, vt oculi, vt aures: aliqua etiam caeterorum membroru[m] vsum adiuuant, 
            vt crura, vt manus: sic immanes quaedam bestiae sibi solum natae sunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Membroru[m] partitio</marginalia_text>
                </position>
            </language>
            <translation>The separation of the parts of the body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pinna; Pinnoteres">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pinna.
                        Pinnoteres.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A sea pen. Pea crabs.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="formicae, apes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Formica[e].
                        Apes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Ants. Bees.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mundum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>.Mundus.</marginalia_text>
                </position>
            </language>
            <translation>The universe.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt co[m]munem vtilitatem nostrae anteponamus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[mmun]is vtilitas n[ost]ra[e] a[n]tepone[n]da</marginalia_text>
                </position>
            </language>
            <translation>The common use is preferred to our own use.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="proditor patriae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Proditor patria[e].</marginalia_text>
                </position>
            </language>
            <translation>A traitor to the homeland.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="doceat chariorem nobis esse patriam, qu&agrave;m nosmetipsos">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Charior nobis Patria, q[uam] nos[met]ip[s]i</marginalia_text>
                </position>
            </language>
            <translation>The homeland is dearer to us than ourselves.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ipsis mortuis terrarum omnium deflagratio consequatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Me mortuo terra misceatur incendio.</marginalia_text>
                </position>
            </language>
            <translation>With my death the land is consumed by an inferno.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in summa solitudine vitam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Vita in solitudine.</marginalia_text>
                </position>
            </language>
            <translation>A life in solitude.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="rationib&uacute;sque prudentiae tradendis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quod scis, alteri trade</marginalia_text>
                </position>
            </language>
            <translation>Impart to another what you know.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt tauris natura datum est, vt pro vitulis contra leones summa vi impet&uacute;que conte[n]dant">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Tauroru[m] pugna cu[m] leonibus</marginalia_text>
                </position>
            </language>
            <translation>A fight between bulls and lions.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de Hercule, &amp; de Libero">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Hercules. Liber.</marginalia_text>
                    <person name="Hercules"/>
                    <person name="Liber"/>
                </position>
            </language>
            <translation>Hercules. Liber.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Iouem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iupiter.</marginalia_text>
                    <person name="Jupiter"/>
                </position>
            </language>
            <translation>Jupiter.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iuris esse vincula">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iuris vincula.</marginalia_text>
                </position>
            </language>
            <translation>The bonds of the law.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Chrysippus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Chrysippus.</marginalia_text>
                    <person name="Chrysippus"/>
                </position>
            </language>
            <translation>Chrysippus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="theatrum, quum co[m]mune">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Theatru[m] comune.</marginalia_text>
                </position>
            </language>
            <translation>A communal theatre.</translation>
        </marginalia>
       
        <marginalia hand="Italian" anchor_text="coniuncti &amp; consociati">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        co[n]iuncti
                        co[n]sociati.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Joined together. Connected.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="ame[n]tur, &agrave; quo initio"/>
        <underline method="pen" type="straight" language="LA" text="procreandi &agrave; natura habitam esse ratione[m]"/>
        <underline method="pen" type="straight" language="LA" text="natura &amp; procreari vellet, &amp; diligi procreatos non curaret"/>
        <underline method="pen" type="straight" language="LA" text="inspici potest, quarum in foetu &amp; in educatione laborem"/>
        <underline method="pen" type="straight" language="LA" text="apparet, &agrave; natura ipsa, vt eos quos genuerimus, amemus, impelli"/>
        <underline method="pen" type="straight" language="LA" text="co[m]munis hominu[m] inter homines naturalis"/>
        <underline method="pen" type="straight" language="LA" text="id ipsum qu&ograve;d"/>
        <underline method="pen" type="straight" language="LA" text="sic immanes quaedam bestiae sibi solum natae sunt"/>
        <underline method="pen" type="straight" language="LA" text="Itaque natura sumus apti ad coetus, concilia, ciuitates. Mundum"/>
        <underline method="pen" type="straight" language="LA" text="numine deorum, e&uacute;mque esse quasi co[m]munem vrbem &amp; ciuitatem hominum"/>
        <underline method="pen" type="straight" language="LA" text="vnumque[m]que nostrum eius mundi esse parte[m], ex quo illud natura consequi"/>
        <underline method="pen" type="straight" language="LA" text="patriae, qu&agrave;m co[m]munis vtilitatis, aut salutis desertor propter"/>
        <underline method="pen" type="straight" language="LA" text="nobis esse patriam, qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="Quoni&aacute;mque illa vox inhumana &amp; scelerata ducitur eorum, 
            qui nega[n]t se recusare quo minus ipsis mortuis terrarum omnium deflagratio consequatur: quod vulgari quodam versu Graeco pronu[n]tiari solet"/>
        <underline method="pen" type="straight" language="LA" text="ad coniunctione[m] congregation&eacute;mque hominu[m]"/>
        <underline method="pen" type="straight" language="LA" text="Impellimur aute[m] natura, vt prodesse velimus qu&agrave;m plurimis, in prim&iacute;sque docendo, 
            rationib&uacute;sque prudentiae tradendis. Itaque non facile est inuenire"/>
        <underline method="pen" type="straight" language="LA" text="ipse non tradat alteri. Ita non solum ad discendum propensi sumus, veru[m]etiam"/>
        <underline method="pen" type="straight" language="LA" text="optimum &amp; maximu[m] dicimus, qu&uacute;mque eundem salutarem, hospitalem, statorem, 
            hoc intelligi volumus, salutem hominu[m] in eius esse tutela. Minime"/>
        <underline method="pen" type="straight" language="LA" text="nos abiecti neglect&iacute;que simus, postulare vt diis immortalibus chari simus, &amp; ab"/>
        <underline method="pen" type="straight" language="LA" text="diligamur"/>
        <underline method="pen" type="straight" language="LA" text="coniuncti &amp;"/>
        <underline method="pen" type="straight" language="LA" text="Chrysippus, caetera nata esse causa hominu[m] &amp; deorum"/>
        <underline method="pen" type="straight" language="LA" text="co[m]munitatis &amp; societatis suae, vt bestiis homines vti ad vtilitate[m] suam possint sine"/>
        <underline method="pen" type="straight" language="LA" text="quoni&aacute;mque ea natura esset hominis, vt cum genere humano quasi ciuile"/>
        <underline method="pen" type="straight" language="LA" text="qui id conseruaret, eum iustum: qui migraret"/>
        <underline method="pen" type="straight" language="LA" text="quo minus suum quidque cuiusque"/>
        <underline method="pen" type="straight" language="LA" text="Quum autem ad tuendos, conserua[n]d&oacute;sque homines homine[m] natum esse videamus"/>
        
