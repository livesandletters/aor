<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0865.tif" reader="John Dee" pagination="307" signature="V.v.ii"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="vigila[n]tius">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>vigilati[us]</marginalia_text>
                </position>
            </language>
            <translation>To be more vigilant.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>benemeritis</marginalia_text>
                </position>
            </language>
            <translation>Well-deserved.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="perennis fuit, &amp; obruitur hominum interitu">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nulla inter ho[m]ines p[er]ennis no[min]is celebritas</marginalia_text>
                </position>
            </language>
            <translation>There is no celebration among men of everlasting reputation.</translation> <!--JG: Very unsure-->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nunc tamen tanto praemio proposito multo vigilantius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pra[e]m[un]ire propositu[m] recte acta[e] vita[e]</marginalia_text> <!--JG: Unsure about 'praemunire'-->
                </position>
            </language>
            <translation>To correctly strenghten a proposition of an act of life.</translation> <!--JG: Unsure-->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Deum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Deus.</marginalia_text>
                </position>
            </language>
            <translation>God.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nundum ex quadam parte mortalem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mundus mortalis. a[ni]m[u]s i[m]mort[alis]</marginalia_text> <!--JG: Unsure-->
                </position>
            </language>
            <translation>Mortal world. Immortal mind.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hic fons, hoc principium est mouendi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animus mouendi fons</marginalia_text>
                </position>
            </language>
            <translation>The mind, the source for moving.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Principium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>principium.</marginalia_text>
                </position>
            </language>
            <translation>Beginning.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="&agrave; principio oriri omnia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>&Agrave; principio oriu[n]tur omnia.</marginalia_text>
                </position>
            </language>
            <translation>Everything arises from a beginning.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Inanimum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Inamimu[m] quid</marginalia_text>
                </position>
            </language>
            <translation>What is inanimate.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="animal">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animal</marginalia_text>
                </position>
            </language>
            <translation>Animal.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="exerce in optimis rebus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Anima[m] exerce in optimus reb[us]</marginalia_text>
                </position>
            </language>
            <translation>To exercise the soul in the best matters.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="contemplans">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Contemplatio</marginalia_text>
                </position>
            </language>
            <translation>Contemplation.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="animi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animoru[m] errores</marginalia_text>
                </position>
            </language>
            <translation>The errors of the minds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ciuis ita necesse est">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ciuis qualis esset.</marginalia_text>
                </position>
            </language>
            <translation>What kind of citizen one may be.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="boni plus, qu&agrave;, multi valent">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Boni plusq[uam] multi valent</marginalia_text>
                </position>
            </language>
            <translation>Quality is worth more than quantity.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="seditio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Seditio</marginalia_text>
                </position>
            </language>
            <translation>Sedition.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="libidines">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Libidines</marginalia_text>
                </position>
            </language>
            <translation>Desires.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Firmiter enim maiores nostris stabilita matrimonia esse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Matrimonia firma.</marginalia_text>
                </position>
            </language>
            <translation>A strong marriage.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="desubito">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>desubito</marginalia_text>
                </position>
            </language>
            <translation>Suddenly.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="obruitur hominum interitu"/>
        <underline method="pen" type="straight" language="LA" text="&amp; obliuione"/>
        <underline method="pen" type="straight" language="LA" text="limes ad caeli aditum"/>
        <underline method="pen" type="straight" language="LA" text="vestigiis ingressus partriis"/>
        <underline method="pen" type="straight" language="LA" text="tamen tanto praemio"/>
        <underline method="pen" type="straight" language="LA" text="non esse te mortale[m], sed corpus hoc. Nec"/>
        <underline method="pen" type="straight" language="LA" text="is es quem forma ista declarat, sed mens cuiusque is est quisque, non ea figura quae digito demonostrari potest. Deum"/>
        <underline method="pen" type="straight" language="LA" text="sic fragile corpus animus sempiternus mouet"/>
        <underline method="pen" type="straight" language="LA" text="quin etiam caeteris quae mouentur, hic fons, hoc principium est mouendi"/>
        <underline method="pen" type="straight" language="LA" text="principium extinctum, nec ipsum ab alio renascetur, nec ex de aliud creabit"/>
        <underline method="pen" type="straight" language="LA" text="necesse est &agrave; principio oriri omnia"/>
        <underline method="pen" type="straight" language="LA" text="vim vllam nasciseatur"/>
        <underline method="pen" type="straight" language="LA" text="est, hanc tu exerce in optimis rebus. Sunt"/>
        <underline method="pen" type="straight" language="LA" text="curae de salute patriae"/>
        <underline method="pen" type="straight" language="LA" text="velocius in hanc sedem &amp; domum suam peruolabit. Idque"/>
        <underline method="pen" type="straight" language="LA" text="eminebit foras, &amp; ea quae extra erunt, contemplans, qu&agrave;m maxime se &agrave; corpore abstrahet"/>
        <underline method="pen" type="straight" language="LA" text="desubito"/>
        
