    <annotation>
        
      <marginalia hand="Italian" anchor_text="suscipere ac polliceri">
          <language ident="LA">
              <position place="head" book_orientation="0">
                  <marginalia_text>
                      suscipere
                      polliceri. ]
                  </marginalia_text>
              </position>
          </language>
          <translation>To undertake; to promise.</translation>
      </marginalia>  
        
        <marginalia hand="Italian" anchor_text="immensum infinit&uacute;mque">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        imme[n]sum
                        infinitum ].
                    </marginalia_text>
                </position>
            </language>
            <translation>Boundless; infinite.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="seposuisse">   
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        seponere
                        disiungere.
                    </marginalia_text>
                </position>
            </language>
            <translation>To set aside. To separate.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="sed cum illa pateant, in prompt&uacute;que sint omnibus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        patet
                        in pro[m]ptu est.
                    </marginalia_text>
                </position>
            </language>
            <translation>It is open; it is at hand.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="aut ornatius explicari, aut planius exprimi possint">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        explicari
                        exprimi } 
                        planius.
                        ornatius.
                    </marginalia_text>
                </position>
            </language>
            <translation>To be deployed and to be expressed more evenly and more elegantly.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="infringi iam ac debilitari videretur">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        infringi
                        debilitare.
                    </marginalia_text>
                </position>
            </language>
            <translation>To be diminished; to weaken.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="quae causa sit eloquentium paucitatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Causa eloque[n]tiu[m] paucitatis
                    </marginalia_text>
                </position>
            </language>
            <translation>The cause of the scarcity of orators.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="omni laude cumulatus orator">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                       Omni laude cumulatus orator.  <!-- DO: Really there are not enough strokes for 'Omni' but I'm assuming this is what it should be --> 
                    </marginalia_text>
                </position>
            </language>
            <translation>An orator abounding in all praise.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="reru[m] cognitione">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Rerum cognitio.
                    </marginalia_text>
                </position>
            </language>
            <translation>Knowledge of matters.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="professi&oacute;que ipsa bene dice[n]di">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Profe&sharp-S;io bene [deleted] dice[n]di.
                    </marginalia_text>
                </position>
            </language>
            <translation>Professing of speaking well.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Philippus, Drus&iacute;que; L. Crassum; Q. Mutius; M. Antonius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Philippus.
                        Drusus.
                        L Crassus.
                        Q Mutius
                        M Antonius.
                    </marginalia_text>
                    <person name="Lucius Marcius Philippus, consul 91 BC"/>
                    <person name="Marcus Livius Drusus"/>
                    <person name="Lucius Licinius Crassus"/>
                    <person name="Quintus Mucius Scaevola Augur"/>
                    <person name="Mark Antony"/>
                </position>
            </language>
            <translation>Lucius Marcius Philippus, consul 91 BC. Marcus Livius Drusus. Lucius Licinius Crassus. Quintus Mucius Scaevola Augur. Marcus Antonius.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="C. Cotta">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        C. Cotta.
                    </marginalia_text>
                    <person name="Gaius Aurelius Cotta"/>
                </position>
            </language>
            <translation>Gaius Aurelius Cotta.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="P. Sulpitius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        P. Sulpitius
                    </marginalia_text>
                    <person name="Publius Sulpicius Rufus"/>
                </position>
            </language>
            <translation>Publius Sulpicius Rufus.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="vsque ad extremum tempus diei collocuti sunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vsq[ue] ad extremu[m] tempus diei.
                    </marginalia_text>
                </position>
            </language>
            <translation>Until the final hours of the day.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Eo autem omni sermone confecto">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Eo sermone co[n]fecto.
                    </marginalia_text>
                </position>
            </language>
            <translation>When that discussion had finished.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Crasso">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Crassus.
                    </marginalia_text>
                    <person name="Lucius Licinius Crassus"/>
                </position>
            </language>
            <translation>Lucius Licinius Crassus.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="in ambulationem ventum esse dicebat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        In ambulatione[m] ventu[m] est
                    </marginalia_text>
                </position>
            </language>
            <translation>There is a coming onto the walkway.</translation>     
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Socrate[m] illum, qui est in Phaedro Platonis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Socrates in pha[e]dro Platonis
                    </marginalia_text>
                    <person name="Socrates"/>
                    <person name="Plato"/>
                    <book title="Phaedrus"/>
                </position>
            </language>
            <translation>Socrates in Plato's Phaedrus.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Cotta">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cotta.
                    </marginalia_text>
                    <person name="Gaius Aurelius Cotta"/>
                </position>
            </language>
            <translation>Gaius Aurelius Cotta.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="ex pristino sermone relaxarentur animi omnium">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        ex p[ri]stino sermone relaxari animos.
                    </marginalia_text>
                </position>
            </language>
            <translation>That their minds were relaxed following the former discourse.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="iucunditas, &amp; tantus in iocando lepos">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Iucunditas 
                        in iocando lepos.
                    </marginalia_text>
                </position>
            </language>
            <translation>Pleasantness. Charm in wit.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="duobus spatiis trib&uacute;sve factis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        duobus spatijs tribusve factis.
                    </marginalia_text>
                </position>
            </language>
            <translation>When two or three turns had been made.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="aquula">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        aquula. parvus rivus, vel rivulus
                    </marginalia_text>
                </position>
            </language>
            <translation>A rivulet. A stream, or a streamlet.</translation>
        </marginalia> 
        
