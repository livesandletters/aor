        <underline method="pen" type="straight" language="LA" text="is diem efficeret, toto"/>
        <underline method="pen" type="straight" language="LA" text="quum aute[m] terras nox opacasset, tum caelum"/>
        <underline method="pen" type="straight" language="LA" text="cernerent astris distinctu[m] &amp; ornatum, lun&aacute;eque"/>
        <underline method="pen" type="straight" language="LA" text="regiones obscurauisse"/>
        <underline method="pen" type="straight" language="LA" text="Sed assiduitate cotidiana, &amp; consuetudinte oculoru[m] assuescunt animi, neque admirantur, 
            neque reuirunt rationes earu[m] rerum quas semper vident, proinde quasi nouitas nos magis qu&agrave;m magnitudo rerum debeat"/>
        <underline method="pen" type="double" language="LA" text="ad exquirendas causas excitare"/>
        <underline method="pen" type="straight" language="LA" text="tam ratos astrorum ordines, t&aacute;mque inter se omnia connexa &amp; apta viderit, neget in his vllam inesse"/>
        <underline method="pen" type="straight" language="LA" text="cum machinatione quada[m] moueri"/>
        <underline method="pen" type="straight" language="LA" text="impetum"/>
        <underline method="pen" type="straight" language="LA" text="vicissitudines anniuersarias"/>
        <underline method="pen" type="straight" language="LA" text="remota subtilitate"/>
        <underline method="pen" type="straight" language="LA" text="mundi, solida &amp; globosa, &amp; vndique ipsa in sese nutibus suis conglobata, vestita floribus, herbis, arboribus, frugibus"/>
        <underline method="pen" type="straight" language="LA" text="multitudo insatiabili varietate herbis, arboribus, frugibus"/>
        <underline method="pen" type="straight" language="LA" text="vel circuru[m]"/>
        <underline method="pen" type="straight" language="LA" text="nec immanitate beluarum efferari, nec stirpium asperitate vastari, quor&uacute;mque"/>
        <underline method="pen" type="straight" language="LA" text="insulae, littor&aacute;que collucent distincta tectis &amp;"/>
        <underline method="pen" type="straight" language="LA" text="genera qu&aacute;mque disparia partim submersarum, partim fluitantium &amp; innantium beluarum, partim ad saxa natiuis testis inhaere[n]tium?"/>
        <underline method="pen" type="straight" language="LA" text="cludit"/>
        <underline method="pen" type="straight" language="LA" text="Isque tum fusus &amp; extenuatus in fertus, tum autem concretus in nubes cogitur, humor&eacute;mque colligens terram auget imbribus, 
            tum effluens huc &amp; illuc, ve[n]tos efficit"/>
        <underline method="pen" type="straight" language="LA" text="spiritus ductus alit"/>
        <underline method="pen" type="straight" language="LA" text="&amp; coercens caeli"/>
        <underline method="pen" type="straight" language="LA" text="extrema ora &amp; determinatio mundi"/>
        <underline method="pen" type="straight" language="LA" text="reuersiones"/>
        <underline method="pen" type="straight" language="LA" text="quadam contrahit terram, tum vicissim laetificat, vt cum caelo hilarata videatur. Luna"/>
        <underline method="pen" type="straight" language="LA" text="congrediens"/>
        <underline method="pen" type="straight" language="LA" text="tum digrediens"/>
        <underline method="pen" type="straight" language="LA" text="dubiecta atque opposita soli"/>
        <underline method="pen" type="straight" language="LA" text="radios eius &amp; lumen obscurat"/>
        
