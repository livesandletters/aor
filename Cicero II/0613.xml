    <page filename="0613.tif" reader="John Dee" pagination="55" signature="Dd.iiii."/>
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="fidelia">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>fidelia</marginalia_text>
                </position>
            </language>
            <translation>Trustworthy.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="natura cupiditatem ingenuit homini veri">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ingenuit natura cupiditate[m] veri</marginalia_text>
                </position>
            </language>
            <translation>Nature has implanted a desire for truth.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="virile">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>virilis</marginalia_text>
                </position>
            </language>
            <translation>Manly.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>No[n] nobis solum sed patria[e] et nostris nati sumus</marginalia_text>
                </position>
            </language>
            <translation>We were born not only for ourselves but for our country and our people.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vera">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>vera</marginalia_text>
                </position>
            </language>
            <translation>True.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ratio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ratio.</marginalia_text>
                </position>
            </language>
            <translation>Reason.</translation>
        </marginalia>

        <marginalia hand="Italian" anchor_text="ordo &amp; moderatio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ordo, Moderatio.</marginalia_text>
                </position>
            </language>
            <translation>Order, moderation.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cuiquam aut dicto proteruo aut facto nocere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cuiq[uam] dicto proteruo aut facto nocere.</marginalia_text>
                </position>
            </language>
            <translation>To harm someone with a rash word or deed.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ad honestatem omnia referantur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ad honestatem o[mn]ia referenda.</marginalia_text>
                </position>
            </language>
            <translation>Everything brought back to honour.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="id solum dicitur honestum quod est populari fama gloriosum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Honestum est, quod est populari fama gloriosum.</marginalia_text>
                </position>
            </language>
            <translation>That which has a glorious reputation in popular opinion is honourable.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="multitudine">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Multitudo</marginalia_text>
                </position>
            </language>
            <translation>A crowd.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="natura victus (cui obsisti non potest)">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Natura[e] obsisti no[n] potest.</marginalia_text>
                </position>
            </language>
            <translation>[The force] of nature cannot be resisted.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="non posse iucunde viui nisi honeste">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Non p[otes]t iucunde viui, nisi honeste</marginalia_text>
                </position>
            </language>
            <translation>It is not possible to live happily, unless you live honourably.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="honestum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Honestum.</marginalia_text>
                </position>
            </language>
            <translation>Honourable.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="laus quae possit &egrave; macello peti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Laus &egrave; macello petita.</marginalia_text>
                </position>
            </language>
            <translation>Praise sought from a rather meagre source.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="verboru[m] amore, quae perraro appellantur ab Epicuro">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Verba qua[e] perraro appella[n]tur ab Epicuro</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>Words which are rarely called upon by Epicurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Oculorum, inquit Plato, est in nobis sensus acerrimus, quibus sapientiam no[n] cernimus. Qu&agrave;m illa ardentis amores excitaret sui, si videretur!">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Platonis dictum.</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>A saying of Plato.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aut vnde est hoc contritum vetustate prouerbium, Quicum in tenebris?">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Prouerb.</marginalia_text>
                </position>
            </language>
            <translation>A proverb.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="co[n]scientia; timore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Conscientia. Timor.</marginalia_text>
                </position>
            </language>
            <translation>Conscience. Fear.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="leuia &amp; perinfirma">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        leuia
                        p[er]infirma
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Weak. Very unstable.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Vt enim co[n]suetudo loquitur; populari fama gloriosum; multitudinis rumore laudetur; &agrave; multitudine laudetur; populari fama; ex insipie[n]tium sermone pendere">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        vt co[n]suetudo loquitur
                        multitudinis rumore laudatur
                        &agrave; multitudine laudatur
                        populari fama gloriosum.
                        populari fama.
                        ex insipie[n]tivm sermone pe[n]dere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                As custom dictates.
                It is praised by the opinion of the crowd.
                It is praised by the crowd.
                Glorious in reputation in popular opinion.
                Well-known in popular opinion.
                To depend upon the talk of the foolish.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="architectari voluptates">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        architectari voluptates.
                    </marginalia_text>
                </position>
            </language>
            <translation>To fashion pleasures.</translation>
        </marginalia>
        
