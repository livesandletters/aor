        <marginalia hand="Italian" anchor_text="genus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Genus quid.</marginalia_text>
                </position>
            </language>
            <translation>What a genus is.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Pars">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pars quid.</marginalia_text>
                </position>
            </language>
            <translation>What a species is.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="homo animalis pars est, Thebani aut Troiani genus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>homo a[n]i[m]alis [gen[us] deleted] pars thebani genus.</marginalia_text>
                </position>
            </language>
            <translation>'man is a species of animal, but a genus of which Thebans or Trojans are species' [Loeb 386, 65].</translation> 
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="genus est omnium nimirum libidinum cupiditas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cupiditas omnium libidinu[m] genus.</marginalia_text>
                </position>
            </language>
            <translation>Desire is the genus of all lusts.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vt nequid posterius praeter conclusionem inferatur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Nihil posterius p[rae]ter co[n]clusio[n]em</marginalia_text>
                </position>
            </language>
            <translation>Nothing following beyond the conclusion.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="permiste cum"/>
        <underline method="pen" type="straight" language="LA" text="Na[m] homo animalis pars est, Thebani aut Troiani genus"/>
        <underline method="pen" type="straight" language="LA" text="est, ne cuius genus posueris, eius secum aliquam, sicuti diuersam &amp; dissimilem partem ponas"/>
        <underline method="pen" type="straight" language="LA" text="ex quibus haec opsa transtulimus, quae conuenire videbantur"/>
        <underline method="pen" type="straight" language="LA" text="vt &amp; prima quaeque pars, vt exposita est in partitione, sic ordine transigatur: &amp; omnibus explicatis, peroratum sit, hoc modo, vt nequid posterius praeter conclusionem inferatur"/>
        <underline method="pen" type="straight" language="LA" text="sic nobis placet &amp; ad singulas partes accedere, &amp; omnibus absolutis, perorare. Nunc de confirmatione"/>
        
        
