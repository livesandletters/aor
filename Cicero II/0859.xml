<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0859.tif" reader="John Dee" pagination="301"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Phoenices">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Phonices. Herod</marginalia_text>
                    <person name="Herod the Great"/>
                    <location name="Phoenicia"/>
                </position>
            </language>
            <translation>Phoenicia, Herod.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Rempublicam fuisse Romanam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Remp[ublicam] Ro[manam] nu[n]qu[am] fuisse.</marginalia_text>
                </position>
            </language>
            <translation>The Roman Republic never has been.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Populu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Populus</marginalia_text>
                </position>
            </language>
            <translation>The people.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quod autem fit iniuste, nec iure fieri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quod iure, id iuste fit</marginalia_text>
                </position>
            </language>
            <translation>What is done by law, that is done justly.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ius, quod">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ius quid.</marginalia_text>
                </position>
            </language>
            <translation>What law?</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vbi iustitia non est, non esse Rempublicam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vbi iustitia no[n] est, no[n] est res publica</marginalia_text>
                </position>
            </language>
            <translation>There were is no justice, is not state.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iniustum esse, vt homines hominibus dominantibus seruiant ">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>inustu[m] est ho[mi]nes hominib[us] domina[n]tibus seruire.</marginalia_text>
                </position>
            </language>
            <translation>It is unjust that men are slaves to authoritarian men.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="veluti a natura sumptum nobile exemplum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Exemplu[m] firmtatis</marginalia_text>
                </position>
            </language>
            <translation>An example of firmness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="daemonibus: tanto magis impiis, quanto magis sibi cum sint immundissimi spiritus tanquam diis sacrificari 
            volunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Da[e]mones sibi sacrificari postulant.</marginalia_text>
                </position>
            </language>
            <translation>Demons demand to be sacrificed to themselves.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="definit esse rem populi"/>
        <underline method="pen" type="straight" language="LA" text="enim iure fit, profecto"/>
        <underline method="pen" type="straight" language="LA" text="ius, quod ei plus potest, vtile est"/>
        <underline method="pen" type="straight" language="LA" text="Iustitia porr&ograve; ea virtus est, quae sua cuique distribuit. Quae"/>
        <underline method="pen" type="straight" language="LA" text="vt homines hominibus dominantibus seruiant"/>
        
    </annotation>
