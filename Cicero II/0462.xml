        <underline method="pen" type="straight" language="LA" text="euolutum illis integumentis dissimulationis tuae, nudat&uacute;mque perspicio"/>
        <underline method="pen" type="straight" language="LA" text="percommode facis"/>
        <underline method="pen" type="straight" language="LA" text="vere agere volueris"/>
        <underline method="pen" type="straight" language="LA" text="vt obliuionis arte[m] qu&agrave;m memoriae malim: grati&aacute;mque habeo"/>
        <underline method="pen" type="straight" language="LA" text="fortunatum"/>
        <underline method="pen" type="straight" language="LA" text="nobilem"/>
        <underline method="pen" type="straight" language="LA" text="nimis illum sordide"/>
        <underline method="pen" type="straight" language="LA" text="se dimidium eius ei, quod pactus esset pro illo carmine, daturu[m]"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; suis Tyndaridis"/>
        <underline method="pen" type="straight" language="LA" text="qui eum magnopere euocarent: surrexisse"/>
        <underline method="pen" type="straight" language="LA" text="Hoc interim spatio conclaue illud, vbi epularetur Scopas, concidisse, ea ruina ipsum oppressum cum suis interisse: quos cum humare velle[n]t sui, neque possent obtritos internoscere vllo modo, Simonides dicitur ex eo qu&ograve;d meminisset, quo eorum loco quisque cubuisset, demo[n]strator vniuscuiusque sepeliendi fuisse"/>
        <underline method="pen" type="straight" language="LA" text="ordine[m] esse maxime, qui memoriae lumen afferret"/>
        <underline method="pen" type="straight" language="LA" text="effingenda animo"/>
        <underline method="pen" type="straight" language="LA" text="sic fore, vt ordinem rerum locorum ordo conseruaret"/>
        <underline method="pen" type="straight" language="LA" text="atque vt locis pro cera, simulachris pro literis vteremur"/>
        <underline method="pen" type="straight" language="LA" text="omnes fixas"/>
        <underline method="pen" type="straight" language="LA" text="omnem descriptum verborum apparatum"/>
        <underline method="pen" type="straight" language="LA" text="vel eum vnde dicas"/>
        <underline method="pen" type="straight" language="LA" text="vt illi non infundere in aures tuas orationem, sed in animo videantur inscribere"/>
        <underline method="pen" type="straight" language="LA" text="qui memoria vigent"/>
        <underline method="pen" type="straight" language="LA" text="confiteor equidem"/>
        <underline method="pen" type="straight" language="LA" text="principem"/>
        <underline method="pen" type="straight" language="LA" text="artis imago quaedam est &amp; similitudo"/>
        <underline method="pen" type="straight" language="LA" text="no[n] vt totum aliquid, cuius in ingeniis nostris pars nulla sit, pariat &amp; procreet: veru[m] vt ea quae sunt orta iam in nobis &amp; procreata"/>
        
        
