        <underline method="pen" type="straight" language="LA" text="illam sapientam constituendae ciuitatis duobus prop&egrave; seculis ant&egrave; cognouit"/>
        <underline method="pen" type="straight" language="LA" text="tulit"/>
        <underline method="pen" type="straight" language="LA" text="gloria clariores"/>
        <underline method="pen" type="straight" language="LA" text="authoritate grauiores"/>
        <underline method="pen" type="straight" language="LA" text="humanitate politiores"/>
        <underline method="pen" type="straight" language="LA" text="prop&egrave; bellum indixeris"/>
        <underline method="pen" type="straight" language="LA" text="ac sic decreui philosophari potius, vt Neoptolemus apud Ennium, paucis"/>
        <underline method="pen" type="straight" language="LA" text="opinionem istorum studiorum, &amp; suspicionem artificii apud eos qui res iudicent, oratori aduersariam esse arbitror"/>
        <underline method="pen" type="straight" language="LA" text="vt e&ograve; reuocetur vnde huc declinauit oratio"/>
        <underline method="pen" type="straight" language="LA" text=""/>            <!-- Greek needed -->
        <underline method="pen" type="straight" language="LA" text="simpliciter"/>
        <underline method="pen" type="straight" language="LA" text="coniuncte sit elatum"/>
        <underline method="pen" type="straight" language="LA" text="se compu[n]gunt suis acuminibus"/>
        <underline method="pen" type="straight" language="LA" text="quibus ante exorsa"/>
        <underline method="pen" type="straight" language="LA" text="potius detexta prop&egrave; retexantur"/>
        <underline method="pen" type="straight" language="LA" text="non liquidu[m], non fusum ac profluens"/>
        <underline method="pen" type="straight" language="LA" text="exile"/>
        <underline method="pen" type="straight" language="LA" text="aridum, concisum"/>
        <underline method="pen" type="straight" language="LA" text="minutum"/>
        <underline method="pen" type="straight" language="LA" text="ad oblectandos animos, ad impellendos"/>
        <underline method="pen" type="straight" language="LA" text="quae non aurificis statera, sed quada[m] populari trutina examina[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="dimittamus"/>
        <underline method="pen" type="straight" language="LA" text="muta nimiu[m] est, in"/>
        <underline method="pen" type="straight" language="LA" text="nimium loquax"/>
        <underline method="pen" type="straight" language="LA" text="co[m]memoras"/>
        <underline method="pen" type="straight" language="LA" text="Erat enim ab isto Aristotele"/>
        <underline method="pen" type="straight" language="LA" text="omnium superioru[m]"/>
        <underline method="pen" type="straight" language="LA" text="hos germanos huius artis magistros"/>
        <underline method="pen" type="straight" language="LA" text="eadem acie me[n]tis"/>
        <underline method="pen" type="straight" language="LA" text="haec quoque aspexit"/>
        <underline method="pen" type="straight" language="LA" text="perqu&agrave;m esset opta[n]da nobis, qui nullam vnquam in illis suis disputationibus rem defendit, quam non probarit: nullam oppugnauit, quam non euerterit"/>
        <underline method="pen" type="straight" language="LA" text="hoc maius est quiddam, qu&agrave;m ab iis, qui haec tradunt &amp; docent, postulandum sit. Ego"/>
        <underline method="pen" type="straight" language="LA" text="rudem"/>
        <underline method="pen" type="straight" language="LA" text="vno opere eandem incudem diem noct&eacute;mque tunde[n]tibus, qui omnes tenuissimas particulas, atque omnia minima mansa (vt aiunt nutrices infantibus pueris) in os inserant"/>
        <underline method="pen" type="straight" language="LA" text="illuc eum rapia[m], vbi non seclusa aliqua aquula teneatur, sed vnde vniuersum flumen erumpat, &amp; illi sedes, &amp; tanquam domicilia omnium argumentoru[m] commonstret"/>
        <underline method="pen" type="straight" language="LA" text="in quo haereat, qui"/>
        
