    <page filename="0621.tif" reader="John Dee" pagination="63"/> 
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Leonidas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Leonidas</marginalia_text>
                    <person name="Leonidas I"/>
                </position>
            </language>
            <translation>Leonidas I.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Praeclarae mortes sunt imperatoriae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pra[e]clara[e] mortes su[n]t imp[er]atoria[e]</marginalia_text>
                </position>
            </language>
            <translation>The deaths of commanders are famous.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Audio equide[m] philosophi voce[m] Epicure">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Epicuri dictum.</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>A saying of Epicurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Metrodori liberos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Metrodori liberi.</marginalia_text>
                    <person name="Metrodorus of Lampsacus the Younger"/>
                </position>
            </language>
            <translation>The children of Metrodorus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="summa">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Summa.</marginalia_text>
                </position>
            </language>
            <translation>The chief points.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Mortem nihil ad nos pertinere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mors nihil ad nos p[er]tinet.</marginalia_text>
                </position>
            </language>
            <translation>Death does not affect us.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sensus omnis extinguatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sensus omnis extinguitur morte</marginalia_text>
                </position>
            </language>
            <translation>Every sense is extinguished in death.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Amynomachus &amp; Timocrates">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Amynomachus
                        Timocrates.
                    </marginalia_text>
                    <person name="Amynomachus"/>
                    <person name="Timocrates"/>
                </position>
            </language>
            <translation>
                Amynomachus. Timocrates.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ad diem agendum natalem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Natalis dies.</marginalia_text>
                </position>
            </language>
            <translation>The date of birth.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Annus</marginalia_text>
                </position>
            </language>
            <translation>A year.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Democritus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Democritus.</marginalia_text>
                    <person name="Democritus"/>
                </position>
            </language>
            <translation>Democritus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="post morte[m] epulis celebrari memoriam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Epulis post morte[m] celebrari Memoriam.</marginalia_text>
                </position>
            </language>
            <translation>The memory to be celebrated with a feast after death.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="dissolutione">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>dissolutio</marginalia_text>
                </position>
            </language>
            <translation>Dissolution.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in quanta[m] hominum facetosam vrbanitate[m] incurratis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>in hominu[m] facetosa[m] vrbanitate[m] incurrere.</marginalia_text>
                </position>
            </language>
            <translation>To incur the wit of humerous men.</translation>   
            <!-- DO: Not sure how to translate 'facetosam' --> 
            <!--JG: Acc to Loeb it's facetorum, translated it as such. See: https://www.loebclassics.com/view/marcus_tullius_cicero-de_finibus_bonorum_et_malorum/1914/pb_LCL040.195.xml-->
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="eam qua erat transfixus hastam"/>
        <underline method="pen" type="straight" language="LA" text="Praeclare mortes sunt imperatoriae"/>
        <underline method="pen" type="straight" language="LA" text="tuas te video co[m]pensare"/>
        <underline method="pen" type="straight" language="LA" text="summorum officioru[m] in extremo spiritu conseruatio"/>
        <underline method="pen" type="straight" language="LA" text="mercedibus euocatam"/>
        <underline method="pen" type="straight" language="LA" text="cum summa"/>
        <underline method="pen" type="straight" language="LA" text="enim dissolutum sit, id esse sine sensu"/>
        <underline method="pen" type="straight" language="LA" text="quum dissolutione"/>
        <underline method="pen" type="straight" language="LA" text="quanuis &amp; belli &amp; humani"/>
        <underline method="pen" type="straight" language="LA" text="nisi quum multa annoru[m] intercesserint milia, vt omnium syderu[m] eodem vnde profecta sint, fiat ad vnum tempus reuersio. Nullus"/>
        <underline method="pen" type="straight" language="LA" text="morte[m] epulis celebrari memoriam"/>
        <underline method="pen" type="straight" language="LA" text="facetosam vrbanitate[m] incurratis"/>
        <underline method="pen" type="straight" language="LA" text="nihil opus est litibus"/>
        
