        <marginalia hand="Italian" anchor_text="incoeptio">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>incoeptio.</marginalia_text>
                </position>
            </language>
            <translation>A beginning.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="syllabatim">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>syllabatim</marginalia_text>
                </position>
            </language>
            <translation>Syllabically.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="crassis occultata &amp; circunfusa tenebris">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        crassis occultata et circumfusa tenebris
                    </marginalia_text>
                </position>
            </language>
            <translation>Hidden in fogs and enclosed in darkness.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Aristoteles">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Aristoteles.</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Aristotle.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Neque enim ortum esse vnquam mundu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mundus infinitus.</marginalia_text>
                </position>
            </language>
            <translation>An infinite world.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Deus omnia nostri causa quum faceret">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Deus o[mn]ia n[ost]ra causa fecit</marginalia_text>
                </position>
            </language>
            <translation>The deity made everything for our sake.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="maiestate[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dei maiestas.</marginalia_text>
                </position>
            </language>
            <translation>The majesty of the deity.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Myrmecides">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Myrmecides</marginalia_text>
                    <person name="Myrmecides"/>
                </position>
            </language>
            <translation>Myrmecides.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Negas sine deo posse quicquam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sine deo nihil</marginalia_text>
                </position>
            </language>
            <translation>Nothing without a deity.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Lampsacenus Strato">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lampsacenus strato.</marginalia_text>
                    <person name="Strato of Lampsacus"/>
                </position>
            </language>
            <translation>Strato of Lampsacus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="somnia censet haec esse Democriti">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Democriti Somnia</marginalia_text>
                    <person name="Democritus"/>
                </position>
            </language>
            <translation>The dreams of Democritus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="quicquid aut sit, aut fiat, naturalibus fieri aut factum esse docet ponderibus &amp; motibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>omnia naturalibus facta ponderibus et motibus</marginalia_text>
                </position>
            </language>
            <translation>Everything made by natural weights and motions.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="sic ille &amp; deum opere magno liberat, &amp; me timore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dei timor. opera</marginalia_text>
                </position>
            </language>
            <translation>Fear of the deity. Works.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="situs partium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>partiu[m] situs</marginalia_text>
                </position>
            </language>
            <translation>The positions of the parts.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Corpora nostra non nouimus, qui sint situs partium, quam vim quaeque pars habeat ignoramus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Anatomia[e].</marginalia_text>
                </position>
            </language>
            <translation>Anatomies.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Empiricis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Empirici.</marginalia_text>
                </position>
            </language>
            <translation>The Empirics.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vt videamus terra penit&uacute;sne defixa sit, &amp; quasi radicibus suis haereat, an media pendeat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Terra an media pe[n]deat</marginalia_text>
                </position>
            </language>
            <translation>Whether the Earth hangs in the middle.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Habitari ait Xenophanes in luna, e&aacute;mque esse terram multaru[m] vrbium &amp; mo[n]tium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Xenophanes Habitari in &Moon; ait, eamq[ue] e&sharp-S;e terra[m] multaru[m] vrbiu[m] et mo[n]tiu[m].</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>Xenophanes says that the moon is inhabited and that it is a land of many cities and mountains.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Antipodas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antipodes.</marginalia_text>
                </position>
            </language>
            <translation>Antipodes.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Nicetas Syracusius, vt ait Theophrastus, caelu[m], solem, lunam, stellas, supera denique omnia stare censet, neque praeter terram rem vllam in mundo moueri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Nicetas syracusius terra[m] stare arbitratur.</marginalia_text>
                    <person name="Hicetas"/>
                </position>
            </language>
            <translation>Hicetas the Syracusian judges that the Earth stays still.</translation>  <!-- DO: Not sure; I think the text is saying the opposite... Maybe 'judges the idea that the Earth stays still'? -->
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="hoc etiam Platonem in Timaeo dicere quidam arbitra[n]tur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plato terra[m] stare ait</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>Plato says that the Earth stays still.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Socrates, liber Aristo Chius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Socrates. Aristo.</marginalia_text>
                    <person name="Socrates"/>
                    <person name="Aristo of Chios"/>
                </position>
            </language>
            <translation>Socrates. Aristo.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="animus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animus</marginalia_text>
                </position>
            </language>
            <translation>The mind.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Dicaearcho">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Dicearchius.</marginalia_text>
                    <person name="Dicaearchus"/>
                </position>
            </language>
            <translation>Dicaearchus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="animus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>De a[n]i[m]o varia[e] qu[a]estio[n]es.</marginalia_text>
                </position>
            </language>
            <translation>Various questions about the mind.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="eluditis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>eludere alique[m].</marginalia_text>
                </position>
            </language>
            <translation>To make fun of someone.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>hinc Copernicus sua[e] [?] [.ccapone[m]] [arripuit].</marginalia_text>  <!-- DO: Not sure; this is discussed here though: https://en.wikipedia.org/wiki/Hicetas -->
                   <person name="Nicolaus Copernicus"/>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="sapiens syllabatim"/>
        <underline method="pen" type="straight" language="LA" text="veniet illud flumen orationis aureum fundens Aristoteles, qui illum"/>
        <underline method="pen" type="straight" language="LA" text="incoeptio"/>
        <underline method="pen" type="straight" language="LA" text="dilapsus &oacute;ccidat"/>
        <underline method="pen" type="straight" language="LA" text="sicut caput &amp; fama[m] tuam"/>
        <underline method="pen" type="straight" language="LA" text="omnia nostri causa quum faceret (sic enim vultis) tantam vim natricu[m], viperar&uacute;mque fecerit, cur mortifera ta[m] multa perniciosa terra mar&iacute;que disperserit? Negatis"/>
        <underline method="pen" type="straight" language="LA" text="cuius quide[m] vos maiestate[m] deducitis vsque ad apium formicar&uacute;mque perfectione[m]: vt etiam inter deos Myrmecides aliquis minutoru[m] opusculorum fabricator fuisse videatur. Negas"/>
        <underline method="pen" type="straight" language="LA" text="Sed quum sacerdotes deoru[m], vacatione[m] habeant: quanto est aequius"/>
        <underline method="pen" type="straight" language="LA" text="Negat opera deorum se vti ad fabricandum mundu[m]"/>
        <underline method="pen" type="straight" language="LA" text="docet omnia effecta esse natura, nec vt ille qui asperis &amp; leuibus, &amp; hamatis vncinat&iacute;sque corporibus concreta haec esse dicat interiecto inani, somnia censet haec esse Democriti non docentis, sed optantis. Ipse"/>
        <underline method="pen" type="straight" language="LA" text="naturalibus fieri aut factum esse docet ponderibus &amp; motibus"/>
        <underline method="pen" type="straight" language="LA" text="Quis enim potest (quum existimet curari se &agrave; deo) no[n] &amp; dies &amp; noctes diuinu[m] nume[m] horrere? &amp; siquid aduersi acciderit (quod cui no[n] accidit) extimescere, ne id iure euenerit? Nec"/>
        <underline method="pen" type="straight" language="LA" text="omnia Luculle crassis occultata &amp; circunfusa tenebris"/>
        <underline method="pen" type="straight" language="LA" text="vt nulla acies humani ingenii tanta sit quae penetrare"/>
        <underline method="pen" type="straight" language="LA" text="Corpora nostra non nouimus, qui sint situs partium, quam vim quaeque pars habeat ignoramus"/>
        <underline method="pen" type="straight" language="LA" text="vt patefacta &amp; detecta mutentur"/>
        <underline method="pen" type="straight" language="LA" text="persecare, aperire, diuidere possumus"/>
        <underline method="pen" type="straight" language="LA" text="videamus terra penit&uacute;sne defixa sit"/>
        <underline method="pen" type="straight" language="LA" text="&amp; quasi radicibus suis haereat, an media pendeat"/>
        <underline method="pen" type="straight" language="LA" text="ait Xenophanes in luna, e&aacute;mque esse terram multaru[m] vrbium &amp; mo[n]tium. Portenta vide[n]tur: sed tame[n] nec ille qui dixit iurare posset ita se rem"/>
        <underline method="pen" type="straight" language="LA" text="quos Antipodas vocatis"/>
        <underline method="pen" type="straight" language="LA" text="quae quum circum axem se summa celeritate conuertat &amp; torqueat, eadem effici omnia, quasi stante terra caelum moueretur. Atque"/>
        <underline method="pen" type="straight" language="LA" text="sed paulo obscurius"/>
        <underline method="pen" type="straight" language="LA" text="vicissim eluditis. Liber igitur &agrave; tali irrisione Socrates, liber Aristo Chius, qui nihil istoru[m]"/>
        <underline method="pen" type="straight" language="LA" text="placuit, rationis, irae, cupiditatis"/>
        <underline method="pen" type="straight" language="LA" text="ita sunt in plerisque contrariaru[m] rationu[m] paria momenta"/>
        
        
