        <marginalia hand="English_secretary" anchor_text="ab iis degatur aetas">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        qua[e] ab ijs degatur a[e]tas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'How they pass their days' [LCL 268:50-51].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        tra[n]silio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I leap.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        imme[n]sa interminata. imme[n]sitas. ] regionu[m] magnitudo in o[mn]es partes.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Immeasurable, endless abundance; the greatness of regions in all parts.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        altitudo p[ro]funditas.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Height, depth.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Velleius
                    </marginalia_text>
                    <person name="Gaius Velleius Paterculus"/>
