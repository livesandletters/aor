    <page filename="0631.tif" reader="John Dee" pagination="73"/>   
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="frequentia; multitudo">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        bonor[um] [ multitudo
                                    freque[n]tia
                                    copia
                                    abunda[n]tia
                                    affluentia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A multitude; a crowd; a supply; an abundance; a plethora of goods.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="vt si quum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>vt si quum.</marginalia_text>
                </position>
            </language>
            <translation>As when.</translation>   
            <!-- DO: Not sure how to translate --> 
            <!--JG: According to Loeb it's 'vt si cum': https://www.loebclassics.com/view/marcus_tullius_cicero-de_finibus_bonorum_et_malorum/1914/pb_LCL040.263.xml -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="rerum omnino discrepantiam, qu&agrave;m verboru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Reru[m] discrepa[n]tia et verboru[m]</marginalia_text> 
                </position>
            </language>
            <translation>A discrepancy of facts and words.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="omnia quae ipsi bona appellant, pertinere dicant ad beate viuendum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Omnia bona co[n]curru[n]t ad beatitatis absolutionem.</marginalia_text>
                </position>
            </language>
            <translation>All good things contribute to absolute happiness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="eculeo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Eculeus.</marginalia_text>
                </position>
            </language>
            <translation>The rack.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="opinio facit non natura">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Opinio no[n] natura.</marginalia_text>
                </position>
            </language>
            <translation>Not a natural opinion.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tria genera bonoru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Tria genera bonoru[m].</marginalia_text>
                </position>
            </language>
            <translation>Three kinds of goods.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="corporis co[m]modorum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Corporis commoda.</marginalia_text>
                </position>
            </language>
            <translation>Bodily advantages.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="valetudinem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>valetudo.</marginalia_text>
                </position>
            </language>
            <translation>Health.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>virtus dignitas, splendor.</marginalia_text>
                </position>
            </language>
            <translation>Virtue, esteem, splendour.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>εὐκαρία</marginalia_text>        <!-- for εὐκαιρία -->
                </position>
            </language>
            <translation>Opportunity.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>κατορθωσίς</marginalia_text>        <!-- for κατόρθωσις -->
                </position>
            </language>
            <translation>Right action.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>κατόρθωμα </marginalia_text>        
                </position>
            </language>
            <translation>Virtuous action.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cothurni laus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cothurni Laus.</marginalia_text>
                </position>
            </language>
            <translation>Praise of a boot.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="bonam morte[m] &amp; bonum partum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mors. Partus.</marginalia_text>
                </position>
            </language>
            <translation>Death. Birth.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quod appellamus extremu[m], quod vltimum, pute[n]t crescere posse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Summu[m] Bonu[m] crescet.</marginalia_text>
                </position>
            </language>
            <translation>The greatest good will extend.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="crescere bonorum finem non putamus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Bonoru[m] finis no[n] crescit.</marginalia_text>
                </position>
            </language>
            <translation>The extreme of goods will not extend.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Vt enim qui demersi sunt in aqua, nihilo magis respirare possunt, si non longe absunt &agrave; summo vt iam i&aacute;mque possint emergere, 
            qu&agrave;m si etiam tum essent in profundo: nec catulus ille qui iam appropinquat vt videat, plus cernit qu&agrave;m is qui modo est natus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Similitudo</marginalia_text>
                </position>
            </language>
            <translation>Comparison.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="offunditur; obscuretur &amp; obruatur">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        offundi
                        obscurari
                        obrui
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be covered.
                To be hidden.
                To be covered up.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="productione temporis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        productio te[m]poris
                    </marginalia_text>
                </position>
            </language>
            <translation>The prolonging of time.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="nostri vero quod aestimatione omnino aliqua dignum sit"/>
        <underline method="pen" type="straight" language="LA" text="complecti vitam beatam putent. An"/>
        <underline method="pen" type="straight" language="LA" text="esse quum eculeo torqueatur"/>
        <underline method="pen" type="straight" language="LA" text="causa: opinio facit non natura vim doloris"/>
        <underline method="pen" type="straight" language="LA" text="vt si quum"/>
        <underline method="pen" type="straight" language="LA" text="sit, quo sit corporis, aut externis bonis plenior: vt"/>
        <underline method="pen" type="straight" language="LA" text="frequentia"/>
        <underline method="pen" type="straight" language="LA" text="pluris sit coniunctum qu&agrave;m sapere ipsum separatim. Nam"/>
        <underline method="pen" type="straight" language="LA" text="Vt enim obscuratur &amp; offunditur luce solis lumen lucernae, &amp; vt interit magnitudine maris AEgaei stilla muriae, 
            &amp; vt in diuitiis Croesi teruncii accessio"/>
        <underline method="pen" type="straight" language="LA" text="gradus vnus in ea via quae est hinc in Indiam: sic quum sit is bonoru[m] finis"/>
        <underline method="pen" type="straight" language="LA" text="obscuretur"/>
        <underline method="pen" type="straight" language="LA" text="obruatur"/>
        <underline method="pen" type="straight" language="LA" text="non fit maior productione temporis"/>
        <underline method="pen" type="straight" language="LA" text="crescendi accessione[m] nullam"/>
        <underline method="pen" type="straight" language="LA" text="atque opportunitate finitur, nec plura paucioribus, nec longinquiora breuioribus antepone[n]tur. Nec"/>
        <underline method="pen" type="straight" language="LA" text="crescere bonorum finem non"/>
        <underline method="pen" type="straight" language="LA" text="aqua, nihilo magis respirare possunt, si non longe absunt &agrave; summo vt iam i&aacute;mque"/>
        
