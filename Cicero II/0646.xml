        <marginalia hand="Italian" anchor_text="nihil est quod tam miseros faciat, qu&agrave;m impietas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Nihil tam miseros facit q[uam] impietas.
                    </marginalia_text>
