        <underline method="pen" type="straight" language="LA" text="noster cognatione patruelis, amore germanus"/>
        <underline method="pen" type="straight" language="LA" text="ambulationem postm&eacute;ridiana[m] conficeremus"/>
        <underline method="pen" type="straight" language="LA" text="vario sermone sex illa &agrave; Dipylo stadia confecimus"/>
        <underline method="pen" type="straight" language="LA" text="nobilitata spatia, solitudo erat ea quam volueramus"/>
        <underline method="pen" type="straight" language="LA" text="audiamus, aut scriptum aliquod legamus"/>
        <underline method="pen" type="straight" language="LA" text="enim mihi Platonis in mentem, quem"/>
        <underline method="pen" type="straight" language="LA" text="conspectu meo"/>
        <underline method="pen" type="straight" language="LA" text="H&icirc;c Speusippus, h&icirc;c Xenocrates, h&icirc;c eius auditor Polemo"/>
        <underline method="pen" type="straight" language="LA" text="sessio fuit"/>
        <underline method="pen" type="straight" language="LA" text="primis auum cogitare"/>
        <underline method="pen" type="straight" language="LA" text="conuertebat ad sese Coloneus"/>
        <underline method="pen" type="straight" language="LA" text="oculos versabatur, quem scis qu&agrave;m admirere"/>
        <underline method="pen" type="straight" language="LA" text="Me quide[m] ad altiore[m] memoriam OEdipodis huc venientis, &amp; illo mollissimo carmine, quaenam essent ipsa haec loca requirentis, 
            species quaedam co[m]mouit, inanis scilicet, sed commouit tame[n]"/>
        <underline method="pen" type="straight" language="LA" text="sed veteris prouerbii admonitu, viuoru[m] memini"/>
        <underline method="pen" type="straight" language="LA" text="Epicuri licet obliuisci, si cupiam, cuius imaginem no[n] modo in tabulis nostri familiares, sed etiam in poculis &amp; in anulis habent"/>
        <underline method="pen" type="straight" language="LA" text="vt acrius aliquanto &amp; attentius de claris viris locoru[m] admonitu cogitemus"/>
        <underline method="pen" type="straight" language="LA" text="vita[m] ediderat"/>
        <underline method="pen" type="straight" language="LA" text="inter se decertate soliti"/>
        <underline method="pen" type="straight" language="LA" text="vt fremitu[m] assuesceret voce vincere"/>
        <underline method="pen" type="straight" language="LA" text="paulum ad dextram de via declinaui"/>
        <underline method="pen" type="straight" language="LA" text="quide[m] infinitum est"/>
        <underline method="pen" type="straight" language="LA" text="Atqui Cicero, inquit, ista studia si ad imitandos summos viros spectant, ingeniosorum sunt: sin tantummodo ad indicia veteris memoriae cognoscenda, 
            curiosoru[m]. Tu aute[m] hortamur omnes, currente[m] quide[m] vt spero, vt eos quos nouisse vis, etiam imitari velis"/>
        <underline method="pen" type="straight" language="LA" text="prim&iacute;sque vt aliquid suoru[m] studioru[m] philosophiae quoque"/>
        <underline method="pen" type="straight" language="LA" text="vel vt te imitetur quem amat, vel vt illud ipsum quod studet, facere pos[sit]"/>
        
