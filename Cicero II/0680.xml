    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="ductores nostri">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        ductores n[ostr]i duces imperatores
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Our commnanders, leaders, generals.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Carere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Carere quid
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lack what.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="regno carere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Regno carere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lack a king.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec sensus in mortuo est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sensus no[n] est in mortuo
                    </marginalia_text>
                </position>
            </language>
            <translation>
                There is no feeling in death.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="L. Brutus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        L. Brutus
                    </marginalia_text>
                    <person name="Lucius Junius Brutus"/>
                </position>
            </language>
            <translation>
                Lucius Junius Brutus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Decius">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Decij
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The gens Decia.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Scipiones">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Scipiones.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The Scipios.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in hoc inest omnis animi co[n]tractio ex metu mortis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Animi contractio in mortis metu vbi sit.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Where is the condensation of the soul in the fear of death.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Hippocentaurum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Hippocentaurus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A centaur.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Camillus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Camillus.
                    </marginalia_text>
                    <person name="Marcus Furius Camillus"/>
                </position>
            </language>
            <translation>
                Marcus Furius Camillus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Cur Mors nos no[n] deterreret asculta paucis  <!--JG: imp. of auscultare?-->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Why death does not frighten us. Listen to small things.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="gloria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Virtute[m] sequitur gloria
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Glory follows virtue.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="somni simillima[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Mors somni simillima
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Death is similar to a dream.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Videtur igitur resurrectione[m] sapere haec similitudo. 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                This similarity, therefore, seems to understand resurrection.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="imagine[m] mortis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sensus Imago mortis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The senses of the image of death.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ea quide[m] dedit vsuram vitae tanquam pecuniae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Natura nobis vita[e], quasi pecunia[e] vsuram dedit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nature gave is enjoyment of life just like that of money.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Callimachus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Callimachus
                    </marginalia_text>
                    <person name="Callimachus"/>
                </position>
            </language>
            <translation>
                Callimachus.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        an resurgemus?
                    </marginalia_text>
                </position>
            </language>
            <translation>
                If we resurrect?
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        cotidie
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Daily.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        in cuius mori
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In its custom.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        alterios no[n] attingit.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                It reaches in no other way.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        decertans
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Disputing.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]tractio animi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The condensation of the soul.
            </translation>
        </marginalia>
        
