        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        fortis.
                    </marginalia_text>
                </position>
            </language>
            <translation>Brave.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aequitatem &amp; fidem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        &AElig;quitas. fides.
                    </marginalia_text>
