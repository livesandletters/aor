        <marginalia hand="Italian" anchor_text="&agrave; falsis discerni; internosci  &agrave; falsis">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Discerni
                        internosci } &agrave;
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be separated; to be distinguished from.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="consentaneum; acco[m]modatum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                       co[n]se[n]taneu[m]
                       accomodatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Suitable.
                Appropriate.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nihil haberet comprehensi, percepti, cogniti, constituti; certi">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        nihil co[m]p[re]hensi
                              p[er]cepti
                              cogniti
                              co[n]stituti } habere.
                              certi
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To have no comprehension; perception; knowledge; agreement; certainty.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quibus nec intelligi quicquam, nec quaeri disputar&iacute;ve potest">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        sine diff[initi]o[n]e nihil intelligitur
                    </marginalia_text>
                </position>
            </language>
            <translation>Without definition, nothing is understood.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Memoriae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        MEMOR&Iuml;A       
                    </marginalia_text>
                </position>
            </language>
            <translation>Memory.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quae potest enim esse memoria falsorum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        nulla est memoria falsoru[m].
                    </marginalia_text>
                </position>
            </language>
