                    <marginalia_text>
                        Scientia qualis
                    </marginalia_text>
                </position>
            </language>
            <translation>What kind of science.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sapientia[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Sapie[n]tia quid.
                    </marginalia_text>
                </position>
            </language>
            <translation>What wisdom is.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cruciatu[m] perferre, intolerabili dolore lacerari">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        cruciatu[m] p[er]ferre
                        intollerabili dolore lacerari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To endure suffering.
                To be wounded by intolerable grief.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="leges imposuerit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        imponere leges.
                    </marginalia_text>
                </position>
            </language>
