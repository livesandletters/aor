        <underline method="pen" type="straight" language="LA" text="nisi figuris hominum constitutis, nihil"/>
        <underline method="pen" type="straight" language="LA" text="Cuius opinionis leuitas confutata &agrave; Cotta, non desiderat orationem"/>
        <underline method="pen" type="straight" language="LA" text="sit animans"/>
        <underline method="pen" type="straight" language="LA" text="nihil eo sit praestantius"/>
        <underline method="pen" type="straight" language="LA" text="respiciens patriam"/>
