<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0301.tif" reader="John Dee" pagination="31"/>   
    <annotation>
        
        <marginalia hand="Italian" anchor_text="laudem modestiae venentur; ex aliorum laboribus libare laudem">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>laude[m] venari libare.</marginalia_text>
                </position>
            </language>
            <translation>To pursue praise, to sip praise.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="mediocriter literatus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>mediocrit[er] literatus</marginalia_text>
                </position>
            </language>
            <translation>Moderately educated.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iam Secundu[m] refellit.</marginalia_text>
                </position>
            </language>
            <translation>Now he refutes the second.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="exempla ponuntur h&icirc;c no[n] confirmandi, neque testificandi causa, sed demonstrandi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Exemplum non esse testimonium.</marginalia_text>
                </position>
            </language>
            <translation>That an example is no evidence.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Hoc igitur interest inter exemplu[m] &amp; testimoniu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quid interest inter exemplum &amp; Testimonium.</marginalia_text>
                </position>
            </language>
            <translation>What differs between an example and evidence.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iam tertiu[m] tollit.</marginalia_text>
                </position>
            </language>
            <translation>Now he destroys the third.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="transcripsissetis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>transcribere.</marginalia_text>
                </position>
            </language>
            <translation>To transcribe.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="non tum cum parere ipsi, &amp; gignere, &amp; proferre debent">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>parere gignere p[ro]ferre</marginalia_text>
                </position>
            </language>
            <translation>To produce, to create, to bring forth.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Si concederem aliena oportere assumere exempla, vincerem vnius oportere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quod si exempla aliena su[n]t sumenda vni[us] no[n] pluriu[m] que[m]admodu[m] greci faciu[n]t, sume[n]da</marginalia_text>
                </position>
            </language>
            <translation>That if examples of others are to be taken, only those of one person and not of many - as the Greeks do - are to be taken.</translation> 
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vtrum vnu[m] omnia, an omnia nemine[m], sed aliud alium putet co[n]sequi posse">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>An vnus aut nemo o[mn]ia consequi potest</marginalia_text>
                </position>
            </language>
            <translation>Whether a single person or no one is able to achieve all [qualities].</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="ne qua in re laudem modestiae venentur, in ea ipsa re sint impudentes"/>
        <underline method="pen" type="straight" language="LA" text="quis est qui possit id quod de arte scripserit, comprobare"/>
        <underline method="pen" type="straight" language="LA" text="Si laboriosum, non statim praeclarum. Sunt"/>
        <underline method="pen" type="straight" language="LA" text="transcripsissetis"/>
        <underline method="pen" type="straight" language="LA" text="quia plan&egrave; rudis id facere nemo poterit, cum feceris, te literatissimum putes, ineptus sis, propterea qu&ograve;d id facile faciat quiuis mediocriter literatus"/>
        <underline method="pen" type="straight" language="LA" text="cum parere ipsi, &amp; gignere, &amp; proferre debent"/>
        <underline method="pen" type="straight" language="LA" text="hoc nulla staret eorum ratio"/>
        <underline method="pen" type="straight" language="LA" text="vtrum vnu[m] omnia, an omnia nemine[m], sed aliud alium"/>
        <underline method="pen" type="straight" language="LA" text="ab omnibus putare omnia"/>
        
        <mark name="pin" method="pen" place="head" language="LA" text="mediocrit[er] literatus"/>
        
        <mark name="pin" method="pen" place="right_margin"/>
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen" anchor_text="ne qua in re laudem modestiae venentur, in ea ipsa re sint impudentes"/>
        
        <drawing type="florilegium" book_orientation="0" place="right_margin" method="pen" anchor_text="An vnus aut nemo o[mn]ia consequi potest"/>
        
    </annotation>
