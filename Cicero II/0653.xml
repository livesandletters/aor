    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0653.tif" reader="John Dee" pagination="95"/>   
    
    <annotation>
        
        
        <marginalia hand="English_secretary" anchor_text="attingat, ad s&eacute;que pertineat">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        attingere
                        p[er]tinere.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To affect.
                To touch.
            </translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="coept&aacute;tque">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>coeptare</marginalia_text>  <!-- DO: I think there is some kind of symbol/mark below this word -->
                </position>
            </language>
            <translation>To venture.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="primus ad omnem vitam tuendam appetitus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Primus a[n]i[m]alium appetitus.</marginalia_text>
                </position>
            </language>
            <translation>The first impulse of living things.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="omnium est natura co[m]munis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Omniu[m] est natura co[mmun]is.</marginalia_text>
                </position>
            </language>
            <translation>Nature is common to all.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="non ita accipiendu[m] est, quasi dicamus vnu[m] esse omnium extremu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>No[n] est vnu[m] o[mn]i[u]m a[n]i[m]aliu[m] extremu[m]</marginalia_text>
                </position>
            </language>
            <translation>There is not one end for all living things.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in omnibus summa co[m]munis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Summa o[mn]i[u]m Comunis.</marginalia_text>
                </position>
            </language>
            <translation>The ultimate end is common to all.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="omnem naturam esse seruatricem sui">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Omnis natura sui seruatrix.</marginalia_text>
                </position>
            </language>
            <translation>All nature is its own preserver.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Ex quo intelligi debet homini id esse in bonis vltimu[m], secu[n]dum naturam viuere: quod ita interpretemur, viuere ex hominis natura vndique perfecta, &amp; nihil require[n]te">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quid in ho[m]i[n]e sit secundum naturam viuere.</marginalia_text>
                </position>
            </language>
            <translation>What in a man may be living according to nature.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="omne animal seipsum diligere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Omne a[n]i[m]al seip[su]m diligit.</marginalia_text>
                </position>
            </language>
            <translation>Every living thing loves itself.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Necesse est quidem siquis sibipsi inimicus est, eum quae bona sunt, mala putare: bona contr&agrave;, quae mala: &amp; quae appete[n]da fugere, &amp; quae fugienda appetere: quae sine dubio vitae sunt euerio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Vita[e] euersio est cu[m] quis sibi inimicus esse coeperit.</marginalia_text>
                </position>
            </language>
            <translation>Life is destroyed when one has begun to be an enemy of oneself.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="apud Terentium">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Heautontimoroumenos.</marginalia_text>
                    <book title="Heauton Timorumenos"/>
                </position>
            </language>
            <translation>Heauton Timorumenos.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in mala scientes irruunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In mala scientes irruunt</marginalia_text>
                </position>
            </language>
            <translation>They knowingly rush into trouble.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="reapse">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>reapse.</marginalia_text>
                </position>
            </language>
            <translation>Really.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="Haec igitur nobis explicanda sunt, sed si enodatius, vos ignoscetis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>enodatius explicare.</marginalia_text>
                </position>
            </language>
            <translation>More thorough.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="terminatione">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>terminatio</marginalia_text>
                </position>
            </language>
            <translation>Determination.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="initio constitutionem confusam habet &amp; incertam"/>
        <underline method="pen" type="straight" language="LA" text="quicquid se attingat, ad s&eacute;que pertineat"/>
        <underline method="pen" type="straight" language="LA" text="coept&aacute;tque"/>
        <underline method="pen" type="straight" language="LA" text="communia, quoniam omnium est natura co[m]munis. Sed"/>
        <underline method="pen" type="straight" language="LA" text="genera, distincta &amp; dispertita sunt"/>
        <underline method="pen" type="straight" language="LA" text="sed vt omnium artium recte dici potest co[m]mune esse, vt in aliqua scientia versetur"/>
        <underline method="pen" type="straight" language="LA" text="scientiam aute[m] suam cuiusque artis esse"/>
        <underline method="pen" type="straight" language="LA" text="tamen in omnibus summa co[m]munis. Et"/>
        <underline method="pen" type="straight" language="LA" text="omnem naturam esse seruatricem sui, &iacute;dque"/>
        <underline method="pen" type="straight" language="LA" text="quae natura vigeant, similem esse finem, non"/>
        <underline method="pen" type="straight" language="LA" text="viuere ex hominis natura vndique perfecta, &amp; nihil require[n]te"/>
        <underline method="pen" type="straight" language="LA" text="sed si enodatius"/>
        <underline method="pen" type="straight" language="LA" text="terminatione rerum"/>
        <underline method="pen" type="straight" language="LA" text="idem reapse primum"/>
        <underline method="pen" type="straight" language="LA" text="est enim infixu[m] in ipsa natura, comprehenditur suis cuiusque sensibus, sic vt contr&agrave; siquis dicere velit, non audiatur"/>
        <underline method="pen" type="straight" language="LA" text="concurre[n]t co[n]trariae"/>
        <underline method="pen" type="straight" language="LA" text="vitae sunt euersio"/>
        <underline method="pen" type="straight" language="LA" text="qui aut laqueos, aut alia exitia"/>
        <underline method="pen" type="straight" language="LA" text="quum in mala scientes irruunt"/>
        <underline method="pen" type="straight" language="LA" text="bellum indixissent, cruciari dies, noctes torqueri vellent: nec"/>
        
        
