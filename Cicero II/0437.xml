<!DOCTYPE transcription SYSTEM "http://www.livesandletters.ac.uk/schema/aor_20141023.dtd">

<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0437.tif" reader="John Dee" pagination="167" signature="l.iiii."/>   
    
    <annotation>
        
        
        <underline method="pen" type="straight" language="LA" text="vnde omnia manant, videre"/>
        <underline method="pen" type="straight" language="LA" text="deferuntur"/>
        <underline method="pen" type="straight" language="LA" text="meditatum nobis in perpetuum"/>
        <underline method="pen" type="straight" language="LA" text="debebit"/>
        <underline method="pen" type="straight" language="LA" text="separatim dicere solemus de genere vniuerso"/>
        <underline method="pen" type="straight" language="LA" text="multa co[m]mentatione atque meditatione paratos atque expeditos"/>
        <underline method="pen" type="straight" language="LA" text="Longu[m] est"/>
        <underline method="pen" type="straight" language="LA" text="Haec sunt omnia ingenii vel mediocris, exercitationis autem vel maximae: arte[m] quidem &amp; praecepta du[m]taxat hactenus requirunt, vt certis dicendi luminibus ornentur"/>
        <underline method="pen" type="straight" language="LA" text="pariuntur"/>
        <underline method="pen" type="straight" language="LA" text="excogitationem non habent difficilem"/>
        <underline method="pen" type="straight" language="LA" text="explicationem magis illustrem perpolit&aacute;mque desyderant"/>
        <underline method="pen" type="straight" language="LA" text="arte tinctum"/>
        <underline method="pen" type="straight" language="LA" text="artem requirit"/>
        <underline method="pen" type="straight" language="LA" text="vis illa diuina virt&uacute;sque cernitur"/>
        <underline method="pen" type="straight" language="LA" text="non recusabo, quo minus perpoliam atque conficiam: quantum consequar"/>
        <underline method="pen" type="straight" language="LA" text="valent"/>
        <underline method="pen" type="straight" language="LA" text="praesto est"/>
        <underline method="pen" type="straight" language="LA" text="qui hoc primus in nostros mores induxit"/>
        <underline method="pen" type="straight" language="LA" text="qui maxime auxit, qui solus effecit"/>
        <underline method="pen" type="straight" language="LA" text="no[n] reuerens assentandi suspicionem"/>
        <underline method="pen" type="straight" language="LA" text="quem aetas nostra tulerit"/>
        <underline method="pen" type="straight" language="LA" text="Itaque siquid est in me, quod iam sperare videor"/>
        <underline method="pen" type="straight" language="LA" text="tantum operae mihi ad audiendum datis"/>
        <underline method="pen" type="straight" language="LA" text="quod non in memoria mea penitus insederit"/>
        <underline method="pen" type="straight" language="LA" text="is qui sum, qua[n]tuscunque sum, adiudicandus omnibus auditis oratoribus, sine vlla dubitatione sic statuo &amp; iudico"/>
        <underline method="pen" type="straight" language="LA" text="ornamenta dicendi"/>
        <underline method="pen" type="straight" language="LA" text="iniqua partitio, si cu[m] ego hunc oratore[m], quem nunc fingo, vt institui, crearo, aluero, confirmaro, trada[m] eum Crasso &amp; vestiendum &amp; orna[n]dum"/>
        <underline method="pen" type="straight" language="LA" text="Neque enim boni est, neque liberalis pare[n]tis, quem procrearit &amp; eduxerit, eum non &amp; vestire &amp; ornare, praesertim cum te locupletem esse negare non possis"/>
        <underline method="pen" type="straight" language="LA" text="in causa peroranda non dubitauit excitare reum Consulare[m], &amp; eius diloricare tunica[m], &amp; iudicibus cicatrices auersas senis Imperatoris ostendere"/>
        <underline method="pen" type="straight" language="LA" text="quos praestare nemo possit"/>
        <underline method="pen" type="straight" language="LA" text="essent exacti"/>
        <underline method="pen" type="straight" language="LA" text="Tribunitia potestas esset constituta"/>
        <underline method="pen" type="straight" language="LA" text="neque reprimi"/>
        <underline method="pen" type="straight" language="LA" text="iure esse conflatam"/>
        <underline method="pen" type="straight" language="LA" text="tam anceps"/>
        <underline method="pen" type="straight" language="LA" text="tam lubricus"/>
        <underline method="pen" type="straight" language="LA" text="co[m]miseratione"/>
        <underline method="pen" type="straight" language="LA" text="enituit, quod tibi omnes dant"/>
        <underline method="pen" type="straight" language="LA" text="quae nu[n]c ad me delegare vis"/>
        <underline method="pen" type="straight" language="LA" text="inter vos in dice[n]do dissimillimi sitis"/>
        <underline method="pen" type="straight" language="LA" text="vt ei nihil neque &agrave; natura denegatu[m], neque &agrave; doctrina no[n] delatu[m] esse videatur"/>
        <underline method="pen" type="straight" language="LA" text="tua suauitate nos priuabis"/>
        <underline method="pen" type="straight" language="LA" text="non potuisse potius, qu&agrave;m &agrave; Crasso dici maluisse"/>
        <underline method="pen" type="straight" language="LA" text="Quin tu"/>
        
        
        <mark name="vertical_bar" method="pen" place="right_margin"/>
        
        
    </annotation>
    
</transcription>