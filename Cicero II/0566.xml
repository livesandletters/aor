    <page filename="0566.tif" reader="John Dee" pagination="8" signature="[Aa.iiii.[v]]"/>   
    <annotation>
        
        <marginalia hand="Italian" anchor_text="nihil omnino esse mome[n]ti">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>nihil o[mn]i[n]o mome[n]ti</marginalia_text>
                </position>
            </language>
            <translation>Nothing of movement whatsoever.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="interiecta &amp; media; in mediis relinquebat; in bonis actionibus pone[n]s; statuebat">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        ponere
                        collocare
                        numerare } in bonis [etc]
                        statuere } hoc esse [?]. [ interiecta media in medijs relinquere.        <!-- DO: Not sure of word after 'esse' --> <!--JG: Me neither-->
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To put.
                To situate.
                To count in good things, etc.
                To establish: this is [?] placed in between, in the middle, left in the middle sections.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in bonis actionibus; virtutis vsum; habitum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        virtutis [ vsus
                        actio
                        Habitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Of virtue the use; the act, the habit.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ex iis alia pluris esse aestima[n]da, alia minoris">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        a[e]stimia[n]da pluris minoris.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Thought to be more; less.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in mediis relinquebat; media locabat; media putabat; inter scientiam &amp; inscientiam, comprehensionem illam quam dixi, collocabat">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        in medijs reli[n]quere
                        h&egrave;c media locare
                        media putare
                        collocare inter [etc].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To leave in the middle sections.
                To situate this as intermediate.
                To consider as intermediate.
                To distinguish between, etc.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="contraherent, in angust&uacute;mque deducerent">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        co[n]trahere
                        in angustu[m] deducere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To gather. To assemble in a small group.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="accepta sensibus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        res subiecta[e]
                        Accepta sensibus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Things subject to; Accepted by the senses.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="tame[n] alia secundum natura[m] dicebat, alia naturae esse contraria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Alia sunt secundum na[t]u[r]a
                                  co[n]traria na[tura]e.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Some are in accordance with nature; contrary to nature.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="dignanda">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Digna[n]da.</marginalia_text>
                </position>
            </language>
            <translation>To be esteemed.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="recte facta sola in bonis actionibus pone[n]s: praue">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Recte ac prave factu[m].</marginalia_text>
                </position>
            </language>
            <translation>Done rightly and wrongly.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="peccata">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>peccata.</marginalia_text>
                </position>
            </language>
            <translation>Sins.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="non omnem virtutem in ratione esse dicerent, sed quasdam virtutes natura aut more perfectas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        virtus alia in r[ati]o[n]e natura et more p[er]fecta.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Some virtues in reason; perfected in nature and custom.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nec tamen virtutem cuiquam adesse quin ea semper vteretur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>virtus sine p[er]petuo vsu nulli adest.</marginalia_text>
                </position>
            </language>
            <translation>Virtue without continuation is of no use.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="carere sapie[n]tem. Qu&uacute;mque eas perturbationes antiqui naturales esse dicerent">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>sapiens caret perturbatio[n]ibus</marginalia_text>
                </position>
            </language>
            <translation>A wise man is without perturbations.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="perturbationes voluntarias">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Perturbationes volu[n]taria[e].</marginalia_text>
                </position>
            </language>
            <translation>Voluntary perturbations.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="perturbationum arbitrabatur matrem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Perturbationu[m] mater</marginalia_text>
                </position>
            </language>
            <translation>The mother of perurbations.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="quinta[m] hanc naturam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quinta Natura.</marginalia_text>
                </position>
            </language>
            <translation>The fifth nature.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Xenocrates">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>XENOCRATES</marginalia_text>
                    <person name="Xenocrates of Chalcedon"/>
                </position>
            </language>
            <translation>Xenocrates.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="sensibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>SENSVS</marginalia_text>
                </position>
            </language>
            <translation>The senses.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="impulsione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Impulsio.</marginalia_text>
                </position>
            </language>
            <translation>An impulse.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        [Ap[er]ta se[n]te[n]tia p[er] disceptatio[n]e - deleted]    
                    </marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="assensionem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Assensio</marginalia_text>
                </position>
            </language>
            <translation>Assent.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Visis non omnibus adiungebat fidem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quibus visis fides</marginalia_text>
                </position>
            </language>
            <translation>Not all appearances are trustworthy.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="comprehensibile">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Comprehensibile.</marginalia_text>
                </position>
            </language>
            <translation>Comprehensible.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="comprehensionem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Comprehensio</marginalia_text>
                </position>
            </language>
            <translation>Comprehension.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nomen hoc duxerat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ducere no[m]en</marginalia_text>
                </position>
            </language>
            <translation>To assign a name.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Quod autem erat sensu comprehensum, idipsum sensum appellabat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sensu[m] vnde.</marginalia_text>
                </position>
            </language>
            <translation>From where the term sense arose.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="si ita erat co[m]prehensum, vt conuelli ratione non posset, scientiam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Scientia vnde</marginalia_text>
                </position>
            </language>
            <translation>From where the term knowledge arose.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="inscientiam nominabat, ex qua existeret etiam opinio quae esset imbecilla">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Inscie[n]tia. Opinio.</marginalia_text>
                </position>
            </language>
            <translation>Ignorance. Opinion.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Sed inter scientiam &amp; inscientiam, comprehensionem illam quam dixi, collocabat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Inter scie[n]tia[m] et inscie[n]tia[m] interiecta est co[m]prehensio.</marginalia_text>
                </position>
            </language>
            <translation>Comprehension is placed between knowledge and ignorance.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="comprehensio facta sensibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Co[m]p[re]hensio facta se[n]sibus</marginalia_text>
                </position>
            </language>
            <translation>Comprehension is made by the senses.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Errorem autem, &amp; temeritatem, &amp; ignora[n]tiam, &amp; opinationem, &amp; suspicione[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Error: temeritas: ignoratia.
                        Opinatio: suspicio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Error. Rashness. Ignorance. Opinion. Suspicion.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nullo modo arbitrabatur quicquam effici posse ab ea quae expers esset corporis; Nec vero aut qu&ograve;d efficeret aliquid, aut qu&ograve;d efficeretur, posse esse non corpus">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Nihil effici p[otes]t ab ea re qua expers est Corporis
                        Quicquid vel efficit vel efficitur est Corpus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Nothing can be acted on by a thing which lacks a body.
                Anything which either acts or is acted on is a body.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="acceptum iam &amp; approbatum">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        acceptu[m]
                        approbatu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Accepted.
                Approved.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Phantasiam, nos Visum appellemus licet">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        phantasia
                        visum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An apparition.
                A vision.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="firmae &amp; consta[n]tis assensionis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        assensio firma
                                 constans
                    </marginalia_text>
                </position>
            </language>
            <translation>Firm and constant assent.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="commutauerat; disserebat">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        co[m]mutatio
                        Disse[n]sio. } &agrave;.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Change [and] conflict from.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="adiungebat fidem; fidem tribuebat">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        adiungere
                        attribuero } fide[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>To join to, to add to trust.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="comprehensio facta sensibus, &amp; vera esse &amp; fidelis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        co[m]prehensio vera
                                       fidelis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                True, trustworthy comprehension.
            </translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="aute[m], etsi nec bona nec mala essent"/>
        <underline method="pen" type="straight" language="LA" text="aestimatione dignanda docebat"/>
        <underline method="pen" type="straight" language="LA" text="recte facta sola in bonis actionibus pone[n]s"/>
        <underline method="pen" type="straight" language="LA" text="est peccata in"/>
        <underline method="pen" type="straight" language="LA" text="superiores non omnem virtutem in ratione esse dicerent, sed quasdam virtute natura aut more perfectas"/>
        <underline method="pen" type="straight" language="LA" text="virtutis vsum"/>
        <underline method="pen" type="straight" language="LA" text="sed ipsum habitum"/>
        <underline method="pen" type="straight" language="LA" text="virtutem cuiquam adesse quin ea semper vteretur"/>
        <underline method="pen" type="straight" language="LA" text="ea contraherent, in angust&uacute;mque"/>
        <underline method="pen" type="straight" language="LA" text="quasi morbis, voluit carere sapie[n]tem"/>
        <underline method="pen" type="straight" language="LA" text="Nam &amp; perturbationes voluntarias"/>
        <underline method="pen" type="straight" language="LA" text="opinion&eacute;sque iudicio suscipi &amp; omnium perturbationum arbitrabatur matrem"/>
        <underline method="pen" type="straight" language="LA" text="quanda[m] inte[m]perantiam"/>
        <underline method="pen" type="straight" language="LA" text="De naturis"/>
        <underline method="pen" type="straight" language="LA" text="arbitrabatur quicquam effici posse ab ea quae expers esset corporis"/>
        <underline method="pen" type="straight" language="LA" text="quasi impulsione oblata extrinsecus, quam ille Phantasiam, nos Visum appellemus"/>
        <underline method="pen" type="straight" language="LA" text="Et teneamus"/>
        <underline method="pen" type="straight" language="LA" text="&amp; quasi accepta"/>
        <underline method="pen" type="straight" language="LA" text="&amp; si ita erat co[m]prehensum, vt conuelli ratione"/>
        <underline method="pen" type="straight" language="LA" text="posset, scientiam: sin aliter, inscientiam nominabat, ex qua existeret etiam opinio quae esset imbecilla, &amp; cum falso incognit&oacute;que communis. Sed"/>
        <underline method="pen" type="straight" language="LA" text="natura, quasi normam scientiae &amp; principium sui dedisset, vnde"/>
        <underline method="pen" type="straight" language="LA" text="vno nomine omnia quae essent aliena firmae &amp; consta[n]tis assensionis, &agrave; virtute sapienti&aacute;que remouebat"/>
        
        
