        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="consociare aut coniungere iniuriam">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        co[n]sociare
                        coniungere ] iniuria[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To unite in; to join in wrongdoing.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="compositum, t&aacute;mque compactum &amp; coagmentatu[m]">
            <language ident="LA">
