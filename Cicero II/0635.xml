        <marginalia hand="Italian" anchor_text="te[m]pori parere, &amp; sequi Deum, &amp; se noscere, &amp; nihil nimis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Tempori pare
                        Deum sequere
                        Teip[s]um nosce.
                        Nihil nimis }
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Submit to the time.
                Follow the god.
                Know yourself.
                Nothing in excess.
            </translation>
        </marginalia>
        
