    <annotation>
        
        <marginalia hand="Italian" anchor_text="beneficentia, quam eandem vel benignitatem vel liberalitatem">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>beneficeita benignitas liberalitatas</marginalia_text>
                </position>
            </language>
            <translation>Beneficence, benevolence, liberality.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="propulsant iniuriam. Nam qui iniuste impetum in quempiam facit, aut ira, aut aliqua perturbatione incitatus, is quasi manus videtur afferre socio: qui 
            aute[m] nec defendit, nec obsistit si potest iniuriae">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>defendere propulsare iniuria obsistere iniuria[e] tutari.</marginalia_text>
                </position>
            </language>
            <translation>Defends, repels injury, oppose injuries, to protect. </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iustitia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iustitia</marginalia_text>
                </position>
            </language>
            <translation>Justice.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iustitiae primum munus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iustitia[e] mumera</marginalia_text>
                </position>
            </language>
            <translation>The obligations of justice.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="priuata natura nulla">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Priuata nulla, natura</marginalia_text>
                </position>
            </language>
            <translation>No private things by nature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Non nobis solum nati sumus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Non nobis solu[m] nati sum[us]</marginalia_text>
                </position>
            </language>
            <translation>We are not born alone for ourselves.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="communes vtilitates in mediu[m] afferrem mutatione officiorum, dando, accipiendo: tum artibus, tum opera, tum faultatibus deuincire 
            hominum inter homines societatem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ho[m]in[es] societas &amp; vita[e] communitas.</marginalia_text>
                </position>
            </language>
            <translation>The society of men and the community way of life.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fidem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Fides.</marginalia_text>
                </position>
            </language>
            <translation>Faith.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iniustitiae genera">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iniustita[e] genera.</marginalia_text>
                </position>
            </language>
            <translation>Kinds of injustice.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="metu">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Metus.</marginalia_text>
                </position>
            </language>
            <translation>Fear.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="auaritia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Avaritia.</marginalia_text>
                </position>
            </language>
            <translation>Avarice.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="diuitiae tum ad vsus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuitiaru[m] vsus.</marginalia_text>
                </position>
            </language>
            <translation>The purpose of riches.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Marcus Crassus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>M[arcus] Crassus</marginalia_text>
                    <person name="Marcus Licinius Crassus"/>
                </position>
            </language>
            <translation>Marcus Crassus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="est vt infinita pecuniae cupiditas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Infinita pecunia[e] cupiditas est.</marginalia_text>
                </position>
            </language>
            <translation>An insatiable desire of riches,</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Maxime autem adducuntur plerique vt eos iustitiae capiat obliuio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iustitia[e] obliuio q[uos] capit maxime</marginalia_text>
                </position>
            </language>
            <translation>Especially when a neglect of justice has seized them.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="excellere, in eo fit plerunque tanta contentio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Contentio ob excelle[n]tiam</marginalia_text>
                </position>
            </language>
            <translation>The contest towards excellence.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="C, Caesaris">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>.C[aius] Ca[e]sar.</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>Caius Caesar.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="gloria cupiditates">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>gloria[e] cupiditas</marginalia_text>
                </position>
            </language>
            <translation>The desire for glory.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Leuiora enim sunt ea quae repentino aliqio motu accidunt, qu&agrave;m ea quae meditata &amp; praeparata">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>subita p[re]paratis leuiora</marginalia_text>
                </position>
            </language>
            <translation>The approached are shorter when prepared.</translation> <!-- M.B. Not sure how to translate this? perhaps this is a more a list of words? -->
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="possunt, non propulsant iniuriam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cur cu[m] possim[us] non propulsam[us] iniurias</marginalia_text>
                </position>
            </language>
            <translation>Why I may not be able to repel injuries.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="co[n]sultato &amp; cogitato fiat iniuria. Leuiora enim sunt ea quae repentino aliquo motu ">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>{ co[n]sulto cogitatio data op[era] repentino motu</marginalia_text>
                </position>
            </language>
            <translation>{ Deliberate, think, offered work, sudden impulse.</translation>
        </marginalia>
        
