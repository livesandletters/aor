    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="virgula diuina">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>virgula diuina.</marginalia_text>
                </position>
            </language>
            <translation>Divine staff.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="totum se in cognitione &amp; scientia collocaret">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>totu[m] se in cognitione &amp; scie[n]tia collocare</marginalia_text> 
                </position>
            </language>
            <translation>To place oneself competely in knowledge and understanding.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="docere, tum discere vellet: tum audire, tum dicere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Docere. discere. audire. dicere.</marginalia_text>
                </position>
            </language>
            <translation>To teach, learn, hear, speak.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ne conseruandae quidem patriae causa sapiens">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Qua[e]da[m] (no[n] patria[e] co[n]serua[n]d[a]e causa) no[n] aget sapiens.</marginalia_text>
                </position>
            </language>
            <translation>A certain person will not act wisely (not for the sake of preserving the fatherland).</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="genus officiorum excellere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>officioru[m] pra[e]stantia</marginalia_text>
                </position>
            </language>
            <translation>The excellence of offices.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="In ipsa autem co[m]munitate sunt gradus officioru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>gradus officioru[m] in co[mmun]itate.</marginalia_text>
                </position>
            </language>
            <translation>The position of offices in a community.</translation>
        </marginalia>
        
