    <annotation>
        
        <marginalia hand="Italian" anchor_text="Quid enim est tam futile, qu&agrave;m quicqua[m] approbare non cognitum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Quid est tam futile, qu&agrave;m quicqu[am] approbare no[n] cognitu[m].</marginalia_text>
                </position>
            </language>
            <translation>What is as futile as approving something unknown.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec vnqua[m] vlli viso assensuru[m], nisi quod tale fuerit, quale falsum esse non possit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>sapiens veris semper asse[n]titur visis</marginalia_text>
                </position>
            </language>
            <translation>A wise person always approves a true vision.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Vt mater geminos internoscit co[n]suetudine oculoru[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Prouerb[ium].</marginalia_text>
                </position>
            </language>
            <translation>A proverb.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="gallinas; hi quum ouum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Gallina[e]. ova.</marginalia_text>
                </position>
            </language>
            <translation>Hens. Eggs.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Habeo enim regula[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>habeo regula[m]</marginalia_text>
                </position>
            </language>
            <translation>I have a rule.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ab hac mihi no[n] licet tra[n]suersum (vt aiu[n]t) digitu[m] discedere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Prouerb[ium].</marginalia_text>
                </position>
            </language>
            <translation>A proverb.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Illud vero perabsurdum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Illud vero perabsurdu[m].</marginalia_text>
                </position>
            </language>
            <translation>That truly very absurd thing.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text></marginalia_text>              <!-- Greek needed -->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="opinaturu[m], id est peccaturu[m] esse sapientem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapiens opinatur</marginalia_text>
                </position>
            </language>
            <translation>A wise person supposes.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="opinari, id est nunqua[m] assentiri rei vel falsae, vel incognitae">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Opinari quid</marginalia_text>
                </position>
            </language>
            <translation>What supposing is.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mysteria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mysteria.</marginalia_text>
                </position>
            </language>
            <translation>Mysteries.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="multis annis post, multo etiam asseuerantius; amicissimum (me autem appellabat) &amp; aliquot annis minorem natu">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        [multis annis post. assevera[n]tius amicissimus aliquot annis minor natu. - deleted]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                [After many years. More emphatically. The friendliest. Some years younger - deleted].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="spoliat nos iudicio, priuat approbatione omni, orbat sensibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        spolio
                        priuo
                        orbo } 
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I strip.
                I deprive.
                I bereave.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cimmeriis quidem, quibus aspectum solis siue deus aliquis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cymerijs solis no[n] fit aspectus</marginalia_text>
                </position>
            </language>
            <translation>The sight of the sun was not made for the Cimmerian people.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cimmeriis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>CYMERII.</marginalia_text> 
                </position>
            </language>
            <translation>The Cimmerian people.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iurat&uacute;sque dixeris">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>iuratus Dicere.</marginalia_text>
                </position>
            </language>
            <translation>To speak under oath.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Horte[n]sius">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Hortensius</marginalia_text>
                    <person name="Quintus Hortensius"/>
                </position>
            </language>
            <translation>Quintus Hortensius.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec mirum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>nec miru[m]</marginalia_text>
                </position>
            </language>
            <translation>Not wonderful.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>asseuera[n]tius</marginalia_text>
                </position>
            </language>
            <translation>More emphatically.</translation>
        </marginalia>
        
