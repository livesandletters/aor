<transcription xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.livesandletters.ac.uk/schema/aor2_18112016.xsd">
    <page filename="0904.tif" reader="John Dee" pagination="346" signature="[Yy.v.[v]]"/> 
    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="ciuiliter contendimus">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ciuiliter co[n]tendere</marginalia_text>
                </position>
            </language>
            <translation>To compete in a civil sphere.</translation>
        </marginalia>
         
        <marginalia hand="English_secretary" anchor_text="crudeles in bello nec immanes">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Crudelis immanis</marginalia_text>
                </position>
            </language>
            <translation>Cruel, inhuman.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Carthaginem &amp; Numantiam funditus sustulerunt. Nollem Corinthums">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Carthago Numantia Corinthus</marginalia_text>
                    <location name="Carthage"/>
                    <location name="Numantia"/>
                    <location name="Corinth"/>
                </position>
            </language>
            <translation>Carthage, Numantia, Corinth.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="loci, maxime, ne posset aliquando ad bellum faciendum locus ipse adhortari">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Locus ad bellu[m] adhortati p[otes]t, vt bolonia gallos.</marginalia_text>
                    <!--JG: Unsure to which place this refers. Could it be Boulogne-sur-le-mer, where the English fought against the French in the sixteenth century?-->
                    <!-- FSF: possibly! French troops marched against Bologna in 1506 -->
                </position>
            </language>
            <translation>The place might encourage war, as does Bolonia to the French.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="paci">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pax.</marginalia_text>
                </position>
            </language>
            <translation>Peace.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Bellu[m] cu[m] misericordia</marginalia_text>
                </position>
            </language>
            <translation>War with mercy.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="belli quidem aequitas sanctissime Feciali Po. Ro. iure perscripta">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Belli a[e]quitas Feciali iure perscripta.</marginalia_text>
                </position>
            </language>
            <translation>The equality of war described by the ceremonial law.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="bellem esse iustu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Bellum iustum.</marginalia_text>
                </position>
            </language>
            <translation>Just war.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="summa erat obseruatio in bello mouendo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>summa obseruatio in bello mouendo</marginalia_text>
                </position>
            </language>
            <translation>Strictest observance in waging war.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Catonis senis est epistola">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Catonis Epistola.</marginalia_text>
                    <person name="Cato the Elder"/>
                </position>
            </language>
            <translation>A letter of Cato.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mansuetudinem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mansuetudo</marginalia_text>
                </position>
            </language>
            <translation>Mildness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="eu[m] quicum bellum geras, tam molli nomine appellari?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Hostis vocabulum.</marginalia_text>
                </position>
            </language>
            <translation>Name of the enemy.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="imperii gloria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Imperij gloria.</marginalia_text>
                </position>
            </language>
            <translation>The glory of the empire.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pyrrhi quidem de captiuis reddendis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pyrrh[us] de captiuis red[dendis].</marginalia_text>
                    <person name="Pyrrhus of Epirus"/>
                </position>
            </language>
            <translation>Pyrrhus on delivering the captives.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hosti promiserint, est in eo ipso fides">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Hosti data fides serua[n]da</marginalia_text>
                </position>
            </language>
            <translation>The trust given to the enemy must be preserved.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="aduersus infimos iustitiam esse seruandam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aduersus infirmos iustitia seruanda.</marginalia_text>
                </position>
            </language>
            <translation>Towards the weakest justice is to be maintained.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="seruoru[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Seruus.</marginalia_text>
                </position>
            </language>
            <translation>Servant.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="funditus sustulerunt. Nollem"/>
        <underline method="pen" type="straight" language="LA" text="ipse adhortari"/>
        <underline method="pen" type="straight" language="LA" text="quos vi deuiceris"/>
        <underline method="pen" type="straight" language="LA" text="deuictas bello"/>
        <underline method="pen" type="straight" language="LA" text="aut rebus repetitis geratur, aut denunciatum ant&egrave; sit &amp; indictum"/>
        <underline method="pen" type="straight" language="LA" text="obseruatio in bello"/>
        <underline method="pen" type="straight" language="LA" text="&amp; proprie in eo qui arma contra ferret, remansit"/>
        <underline method="pen" type="straight" language="LA" text="proposita est, minus acerbe gere[n]da sunt"/>
        <underline method="pen" type="straight" language="LA" text="ciuiliter contendimus"/>
        <underline method="pen" type="straight" language="LA" text="Regalis san&egrave;"/>
        <underline method="pen" type="straight" language="LA" text="infirma conditio &amp; fortuna seruoru[m]"/>
        
