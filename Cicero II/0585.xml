    <annotation>
        
        <marginalia hand="Italian" anchor_text="merces erat Dialecticorum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Merces Dialecticoru[m]</marginalia_text>
                </position>
            </language>
            <translation>The payment of dialecticians.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ab eo enim Stoico Dialecticorum didicerat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Diogenes Carneadis magister.</marginalia_text>
                    <person name="Diogenes of Sinope"/>
                    <person name="Carneades"/>
                </position>
            </language>
            <translation>Diogenes of Sinope, the teacher of Carneades.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sequor igitur eas vias quas didici ab Antiocho">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Antiochus cicer. magister</marginalia_text>
                    <person name="Antiochus of Ascalon"/>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Antiochus of Ascalon, the teacher of Cicero.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ostendam&uacute;sque qui simus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ostendet qui sit.</marginalia_text>
                </position>
            </language>
            <translation>He will show who he is.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quatuor eius libri sunt de sustine[n]dis assensionibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quatuor Clitomachi Libri de sustine[n]dis assensionibus.</marginalia_text>
                    <book title="On the Withholding of Assent"/>
                </position>
            </language>
            <translation>The four books of Clitomachus about withholding agreement.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="probabile">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Probabile.</marginalia_text>
                </position>
            </language>
            <translation>Probable.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="similia veri">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Probabilia veri similia sunt.</marginalia_text>
                </position>
            </language>
            <translation>Probable things are similar to true things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quid enim, conscendens nauem sapiens, num co[m]prehensum animo habet atque perceptum. se ex sente[n]tia nauigaturum?">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>futuroru[m] cognitio</marginalia_text>
                </position>
            </language>
            <translation>Knowledge of things to be.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="facili&oacute;rque erit vt albam esse niuem probet, qu&agrave;m erat Anaxagoras, qui id non modo ita esse negabat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Anaxagora[e] nix nigra</marginalia_text>
                    <person name="Anaxagoras"/>
                </position>
            </language>
            <translation>The black snow of Anaxagoras.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Neque nos co[n]tra sensus aliter dicimus ac Stoici, qui multa falsa esse dicunt, long&eacute;que aliter se habere ac sensibus videantur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Stoicoru[m] de sensibus opinio.</marginalia_text>
                </position>
            </language>
            <translation>The opinions of the Stoics on the senses.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Clitomacho">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Clitomachus</marginalia_text>
                    <person name="Clitomachus"/>
                </position>
            </language>
            <translation>Clitomachus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="C. Lucilium scripsit poeta[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lucilius po&euml;ta.</marginalia_text>
                    <person name="Gaius Lucilius"/>
                </position>
            </language>
            <translation>Gaius Lucilius the poet.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Neque tamen habere insignem illa[m] &amp; propriam percipiendi nota[m]">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        insignis
                        propria ] nota.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A distinct [and] particular mark.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Mecu[m] vero quid habent litium, qui ipsorum disciplinam sequor"/>
        <underline method="pen" type="double" language="LA" text="minam"/>
        <underline method="pen" type="straight" language="LA" text="Diogenes reddet"/>
        <underline method="pen" type="straight" language="LA" text="totu[m] tortuosum genus disputandi relinquamus, ostendam&uacute;sque qui simus, iam explicata tota Carneadis sententia, 
            Antiochi ista corruent vniuersa"/>
        <underline method="pen" type="straight" language="LA" text="qui vsque ad senectutem"/>
        <underline method="pen" type="straight" language="LA" text="etenim contra naturam"/>
        <underline method="pen" type="straight" language="LA" text="omnis vitae ea, quam tu Luculle co[m]memorabas, euersio"/>
        <underline method="pen" type="straight" language="LA" text="sit probabilitati illi contrarium: vtetur"/>
        <underline method="pen" type="straight" language="LA" text="se ex sente[n]tia nauigaturum? qu&icirc; potest"/>
        <underline method="pen" type="straight" language="LA" text="facili&oacute;rque erit vt albam"/>
        <underline method="pen" type="straight" language="LA" text="est &egrave; saxo sculptus, aut &egrave; robore dolatus: habet corpus, habet animu[m], mouetur me[n]te, mouetur sensibus, vt ei vera multa videantur"/>
        <underline method="pen" type="straight" language="LA" text="Vt taceam: co[n]clusio ipsa loquitur nihil"/>
        <underline method="pen" type="straight" language="LA" text="quiuis arbitratu suo reprehe[n]dat, quod"/>
        <underline method="pen" type="straight" language="LA" text="Sunt enim mihi nota propterea, qu&ograve;d earum ipsaru[m] rerum"/>
        
