        <marginalia hand="Italian" anchor_text="Sed Antipatro hoc idem postula[n]ti quum diceret, e&iacute;que affirmaret nihil posse percipi consentaneu[m] esse, 
            vnu[m] tame[n] illud dicere percipi posse consentaneu[m] esse, vt alia no[n] possent, Carneades acutius resistebat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Nihil p[er]cipi, falsu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>Nothing is perceived, false.</translation>
        </marginalia>
        
