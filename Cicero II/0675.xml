        <marginalia hand="Italian" anchor_text="at quale sit vides: si ne id quidem, at qua[n]tum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        quid. quale. qua[n]tum
                    </marginalia_text>
                </position>
            </language>
            <translation>
                What, of what kind, how much.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="memoriam esse signataru[m] rerum in mente vestigia">
            <language ident="LA">
