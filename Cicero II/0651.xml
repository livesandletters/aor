        <underline method="pen" type="straight" language="LA" text="an possit aut labefactari, aut eripi rebus"/>
        <underline method="pen" type="straight" language="LA" text="nonnunquam variari"/>
        <underline method="pen" type="straight" language="LA" text="Haec mihi videtur delicatior, vt ita dicam"/>
        <underline method="pen" type="straight" language="LA" text="Nicomachu[m], cuius accurate scripti de moribus libri, dicuntur illi quide[m] esse Aristotelis, sed non video cur non potuerit patri similis esse filius"/>
        <underline method="pen" type="straight" language="LA" text="est magnus"/>
        <underline method="pen" type="straight" language="LA" text="Lysias &amp; oratione locuples, rebus"/>
        <underline method="pen" type="straight" language="LA" text="Co[n]cinnus deinde &amp; elegans huius Aristo"/>
        <underline method="pen" type="straight" language="LA" text="Scripta san&egrave; &amp; multa &amp; polita, sed nescio quo pacto, authoritatem oratio non habet. Praetereo multos, in his doctum homine[m] &amp; suauem Hieronymum"/>
        <underline method="pen" type="straight" language="LA" text="quidem est grauitate proximus, &amp; redundat"/>
        <underline method="pen" type="straight" language="LA" text="patriis institutis manet"/>
        <underline method="pen" type="straight" language="LA" text="quoque suus est"/>
        <underline method="pen" type="straight" language="LA" text="Nam caeteris in rebus siue praetermissum, siue ignoratum est quippiam, non plus incommodi est qu&agrave;m quanti quaeque earum rerum est, in quibus neglectum est aliquid. Summum aute[m] bonum si ignoretur, viuendi rationem ignorari necesse est: ex quo tantus error consequitur, vt quem in portum se recipiant scire non possint"/>
        <underline method="pen" type="straight" language="LA" text="Negabat igitur vllam esse artem, quae ipsa &agrave; se proficisceretur"/>
        <underline method="pen" type="straight" language="LA" text="per se inuitaret &amp; alliceret appetitum"/>
        <underline method="pen" type="straight" language="LA" text="vltimum, fons reperiendus est in"/>
        <underline method="pen" type="straight" language="LA" text="prima inuitame[n]ta naturae"/>
        <underline method="pen" type="straight" language="LA" text="primum declinatu[m] dolore[m]"/>
        
        
