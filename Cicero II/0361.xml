        <marginalia hand="Italian" anchor_text="Quoniam igitur in generali constitutione tandiu propter eius partium multitudinem commoramur, ne forte varietate, dissimilitudine rerum deductus alicuius animus in quendam errorem deferatur">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        multitudo
                        varietas
                        dissimilitudo.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A great number; variety; difference.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                       nota.
                    </marginalia_text>
                </position>
            </language>
            <translation>Note.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text=".L. Licinius Crassus">
