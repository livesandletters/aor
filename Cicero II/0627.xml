        <marginalia hand="Italian" anchor_text="membroru[m], id est partium corporis, alia vide[n]tur propter eorum vsum &agrave; natura esse donata; alia autem nullam ob vtilitatem, quasi ad quendam ornatum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Membra humani corporis alia ad vsu[m], alia ad ornatu[m].</marginalia_text>
                </position>
            </language>
            <translation>The limbs of the human body: some for a use, others for decoration.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quum de rebus grandioribus dicas, ipsae res verba rapiunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cum de rebus grandioribus dicas, ip[sa]e res verba rapiunt.</marginalia_text>
                </position>
            </language>
            <translation>When you speak about grander things, the things themselves seize the words.</translation>
        </marginalia>
        
