        <underline method="pen" type="straight" language="LA" text="[alie]nari autem ab"/>
        <underline method="pen" type="straight" language="LA" text="nisi sensum habere[n]t"/>
        <underline method="pen" type="straight" language="LA" text="ascita natura diligamus"/>
        <underline method="pen" type="straight" language="LA" text="vtrumuis liceat, aptas malit &amp; integras omnes partes corporis, qu&agrave;m eodem vsu imminutas &amp; detortas habere"/>
        <underline method="pen" type="straight" language="LA" text="propter se asciscendas arbitramur, qu&ograve;d"/>
        <underline method="pen" type="straight" language="LA" text="intersit, siquid ratione per se ipsi inuenerunt. Artes"/>
        <underline method="pen" type="straight" language="LA" text="assumptione"/>
        <underline method="pen" type="straight" language="LA" text="Iam membroru[m], id est partium"/>
        <underline method="pen" type="straight" language="LA" text="vt ea quae sunt intus in"/>
        <underline method="pen" type="straight" language="LA" text="quasi ad quendam ornatum"/>
        <underline method="pen" type="straight" language="LA" text="Sunt enim quasi prima elementa naturae, quibus vbertas orationis adhiberi vix potest, nec equidem eam cogito consectari"/>
        <underline method="pen" type="straight" language="LA" text="Veruntamen quum de rebus grandioribus dicas, ipsae res verba rapiunt: ita fit quum grauior, tum etia[m] splendidior oratio"/>
        <underline method="pen" type="straight" language="LA" text="Sed tamen omne quod de re bona dilucide dicitur, mihi praeclare dici videtur"/>
        <underline method="pen" type="straight" language="LA" text="aute[m] res dicere ornate velle"/>
        <underline method="pen" type="straight" language="LA" text="aut ipsum secu[n]dum naturam sit, aut tale quid efficiat, vt selectione dignum propterea sit, quod aliquod pondus habeat dignum aestimatione"/>
        <underline method="pen" type="straight" language="LA" text="vt se co[n]seruet in naturae"/>
        <underline method="pen" type="straight" language="LA" text="selectione, &amp; item reiectione sequuntur"/>
        <underline method="pen" type="straight" language="LA" text="Prima est enim conciliatio hominis"/>
        <underline method="pen" type="straight" language="LA" text="vid&iacute;tque rerum age[n]darum ordinem, &amp; vt ita dicam, co[n]cordiam: multo"/>
        <underline method="pen" type="straight" language="LA" text="tame[n] id solum vi"/>
        <underline method="pen" type="straight" language="LA" text="naturae conciliationibus honesta"/>
        
