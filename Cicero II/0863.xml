    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="vim lachrymarum profudi">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>vim lachrymar[um] p[ro]fundere</marginalia_text>
                </position>
            </language>
            <translation>To pour out the strength of tears.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="fletu represso loqui posse coepi">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>fletu represso Loqui possi coepi</marginalia_text>
                </position>
            </language>
            <translation>To begin to be able to speak with surpressed weeping.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="certum esse in caelo, ac definitum locum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Certus in co[e]lo definitus&apos;q[ue] Locus quibus fuerit.</marginalia_text>
                </position>
            </language>
            <translation>A certain and definite place in the sky in which he was.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ciuitates">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ciuitates</marginalia_text>
                </position>
            </language>
            <translation>Societies.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animoru[m] i[m]mortalis</marginalia_text>
                </position>
            </language>
            <translation>the immortality of minds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quum haec est vita, vt Aphricanum audio dicere, quid moror in terris">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mori, viuere est.</marginalia_text>
                </position>
            </language>
            <translation>To die is to live.</translation>
        </marginalia>

        <marginalia hand="Italian" anchor_text="globum, quem in hoc te[m]plo medio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>globus in templo medio</marginalia_text>
                </position>
            </language>
            <translation>A sphere in the middle of the temple.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sydera">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sydera.</marginalia_text>
                </position>
            </language>
            <translation>Stars.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iustitia[m] cole &amp; pietatem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iustitia[m] cole &amp; pietatem.</marginalia_text>
                </position>
            </language>
            <translation>Foster justice and devotion.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Beatorum sedes</marginalia_text>
                </position>
            </language>
            <translation>The seats of the blessed.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="circus elucens, quem vos (vt &agrave; Graiis accepistis) orbem Lacteum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Lacteus circulus</marginalia_text>
                </position>
            </language>
            <translation>Milky Way.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Stellarum autem globi terrae magnitudinem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>stellaru[m] globi, terra[e] magnitudine[m] superant vincuntq[ue]</marginalia_text>
                </position>
            </language>
            <translation>The crowd of the stars is greater and defeats the greatness of the world.</translation>
        </marginalia>

        <marginalia hand="English_secretary" anchor_text="eae stellae">
            <language ident="EN">
                <position place="intext" book_orientation="0">
                    <marginalia_text>starse other</marginalia_text>
                </position>
            </language>
        </marginalia>

        <marginalia hand="Italian" anchor_text="humi defixa tua mens">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Mens humi defixa</marginalia_text>
                </position>
            </language>
            <translation>The mind of the soil is motionless.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Orbes novem.</marginalia_text>
                </position>
            </language>
            <translation>Nine worlds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mortale &amp; caducum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Quicquid hic est a[nim]is exceptis mortale &amp; caducu[m] est.</marginalia_text>
                </position>
            </language>
            <translation>Whatever is taken from the mind is mortal and perishable.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dulcis sonus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sonus spaha[e]raru[m]</marginalia_text>
                </position>
            </language>
            <translation>The sound of the spheres.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>firmame[n]ti acutus sonus</marginalia_text>
                </position>
            </language>
            <translation>A high-pitched sound from the firmament.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>iuiu&sharp-S;us</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>iuiu&sharp-S;us.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="prosperus &amp; salutaris">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>prosperus salutaris</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>videri possit dubu[m] q[uo]modo novem orbes tria suis sed terru[m]pro non supputat.</marginalia_text>
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>istis corporis costodijs liberari ex ho[m]i[nu][m] vita migrare corpore laxari</marginalia_text> <!-- FSF: not sure -->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="atque">
            <language ident="EN">
                <position place="intext" book_orientation="0">
                    <marginalia_text>thou</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Omnibus qui patriam co[n]seruauerint, adiuuerint, auxerint, certum esse in caelo, ac definitum locum, vbi beat aeuo sempiterno fruantur. 
            Nihil enim est illi principi Deo, qui omnem hunc mundum regit, quod quide[m] in terris fiat acceptius, qu&agrave;m concilia, coetusque hominum iure sociati, quae ciuitates appellantur. Harum 
            rectores &amp; conseruatores hinc profecti, huc reuerte[n]tur. Hic ego, etsi eram perterritus"/>
        <underline method="pen" type="straight" language="LA" text="Imm&ograve; ver&ograve;, inquit, hi viuunt qui ex corporum vinculis tanquam &egrave; carcere euolaure[n]t. Vestra ver&ograve; quae dicitur 
            vita, mors est"/>
        <underline method="pen" type="straight" language="LA" text="vim lachrymarum"/>
        <underline method="pen" type="straight" language="LA" text="fletu represso loqui"/>
        <underline method="pen" type="straight" language="LA" text="pater sanctissime atque optime"/>
        <underline method="pen" type="straight" language="LA" text="nisi enim quum deus is, cuius hoc templum est omne quod conspicis, istis te corporis custodiis liberauerit, hic tibi aditus patere non potest. 
            Homines enim sunt hac lege generati, qui tuerentur illum globum, quem in hoc te[m]plo medio vides, quae terra dicitur. H&iacute;sque animus datus est ex illis sempiternis ignibus, quae sydera &amp; 
            setellas vocatis, quae globosae &amp; rotundae diuinis animatae mentibus circulos suos orb&eacute;sque conficiunt celeritate mirabili. Quare &amp; tibi Publi, &amp; piis omnibus retinendus animus est 
            in custodia corporis: nec iniussu eius, &agrave; quo ille est vobis datus, ex hominum vita migrandum est, ne munus humanum assignatum &agrave; deo, defugisse videamini"/>
        <underline method="pen" type="straight" language="LA" text="iustitia[m] cole &amp; pietatem: quae quu[m] sit"/>
        <underline method="pen" type="straight" language="LA" text="ea vita via est in caelum, &amp; in hunc coetum eorum, qui iam vixerunt, &amp; corpore laxati illum incolunt locum quem vides"/>
        <underline method="pen" type="straight" language="LA" text="eae"/>
        <underline method="pen" type="straight" language="LA" text="quae vltima caelo, citima terris luce lucebat aliena. Stellarum autem globi terrae magnitudinem facil&egrave; vincebant"/>
        <underline method="pen" type="straight" language="LA" text="attingimus"/>
        <underline method="pen" type="straight" language="LA" text="quosque humi defixa tua mens erit? N&uacute;mne"/>
        <underline method="pen" type="straight" language="LA" text="quo sunt infixi illi, qui voluu[n]tur stellarum cursus sempiterni"/>
        <underline method="pen" type="straight" language="LA" text="VII, qui versantur retro contrario motu atque caelum"/>
        <underline method="pen" type="straight" language="LA" text="generi prosperis"/>
        <underline method="pen" type="straight" language="LA" text="Sol obtinet dux &amp; princeps, &amp; moderator luminum reliquorum, mens mundi &amp; temperatio, tanta magnitudine vt cuncta sua luce lustret 
            &amp; compleat. Hunc"/>
        <underline method="pen" type="straight" language="LA" text="Luna radiis solus accensa conuertitur"/>
        <underline method="pen" type="straight" language="LA" text="nisi mortale &amp; caducum, praeter animos generi hominum munere deorum datos. Supra lunam aterna omnia."/>
        <underline method="pen" type="straight" language="LA" text="nutu suo pondera"/>
        <underline method="pen" type="straight" language="LA" text="vt me recepi"/>
         <underline method="pen" type="straight" language="LA" text="qui interuallis coniunctus imparibus, sed tamen pro rata portione distinctis, impulsu &amp; motu ipsorum orbiu[m] efficitur, qui acuta cum"/>
        <underline method="pen" type="straight" language="LA" text="te[m]perans aequabiliter co[n]centus efficit, nec enim"/>
        <underline method="pen" type="straight" language="LA" text="summus ille caeli stelliferi cursus, cuius co[n]uersio est incitatior, acuto &amp; excitato mouetur"/>
        
