        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ho[m]i[nu]m ortus p[ro]creatio</marginalia_text>
                </position>
            </language>
            <translation>The birth and procreation of men.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="astro ingenerata &amp; tradita">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>astro ingenerata tradita</marginalia_text>
                </position>
            </language>
            <translation>Implanted and handed over to a star.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="linguae sic inhaererent">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>lingua inha[e]ret</marginalia_text>
                </position>
            </language>
            <translation>He is 'tongue-tied' [Loeb 154, 479].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="multi etiam naturae vitium meditatione atque exercitatione sustulerunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Natura[e] vitiu[m] meditatio[n]e atq[ue] exercitatione multi sustuleru[n]t</marginalia_text>
                </position>
            </language>
            <translation>'Many more have corrected a natural defect by intelligent exertion' [Loeb 154, 479].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ptoloma[e]us concedit libro. 2&o_super;</marginalia_text>
                    <person name="Ptolemy"/>
                </position>
            </language>
            <translation>Ptolemy conceded [this] in book two.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="plus terrarum situs, qu&agrave;m lunae ad nascendum valere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Plus terrar[u]m situs q[uam] &Moon; tractus ad nascendu[m] valit.</marginalia_text>
                    <symbol_in_text name="Moon"/>
                </position>
            </language>
            <translation>'Oneâ€™s birth is more affected by local environment than by the condition of the moon' [Loeb 154, 479].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Panaetius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pana[e]tius</marginalia_text>
                    <person name="Panaetius"/>
                </position>
