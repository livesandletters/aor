            <translation>Panaetius.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Excipiunt huiusmodi causas generales</marginalia_text>
                </position>
            </language>
            <translation>They receive the general causes of this kind.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="exitus">
