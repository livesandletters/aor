        <marginalia hand="Italian" anchor_text="ardentes cum cupiditate, tum metu, tum conscientia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ardentes { Cupiditate. Metu. Conscie[n]tia.</marginalia_text>
                </position>
            </language>
            <translation>Burning by desire, fear, [and] conscience.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="duplice[m] poenam esse diuinam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diuina po[e]na duplex</marginalia_text>
                </position>
            </language>
            <translation>Twofold divine punishment.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quid Dijs co[n]secrari debet: ex Platone</marginalia_text>
                    <person name="Plato"/>
                </position>
            </language>
            <translation>What ought to be consecrate to the Gods, from Plato.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="non solum scire aliquid artis est, sed quaedam ars etiam doce[n]di">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>H[om]o solu[m] scire aliquid artis est sed etiam qua[e]da[m] est docendi ars</marginalia_text>
                </position>
            </language>
            <translation>Man is only to know something of art, but also the art of teaching something.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="cum religione">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Religio</marginalia_text>
                </position>
            </language>
            <translation>Devotion.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="perparua sint: de sacris">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>perpetua sint sacra</marginalia_text>
                </position>
            </language>
            <translation>Perpetual things are sacred.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Haeredum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
