    <annotation>
        
       <marginalia hand="English_secretary" anchor_text="dialecticas captiones">
           <language ident="LA">
               <position place="head" book_orientation="0">
                   <marginalia_text>dialectic[a]e captio[n]es</marginalia_text>
               </position>
           </language>
           <translation>Quibbling dialectics.</translation>
       </marginalia> 
        
        <marginalia hand="English_secretary" anchor_text="misceat mulsum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>miscere mulsu[m]</marginalia_text>
                </position>
            </language>
            <translation>To mix honeyed wine.</translation>
        </marginalia> 
        
        <marginalia hand="English_secretary" anchor_text="dulciter ac iucunde">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Iucund&egrave;
                        dulciter ]
                        [deleted]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Sweetly; pleasantly.
            </translation>
        </marginalia> 
        
        <marginalia hand="English_secretary" anchor_text="ruit in dicendo">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ruit in dice[n]do</marginalia_text>
                </position>
            </language>
            <translation>He fails in speaking.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="nec laetantiu[m] nec dolentiu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ho[m]i[n]es nec la[e]tantes nec Dolentes.</marginalia_text>
                </position>
            </language>
            <translation>Men neither rejoicing nor grieving.</translation>
        </marginalia> 
        
        <marginalia hand="Italian" anchor_text="Rhetorice igitur, inquam, nos mauis qu&agrave;m dialectice disputare">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Rhetoric&egrave; &amp; dialectic&egrave; disputare</marginalia_text>
                </position>
            </language>
            <translation>To argue rhetorically and dialectically.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qu&ograve;d latius loquerentur Rhetores, Dialectici autem compressius">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Latius loquu[n]tur Rhetores 
                        Compressius Dialectici
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Rhetoricians speak more widely.
                Dialecticians [speak] in a more compressed way.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="quum populariter loquatur, esse interdum paulo hebetiorem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Rhetorica hebetior quu[m] populariter loquatur</marginalia_text>
                </position>
            </language>
            <translation>Rhetoric [is] blunter when spoken popularly.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Contra Epicuru[m] Dialectica[m] sperne[n]tem</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>Against Epicurus rejecting dialectics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="voluptatem tueretur, quam Aristippus, id est qua sensus dulciter ac iucunde mouetur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aristippi voluptas</marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>Pleasure according to Aristippus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aristoteles, qui virtutis vsum cum vitae perfectae prosperitate coniunxit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aristotelis foelicitas</marginalia_text>
                    <person name="Aristotle"/>
                </position>
            </language>
            <translation>Happiness according to Aristotle.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Callipho">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Callipho.</marginalia_text>
                    <person name="Calliphon"/>
                </position>
            </language>
            <translation>Calliphon.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Diodotus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Diodorus.</marginalia_text>
                    <person name="Diodorus of Tyre"/>
                </position>
            </language>
            <translation>Diodorus of Tyre.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Duae sunt enim res quoque, ne tu verba solum putes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Res: verba.</marginalia_text>
                </position>
            </language>
            <translation>Things. Words.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ab illo Aristippeo genere voluptatis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aristippeu[m] genus volupt.</marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>The Aristippean kind of pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Epicuri</marginalia_text>            
                    <person name="Epicurus"/>
                </position>
            </language>
            <language ident="EL">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text></marginalia_text> <!-- DO: Greek needed -->
                </position>
            </language>
            <translation></translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="alia sentit, inquam, alia loquitur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Alia se[n]tit, alia loquitur</marginalia_text>
                </position>
            </language>
            <translation>He feels one way, he speaks another.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="finibus">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>finis definitio</marginalia_text>
                </position>
            </language>
            <translation>End. Definition.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Graece egregie loquatur">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>gra[e]c&egrave; egregie loqui.</marginalia_text>
                </position>
            </language>
            <translation>To speak excellently in Greek.</translation>
        </marginalia>
        
