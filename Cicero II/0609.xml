        <underline method="pen" type="straight" language="LA" text="attinet luxuriosis vllam exceptionem dari, aut"/>
        <underline method="pen" type="straight" language="LA" text="asotos, prim&ugrave;m"/>
        <underline method="pen" type="straight" language="LA" text="edant de patella"/>
        <underline method="pen" type="straight" language="LA" text="narthecio"/>
        <underline method="pen" type="straight" language="LA" text="qui in mensam vomant"/>
        <underline method="pen" type="straight" language="LA" text="se rursus ingurgitent, qui solem, vt aiunt, neque occidentem vnquam viderint"/>
        <underline method="pen" type="straight" language="LA" text="nec orientem, qui consumptis patrimoniis egeant"/>
        <underline method="pen" type="straight" language="LA" text="Nemo nostru[m] istius generis asotos iucunde putat viuere, mundos, elegantes, optimis cocis, pistoribus, piscatu, aucupio, venatione, 
            his omnibus exquisitis vita[n]tis cruditate[m], quibus vinu[m] diffusum &egrave; pleno sit hirsiphon, vt ait Lucilius, cui nihil dum sit, ius &amp; sarculos abstulerit, adhibe[n]tis ludos, &amp; quae sequuntur illa, 
            quibus detractis clamat Epicurus se nescire quid sit bonu[m]. adsint etia[m] formosi pueri"/>
        <underline method="pen" type="straight" language="LA" text="qui ministrent, resplendeat iis vestis, argentu[m], odrilitiu[m] locus ipse, aedificiu[m]"/>
        <underline method="pen" type="straight" language="LA" text="vt cui cor sapiat, ei non sapiat palatus, sed"/>
        <underline method="pen" type="straight" language="LA" text="gumas"/>
        <underline method="pen" type="straight" language="LA" text="Coenasti in vita nunquam bene, quum omnia in isto Consumis, squilla, atque acipensere cum decumano"/>
        <underline method="pen" type="straight" language="LA" text="ita grauiter &amp; seuere"/>
        <underline method="pen" type="straight" language="LA" text="horto condito"/>
        <underline method="pen" type="straight" language="LA" text="Veniebat enim ad coenam, vt satiaret animo quieto desyderia"/>
        <underline method="pen" type="straight" language="LA" text="quod bene, id recte, frugaliter, honeste: ille porr&ograve; male, praue, nequiter, turpiter coenabat"/>
        <underline method="pen" type="straight" language="LA" text="igitur nec Lapathi suauitate[m] acipenseri Gallonii Laelius anteponebat"/>
        <underline method="pen" type="straight" language="LA" text="ineleganter"/>
        <underline method="pen" type="straight" language="LA" text="hoc est non diuidere, sed"/>
        <underline method="pen" type="straight" language="LA" text="Vitiosum est enim in diuide[n]do"/>
        
