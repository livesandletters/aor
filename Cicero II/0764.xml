            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ea quidem tota esse ignea duorum sensuu[m] testimonio co[n]firmari Cleanthes">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
