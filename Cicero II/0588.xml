        <underline method="pen" type="straight" language="LA" text="ne subarroga[n]ter"/>
        <underline method="pen" type="straight" language="LA" text="restitero Peripateticis"/>
        <underline method="pen" type="straight" language="LA" text="tam inter se amantes"/>
        <underline method="pen" type="straight" language="LA" text="plura enim vera discrepantia esse non possunt"/>
        <underline method="pen" type="straight" language="LA" text="Vtrum igitur nos impude[n]tes qui labi nolumus, an illi arrogantes"/>
        <underline method="pen" type="straight" language="LA" text="sibi persuaserint scire se solos omnia"/>
        <underline method="pen" type="straight" language="LA" text="Sed discedamus &agrave; nobismetipsis"/>
        <underline method="pen" type="straight" language="LA" text="quaero rationes eas quae ex coniectura pende[n]t, quae disputationibus huc &amp; illuc trahu[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="nullam adhibent persuadendi necessitatem"/>
        <underline method="pen" type="straight" language="LA" text="qui omnia vobis quae describunt probant"/>
        <underline method="pen" type="straight" language="LA" text="quibus non concessis digitu[m] progredi no[n] possunt"/>
        <underline method="pen" type="straight" language="LA" text="quod magnitudine[m] nullam"/>
        <underline method="pen" type="straight" language="LA" text="extremitatem &amp; quasi librame[n]tum"/>
        <underline method="pen" type="straight" language="LA" text="liniamento sine vlla latitudine"/>
        <underline method="pen" type="straight" language="LA" text="adiiciam iusiurandu[m]"/>
        <underline method="pen" type="straight" language="LA" text="efficitur multis partibus solem maiore[m]"/>
        <underline method="pen" type="straight" language="LA" text="quae vim afferunt in"/>
        <underline method="pen" type="straight" language="LA" text="Sed sit ingenio diuino"/>
        <underline method="pen" type="straight" language="LA" text="quaestiones infinitas: tantu[m] de principiis rerum, &egrave; quibus omnia constant"/>
        <underline method="pen" type="straight" language="LA" text="populari &amp; sodali suo non"/>
        <underline method="pen" type="straight" language="LA" text="infinitatem naturae"/>
        <underline method="pen" type="straight" language="LA" text="sed ex ea particulas similes inter se minutas, eas primum"/>
        <underline method="pen" type="straight" language="LA" text="neque id esse mutabile, &amp; id esse deum: neque natum vsquam quicquam &amp; sempiternum conglobata figura"/>
        <underline method="pen" type="straight" language="LA" text="plenu[m] &amp; inane"/>
        <underline method="pen" type="straight" language="LA" text="vberior in"/>
        <underline method="pen" type="straight" language="LA" text="haec peruulgata, &amp; nota quatuor"/>
        <underline method="pen" type="straight" language="LA" text="Plato ex materia in se omnia recipiente mundum esse factum censet &agrave; Deo sempiternum"/>
        <underline method="pen" type="straight" language="LA" text="caeteri tot viri &amp; tanti repudiati ab eo condemnat&iacute;que"/>
        <underline method="pen" type="straight" language="LA" text="habere me[n]tem, quae &amp; se &amp; ipsum fabricata sit, &amp; omnia moderetur, moueat, regat"/>
        <underline method="pen" type="straight" language="LA" text="persuasum etiam solem, lunam, stellas omnes, terram, mare, deos esse"/>
        <underline method="pen" type="straight" language="LA" text="animalis intellige[n]tia per omnia ea permeet &amp; transeat: fore tame[n] aliqua[n]do vt omnis hic mundus ardore deflagret. Sint"/>
        
