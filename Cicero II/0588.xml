        <marginalia hand="Italian" anchor_text="mundus ardore deflagret">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Mundus ardore deflagrabit tandem.</marginalia_text>
                </position>
            </language>
