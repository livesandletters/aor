        <marginalia hand="Italian" anchor_text="sapie[n]te">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapiens.</marginalia_text>
                </position>
            </language>
            <translation>A wise person.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="In tres igitur partes &amp; &agrave; plerisque &amp; &agrave; nobismetipsis distributa sapientia est">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapientia in tres est partes distributa.</marginalia_text>
                </position>
            </language>
            <translation>Wisdom is separated into three parts.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Geometrae">
            <language ident="LA">
