        <underline method="pen" type="straight" language="LA" text="qui fulcire putatur porticu[m] Stoicorum"/>
        <underline method="pen" type="straight" language="LA" text="in co[n]suetudine proba[n]tur"/>
        <underline method="pen" type="straight" language="LA" text="certe tam multa no[n] collegisset, quae nos fallere[n]t probabilitate magna"/>
        <underline method="pen" type="straight" language="LA" text="nisi videret his"/>
        <underline method="pen" type="straight" language="LA" text="n&oacute;nne putare[m] post illos veteres tot seculis inueniri verum potuisse, tot ingeniis, tant&iacute;sque studiis quaere[n]tibus"/>
        <underline method="pen" type="straight" language="LA" text="nec solum posse, sed ita necesse esse sapienti, visa est Arcesilae cum"/>
        <underline method="pen" type="straight" language="LA" text="ex eo quod esset sicut esset impressum &amp; signatum &amp; effectum"/>
        <underline method="pen" type="straight" language="LA" text="falsum. Incubuit aute[m] in eas disputationes"/>
        <underline method="pen" type="straight" language="LA" text="no[n] eiusde[m]modi"/>
        <underline method="pen" type="straight" language="LA" text="hoc magis ab eo disputatu[m], qu&agrave;m probatum puto"/>
        <underline method="pen" type="straight" language="LA" text="idcirco heri no[n] necessario loco co[n]tra sensus tam multa"/>
        <underline method="pen" type="straight" language="LA" text="infracto remo neque columbae collo co[m]moueri"/>
        <underline method="pen" type="straight" language="LA" text="lacerat ista causa"/>
        <underline method="pen" type="straight" language="LA" text="confidere suis testibus, &amp; importune insistere"/>
        <underline method="pen" type="straight" language="LA" text="quum oculum torsisset"/>
        <underline method="pen" type="straight" language="LA" text="opinionis enim esse mendaciu[m], no[n]"/>
        <underline method="pen" type="straight" language="LA" text="Desine quaeso co[m]munibus locis, domi nobis ista nascuntur"/>
        <underline method="pen" type="straight" language="LA" text="Vt enim vera videamus"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m longe videbimus"/>
        <underline method="pen" type="straight" language="LA" text="sed intendi acies longius non potest"/>
        <underline method="pen" type="straight" language="LA" text="O praeclarum prospectum"/>
        <underline method="pen" type="straight" language="LA" text="audacter isti vestro deo, me"/>
        <underline method="pen" type="straight" language="LA" text="sub oculis sunt"/>
        <underline method="pen" type="straight" language="LA" text="neque ipsi nos suspicere possunt"/>
        <underline method="pen" type="straight" language="LA" text="nobis aer crassus offunditur"/>
        
