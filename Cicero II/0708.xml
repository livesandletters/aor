        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        obdurescere callu[m] obduci
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To be persistent, to cover with a hard feeling.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="opinionis esse, non naturae malum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Malum opinionis natura
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bad of opinion by nature.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
