            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="stellae natura flammeae: quocirca terrae, maris, aquarum vaporibus aluntur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
