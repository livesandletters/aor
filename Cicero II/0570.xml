        <underline method="pen" type="straight" language="LA" text="quin contra suum doctorem librum"/>
        <underline method="pen" type="straight" language="LA" text="Heraclitu[m] studiose audirem"/>
        <underline method="pen" type="straight" language="LA" text="dedi Antiocho operam"/>
        <underline method="pen" type="straight" language="LA" text="secundum fratrem plurimu[m]"/>
        <underline method="pen" type="straight" language="LA" text="multu[m] temporis in ista vna disputatione"/>
        <underline method="pen" type="straight" language="LA" text="ista quae sunt heri defensa"/>
        <underline method="pen" type="straight" language="LA" text="mentitur, tamen est aduersarius lenior"/>
        <underline method="pen" type="straight" language="LA" text="aliquos ex antiquis claros viros proferunt, quos dicant fuisse populares"/>
        <underline method="pen" type="straight" language="LA" text="similes esse videantur"/>
        <underline method="pen" type="straight" language="LA" text="vero sapientissimos"/>
        <underline method="pen" type="straight" language="LA" text="philosophia[m] bene iam constituta[m]"/>
        <underline method="pen" type="straight" language="LA" text="Arcesilae calumnia conferenda"/>
        <underline method="pen" type="straight" language="LA" text="tamen isti physici raro admodum, quum haerent, aliquo loco exclamant, quasi mente incitati: Empedocles quide[m], vt interdum mihi furere videatur, 
            abstrusa esse"/>
        <underline method="pen" type="straight" language="LA" text="sentire, nihil"/>
        <underline method="pen" type="straight" language="LA" text="Qu&ograve;d si illi tum in nouis rebus, quasi modo nascentes haesitauerunt, nih&iacute;lne"/>
        <underline method="pen" type="straight" language="LA" text="seculis, summis ingeniis, maximis studiis explicatum putamus? N&oacute;nne"/>
        <underline method="pen" type="straight" language="LA" text="qui co[n]stitutam philosophia[m]"/>
        <underline method="pen" type="straight" language="LA" text="eorum authoritate delitesceret, qui negauissent quicquam sciri, aut percipi posse"/>
        <underline method="pen" type="straight" language="LA" text="Peripateticos &amp; Academicos nominibus differentes, re congruentes"/>
        <underline method="pen" type="straight" language="LA" text="verbis magis, qu&agrave;m sententiis dissenserunt"/>
        <underline method="pen" type="straight" language="LA" text="ita quum aliud diceret atque sentiret"/>
        <underline method="pen" type="straight" language="LA" text="libenter vti solitus est ea dissimulatione, quam Graeci voca[n]t"/>     <!-- Greek needed -->
        <underline method="pen" type="straight" language="LA" text="Africano fuisse Fannius: &iacute;dque propterea vitiosum in illo non"/>
        <underline method="pen" type="straight" language="LA" text="fuerit in"/>
        <underline method="pen" type="straight" language="LA" text="Sed fuerint illa vetera, si vultis, incognita"/>
        <underline method="pen" type="straight" language="LA" text="clarissimis rebus tenebras obducere"/>
        <underline method="pen" type="straight" language="LA" text="quanquam floruit cum acumine ingenii, tum admirabili quodam lepore"/>
        <underline method="pen" type="straight" language="LA" text="admodu[m] florueru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="multitudo libroru[m] ingenii no[n] minus in hoc, qu&agrave;m in Carneade eloque[n]tiae"/>
        <underline method="pen" type="straight" language="LA" text="Rhodio suauitatis"/>
        
