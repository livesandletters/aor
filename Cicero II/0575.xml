            </language>
            <translation>To expand.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="prim&ugrave;m; deinde; tum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        p[ri]mu[m]
                        deinde
                        tu[m].
                    </marginalia_text>
                </position>
            </language>
