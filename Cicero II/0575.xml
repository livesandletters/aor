        <marginalia hand="Italian" anchor_text="Eorum quae videntur alia vera sunt, alia falsa">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Eoru[m] qua[e] vide[n]tur alia vera alia falsa.</marginalia_text>
                </position>
            </language>
            <translation>Some of them seem true and others false.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="praeteruolant">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>p[rae]tervolare</marginalia_text>
                </position>
            </language>
