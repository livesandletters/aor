        <marginalia hand="English_secretary" anchor_text="Diuidunt enim in partes, &amp; eas quide[m] magnas; conc&iacute;dunt etia[m] minutius; in minim dispertiunt">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        diuidere in magnas partes.
                        co[n]cidere minutius.
                        dispertire in minima.
                        secare.
                        partiri.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To divide in large parts.
                To break down into smaller parts.
                To distribute in the smallest parts.
                To cut up.
                To part.
            </translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="omnin&oacute;que ant&egrave; videri aliquid qu&agrave;m agamus"/>
        <underline method="pen" type="straight" language="LA" text="rationis quasi fundamenta cognoscere. Co[m]ponunt"/>
        <underline method="pen" type="straight" language="LA" text="quasi contineant omne[m] hanc quaestione[m]"/>
        <underline method="pen" type="straight" language="LA" text="Quibus positis, vnius argumenti co[n]clusione, tota ab his causa comprehe[n]ditur. Composita aute[m] ea conclusio"/>
        <underline method="pen" type="straight" language="LA" text="Quae aute[m] sumu[n]t vt co[n]cluda[n]t id quod volunt, ex his duo sibi puta[n]t co[n]cedi"/>
        <underline method="pen" type="straight" language="LA" text="multa &amp; varia oratione defendunt"/>
        <underline method="pen" type="straight" language="LA" text="proposita non praeteruolant, sed ita dilatant"/>
        <underline method="pen" type="straight" language="LA" text="quam obscurari"/>
        <underline method="pen" type="straight" language="LA" text="conc&iacute;dunt etia[m] minutius"/>
        <underline method="pen" type="straight" language="LA" text="in minima dispertiunt"/>
        <underline method="pen" type="straight" language="LA" text="ego subtilitatem"/>
        <underline method="pen" type="straight" language="LA" text="Definitiones enim &amp; partitiones, horu[m] luminibus vtens oratio, tum similitudines dissimilitudin&eacute;sque, &amp; earum tenuis &amp; acuta distinctio, fidnetiu[m] est hominu[m]. illa vero &amp; firma"/>
        <underline method="pen" type="straight" language="LA" text="quid enim"/>
        <underline method="pen" type="straight" language="LA" text="se dilucide perspicere, nec"/>
        <underline method="pen" type="straight" language="LA" text="co[m]munione visorum"/>
        <underline method="pen" type="straight" language="LA" text="argume[n]tum non potest, nisi his quae ad concludendu[m] sumpta erunt ita probatis, vt falsa eiusdemmodi nulla possint esse. Ergo"/>
        <underline method="pen" type="straight" language="LA" text="quid potest reperiri quod ipsum sibi repugnet magis"/>
        <underline method="pen" type="straight" language="LA" text="aliquid patefacturam"/>
        <underline method="pen" type="straight" language="LA" text="non tam esse qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="conuincuntur, quum"/>
        <underline method="pen" type="straight" language="LA" text="congruentibus sumunt tam veheme[n]ter repugnantia"/>
        <underline method="pen" type="straight" language="LA" text="nobis assentati"/>
        
        
