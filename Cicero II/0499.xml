        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="Quid igitur, inquit, est causae">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>quid est caus[a]e, cur.</marginalia_text>
                </position>
            </language>
            <translation>What is of the cause; why.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="peringeniosis hominibus neque satis doctis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>p[er]ingeniosus neq[ue] satis doctus</marginalia_text>
                </position>
            </language>
            <translation>Very intelligent and not educated enough.</translation>
        </marginalia>
        
        
