        <marginalia hand="Italian" anchor_text="vectores">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        vectores
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Passengers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pythagoras">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Pythagoras
                    </marginalia_text>
                    <person name="Pythagoras"/>
                </position>
            </language>
            <translation>
                Pythagoras.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fortunam &agrave; deo petendam, &agrave; seipso sumendam esse sapientiam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fortuna &agrave; deo petenda &agrave; nobis sumenda sapientia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                They must pray to god for fortune and obtain wisdom by themselves.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Improborum igitur prosperitates secund&aacute;eque res redargunt, vt Diogenes dicebat, vim omnem deorum ac potestatem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Improboru[m] prosperitate secund[a]eq[ue] res Deorum vedargiunt vim et potest[atem]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                'The prosperity and good fortune of the wicked [...] disprove the might and power entirely' [LCL 268:374-75].
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Diagoras quum Samothraciam venisset, Atheos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Diagoras Atheos
                    </marginalia_text>
                    <person name="Diagoras of Melos"/>
                </position>
            </language>
            <translation>
                Diagoras of Melos from Athens.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ex tot tabulis pictis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Tabula[e] picta[e].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Written tables.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        sic nos
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Thus, us.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Non animaduertunt, inquit, omnia dii">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Dij no[n] o[mn]ia animadu[er]tunt
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The gods do not pay attention to everything.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="expetantur eae poenae &agrave; liberis, ac nepotibus ac posteris">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Poena[e] expetu[n]tur a liberis. &agrave; nepotibus, &agrave; posteris
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Children expect punishment from their relatives and the elders.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Tantalidaru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Tantalida[e].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Tantalids.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Myrtili">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Myrtilus
                    </marginalia_text>
                    <person name="Myrtilus"/>
                </position>
            </language>
            <translation>
                Myrtilus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Hipponactis iambus laeserat, aut qui erat Archilochii">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Hipponax. Archilochus
                    </marginalia_text>
                    <person name="Hipponax"/>
                    <person name="Archilochus"/>
                </position>
            </language>
            <translation>
                Hipponax, Archilochus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="AEgisthi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        &AElig;gisthus.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Aegisthus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Paridis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Paris
                    </marginalia_text>
                    <person name="Paris of Troy"/>
                </position>
            </language>
            <translation>
                Paris of Troy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Critolaus, inquam, euertit Corinthu[m], Carthadinem Hasdrubal">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Critolaus. Hasdrubal
                    </marginalia_text>
                    <person name="Critolaus"/>
                    <person name="Hasdrubal II"/>
                </position>
            </language>
            <translation>
                Critolaus, Hasdrubal II.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="irasci posse negatis deum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Deus irasci non p[otes]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God cannot be made angry.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nihil  esse quod deus efficere non possit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Deus o[mn]ipotens est
                    </marginalia_text>
                </position>
            </language>
            <translation>
                God is omnipotent.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Materia enim ex qua &amp; in qua omnia sint">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Rerum Materia ex qua &amp; in qua o[mn]ia sunt flexibilis &amp; co[m]mutabilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The material of which and in which everything is mae, is flexible and changeable.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Contra Dei prouide[n]tiam.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Against divine providence.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="somnia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Somnia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Dream.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Vota.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Vows.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Fac esse distentam, coelum versantur terram tuentem, maria moderantem, cur tam multos deos nihil agere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mens Diuina distenta terra[m] tuens, mare moderatur maximu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The divine mind is distracted, watching the earth and controlling the seas
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        sup[er]stitiose familiter.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Superstitiously, in a familiar manner.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        versu vulneratus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Wounded by destruction.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="opulentos, copiosos"/>
        <underline method="pen" type="straight" language="LA" text="Improborum igitur prosperitates secund&aacute;eque res redarguunt, 
            vt Diogenes dicebat, vim omnem deorum ac potestatem. At no[n]nunquam bonos exitus habe[n]t boni: eos quidem arripimus attribuim&uacute;sque sine"/>
        <underline method="pen" type="straight" language="LA" text="vlla ratione diis immortalibus. At"/>
        <underline method="pen" type="straight" language="LA" text="ex tot tabulis pictis qu&agrave;m"/>
        <underline method="pen" type="straight" language="LA" text="vectores"/>
        <underline method="pen" type="straight" language="LA" text="vt ad prosperam aduers&aacute;mve fortuna[m] qualis sis, aut que[m]admodum vixeris, nihil intersit"/>
        <underline method="pen" type="straight" language="LA" text="etia[m] siquis morte poenas sceleris effugerit, expetantur eae poenae &agrave; liberis, ac nepotibus ac posteris. O"/>
        <underline method="pen" type="straight" language="LA" text="satietas supplicii"/>
        <underline method="pen" type="straight" language="LA" text="Portenta enim ab vtrisque &amp; flagitia dicuntur, &agrave; deo immissum dolorem non conceptum &agrave; seipso continebat"/>
        <underline method="pen" type="straight" language="LA" text="quum culpae pen&egrave;"/>
        <underline method="pen" type="straight" language="LA" text="illos oculos orae maritimae effoderunt"/>
        <underline method="pen" type="straight" language="LA" text="hominum membra nulla contentione, mente ipsa ac voluntate moueantur, sic numine deorum omnia fingi, moueri, 
            mutar&iacute;que posse. Neque id dicitis superstitiose atque aniliter, sed physica constant&iacute;que"/>
        <underline method="pen" type="straight" language="LA" text="Materiam enim rerum ex qua &amp; in qua omnia sint, totam esse flexibilem &amp; commutabilem, 
            vt nihil sit quod non ex ea quanuis subito fingi conuert&iacute;que possit"/>
        <underline method="pen" type="straight" language="LA" text="Eius autem vniuersae fictricem &amp; moderatricem diuinam esse prouidentiam. Hanc igitur quocunque se moueat, efficere posse quidquid velit. Itaque"/>
        <underline method="pen" type="straight" language="LA" text="de somniorum veritate sententia, atque iidem"/>
        <underline method="pen" type="straight" language="LA" text="etiam vota suscipi dicitis oportere"/>
        <underline method="pen" type="straight" language="LA" text="esse distentam, coelum versantem, terram tuentem, maria moderantem, cur"/>
        
