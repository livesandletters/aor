            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Timor</marginalia_text>
                </position>
            </language>
            <translation>Fear.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Apollo, qui vmbilicum certum terrarum obsides">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Apollo umbilicu[m] terra[e] obsidet</marginalia_text>
                </position>
            </language>
            <translation>Apollo takes possession of the earth's core.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Croesus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cro[e]sus</marginalia_text>
                    <person name="Croesus"/>
                </position>
            </language>
            <translation>Croesus.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="oraculis Chrysippus totum volumen impleuit, partim falsis, vt ego opinor, partim vasu veris, vt fit in omni oratione saepissime: 
            partim flexiloquis &amp; obscuris, vt interpres egeat interprete, &amp; sors ipsa ad sortes referenda sit: ambiguis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>oracula falsa casu vera flexiloqua obscura ambigua</marginalia_text>
                </position>
            </language>
            <translation>False, accidentally true, equivocal, obscure, and ambiguous prophecies.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>op[er]tis</marginalia_text> <!--JG: Unsure-->
                </position>
            </language>
            <translation>Hidden.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>partim ficta ap[er]te, partim effutita temere</marginalia_text>
                </position>
            </language>
            <translation>Partly openly feigned, partly rashly blurted out.</translation>
        </marginalia>
        
