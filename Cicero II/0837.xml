    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="fulguratores">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>fulguratores</marginalia_text>
                </position>
            </language>
            <translation>Priests who interpreted omens from lightning.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="hominum fama">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>ho[m]i[nu]m fama.</marginalia_text>
                </position>
            </language>
            <translation>The fame of men.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Adhibuit etiam latebram obscuritatis">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>adhibere latebra[m] obscuritatis</marginalia_text>
                </position>
            </language>
            <translation>'To employ a maze of obscurity' [LCL 154, 497].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="varietas quae est propria fortunae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Varietas fortuna[e] p[ro]p[ri]a</marginalia_text>
                </position>
            </language>
            <translation>The particular variety of fortune.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="aliquid sicut praedictum sit euaserit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>aliquid vere p[rae]dictum est.</marginalia_text>
                </position>
            </language>
            <translation>Something is foretold truly.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="furor iste, quem diuinum vocatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Diuninus furor.</marginalia_text>
                </position>
            </language>
            <translation>Divine madness.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Regem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>forsa[n] de CHR[IST]O</marginalia_text>
                    <person name="Jesus Christ"/>
                </position>
            </language>
            <translation>Perhaps on Christ.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Callide enim qui illa composuit perfecit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Callide sybylla scipsit.</marginalia_text>
                    <person name="Sibyl"/>
                </position>
            </language>
            <translation>The Sibyl wrote skilfully.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="praedictum videretur hominum &amp; temporum definitione sublata">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ho[m]i[nu]m temporu[m]q[ue] Definitio addatur p[rae]dictis</marginalia_text>
                </position>
            </language>
            <translation>The end of men and times is also said in the predictions.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="ἀκροστιχίς">
            <language ident="EL">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>ἀκροστιχίς</marginalia_text>
                    <book title="Sibylline oracles"/>
                </position>
            </language>
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>sibylla[e]</marginalia_text>
                    <person name="Sibyl"/>
                </position>
            </language>
            <translation>The acrostic poem of Sibyl.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Cassandra">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cassandra.</marginalia_text>
                    <person name="Cassandra"/>
                </position>
            </language>
            <translation>Cassandra.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="fabulis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Fabula[e]</marginalia_text>
                </position>
            </language>
            <translation>Fables.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="exercitus Caesaris">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ca[e]saris exercitus</marginalia_text>
                    <person name="Julius Caesar"/>
                </position>
            </language>
            <translation>Caesar's army.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="praelii nemo nostrum erat quin timeret, sed ita vt constantibus hominibus par erat, non aperte">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Casus p[rae]lij nemimi co[n]sta[n]ti timendus aperte est</marginalia_text>
                </position>
            </language>
            <translation>The plight of the battle must be feared openly by no one who is steadfast.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="timeret">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Timor</marginalia_text>
                </position>
            </language>
            <translation>Fear.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Apollo, qui vmbilicum certum terrarum obsides">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Apollo vmbilicu[m] terra[e] obsidet</marginalia_text>
                </position>
            </language>
            <translation>Apollo takes possession of the earth's core.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Croesus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cro[e]sus</marginalia_text>
                    <person name="Croesus"/>
                </position>
            </language>
            <translation>Croesus.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="oraculis Chrysippus totum volumen impleuit, partim falsis, vt ego opinor, partim casu veris, vt fit in omni oratione saepissime: 
            partim flexiloquis &amp; obscuris, vt interpres egeat interprete, &amp; sors ipsa ad sortes referenda sit: ambiguis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>oracula falsa casu vera flexiloqua obscura ambigua</marginalia_text>
                </position>
            </language>
            <translation>False, accidentally true, equivocal, obscure, and ambiguous prophecies.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="opertis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>op[er]tis</marginalia_text>
                </position>
            </language>
            <translation>Hidden.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="partim ficta aperte, partim effutita temere">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>partim ficta ap[er]te, partim effutita temere</marginalia_text>
                </position>
            </language>
            <translation>Partly openly feigned, partly rashly blurted out.</translation>
        </marginalia>
        
