        <marginalia hand="English_secretary" anchor_text="fulguratores">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>fulguratores</marginalia_text>
                </position>
            </language>
            <translation>Priests who interpreted omens from lightning.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="hominum fama">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>homi[num] fama.</marginalia_text>
                </position>
            </language>
            <translation>The fame of men.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="Adhibuit etiam latebram obscuritatis">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>adhibere latebra[m] obscuritatis</marginalia_text>
                </position>
            </language>
            <translation>To employ a maze of obscurity [154, 497].</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="varietas quae est propria fortunae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Varietas fortuna[e] p[ro]p[i]a</marginalia_text>
                </position>
            </language>
            <translation>the particular variety of fortune.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="aliquid sicut oraedictum sit euaserit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>aliquid vere p[rae]dictum est.</marginalia_text>
                </position>
            </language>
            <translation>Something is actually foretold.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="furor iste, quem diuinum vocatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Diuninus furor.</marginalia_text>
                </position>
            </language>
