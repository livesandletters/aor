        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Sapientia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sapie[n]tia[e] vis.</marginalia_text>
                </position>
            </language>
            <translation>The force of wisdom.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cupiditates enim sunt insatiabiles">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cupiditates insatiabiles</marginalia_text>
                </position>
            </language>
            <translation>Insatiable desires.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quae est enim aut vtilior, aut ad bene viuendum aptior partitio, qu&agrave;m illa qua est vsus Epicurus?">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cupiditatu[m] partitio secu[n]du[m] Epicurum.</marginalia_text>
                    <person name="Epicurus"/>
                </position>
            </language>
            <translation>The division of desires according to Epicurus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="natura diuitias, quibus contenta sit, &amp; parabiles &amp; terminatas habet">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Natura[e] diuitia[e] parabiles et terminata[e].</marginalia_text>
                </position>
            </language>
            <translation>The obtainable and limited riches of nature.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sapienti&aacute;mque">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sapientia</marginalia_text>
                </position>
            </language>
            <translation>Wisdom.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="insipientiam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Insipientia.</marginalia_text>
                </position>
            </language>
            <translation>Foolishness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Te[m]perantia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Tempera[n]tia. 2</marginalia_text>
                </position>
            </language>
            <translation>Temperance, 2.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="stare etia[m] oportet">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>stare oportet.</marginalia_text>
                </position>
            </language>
            <translation>It is right to stay.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="victi &amp; debilitati obiecta specie voluptatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>victi et debilitati obiecta spe[cie] voluptatis.</marginalia_text>
                </position>
            </language>
            <translation>Intentions overcome and weakened by the spectacle of pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="voluptatem maxima[m] adipiscuntur praetermitte[n]da voluptate">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>voluptate[m] maxima[m] adipiscuntur pra[e]termitte[n]da volup.</marginalia_text>
                </position>
            </language>
            <translation>They obtain the greatest pleasure by forgoing pleasure.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fortitudinis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>fortitudo 3.</marginalia_text>
                </position>
            </language>
            <translation>Fortitude, 3.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="industria">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Industria</marginalia_text>
                </position>
            </language>
            <translation>Industry.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="debilitate[m] animi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animi debilitas.</marginalia_text>
                </position>
            </language>
            <translation>A weakening of the mind.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="robustus animus &amp; excelsus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animus robustus et excelsus.</marginalia_text>
                </position>
            </language>
            <translation>A robust and elevated mind.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nec timiditate[m] ignaui&aacute;mque vituperari, nec fortitudine[m] patienti&aacute;mque laudari">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Timiditas. Ignauia.
                        fortitudo. patie[n]tia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Timidity. Idleness. Fortitude. Palience.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Iustitia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iustitia 4.</marginalia_text>
                </position>
            </language>
            <translation>Justice, 4.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="tranquillet animos">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>tranquillare a[n]i[m]os</marginalia_text>
                </position>
            </language>
            <translation>To calm minds.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="quae moestitiam"/>
        <underline method="pen" type="straight" language="LA" text="exhorrescere metu non sinat"/>
        <underline method="pen" type="straight" language="LA" text="praeceptrice in tranquillitate viui potest"/>
        <underline method="pen" type="straight" language="LA" text="omnium cupiditatu[m] ardore restincto"/>
        <underline method="pen" type="straight" language="LA" text="Cupiditates enim sunt insatiabiles, quae non modo singulos homines, sed vniuersas familias euertunt, tota[m] etiam labefacta[n]t saepe remp."/>
        <underline method="pen" type="straight" language="LA" text="Ex cupiditatibus, odia, dissidia, discordiae, seditiones, bella nascu[n]tur. Nec"/>
        <underline method="pen" type="straight" language="LA" text="foris solum iactant, nec tantum in alios caeco impetu incurrunt, sed intus etiam in animis inclusae inter se dissident atque discordant, 
            ex quo vitam amarissima[m] necesse est effici: vt sapiens solum amputata circuncis&aacute;que inanitate omni &amp; errore, naturae finibus co[n]tentus sine aegritudine possit &amp; sine metu viuere"/>
        <underline method="pen" type="straight" language="LA" text="opera multa nec impensa expleantur"/>
        <underline method="pen" type="straight" language="LA" text="ipsa natura diuitias, quibus contenta sit, &amp; parabiles &amp; terminatas habet"/>
        <underline method="pen" type="straight" language="LA" text="autem cupiditatum nec modus vllus, nec finis inueniri potest. Qu&ograve;d si vitam omne[m] perturbari videmus errore &amp; inscitia, 
            sapienti&aacute;mque esse solam quae nos &agrave; libidinu[m] impetu &amp; formidinu[m] terrore vindicet, &amp; ipsius fortunar modice ferre doceat iniurias, 
            &amp; omnes doceat vias quae ad quietem &amp; tranquillitate[m] ferant: quid est cur dubitemus dicere &amp; sapientiam propter voluptatem expete[n]dam, &amp; insipientiam propter molestias esse fugiendam"/>
        <underline method="pen" type="straight" language="LA" text="Te[m]perantia est enim quae in rebus aut expete[n]dis aut fugiendis, vt ratione[m] sequamur, monet"/>
        <underline method="pen" type="straight" language="LA" text="sed stare etia[m] oportet"/>
        <underline method="pen" type="straight" language="LA" text="non possunt, victi &amp; debilitati obiecta specie voluptatis tradunt se libidinibus constringendos, nec quid prouenturum sit, prouident"/>
        <underline method="pen" type="straight" language="LA" text="dolore, tum in morbos graues"/>
        <underline method="pen" type="straight" language="LA" text="tum in damna, tum in dedecora incurru[n]t, saepe etiam legum iudicior&uacute;mque poenis obligantur"/>
        <underline method="pen" type="straight" language="LA" text="voluptatem maxima[m] adipiscuntur praetermitte[n]da voluptate. Iidem etia[m] dolorem saepe perpetiuntur, ne si id no[n] faciant, incidant in maiorem. Ex"/>
        <underline method="pen" type="straight" language="LA" text="laborum perfunctio, neque perpessio doloru[m] per se ipsa allicit, nec patie[n]tia, nec assiduitates"/>
        <underline method="pen" type="straight" language="LA" text="nec vigiliae, nec ea ipsa quae laudatur industria"/>
        <underline method="pen" type="straight" language="LA" text="Vt enim mortis metu omnis quietae vitae status perturbatur, &amp; vt succu[m]bere doloribus"/>
        <underline method="pen" type="straight" language="LA" text="e&oacute;que humili animo imbecill&oacute;que ferre miseru[m] est, ob e&aacute;mq[ue] debilitate[m] animi multi pare[n]tes reliqueru[n]t, 
            multi amicos, no[n]nulli patria[m], plerique aute[m] seipsos penitus perdideru[n]t"/>
        <underline method="pen" type="straight" language="LA" text="sic"/>
        <underline method="pen" type="straight" language="LA" text="omni est liber cura, &amp; rigore animi etia[m] morte[m] conte[m]nit: qua qui affecti sunt,
            in eade[m] causa sunt qua antequa[m] nati: &amp; ad dolores ita paratus est"/>
        <underline method="pen" type="straight" language="LA" text="meminerit maximos morte finiri, paruos multa habere interualla requietis: mediocrium nos esse dominos, 
            vt si tolerabiles sint feramus: sin minus, aequo animo &egrave; vita, quu[m] ea no[n] placeat, ta[n]qua[m] &egrave; theatreo exeamus. Quibus"/>
        <underline method="pen" type="straight" language="LA" text="fortitudine[m] copulatas esse docui"/>
        <underline method="pen" type="straight" language="LA" text="modo nec diuelli nec distrahi"/>
        <underline method="pen" type="straight" language="LA" text="quod tranquillet animos, tum spe nihil earu[m] rerum defuturu[m] quas natu[ra]"/>
        
