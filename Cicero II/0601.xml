        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="praeceptrice">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>pra[e]ceptrix.</marginalia_text>
                </position>
            </language>
