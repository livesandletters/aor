    <annotation>
        
        <marginalia hand="Italian" anchor_text="summissum &amp; abiectum">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>summissus abiectus</marginalia_text>
                </position>
            </language>
            <translation>Quiet. Humble.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="se effere[n]tem">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>se effere[n]s dedecus co[n]cipere.</marginalia_text>
                </position>
            </language>
            <translation>Busying themselves to contain disgrace.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="relaxare animos, &amp; dare se iucunditati">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>relaxare a[n]i[m]os dare se iucunditati.</marginalia_text>
                </position>
            </language>
            <translation>To relaxe the minds, to give themselves to pleasantness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="languori se desidia&eacute;que">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>languori desidi[a]e } dedere se</marginalia_text>
                </position>
            </language>
            <translation>To give themselves to feebleness and idleness.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="adolescentis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Adolescentu[m] officiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The duty of the adolescents.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="senum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Senes.</marginalia_text>
                </position>
            </language>
            <translation>Old men.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Senibus aute[m] labores">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Senu[m] officiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The duty of old men.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Luxuria">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Luxuria</marginalia_text>
                </position>
            </language>
            <translation>Luxury.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="libidinum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Libido</marginalia_text>
                </position>
            </language>
            <translation>Desire.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="magistratuum, de priuatorum, de ciuium, de peregrinoru[m] officiis">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Magistratus officiu[m]</marginalia_text>
                </position>
            </language>
            <translation>The duty of a magistrate.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Priuatum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Priuatus.</marginalia_text>
                </position>
            </language>
            <translation>Private.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Peregrini">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Peregrinus</marginalia_text>
                </position>
            </language>
            <translation>Foreigner.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="constantiam">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Constantia</marginalia_text>
                </position>
            </language>
            <translation>Perseverance.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Corpus n[ost]r[u]m.</marginalia_text> 
                </position>
            </language>
            <translation>Our body.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="fabricam imitata est hominu[m] verecundia">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ho[m]i[nu]m verecu[n]dia, natura[e] fabrica[m] est imitata</marginalia_text>
                </position>
            </language>
            <translation>'Man's modesty has followed this careful contrivance of nature' [Walter Miller, 1913].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="petulantia vacat, nec orationis obscoenitas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Petula[n]tia. or[ati]o[n]is obsco[e]nitas</marginalia_text>
                </position>
            </language>
            <translation>Insolence. The indecency of speech.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cynici">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Cynici.</marginalia_text>
                </position>
            </language>
            <translation>Cynics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Staus, incessus, sessio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>status. incessus. sessio.</marginalia_text>
                </position>
            </language>
            <translation>Standing still, walking, setting.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="histrionibus oratorib&uacute;sque concedendum est, vt his haec apta sint, nobis dissoluta. Scenicorum quidem mos tanta[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Ergo histrio hoc videbit in scena q[uod] sapiens vir non videbit in vita?</marginalia_text>
                </position>
            </language>
            <translation>Therefore this actor will see in the theater what a wise man will not see in life.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="verecundia[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Verecundia</marginalia_text>
                </position>
            </language>
            <translation>Shame.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pulchritudinis duo genera">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Pulchritudinis duo genera</marginalia_text>
                </position>
            </language>
            <translation>Two kinds of excellence.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="abhorret ab oculorum auri&uacute;mque approbatione">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>contego abdo. occulto. removere ab oculis abhorret ab oculor[um] auriumq[ue] approbatione</marginalia_text>
                </position>
            </language>
            <translation>To conceal, head, cover up. To remove from [one's] eyes. 'Is offensive to our eyes and ears' [Walter Miller, 1913].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="effoeminatum aut molle, &amp; nequid durum aut rusticum">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>effo[e]minatu[m] molle duru[m] rusticu[m].</marginalia_text>
                </position>
            </language>
            <translation>Effeminate, over-nice, coarse, boorish [Walter Miller, 1913].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="venustatem">
            <language ident="EN">
                <position place="intext" book_orientation="0">
                    <marginalia_text>beawtie</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dignitatem">
            <language ident="LA">
                <position place="intext" book_orientation="0">
                    <marginalia_text>cumlines</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="distictione"/>
        <underline method="pen" type="straight" language="LA" text="Ineuntis enim aetatis inscitia, senum constituenda &amp; regenda prudentia"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; libidnibus arcenda est, exerce[n]d&aacute;que in labore patienti&aacute;que &amp; animi &amp; corporis: vt eorum"/>
        <underline method="pen" type="straight" language="LA" text="quod erit facilius, si in eiusmodi quidem rebus maiores natu velint interesse"/>
        <underline method="pen" type="straight" language="LA" text="languori se desidi&aacute;eque dedat"/>
        <underline method="pen" type="straight" language="LA" text="dedecus concipit"/>
        <underline method="pen" type="straight" language="LA" text="impundentiorem intempera[n]tiam"/>
        <underline method="pen" type="straight" language="LA" text="deber&eacute;que eius dignitate, &amp; decus sustinere"/>
        <underline method="pen" type="straight" language="LA" text="summissum &amp; abiectum"/>
        <underline method="pen" type="straight" language="LA" text="nihil praeter suum negocium agere, nihil de alieno inquirere, minim&eacute;que esse in aliena repub. curiosum"/>
        <underline method="pen" type="straight" language="LA" text="esset species honesta, eam"/>
        <underline method="pen" type="straight" language="LA" text="eas contexit atque abdidit. Hanc"/>
        <underline method="pen" type="straight" language="LA" text="occultissime pareant"/>
        <underline method="pen" type="straight" language="LA" text="qu&oacute;dque facere turpe non est, modo occulte, id dicere, obscoenum est"/>
        <underline method="pen" type="straight" language="LA" text="liberis dare operam"/>
        <underline method="pen" type="straight" language="LA" text="nequid effoeminatum aut molle, &amp; nequid durum aut rusticum"/>
        <underline method="pen" type="straight" language="LA" text="vt his haec apta sint, nobis dissoluta"/>
        
