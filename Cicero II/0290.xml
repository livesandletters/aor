    <annotation>
        
        
        <marginalia hand="Italian" anchor_text="CONCLVSIONES; tripertitae sunt">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Conclusiones tripertita[e].</marginalia_text>
                </position>
            </language>
            <translation>Threefold conclusions.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="In quatuor locis vti possumus conclusionibus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>In quot &amp; quib[us] locis vti co[n]clusionibus possimus.</marginalia_text>
                </position>
            </language>
            <translation>In how many and in which places we can use conclusions.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Enumeratio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Enumeratio.</marginalia_text>
                </position>
            </language>
            <translation>Enumeration.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Item curandum est, ne aut ab exordio, aut &agrave; narratione repetatur orationis enumeratio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>vnde enumeratio exordietur.</marginalia_text>
                </position>
            </language>
            <translation>From where the enumeration will begin.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Amplificatio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Amplificatio.</marginalia_text>
                </position>
            </language>
            <translation>Amplification.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Loci co[m]munes ex dece[m] praeceptis co[m]modissime sumuntur adauge[n]di criminis causa">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Loci co[mmun]es adaugendi criminis caussa ex decem praeceptis.</marginalia_text>
                </position>
            </language>
            <translation>Commonplaces from ten formulae in order to amplify accusations .</translation>  <!-- DO: Not sure about translation - how 'adaugendi' fits in - 'Commonplaces to be amplified with the cause...'? NBW: This isn't as literal a translation, but I think adaugendi criminis are attached to causa in the ablative "in the case of augmenting an accusation"  -->
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="singulare esse maleficium, spurcum, nefarium, inusitatum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>maleficiu[m] singulare spurcum nefarium invsitatum</marginalia_text>
                </position>
            </language>
            <translation>A singular, base, nefarious and unheard of crime.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Misericordia co[m]mouebitur auditoris animus">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quo modo auditoris a[n]i[m]us misericordia mouebitur.</marginalia_text>
                </position>
            </language>
            <translation>How the spirit of the listener will be moved by pity.</translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="Item curandum est, ne aut ab exordio, aut &agrave; narratione repetatur orationis"/>
        <underline method="pen" type="straight" language="LA" text="dicemus volu[n]tario facinori nulla[m] esse excusatione[m], sed imprude[n]tiae"/>
        <underline method="pen" type="straight" language="LA" text="stuprare ingenuam"/>
        <underline method="pen" type="straight" language="LA" text="Commiserationem breuem esse oportet. Nihil enim lachryma"/>
        
        
