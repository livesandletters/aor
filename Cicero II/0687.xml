    <annotation>
        
        <marginalia hand="Italian" anchor_text="obiiciebatur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Motus mortis.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The commotion of death.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nam efficit hoc philosophia: medetur animis, inanes solicitudines detrahit, cupiditatibus liberat, pellit timores">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Philosophia[e] co[m]moditates.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The utility of philosophy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="FORTES enim non modo fortuna adiuuat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Fortes fortuna adiuuat
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Fortune aids the powerful.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ratio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ratio
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Reason.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Philosophi inani ostentatione laborant pleriq[ue] omnes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Foolish philosophers and many more work by showing off.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="vt cum eorum vita mirabiliter pugnet oratio">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ne cu[m] vita pugnet Or[ati]o.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Lest the speech fights with life.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="philosophus in ratione vitae peccans">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Philosophus magister vita[e]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A philosopher is a teacher of life.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nam vt agri no[n] omnes frugiferi sunt qui colu[n]tur ... animi non omnes culti fructu[m] feru[n]t">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Animi no[n] omnes culti fructum ferent, vt agri no[n] omnes su[n]t frugiferi qui coluntur.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Not all minds bear fruit of the farmed lands, so that not all tilled fields are fruit-bearing.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cultura aute[m] animi philosophia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Animi Cultura Philosophia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Training of the mind through philosophy.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Propositum de quo est dissere[n]du[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A practive of which one should learn.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Ratione[m] quo ea me cunq[ue] ducet seq[ua]r
                    </marginalia_text>
                </position>
            </language>
            <translation>
                I shall follow the reasoning through which she will lead me wherever.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aristippus non dubitauit dolorem summum malum dicere. 
            Dehinc ad hanc eneruatam muliebr&eacute;mque sententiam satis docilem se Epicurus praebuit. Hunc post, Rhodius Hieronymus dolore vacare summum bonum dixit, 
            tantum in dolore duxit mali. Caeteri praeter Zenonem, Aristonem, Pyrrhonem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Aristippus. Epicurus, Rhodius, Hieronimus. Zeno Aristo. Pyrho.
                    </marginalia_text>
                    <person name="Aristippus"/>
                    <person name="Epicurus"/>
                    <person name="Hieronymus of Rhodes"/>
                    <person name="Zeno of Citium"/>
                    <person name="Aristo of Chios"/>
                    <person name="Pyrrho"/>
                </position>
            </language>
            <translation>
                Aristippus, Epicurus, Hieronymus of Rhodes, Zeno of Citium, Aristo of Chios, Pyrrho.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="magistra vitae philosophia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Philosophia vita[e] magistra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Philosophy is the teacher of life.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="eneruatam muliebr&eacute;mque sententiam">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        eneruata et muliebris se[n]tentia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                An opinion weakened also by women.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="de sententia esse deiectu[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        de sente[n]tia deijci Verbo de se[n]te[n]tia desistere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To retreat from an opinion, To desist from the word of an opinion.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="obiiciebatur interdum animo metus"/>
        <underline method="pen" type="straight" language="LA" text="cogitanti, fore aliquando finem huius lucis, &amp; amissionem omnium vitae co[m]modorum"/>
        <underline method="pen" type="straight" language="LA" text="philosophia: medetur animis, inanes solicitudines detrahit, cupiditatibus liberat, pellit timores. Sed"/>
        <underline method="pen" type="straight" language="LA" text="valet multum, quum est idoneam complexa naturam. FORTES"/>
        <underline method="pen" type="straight" language="LA" text="praeceptis confirmat vim fortitudinis"/>
        <underline method="pen" type="straight" language="LA" text="forti contra mortem habita insedit"/>
        <underline method="pen" type="straight" language="LA" text="Videre licet alios tanta leuitate &amp; iactatione, his vt fuerit non didicisse melius: alios pecuniae cupidos, gloria nonnullos, 
            multos libidinum seruos, vt cum eorum vita mirabiliter pugnet oratio"/>
        <underline method="pen" type="straight" language="LA" text="hoc turpior sit, qu&ograve;d in eo ipso peccet, cuius profitetur scientiam"/>
        <underline method="pen" type="straight" language="LA" text="cuius magister esse vult, labitur, art&eacute;mque vitae professus delinquit in vita"/>
        <underline method="pen" type="straight" language="LA" text="animi non omnes culti"/>
        <underline method="pen" type="straight" language="LA" text="Atque vt in eode[m] simili verser, vt ager quanuis fertilis sine cultura fructuosus esse non potest, sic sine doctrina animus"/>
        <underline method="pen" type="straight" language="LA" text="sine altera debilis. Cultura aute[m] animi philosophia est, quae extrahit vitia radicitus, &amp; praeparat animos ad satus accipiendos, 
            e&aacute;que mandat his, &amp; (vt ita dicam) serit, quae adulta fructus vberrimos ferant. Agamus"/>
        <underline method="pen" type="straight" language="LA" text="cito de sententia esse deiectu[m]"/>
        <underline method="pen" type="straight" language="LA" text="appetendus, subeundus, excipiendus"/>
        <underline method="pen" type="straight" language="LA" text="anim&oacute;que mihi opus est non repugnante"/>
        <underline method="pen" type="straight" language="LA" text="princeps &amp; authoritate &amp; antiquitate Socraticus"/>
        <underline method="pen" type="straight" language="LA" text="hanc eneruatam muliebr&eacute;mque sententiam satis"/>
        <underline method="pen" type="straight" language="LA" text="eo magistra vitae philosophia"/>
        
