    <annotation>
        
        <marginalia hand="Italian" anchor_text="sententiam quam adamauerunt, pugnacissime defendere, qu&agrave;m sine pertinacia quod consta[n]tissime dicatur exquirer">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        pugnacissime
                        co[n]stantissime.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Very aggressively.
                Very constanly.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="cura studi&oacute;que">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        cura
                        studiu[m]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Care.
                Study.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="est obstructa difficultatibus, e&aacute;que est &amp; in ipsis rebus obscuritas, &amp; in iudiciis nostris infirmitas">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        Difficultas
                        obscuritas
                        infirmitas
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Difficulty.
                Obscurity.
                Infirmity.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="liberiores &amp; solutiores">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>
                        liberiores
                        solutioros
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Freer.
                More unrestricted.
            </translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="nec vt omnia quae praescripta &amp; quasi imperata sint, defendamus, necessitate vlla cogimur">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>vt omnia qu[a]e p[rae]scripta [sunt - deleted] et quasi imp[er]ata sunt, defendam[us], nulla necessitate cogimur</marginalia_text>
                </position>
            </language>
            <translation>'We are bound by no compulsion to support all the dogmas laid down for us almost as edicts by certain masters.' [Loeb 268, 475]</translation> 
        </marginalia>
        
     
        <marginalia hand="Italian" anchor_text="cognitio multis est obstructa difficultatibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Cognitionis difficultas</marginalia_text>
                </position>
            </language>
            <translation>A difficulty of understanding.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="neque nostrae disputationes quicquam aliud agunt, nisi vt in vtra[m]que partem dicendo &amp; audiendo eliciant, &amp; tanquam exprimant aliquid quod aut veru[m] sit, aut ad id qua[m]proxime accedat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Disputatio[n]es ciceronis cur institut[a]e.</marginalia_text>
                    <person name="Cicero"/>
                </position>
            </language>
            <translation>Why Cicero's debates were instigated.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nos probabilia multa habemus, quae sequi facile, affirmare vix possumus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>probabilia. certa.</marginalia_text>
                </position>
            </language>
            <translation>Probable. Certain.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Partes.</marginalia_text>
                </position>
            </language>
            <translation>Parts.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Leuitas aliquoru[m].</marginalia_text>
                </position>
            </language>
            <translation>The fickleness of some.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Statuere enim qui sit sapiens, vel maxime videtur esse sapientis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>statuere quid sit sapiens maxime videtur esse sapientis.</marginalia_text>
                </position>
            </language>
            <translation>To establish what is wise seems to be [a task] specifically for a wise man.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="nescio quomodo plerique errare malunt">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ho[m]ini a[n]i[m]us p[er]uersus.</marginalia_text>
                </position>
            </language>
            <translation>The perverse mind of man.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Hortensii villa">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Hortensij Villa.</marginalia_text>
                    <person name="Quintus Hortensius"/>
                </position>
            </language>
            <translation>The villa of Quintus Hortensius.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Baulos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Bauli.</marginalia_text>
                    <location name="Bauli"/>
                </position>
            </language>
            <translation>Bauli.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="in xysto">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In Xisto</marginalia_text>
                </position>
            </language>
            <translation>In the colonnade.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="vacuo animo illum audieba[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>vacuo a[n]i[m]o audire.</marginalia_text>
                </position>
            </language>
            <translation>To listen with an empty mind.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ad audiendum animos ereximus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>ad audie[n]du[m] a[n]i[m]os erexim[us].</marginalia_text>
                </position>
            </language>
            <translation>We incited our minds to listen.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Heraclitus Tyrius">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Heraclytus Tyrius.</marginalia_text>
                    <person name="Heraclitus of Tyre"/>
                </position>
            </language>
            <translation>Heraclitus of Tyre.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="leniter. Et quide[m] isti libri duo Philonis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>leniter / Duo Philonis libri</marginalia_text>
                    <person name="Philo of Larissa"/>
                </position>
            </language>
            <translation>Gently. Two books of Philo of Larissa.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="homo natura lenissimus (nihil enim poterat fieri illo mitius) stomachari tame[n] coepit">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>vel mitissimi interdum stomachantur.</marginalia_text>
                </position>
            </language>
            <translation>Even the calmest people sometimes lose their temper.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="Philone">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Philo.</marginalia_text>
                    <person name="Philo of Larissa"/>
                </position>
            </language>
            <translation>Philo of Larissa.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="si ventus esst">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>Si ventus esset.</marginalia_text>
                </position>
            </language>
            <translation>If there was a wind.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="homo sane in ista philosophia, quae nunc prop&egrave; dimissa reuocatur, probatus &amp; nobilis">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        homo in philosophia } probatus
                                              nobilis
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A man in philosophy: esteemed and noble.
            </translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="&iacute;dque summa cura studi&oacute;que co[n]quirimus"/>
        <underline method="pen" type="straight" language="LA" text="multis est obstructa difficultatibus, e&aacute;que est &amp; in ipsis rebus obscuritas, &amp; in iudiciis nostris infirmitas, vt non sine causa antiquissimi &amp; doctissimi inuenire se posse quod cuperent diffisi sint: tamen nec illi defecerunt, neque nos studium exquirendi defatigati relinquemus: neque nostrae disputationes quicquam aliud agunt, nisi vt in vtra[m]que partem dicendo &amp; audiendo eliciant, &amp; tanquam exprimant aliquid quod aut veru[m] sit, aut ad id qua[m]proxime accedat"/>
        <underline method="pen" type="straight" language="LA" text="Hoc autem liberiores &amp; solutiores sumus, qu&ograve;d"/>
        <underline method="pen" type="straight" language="LA" text="nec vt omnia quae praescripta &amp; quasi imperata sint, defendamus, necessitate vlla cogimur. Na[m] caeteri prim&ugrave;m ant&egrave; tenentur astricti, qu&agrave;m quid esset optimu[m] iudicare potuerunt. Deinde infirmissimo tempore aetatis aut obsecuti amico cuidam, aut vna alicuius, quem primu[m] audieru[n]t, oratione capti, de rebus incognitis iudicant, &amp; ad quancu[m]que sunt disciplinam quasi te[m]pestate delati, ad eam tanqua[m] ad saxum adhaerescunt"/>
        <underline method="pen" type="straight" language="LA" text="Statuere enim qui sit sapiens, vel maxime videtur esse sapientis"/>
        <underline method="pen" type="straight" language="LA" text="Sed nescio quomodo plerique errare malunt, e&aacute;mque sententiam quam adamauerunt, pugnacissime defendere, qu&agrave;m sine pertinacia quod consta[n]tissime dicatur exquirere"/>
        <underline method="pen" type="straight" language="LA" text="si ventus esset"/>
        <underline method="pen" type="straight" language="LA" text="tum eode[m] in spatio co[n]sedimus"/>
        <underline method="pen" type="straight" language="LA" text="heri, inquit, id quod quaerebatur pen&egrave; explicatu[m] est, vt tota fer&egrave; quaestio tractata videatur, tamen expecto ea quae tu pollicitus es Luculle, ab Antiocho audita dicturum"/>
        <underline method="pen" type="straight" language="LA" text="Totam enim rem Lucullo integram seruari"/>
        <underline method="pen" type="straight" language="LA" text="me enim ea quae in promptu erant, dicta sunt: &agrave; Lucullo aute[m] reconditiora desydero"/>
        <underline method="pen" type="straight" language="LA" text="Hortensi co[n]turbat me expectatio tua"/>
        <underline method="pen" type="straight" language="LA" text="etsi nihil est his qui placere volunt, tam aduersarium. sed"/>
        <underline method="pen" type="straight" language="LA" text="quibus si no[n] fuerint, non vinci me malim, qu&agrave;m vincere"/>
        <underline method="pen" type="straight" language="LA" text="se causa habet, etsi hesterno sermone labefacta est, mihi tamen videtur esse verissima"/>
        <underline method="pen" type="straight" language="LA" text="ad audiendum animos ereximus"/>
        <underline method="pen" type="straight" language="LA" text="prop&egrave; dimissa reuocatur"/>
        <underline method="pen" type="straight" language="LA" text="primu[m] in Antiochi manus venerant"/>
        <underline method="pen" type="straight" language="LA" text="homo natura lenissimus (nihil enim poterat fieri illo mitius) stomachari tame[n] coepit. Mirabar"/>
        <underline method="pen" type="straight" language="LA" text="Heracliti memoria[m] implorans"/>
        <underline method="pen" type="straight" language="LA" text="illos libros dicere[n]t descripsisse"/>
        
