    <page filename="0582.tif" reader="John Dee" pagination="24" signature="[Bb.iiii.[v]]"/>   
    <annotation>
        
        <marginalia hand="Italian" anchor_text="Maiora fortasse quaeris. Quid potest esse sole maius, que[m] mathematici amplius duodeuiginti partibus confirmant maiorem esse qu&agrave;m terra[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Magnitudo.</marginalia_text>
                </position>
            </language>
            <translation>Magnitude.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="credulo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Credulus.</marginalia_text>
                </position>
            </language>
            <translation>Credulous.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quatuor sunt capita, quae concludunt nihil esse quod nosci, percipi, comprehe[n]di possit">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quatuor capita qua[e] concludu[n]t o[m]n[i]u[m] reru[m] ignorationem.</marginalia_text>
                </position>
            </language>
            <translation>Four chapters which conclude that people are ignorant of all things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Qui igitur P. Seruilium Geminu[m] videbatm si Quintum se videre putabat">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        P Seruilius gemin[us]
                        Quintus.
                    </marginalia_text>
                    <person name="Publius Servilius Geminus"/>
                    <person name="Quintus Servilius Geminus"/>
                </position>
            </language>
            <translation>
                Publius Servilius Geminus. Quintus Servilius Geminus.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="similitudinem in rerum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Similitudo reru[m].</marginalia_text>
                </position>
            </language>
            <translation>A similarity in things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Nullum esse pilum omnibus rebus talem qualis sit pilus alius, nullum granum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pilus
                        granu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A hair. A grain.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="An tibi erit quaerendus annularius aliqui, quoniam gallinarium inuenisti Deliacum illum; Pictor">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Anularius
                        gallinarius.
                        Pictor.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A ring-maker. A poultry-farmer. A painter.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qua[n]do? cur? quomodo?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Qua[n]do, cur, q[uo]modo.</marginalia_text>
                </position>
            </language>
            <translation>When, why, how.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Chrysippo">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Chrysippus.</marginalia_text>
                    <person name="Chrysippus"/>
                </position>
            </language>
            <translation>Chrysippus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nota; notione">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        nota
                        notio ]
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A mark.
                A mark of acquaintance.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="annularius aliqui">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>aliqui anularius</marginalia_text>
                </position>
            </language>
            <translation>A certain ring-maker.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="mathematici amplius duodeuiginti partibus confirmant maiorem esse qu&agrave;m terra[m]"/>
        <underline method="pen" type="straight" language="LA" text="pedalis"/>
        <underline method="pen" type="straight" language="LA" text="quum ille sol, qui tanta incitatione fertur, vt celeritas eius quanta sit ne cogitari"/>
        <underline method="pen" type="straight" language="LA" text="possit, tamen nobis stare videatur"/>
        <underline method="pen" type="straight" language="LA" text="vt minuam controuersiam"/>
        <underline method="pen" type="straight" language="LA" text="paruulis"/>
        <underline method="pen" type="straight" language="LA" text="vos quibuscu[m] res est, id quoque conceditis. omnis pugna de quarto"/>
        <underline method="pen" type="straight" language="LA" text="distinctione sublata"/>
        <underline method="pen" type="straight" language="LA" text="pugnas omnino, sed cu[m] aduersario"/>
        <underline method="pen" type="straight" language="LA" text="quid habes explorati"/>
        <underline method="pen" type="straight" language="LA" text="sui generis esse, nihil esse idem quod sit aliud. Stoicu[m] est quide[m], nec admodum"/>
        <underline method="pen" type="straight" language="LA" text="Haec refelli possunt, sed pugnare nolo"/>
        <underline method="pen" type="straight" language="LA" text="notione"/>
        <underline method="pen" type="straight" language="LA" text="annularius aliqui"/>
        <underline method="pen" type="straight" language="LA" text="Sed adhibes artem aduocatam etiam sensibus"/>
        <underline method="pen" type="straight" language="LA" text="videtur contra te valere"/>
        <underline method="pen" type="straight" language="LA" text="quanto artificio esset sensus nostros ment&eacute;mque &amp;"/>
        <underline method="pen" type="straight" language="LA" text="constructionem hominis fabricata natura, cur non extimescam opinandi temeritate[m]"/>
        <underline method="pen" type="straight" language="LA" text="ista fabrica est"/>
        <underline method="pen" type="straight" language="LA" text="Tractantur ista ingeniose, disputa[n]tur etia[m] eleganter"/>
        <underline method="pen" type="straight" language="LA" text="res iam vniuersas profunda[m], de quibus volumina impleta sunt no[n] &agrave; nostris solum, sed"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; Chrysippo: de quo queri solent Stoici"/>
        <underline method="pen" type="straight" language="LA" text="quae &agrave; te dilige[n]tissime"/>
        
