    <annotation>
        
        <marginalia hand="English_secretary" anchor_text="praecipua quaeda[m]">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>pr[a]ecipua qu[a]eda[m]</marginalia_text>
                </position>
            </language>
            <translation>Certain distinctions.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="feraru[m] natura no[n] est illa quidem deprauata mala disciplina">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Natura. Disciplina</marginalia_text>
                </position>
            </language>
            <translation>Nature. Discipline.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="infantem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Infans.</marginalia_text>
                </position>
            </language>
            <translation>An infant.</translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Animaliu[m] sagacitas.</marginalia_text>
                </position>
            </language>
            <translation>The shrewdness of living things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="primis naturalibus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Prima in animali</marginalia_text>
                </position>
            </language>
            <translation>Primary in a living thing.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="ab ipso capite fluere necesse est omne[m] ratione[m] bonoru[m] &amp; maloru[m]">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Omnis r[ati]o bonoru[m] et maloru[m] ab ip[s]o capite fluit.</marginalia_text>
                </position>
            </language>
            <translation>The whole reasoning of goods and evils flows from the head itself.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="nata est sententia veterum Academicorum &amp; Peripateticorum, vt finem bonorum dicerent secundum naturam viuere, id est virtute adhibita frui primis &agrave; natura datis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Veteru[m] academicoru[m] &amp; peripateticorum Beata vita</marginalia_text>
                </position>
            </language>
            <translation>The happy life of the old Academics and Peripatetics.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="honeste viuere">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Honeste viuere</marginalia_text>
                </position>
            </language>
            <translation>To live honourably.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="tres sunt fines expertes honestatis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Tres fines expertes Honestat[is]</marginalia_text>
                </position>
            </language>
            <translation>Three ends without honour.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pyrrho, Aristo, Herillus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Pyrrho. Aristo. Herillus</marginalia_text>
                    <person name="Pyrrho"/>
                    <person name="Aristo of Chios"/>
                    <person name="Herillus"/>
                </position>
            </language>
            <translation>Pyrrho. Aristo of Chios. Herillus.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sensibus ipsis iudicari">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sensus iudices</marginalia_text>
                </position>
            </language>
            <translation>The senses, judges.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Priuataru[m] litium iudices sumus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Priuataru[m] litiu[m] Iudices sumus</marginalia_text>
                </position>
            </language>
            <translation>We are judges of private cases.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="iudicatum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iudicatum.</marginalia_text>
                </position>
            </language>
            <translation>Judgement.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="sapientia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Sapientia</marginalia_text>
                </position>
            </language>
            <translation>Wisdom.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="virtutibus, quas ratio rerum omnium dominas">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>virtutes reru[m] o[mn]i[u]m do[m]i[n]a[e].</marginalia_text>
                </position>
            </language>
            <translation>Virtues, the mistresses of all things.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="etiam atque etia[m] consyderet">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Etia[m] atq[ue] etia[m] co[n]syderet.</marginalia_text>
                </position>
            </language>
            <translation>It considers now and yet again.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Quantu[m] enim potero minuam conte[n]tiones">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Qua[n]tum potero minua[m] co[n]te[n]tiones</marginalia_text>
                </position>
            </language>
            <translation>'I will narrow the issue as far as possible' [Loeb 40, 127].</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Aristippi Cyrenaicor&uacute;mque">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Aristippus
                        Cyrenaici.
                    </marginalia_text>
                    <person name="Aristippus"/>
                </position>
            </language>
            <translation>
                Aristippus.
                The Cyrenaics.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="intellige[n]dum &amp; age[n]dum esse natum, quasi mortalem deum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Homo, mortalis Deus ad intelligendu[m] et age[n]dum natus.</marginalia_text>
                </position>
            </language>
            <translation>Man, a mortal god born for understanding and action.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="liquidius">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>liquidius</marginalia_text>
                </position>
            </language>
            <translation>Clearer.</translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="in summi boni sede">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        loco
                        sede ] summi boni.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In the place and in the seat of the greatest good.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="est veritum">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>veritu[m] est.</marginalia_text>
                </position>
            </language>
            <translation>It is true.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="de industria, aliud"/>
        <underline method="pen" type="straight" language="LA" text="sed ta[n]tum"/>
        <underline method="pen" type="straight" language="LA" text="Omne enim animal simul vt ortu[m] est, &amp; seipsum, &amp; omnes partes suas diligit: du&aacute;sque, quae maximae sunt, 
            in primis amplectitur, animum scilicet &amp; corpus, deinde vtriusque partes"/>
        <underline method="pen" type="straight" language="LA" text="praecipua quae"/>
        <underline method="pen" type="straight" language="LA" text="integritatem corporis, non valetudine[m], summae"/>
        <underline method="pen" type="straight" language="LA" text="fluere necesse est omne[m] ratione[m] bonoru[m] &amp; maloru[m]"/>
        <underline method="pen" type="straight" language="LA" text="dicerent secundum naturam viuere, id est virtute adhibita frui primis &agrave; natura datis"/>
        <underline method="pen" type="straight" language="LA" text="cum intelligentia earum"/>
        <underline method="pen" type="straight" language="LA" text="reiicient&eacute;que"/>
        <underline method="pen" type="straight" language="LA" text="vnus Aristippi"/>
        <underline method="pen" type="straight" language="LA" text="Epicuri"/>
        <underline method="pen" type="straight" language="LA" text="alter Hieronymi, Carneadis"/>
        <underline method="pen" type="straight" language="LA" text="diu abiecti"/>
        <underline method="pen" type="straight" language="LA" text="vt extrema cum initiis conuenirent"/>
        <underline method="pen" type="straight" language="LA" text="qu&ograve;d ait sensibus ipsis iudicari voluptatem bonum esse, dolorem"/>
        <underline method="pen" type="straight" language="LA" text="qu&agrave;m nobis leges permittunt"/>
        <underline method="pen" type="straight" language="LA" text="Priuataru[m] litium"/>
        <underline method="pen" type="straight" language="LA" text="Siquid mei iudicii"/>
        <underline method="pen" type="straight" language="LA" text="asperum, prope longe, stare mouere, quadratu[m] rotu[n]dum"/>
        <underline method="pen" type="straight" language="LA" text="adhibita prim&ugrave;m diuinarum humanar&uacute;mque rerum scientia, quae potest appellari"/>
        <underline method="pen" type="straight" language="LA" text="Deinde adiunctis virtutibus, quas ratio rerum omnium dominas, tu voluptatum satellites &amp; ministras esse voluisti"/>
        <underline method="pen" type="straight" language="LA" text="in summi boni sede, qua[m]"/>
        <underline method="pen" type="straight" language="LA" text="qua[m] quum honestate ornatissima[m], tu[m] etia[m] ipsis initiis naturae, &amp; totius perfectione vitae locupletata[m] videbit"/>
        <underline method="pen" type="straight" language="LA" text="liquidius"/>
        <underline method="pen" type="straight" language="LA" text="Quantu[m] enim potero minuam conte[n]tiones, omn&eacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="nulla inest virtutis adiunctio, omnino &agrave; philosophia semouendas putabo"/>
        <underline method="pen" type="straight" language="LA" text="quos non est veritum"/>
        <underline method="pen" type="straight" language="LA" text="maxima dulcedine sensum moueret"/>
        <underline method="pen" type="straight" language="LA" text="Hi no[n] viderunt vt ad cursum equum, ad ara[n]dum bouem, ad indaga[n]dum canem: sic homine[m] ad duas res, 
            vt ait Aristoteles, intellige[n]dum &amp; age[n]dum esse natum, quasi mortalem deum: contr&aacute;que vt tardam aliquam &amp; languidam pecudem"/>
                
