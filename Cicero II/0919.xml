        <marginalia hand="Italian" anchor_text="inscie[n]tia temporis">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>inscie[n]tia temporis.</marginalia_text>
                </position>
            </language>
            <translation>Ignorance of the age.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="dedeceat in illos">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>dedeceat in illos.</marginalia_text>
                </position>
            </language>
            <translation>It is becoming to those.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="meditetur, aut siquid aliud attentius cogitet">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>meditari attentius cogitare</marginalia_text>
                </position>
            </language>
            <translation>To meditate, to think more carefully.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="loci &amp; temporis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Locus. Tempus.</marginalia_text>
                </position>
            </language>
            <translation>Place and time.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="meditetur, aut si quid aliud attentius cogitet, no[n] reprehe[n]datur. at hoc idem si in co[n]uiuio faciat">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>In co[n]viuio ne mediteris.</marginalia_text>
                </position>
            </language>
            <translation>At a banquet you shall not ponder.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="parua videntur esse delicta">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Parua delicta.</marginalia_text>
                </position>
            </language>
            <translation>Trivial faults.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="multo etiam magis, quos maior &amp; melior actionum qu&agrave;m sonorum concentus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Multo melior actionu[m] q[uam] sonoru[m] concentus.</marginalia_text>
                </position>
            </language>
            <translation>Concord of actions is much better than that of sounds.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="oculorum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Oculi.</marginalia_text>
                </position>
            </language>
            <translation>Eyes.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="siquid deceat in illos, vitemus ipsi. Fit enim nescio quo modo, vt magis in allis cernamus qu&agrave;m in nobistmetipsis delinquitur">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Si quid delinquitur magis in alijs cernemus q[uam] in nobismetip[s]is</marginalia_text>
                </position>
            </language>
            <translation>What is missing we can discern so much better in others than in ourselves.</translation> 
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="adhibere doctos homines, vel etiam vsu peritos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Doctos adhibe &amp; vsu [per]itos</marginalia_text>
                </position>
            </language>
            <translation>Consult the learned and the skillful by practice.</translation>
        </marginalia>
        
