    <annotation>
        
        <marginalia hand="Italian" anchor_text="contemnendus omnis dolor ... sepeliendus dolor">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        co[n]te[m]nendus dolor o[mn]is sepeliendus dolor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                One should neglect all pain. To be buried pain.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="enuntiante commissa, prodente co[n]scios">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        enu[n]tiare co[m]missa. prodere co[n]scios
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To announce an undertaking. To create accomplices.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Prudenti&aacute;mne vis esse, sine qua ne intelligi quidem vlla virtus potest?">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Nulla virtus sine prude[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                No virtue without prudence.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Corinthiis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Corinthia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Corinthian.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="amitti non potest virtus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        virtus amitti no[n] p[otes]t.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A virtue cannot be dismissed.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="in lecto humido">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        in lecto Humido
                    </marginalia_text>
                </position>
            </language>
            <translation>
                In a damp place.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="eum opprimi dico patientia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Dolor opprime[n]dus patie[n]tia
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pain has to be oppressed with endurance.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Cetum quidem leges">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Cretum leges
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The laws of the Cretans.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Lycurgi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Lycurgus
                    </marginalia_text>
                    <person name="Lycurgus of Sparta"/>
                </position>
            </language>
            <translation>
                Lycurgus of Sparta.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Spartae">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Spartae
                    </marginalia_text>
                    <location name="Sparta"/>
                </position>
            </language>
            <translation>
                In Sparta.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="mos valet, rationon valebit? Interest aliquid inter laborem &amp; dolorem">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Mos. Ratio. Labor. Dolor.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Custom, reason, labour, pain.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="industrios ... laboriosos">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Industrij. Laboriosi.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The industrious and laborious.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="O verborum inops interdu[m], quibus abundare te semper putas, Graecia">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Gra[e]cia interdum verboru[m] inops.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Greek is sometimes destitute of words.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="C. Mario">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        C. Marius.
                    </marginalia_text>
                    <person name="Gaius Marius"/>
                </position>
            </language>
            <translation>
                Gaius Marius.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="corpora iuuenum firmari labore">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Corpora Labore firmari
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Bodies are hardened by labour.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        fo[e]minaru[m] Labores et exercitationes
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Tasks and exercises of women.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="labor quasi callum quoddam obducit dolori">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Labor callum obducit dolori.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Laour brings callousness to pain.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Militiam vero nostram dico non Spartiatarum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Spartiataru[m] Militia.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The military service of the Spartans. 
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="exercitus">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Exercitus
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Army.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Arma enim membra militis">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Arma militis membra
                    </marginalia_text>
                </position>
            </language>
            <translation>
                The arms of the soldiers.
            </translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="exercitatio legionum">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>
                        Legionu[m] exercitatio.
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Exercise of the troops.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="varices">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        Varices
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Varicose veins.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        pungit dolor. fodit
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Paint stings [and] stabs.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="callum quoddam obducit">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        callum obducere
                    </marginalia_text>
                </position>
            </language>
            <translation>
                To lead to callousness.
            </translation>
        </marginalia>
        
        <marginalia hand="English_secretary" anchor_text="dolor intercurrit">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        hij intercurrit dolor
                    </marginalia_text>
                </position>
            </language>
            <translation>
                Pain moves them.
            </translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="Prudenti&aacute;mne vis esse, sine qua ne intelligi quidem vlla virtus potest? Quid"/>
        <underline method="pen" type="straight" language="LA" text="enuntiante commissa"/>
        <underline method="pen" type="straight" language="LA" text="Afflict&uacute;sne &amp; iacens &amp; lamentabili voce deplorans audies"/>
        <underline method="pen" type="straight" language="LA" text="sepeliendus dolor"/>
        <underline method="pen" type="straight" language="LA" text="in lecto humido"/>
        <underline method="pen" type="straight" language="LA" text="Eiulatu, questu, gemitu, fremitibus, Resonando multum flebiles voces refert"/>
        <underline method="pen" type="straight" language="LA" text="si modo est aliqua patientia: si nulla"/>
        <underline method="pen" type="straight" language="LA" text="nomine gloriosi sumus"/>
        <underline method="pen" type="straight" language="LA" text="it&eacute;mque Lycurgi, laboribus erudiunt iuuentuem venando, currendo, esuriendo, sitiendo, algendo, aestuando"/>
        <underline method="pen" type="straight" language="LA" text="non modo nemo exclamauit vnquam, sed ne ingemuit quidem"/>
        <underline method="pen" type="straight" language="LA" text="sunt finitima omnino, sed"/>
        <underline method="pen" type="straight" language="LA" text="Labor, est functio quaedam vel animi vel corporis grauioris operis &amp; muneris. Dolor autem motus asper in corpore &agrave; sensibus alienus"/>
        <underline method="pen" type="straight" language="LA" text="O verborum inops interdu[m], quibus abundare te semper putas, Graecia. Aliud"/>
        <underline method="pen" type="straight" language="LA" text="Quum varices secabantur C. Mario"/>
        <underline method="pen" type="straight" language="LA" text="Consuetudo enim laborum perpessionem dolorum efficiet faciliorem"/>
        <underline method="pen" type="straight" language="LA" text="iuuenum firmari labore voluerunt"/>
        <underline method="pen" type="straight" language="LA" text="quae caeteris in vrbibus mollissimo cultu, parietum vmbris occuluntur"/>
        <underline method="pen" type="straight" language="LA" text="Lacaenas virgines, quibus magis palaestra, Eurotas, sol, puluis, labor, militia in studio est, qu&agrave;m fertilitas"/>
        <underline method="pen" type="straight" language="LA" text="&amp; dolor intercurrit. No[n]nunquam impelluntur, feruntur, abiiciuntur, cadunt"/>
        <underline method="pen" type="straight" language="LA" text="ipse labor quasi callum quoddam obducit"/>
        <underline method="pen" type="straight" language="LA" text="quantus agminis ferre plus dimidiati mensis cibaria, ferre siquid ad vsum velint, ferre vallum: nam scutum, 
            gladium, galeam in onere nostri milites no[n] plus numera[n]t, qu&agrave;m humeros, lacertos, manus. Arma enim membra militis esse dicunt"/>
        <underline method="pen" type="straight" language="LA" text="expeditis armis"/>
        <underline method="pen" type="straight" language="LA" text="ex hoc ille animus in praeliis paratus ad vulnera"/>
        <underline method="pen" type="straight" language="LA" text="inexercitatum militem"/>
        
