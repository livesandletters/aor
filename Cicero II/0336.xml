    <annotation>
        
        <marginalia hand="Italian" anchor_text="&agrave; rhetoribus iis, qui elegantissimi atque artificiosissimi putati sunt">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>rhetor elegantiss[imus] artificiosiss[imus].</marginalia_text>
                </position>
            </language>
            <translation>A most artful and most skilled rhetorician.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="approbare &amp; firmare">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>approbo firmo ]</marginalia_text>
                </position>
            </language>
            <translation>I prove, I affirm.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="ab Aristotele &amp; Theophrasto">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Aristoteles. Theoprastus.</marginalia_text>
                    <person name="Aristotle"/>
                    <person name="Theophrastus"/>
                </position>
            </language>
            <translation>Aristotle. Theophrastus.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="separatum quidda[m] est &agrave; propositione approbatio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quod approbatio sit qua[e]dam res separata &agrave; propositione.</marginalia_text>
                </position>
            </language>
            <translation>That proof may be a separate entity to the premise.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="hanc velle approbare &amp; firmare nihil attinet">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Perspicue vera nihil attinet firmare.</marginalia_text>
                </position>
            </language>
            <translation>Evidently no one hesitates to affirm truths.</translation>  
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="separatum quiddam est extra assumptionem approbatio">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quod a&sharp-S;umptio sit separata res ab approbatione.</marginalia_text>
                </position>
            </language>
            <translation>That assumption may be an entity separate from proof.</translation>  
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="sapere">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Sapere.</marginalia_text>
                </position>
            </language>
            <translation>To know.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="dicendum videtur, ne temere secuti putemur, &amp; breuiter dice[n]dum"/>
        <underline method="pen" type="straight" language="LA" text="huiusmodi rebus diutius, qu&agrave;m ratio praecipiendi postulat, co[m]moremur. Si quadam"/>
        <underline method="pen" type="straight" language="LA" text="Quod enim adiu[n]gi &amp; separari ab aliquo potest, id no[n] potest idem esse, quod est id ad quod adiungitur, &amp; &agrave; quo separatur"/>
        
