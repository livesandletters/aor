    <annotation>
   
        <marginalia hand="English_secretary" anchor_text="adumbrantur">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>adumbra[n]tur.</marginalia_text>
                </position>
            </language>
           <translation>They are outlined.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="pueri, in quibus vt in speculis natura cernitur">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>In pueris tanq[uam] in speculis natura cernitur</marginalia_text>
                </position>
            </language>
            <translation>Nature is seen in boys just as in a mirror.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="qui non moueatur &amp; offensione turpitudinis, &amp; comprobatione honestatis?">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Quis no[n] mouetur offensione turpitudinis, co[m]p[ro]batione Honestatis?</marginalia_text>
                </position>
            </language>
            <translation>Who is not moved to offense by baseness, to approval by honour?</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Pullum Numitorem, Fregellanum proditorem; Codrum; Erichthei filias; Tubuli; Aristidem">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>
                        Pullus Numitor
                        Fregellanus.
                        Codrus.
                        Ericthei filia[e]
                        Tubulus.
                        Aristides. }
                    </marginalia_text>
                    <person name="Codrus"/>
                    <person name="Erechtheus II of Athens"/>
                    <person name="Lucius Hostilius Tubulus"/>
                    <person name="Aristides"/>
                </position>
            </language>
            <translation>
                Pullus Numitorius of Fregellae.  <!-- DO: I can't identify him -->
                Codrus.
                The daughters of Erechtheus.
                Tubulus.
                Aristides.
            </translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="clamores vulgi">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Clamores vulgi.</marginalia_text>
                </position>
            </language>
            <translation>The shouts of the common people.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="contra vtilitatem etiam conseruatur fides">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>fides. vtilitas.</marginalia_text>   <!-- DO: Not sure if capital 'v', they seem to be interchangeable -->
                </position>
            </language>
            <translation>Trust. Advantage.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Romanoru[m] res gesta[e].</marginalia_text>
                </position>
            </language>
            <translation>Things done by the Romans.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="stuprum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Stuprum.</marginalia_text>
                </position>
            </language>
            <translation>Dishonour.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="charitas">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Charitas.</marginalia_text>
                </position>
            </language>
            <translation>Affection.</translation>
        </marginalia>
        
        
        <marginalia hand="Italian">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Iusticia[e] ancilla[e] et fideles pedissequa[e].</marginalia_text>
                </position>
            </language>
            <translation>Fair maids and faithful waiting women.</translation>  <!-- DO: Not sure about 'iusticiae' -->
        </marginalia>
        
        
        <marginalia hand="Italian" anchor_text="co[n]spiratio, consens&uacute;sque virtutu[m]">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Conspiratio, et co[n]se[n]sus virtutum.</marginalia_text>
                </position>
            </language>
            <translation>The union and agreement of virtues.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="&agrave; primo satu">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>&agrave; primo satu.</marginalia_text>
                </position>
            </language>
            <translation>Straight from birth.</translation>
        </marginalia>
        
        
        <marginalia hand="English_secretary" anchor_text="co[n]spiratio, consens&uacute;sque virtutu[m]; coniunctio confusi&oacute;que virtutum">
            <language ident="LA">
                <position place="tail" book_orientation="0">
                    <marginalia_text>
                        co[n]iunctio
                        co[n]spiratio
                        co[n]se[n]sus
                        co[n]fusio ] virtutu[m].
                    </marginalia_text>
                </position>
            </language>
            <translation>
                A combination;
                a union;
                an agreement;
                a mixture ] of virtues.
            </translation>
        </marginalia>
        
        
        <underline method="pen" type="straight" language="LA" text="indicant pueri, in quibus"/>
        <underline method="pen" type="straight" language="LA" text="quae memoria est in his bene merentium"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; natura"/>
        <underline method="pen" type="straight" language="LA" text="Quis est qui non oderit libidinosam"/>
        <underline method="pen" type="straight" language="LA" text="Aristidem mortuum non"/>
        <underline method="pen" type="straight" language="LA" text="quantopere in audiendo in legend&oacute;que moueamur, quum pie"/>
        <underline method="pen" type="straight" language="LA" text="quum magno animo aliquid factum cognoscimus"/>
        <underline method="pen" type="straight" language="LA" text="conturbatio errant&iacute;que"/>
        <underline method="pen" type="straight" language="LA" text="sed contra vtilitatem etiam conseruatur fides"/>
        <underline method="pen" type="straight" language="LA" text="Imperatores pro salute patriae sua capita"/>
        <underline method="pen" type="straight" language="LA" text="&amp; quae per vim"/>
        <underline method="pen" type="straight" language="LA" text="dignitatis splendore ductos immemores fuisse vtilitatum suarum, n&oacute;sque quum ea laudemus, nulla alia re nisi honestate duci"/>
        <underline method="pen" type="straight" language="LA" text="enim sum copiam"/>
        <underline method="pen" type="straight" language="LA" text="nihil est tam illustre, nec quod latius pateat, qu&agrave;m coniunctio"/>
        <underline method="pen" type="straight" language="LA" text="&agrave; primo satu, quo"/>
        <underline method="pen" type="straight" language="LA" text="serpit sensim for&agrave;s cognationibus prim&ugrave;m"/>
        <underline method="pen" type="straight" language="LA" text="p&ograve;st vicinitatibus cum ciuibus, &amp; iis qui publice socii atque amici"/>
        <underline method="pen" type="straight" language="LA" text="deinde totius co[m]plexu gentis humanae"/>
        <underline method="pen" type="straight" language="LA" text="cui sunt adiunctae, pietas, bonitas, liberalitas, benignitas, comitas"/>
        <underline method="pen" type="straight" language="LA" text="fundet vsu in"/>
        <underline method="pen" type="straight" language="LA" text="virtutes, sic illas expetet"/>
        <underline method="pen" type="straight" language="LA" text="dico co[n]spiratio, consens&uacute;sque"/>
        <underline method="pen" type="straight" language="LA" text="haec coniunctio confusi&oacute;que virtutum"/>
        
        
