    <annotation>
        
        <marginalia hand="Italian" anchor_text="Philippum">
            <language ident="LA">
                <position place="left_margin" book_orientation="0">
                    <marginalia_text>Philippus</marginalia_text>
                    <person name="Lucius Marcius Philippus, consul 91 BC"/>   
                </position>
            </language>
            <translation>Philippus.</translation>
        </marginalia>
        
        <underline method="pen" type="straight" language="LA" text="illuc redeo"/>
        <underline method="pen" type="straight" language="LA" text="neque tanti sit illud quod prodesse videatur, vt cum aliquo malo coniu[n]gatur"/>
        <underline method="pen" type="straight" language="LA" text="expendere"/>
        <underline method="pen" type="straight" language="LA" text="prae nobis est fere[n]da"/>
        <underline method="pen" type="straight" language="LA" text="reliquae duae sicuti sanguis in corporibus, sic illae in perpetuis orationibus fusae esse debebu[n]t"/>
        <underline method="pen" type="straight" language="LA" text="vt ad eoru[m] mentes, apud quos agitur, mouendas permanare possint"/>
        <underline method="pen" type="straight" language="LA" text="digredi tamen ab eo quod proposueris atque agas"/>
        <underline method="pen" type="straight" language="LA" text="plenissimae, quae plurimos exitus dant ad eiusmodi digressionem"/>
        <underline method="pen" type="straight" language="LA" text="impellantur"/>
        <underline method="pen" type="straight" language="LA" text="reflectantur"/>
        <underline method="pen" type="straight" language="LA" text="ea prima colloca[n]t"/>
        <underline method="pen" type="straight" language="LA" text="plureis adhibe[n]t patronos"/>
        <underline method="pen" type="straight" language="LA" text="vti quenque eorum minimum putant esse, ita eum primum volunt dicere"/>
        <underline method="pen" type="straight" language="LA" text="eorum expectationi, qui audiu[n]t, quamcelerrime succurratur"/>
        <underline method="pen" type="straight" language="LA" text="Male enim se res habet, quae non statim, vt dici coepta est, melior fieri videtur"/>
        <underline method="pen" type="straight" language="LA" text="in mediam turbam atque in gregem coniiciantur"/>
        <underline method="pen" type="straight" language="LA" text="exile"/>
        <underline method="pen" type="straight" language="LA" text="nugatorium"/>
        <underline method="pen" type="straight" language="LA" text="vulgare"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mune"/>
        <underline method="pen" type="straight" language="LA" text="accurata"/>
        <underline method="pen" type="straight" language="LA" text="acuta"/>
        <underline method="pen" type="straight" language="LA" text="instructa sente[n]tiis"/>
        <underline method="pen" type="straight" language="LA" text="apta verbis"/>
        <underline method="pen" type="straight" language="LA" text="causaru[m] propria esse debe[n]t"/>
        <underline method="pen" type="straight" language="LA" text="cognitio"/>
        <underline method="pen" type="straight" language="LA" text="co[m]mendatio orationis"/>
        <underline method="pen" type="straight" language="LA" text="permulcere atque allicere debet"/>
        <underline method="pen" type="straight" language="LA" text="in primis disertum"/>
        <underline method="pen" type="straight" language="LA" text="ad dicendu[m] surgere, vt quod primum verbu[m] habiturus sit, nesciat"/>
        <underline method="pen" type="straight" language="LA" text="&amp; ait idem, cum brachium concalefecerit, tum se solere pugnare: neque attendit eos ipsos, vnde hoc simile ducat, illas primas hastas ita iactare leniter, vt &amp; venustati vel maxime seruiant, &amp; reliquis viribus suis consulant"/>
        <underline method="pen" type="straight" language="LA" text="pugnax non"/>
        <underline method="pen" type="straight" language="LA" text="sed si in ipso illo gladiatorio vitae certamine"/>
        <underline method="pen" type="straight" language="LA" text="quod ferro decernitur, tamen ante co[n]gressum multa fiunt, quae non ad vulnus, sed ad spe[ciem]"/>
        
        <mark name="vertical_bar" method="pen" place="left_margin"/>
        
