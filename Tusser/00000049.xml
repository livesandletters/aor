        <underline method="pen" type="straight" language="EN" text="broken vpland"/>
        <underline method="pen" type="straight" language="EN" text="with water opprest"/>
        <underline method="pen" type="straight" language="EN" text="much doonged"/>
        <underline method="pen" type="straight" language="EN" text="for wheat is"/>
        <underline method="pen" type="straight" language="EN" text="winter, annoieth"/>
