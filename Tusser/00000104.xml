        <underline method="pen" type="straight" language="EN" text="all in good order, fast"/>
        <underline method="pen" type="straight" language="EN" text="what euer is needfull, to find by and by"/>
        <underline method="pen" type="straight" language="EN" text="houses and barnes, would"/>
        <underline method="pen" type="straight" language="EN" text="things amended, er haruest come"/>
        <underline method="pen" type="straight" language="EN" text="Things thus set in order, in quiet"/>
        <underline method="pen" type="straight" language="EN" text="shall further thy haruest, and pleasure"/>
        <underline method="pen" type="straight" language="EN" text="bushes and thorne, with the shrubs"/>
        <underline method="pen" type="straight" language="EN" text="woodsere"/>
        <underline method="pen" type="straight" language="EN" text="sommer, cut downe"/>
        <underline method="pen" type="straight" language="EN" text="danger in woodsere, let hacking alone"/>
