    <annotation>
        
        <marginalia hand="English_secretary">
            <language ident="EN">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Weaning</marginalia_text>
                </position>
            </language>
        </marginalia>
        
        <underline method="pen" type="straight" language="EN" text="grubbing or pulling of"/>
        <underline method="pen" type="straight" language="EN" text="now stub vp the bushes"/>
        <underline method="pen" type="straight" language="EN" text="so hack them beliue"/>
        <underline method="pen" type="straight" language="EN" text="thy bushes, nor pasture"/>
        <underline method="pen" type="straight" language="EN" text="with turfes"/>
        <underline method="pen" type="straight" language="EN" text="euerie hole"/>
        <underline method="pen" type="straight" language="EN" text="labour is little, the profit is gay"/>
        <underline method="pen" type="straight" language="EN" text="stones, go and gather vp cleene"/>
        <underline method="pen" type="straight" language="EN" text="hurting of sieth, or for harming of"/>
        <underline method="pen" type="straight" language="EN" text="New prowler"/>
        <underline method="pen" type="straight" language="EN" text="frost is at hardest, then carriage is best"/>
        <underline method="pen" type="straight" language="EN" text="broome or good pasture"/>
        <underline method="pen" type="straight" language="EN" text="warme barth and in"/>
        <underline method="pen" type="straight" language="EN" text="often well to them, for foxes"/>
        <underline method="pen" type="straight" language="EN" text="and for brembles, for vermin and hogs"/>
        <underline method="pen" type="straight" language="EN" text="daintie"/>
        <underline method="pen" type="straight" language="EN" text="sooner the better, for"/>
        <underline method="pen" type="straight" language="EN" text="yeerly by twinning,"/>
        <underline method="pen" type="straight" language="EN" text="lamb of such twinners, for breeders"/>
        <underline method="pen" type="straight" language="EN" text="twinlings be twiggers, encrease"/>
        <underline method="pen" type="straight" language="EN" text="twigging, Peccantem"/>
        <underline method="pen" type="straight" language="EN" text="huswife to reare, or"/>
        <underline method="pen" type="straight" language="EN" text="no rearing, but"/>
        
        <underline method="pen" type="straight" language="EN" text="Stubbing"/>
        <underline method="pen" type="straight" language="EN" text="Rearing of lambs."/>
        <underline method="pen" type="straight" language="EN" text="Rearing"/>
        
        <mark name="bracket" method="pen" place="right_margin" language="EN" text="Looke often well to them, for foxes and dogs: for pits and for brembles, for vermin and hogs."/>
        <mark name="bracket" method="pen" place="right_margin" language="EN" text="Ewes yeerly by twinning, rich maisters doo make, the lamb of such twinners, for breeders go take."/>
        <mark name="plus_sign" method="pen" place="right_margin" language="EN" text="Rearing of lambs."/>
        <mark name="bracket" method="pen" place="right_margin" language="EN" text="Calues likely that come, between Christmas &amp; Lent, take huswife to reare, ro else after repent."/>
        
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="grubbing"/>
        <mark name="plus_sign" method="pen" place="intext" language="EN" text="now stub vp the bushes"/>
