    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="drie vp, old mulley thy"/>
        <underline method="pen" type="straight" language="EN" text="crooked and aged, to fatting put now."/>
        <underline method="pen" type="straight" language="EN" text="Then sell to thy profit, both butter"/>
        <underline method="pen" type="straight" language="EN" text="buieth it sooner, the more"/>
        <underline method="pen" type="straight" language="EN" text="looke brownish, then are"/>
        <underline method="pen" type="straight" language="EN" text="discharged, of euerie"/>
        <underline method="pen" type="straight" language="EN" text="vntangled, aboue"/>
        <underline method="pen" type="straight" language="EN" text="soutage or haier"/>
        <underline method="pen" type="straight" language="EN" text="sauing and riddance, is husbandrie best"/>
        <underline method="pen" type="straight" language="EN" text="skilfullie drieth, their hops"/>
        <underline method="pen" type="straight" language="EN" text="on a soller, oft turning"/>
        <underline method="pen" type="straight" language="EN" text="dried, will abide, foule weather"/>
        <underline method="pen" type="straight" language="EN" text="drieng and lieng, in loft doo dispaire"/>
        <underline method="pen" type="straight" language="EN" text="close them up drie in a hogshed"/>
        <underline method="pen" type="straight" language="EN" text="canuas or soutage, is better than that"/>
        
        <underline method="pen" type="straight" language="EN" text="or selling of butter"/>
        <underline method="pen" type="straight" language="EN" text="K&eacute;eping of hops"/>
        
