    <annotation>
        
        <underline method="pen" type="straight" language="EN" text="that thine is lawfullie"/>
        <underline method="pen" type="straight" language="EN" text="furnish house with housholdry"/>
        <underline method="pen" type="straight" language="EN" text="none to k&eacute;epe for brauerie"/>
        <underline method="pen" type="straight" language="EN" text="suffer none liue idlelie"/>
        <underline method="pen" type="straight" language="EN" text="k&eacute;epe no more but n&eacute;edfullie"/>
        <underline method="pen" type="straight" language="EN" text="To raise betimes the lubberlie"/>
        <underline method="pen" type="straight" language="EN" text="walke thy pastures vsuallie"/>
        <underline method="pen" type="straight" language="EN" text="ill neighbours subtiltie"/>
        <underline method="pen" type="straight" language="EN" text="hate reuengement hastilie"/>
        <underline method="pen" type="straight" language="EN" text="neighbor neighborly"/>
        <underline method="pen" type="straight" language="EN" text="answere stranger ciuilie"/>
        <underline method="pen" type="straight" language="EN" text="shew him not thy secresie"/>
        <underline method="pen" type="straight" language="EN" text="offer no man villeny"/>
        <underline method="pen" type="straight" language="EN" text="learne how foe to pacifie"/>
        <underline method="pen" type="straight" language="EN" text="him not too trustilie"/>
        <underline method="pen" type="straight" language="EN" text="keepe thy touch substanciallie"/>
        
        <underline method="pen" type="straight" language="EN" text="make thy bandes aduisedly"/>
        <underline method="pen" type="straight" language="EN" text="com not bound through suerty"/>
        <underline method="pen" type="straight" language="EN" text="To meddle not with vsurie"/>
        <underline method="pen" type="straight" language="EN" text="liuing shiftingly"/>
        <underline method="pen" type="straight" language="EN" text="treason, endeth horribly"/>
        <underline method="pen" type="straight" language="EN" text="to eschew ill co[m]pany"/>
        <underline method="pen" type="straight" language="EN" text="To stop mischa[n]ce, through policy"/>
        <underline method="pen" type="straight" language="EN" text="beare thy crosses paciently"/>
        <underline method="pen" type="straight" language="EN" text="to k&eacute;epe from miserie"/>
        <underline method="pen" type="straight" language="EN" text="age, comming"/>
        <underline method="pen" type="straight" language="EN" text="liue in conscience quietly"/>
        <underline method="pen" type="straight" language="EN" text="To ease thy sicknes sp&eacute;edilie"/>
        <underline method="pen" type="straight" language="EN" text="er helpe be past recouerie"/>
        <underline method="pen" type="straight" language="EN" text="for witches prooue vnluckilie"/>
        <underline method="pen" type="straight" language="EN" text="to climbe to thrift, by husbandrie"/>
        <underline method="pen" type="straight" language="EN" text="both reach"/>
        <underline method="pen" type="straight" language="EN" text="teach"/>
        <underline method="pen" type="straight" language="EN" text="by thrift"/>
        <underline method="pen" type="straight" language="EN" text="to shift"/>
        
