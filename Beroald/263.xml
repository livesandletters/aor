        <marginalia hand="Italian" anchor_text="de auro Schaba" id="RCPD126.6:263:m0001">
            <language ident="LA">
                <position place="head" book_orientation="0">
                    <marginalia_text>Quid si fuerit de auro Ophir prope Schaba in Oreente?</marginalia_text> 
                    <location name="Ophir"/>
                    <location name="Saba"/>
                </position>
            </language>
            <translation>What if it was about the gold [of?] Ophir near Saba in the Orient?</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Scheba &amp; Seba; Schaba &amp; Saba">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Schaba et Seba.</marginalia_text>
                    <location name="Saba"/>
                    <location name="Sheba"/>
                </position>
            </language>
            <translation>Saba and Sheba.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Iosephi">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Iosephus. Regina[m] &AElig;thiopia[e] vult esse [Quae?] ad [?] venit</marginalia_text>  <!-- DO: Can't make some of this out, and not sure about translation --> 
                    <!-- FSF: I am not sure either; the first word missing ends with a d, maybe quid?, and the second one starts sat, I think -->  <!-- DO: Second one 'Salomonem' or 'Salomone[m]'? -->
                    <person name="Titus Flavius Josephus"/>
                    <person name="Queen of Sheba"/>
                    <person name="Solomon"/>
                </position>
            </language>
            <translation>Josephus. She wants to be Queen of Ethiopia, who came to Solomon.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Qu&agrave;m etiam fallantur qui istam regionem Heuila, sunt interpretati Indiam">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ista Seba no[n] est in India.</marginalia_text>
                    <location name="Sheba"/>
                    <location name="India"/>
                </position>
            </language>
            <translation>That Sheba is not in India.</translation>
        </marginalia>
        
        <marginalia hand="Italian" anchor_text="Ptolomaeo">
            <language ident="LA">
                <position place="right_margin" book_orientation="0">
                    <marginalia_text>Ptoloma[e]o</marginalia_text>
                    <person name="Ptolemy"/>
                </position>
            </language>
            <translation>By Ptolemy.</translation>
        </marginalia>
        
